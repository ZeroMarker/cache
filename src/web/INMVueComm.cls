/// create:
/// description: 计算时间差
/// other: w ##class(web.INMVueComm).timeDifference("2017-08-28","08:00","2017-08-28","12:00")
ClassMethod timeDifference(stdate, sttime, enddate, endtime)
{
	q:(stdate="")!(enddate="")!(stdate>enddate) ""
	s:stdate["-" stdate=$zdh(stdate,3)
	s:enddate["-" enddate=$zdh(enddate,3)
	s:sttime[":" sttime=$zth(sttime,1)
	s:endtime[":" endtime=$zth(endtime,1)
	i (sttime'="")&&(endtime'="") d
	.i stdate=enddate s minute=endtime-sttime
	.e  i sttime>endtime s minute=(enddate-stdate)*24*3600+(endtime+86400-sttime)
	.e  s minute=(enddate-stdate)*24*3600+(endtime-sttime)
	e  d
	.s minute=(enddate+1-stdate)*24*3600
	s day=+minute\(24*3600)
	s hour=$fn(+minute#(24*3600)/3600,"",1)
	s ret=""
	i day'=0 s ret=day_"天"
	i hour'=0 s ret=ret_hour_"小时"
	q ret
}

