ClassMethod GetQueryDataA(rset As %Library.ResultSet) As %String
{
	s $zt="errr"
	w "{""rows"":["
	s results=0
	s start=$G(%request.Data("start",1))
	s limit=$G(%request.Data("limit",1))
	i start="" s start=0
	i start<0 s start=0
	i limit=""  s limit=100000
	s outResults=0
	while rset.Next()'=0
	{
		i (results>=+start)&(results<+(start+limit)) {
			i outResults'=0 w ","
			s columns=rs.GetColumnCount()
			f col = 1:1:columns 
			{
				s aa=rset.GetData(col)
		       	s l=$L(aa,"^")
		       	s va=$P(aa,"^",l)
		       	w "{"
		       	
		       	f i=1:1:l
		       	{
			       	s vv=$P(aa,"^",i)
			     	i vv="" 
			     	{
				     	continue
			     	}
			     	s na=$P(vv,"|",1)
			     	s nv=$P(vv,"|",2,$L(vv,"|"))
			     	s nv=##Class(web.INMCommon).Escape(nv)
			     	w """"_na_""":"""_nv_""""
			     	;w """"_na_""":"""_nv_""""
                 	i va=""
                 	{
	                 	i i'=(l-1) w ","
                 	}
                 	i va'=""
                 	{
	                 	i i'=(l) w ","
                 	}
			   	}
	           	w "}"
			 }
			 s outResults=outResults+1
		}
		s results=results+1
	}
	w "],""results"":"""_results_"""}"
	q ""
errr
	s ^tmp("err")=$ze
}

