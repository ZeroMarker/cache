Class web.DHCEQVersion Extends %Library.RegisteredObject [ ClassType = "", Not ProcedureBlock ]
{

/// 基础版本信息
Parameter BaseDate = "2015-10-15";

Parameter BaseVersion = "4.1.3";

/// w ##Class(web.DHCEQVersion).GetVer()
ClassMethod GetVer()
{
	;w ..#BaseVersion
	q "BaseVersion:"_..#BaseVersion_"    BaseDate:"_..#BaseDate
}

/// d ##Class(web.DHCEQVersion).GetHistory()
ClassMethod GetHistory()
{
	;显示基础版本
	w "基础版本信息:"
	w ..GetVer(),!
	;显示基础版本安装后，在客户处修改的问题记录,按先后顺序由上至下
	w "修改记录信息:",!
	w ..ModifiedRecord("2011-10-24","JDL","修改内容","JDL001,JDL002"),!
	w ..ModifiedRecord("2011-10-26","JDL","修改内容","JDL004"),!
}

ClassMethod ModifiedRecord(ModifiedDate, ModifiedUser, ModifiedContent, ModifiedBugIDs)
{
	;q "Modified:"_ModifiedDate_"("_ModifiedUser_")  修改内容:"_ModifiedContent_"("_ModifiedBugIDs_")"
	q ""_ModifiedDate_"("_ModifiedUser_")  "_ModifiedContent_"("_ModifiedBugIDs_")"
}

