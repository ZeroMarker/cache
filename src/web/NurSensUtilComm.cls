ClassMethod SplitStr(parr As %String, s1 As %String, s2 As %String, tmp As %String) As %String
{
	q:parr="" 0
	k tmp
	s len=$L(parr,s1)
	f i=1:1:len 
	{
		s item=$P(parr,s1,i)
		continue:item=""
		s key=$P(item,s2,1)
		continue:key=""
		s val=$P(item,s2,2,$L(item,s2))
 		s tmp(key)=$tr(val,"__","||")
	}
	q $d(tmp)
}

