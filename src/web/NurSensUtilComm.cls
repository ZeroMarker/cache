ClassMethod GetQueryData(rs As %Library.ResultSet) As %String
{
	w "{""rows"":["
	s results=0
	s start=$G(%request.Data("start",1))
	s limit=$G(%request.Data("limit",1))
	i start="" s start=0
	i limit=""  s limit=100000
	s outResults=0
	while rs.Next()'=0 
	{
		i (results>=+start)&(results<+(start+limit)) 
		{
			i outResults'=0 w ","
			s columnNum=rs.GetColumnCount()
			w "{"
			f i=1:1:columnNum 
			{
				s columnName=rs.GetColumnHeader(i)
				s columnValue=rs.GetDataByName(columnName)
				;s columnValue=$zcvt(columnValue,"O","JS")
				//s columnValue=columnValue_results
				s columnValue=..FilterSpecialSymbols(columnValue)
				w """"_columnName_""":"""_columnValue_""""
				i i'=columnNum w ","
			}
			w "}"
			s outResults=outResults+1
		}
		s results=results+1
	}
	w "],""results"":"_results_"}"
	q ""
}

/// create:2019-11-12
/// description: 计算时间差
/// other: w ##class(web.NurSensUtilComm).timeDifference("2017-08-28","08:00","2017-08-28","12:00")
ClassMethod timeDifference(stdate, sttime, enddate, endtime)
{
	q:(stdate="")!(enddate="")!(stdate>enddate) ""
	s:stdate["-" stdate=$zdh(stdate,3)
	s:enddate["-" enddate=$zdh(enddate,3)
	s:sttime[":" sttime=$zth(sttime,1)
	s:endtime[":" endtime=$zth(endtime,1)
	i (sttime'="")&&(endtime'="") d
	.i stdate=enddate s minute=endtime-sttime
	.e  i sttime>endtime s minute=(enddate-stdate)*24*3600+(endtime+86400-sttime)
	.e  s minute=(enddate-stdate)*24*3600+(endtime-sttime)
	e  d
	.s minute=(enddate+1-stdate)*24*3600
	s day=+minute\(24*3600)
	s hour=$fn(+minute#(24*3600)/3600,"",1)
	b ;00
	s ret=""
	i day'=0 s ret=day_"天"
	i hour'=0 s ret=ret_hour_"小时"
	q ret
}

