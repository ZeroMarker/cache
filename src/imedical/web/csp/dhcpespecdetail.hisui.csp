<!--
 * FileName: dhcpespecdetail.hisui.csp
 * Anchor: xy
 * Date: 2019-06-25
 * Description: HISUI 标本采集信息
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<ADDINS require="CmdShell"/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	/*padding: 0 10px;*/
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 10px;
}
.newclsInvalid{
	border-color: red;
	color: red;
}
.panel-header{
	border-bottom: 0;
}


</style>
<SCRIPT language="Cache" RUNAT="SERVER"> 
 s BeginDate=$g(%request.Data("BeginDate",1))
 s EndDate=$g(%request.Data("EndDate",1))
 s Type=$g(%request.Data("Type",1)) 
</SCRIPT> 
<script type="text/javascript">
	var BeginDate="#(BeginDate)#"
	var EndDate="#(EndDate)#"
	var Type="#(Type)#"
		
</script>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:102px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:4px 4px 0 0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="BeginDate">开始日期</label></td>
					<td>
						<input id="BeginDate" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1"><label for="Name">姓名</label></td>
					<td>
						<input class="textbox" id="Name"/>
					</td>
				 	
					<td class="r-label r-label-1"><label for="Type">类型</label></td>
					<td>
						<input class="hisui-combobox" type="text" Id="Type"/>
					</td>
					<td class="r-label r-label-1"></td>
					<td>
            			<input class='hisui-checkbox' type="checkbox" Id="ShowTotal"/><label for="ShowTotal">显示合计</label>
         			 </td>
         			 
         			 <td class="r-label r-label-1">
						<a id="BPrintASpecimen" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">标本采集清单</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					
					
				</tr>
				<tr>
					<td class="r-label" ><label for="EndDate">结束日期</label></td>
					<td>
						<input id="EndDate" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1"><label for="VIPLevel">VIP等级</label></td>
					<td>
						<input class="hisui-combobox" type="text" Id="VIPLevel" style="width:155px;"/>
					</td>
					<td class="r-label r-label-1"><label for="Container">容器</label></td>
					<td>
						<select id="Container" class="hisui-combogrid"  style="width:150px;" data-options=""></select> 
					</td>
					<td class="r-label r-label-1"><td></td>
					
					<td class="r-label r-label-1">
						<a id="BGetLisResult" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-list'">获取检验结果</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
				</tr>
				
						
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="SpecDetailTab"></table>
				</div>
        	</div>
</div>


<script type="text/javascript" src="../scripts/DHCPESpecDetail.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>

  
</body>		
</html>
			
		
