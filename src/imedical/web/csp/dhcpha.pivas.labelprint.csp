<!DOCTYPE html>
<!--
	配液打签-批量
	csp: csp/dhcpha.pivas.labelprint.csp
	js:  scripts/pharmacy/pivas/labelprint.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD> </TRAK:HEAD>
    <HISUI />
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css">
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/pivas/css/ui.css">
</HEAD>

<body style="overflow:hidden;">
    <div class="dhcpha-win-mask">
        <img src="../scripts/pharmacy/common/image/page-loading.gif"></img>
    </div>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',split:true,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">
            <div class="hisui-layout" fit="true" border="false">
                <div data-options="region:'north',title:'配液打签',iconCls:'icon-print',headerCls:'panel-header-gray',split:true,collapsible:false"
                    border="true" class="dhcpha-north-r3">
                    <table class="dhcpha-north-tbl">
                        <tr>
                            <td>
                                <label for="dateStart">用药开始日期</label>
                                <input id="dateStart" class="hisui-datebox dhcpha-date" type="text" />
                            </td>
                            <td>
                                <label for="cmbLocGrp">科室组</label>
                                <input id="cmbLocGrp" type="text" />
                            </td>
                            <td>
                                <label for="cmbPivaCat">配液大类</label>
                                <input id="cmbPivaCat" type="text" />
                            </td>
                            <td>
                                <label for="cmbPriority">医嘱优先级</label>
                                <input id="cmbPriority" type="text" />
                            </td>
                            <td>
                                <label for="cmbInstruc">用法</label>
                                <input id="cmbInstruc" type="text" />
                            </td>
                            <td>

                            </td>
                        </tr>
                        <tr>
                            <td>
                                <label for="dateEnd">用药结束日期</label>
                                <input id="dateEnd" class="hisui-datebox dhcpha-date" type="text" />
                            </td>
                            <td>
                                <label for="cmbWard">病区</label>
                                <input id="cmbWard" type="text" />
                            </td>
                            <td>
                                <label for="cmbWorkType">工作组</label>
                                <input id="cmbWorkType" type="text" />
                            </td>
                            <td>
                                <label for="cmgIncItm">药品</label>
                                <input id="cmgIncItm" type="text" />
                            </td>
                            <td>
                                <label for="cmbPack">打包</label>
                                <input id="cmbPack" type="text" />
                            </td>
                            <td>

                            </td>
                        </tr>
                        <tr>
                            <td colspan="4" style="text-align:left">
                                <div id="DivBatNo">　　　　批次</div>
                            </td>
                            <td>
                                <a class="hisui-linkbutton" plain='false' iconCls='icon-w-find' id="btnFind">查询</a>
                            </td>

                        </tr>
                    </table>
                </div>
                <div data-options="region:'center'" border="false">
                    <div class="hisui-layout" fit="true">
                        <div data-options="region:'west',split:true,width:300" border="false"
                            style="border-radius: 4px;">
                            <div id="tabsTotal" class="hisui-tabs" fit="true">
                                <div data-options="title:'病区列表'">
                                    <div class="hisui-layout" fit="true" border="false">
                                        <div data-options="region:'center'" border="false">
                                            <table id="gridWard"></table>
                                        </div>
                                        <div data-options="region:'south'" border="false"
                                            style="text-align:center;padding-bottom:10px">
                                            <a class="hisui-linkbutton" iconCls='icon-w-find' plain="false"
                                                style="overflow:hidden;" id="btnFindDetail">查询</a>
                                        </div>
                                    </div>
                                </div>
                                <div data-options="title:'按登记号'">
                                    <div class="hisui-layout" fit="true" border="false">
                                        <div data-options="region:'center'" border="false">
                                            <table id="gridAdm"></table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div data-options="region:'center'" border="false" style="border-radius: 4px;">
                            <div id="tabsDetail" class="hisui-tabs" fit="true">
                                <div data-options="title:'工作组'">
                                    <table id="gridWorkType"></table>
                                </div>
                                <div data-options="title:'医嘱明细'">
                                    <div class="hisui-layout" fit="true" border="false">
                                        <div data-options="region:'center'" border="false">
                                            <table id="gridOrdExe"></table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="gridWardBar">
    </div>
    <div id="gridWorkTypeBar">
        <table>
            <tr>
                <td>
                    <a class="hisui-linkbutton" plain='true' iconCls='icon-print' id="btnPrint">打印</a>
                </td>
                <td>
                    <div class="datagrid-btn-separator"></div>
                </td>
                <td style="padding-left:10px;padding-top:2px;">
                    <input class='hisui-radio' type="radio" label='工作组顺序打印' name='wtSortGrp' value='1' checked>
                </td>
                <td style="padding-left:10px;padding-top:2px;">
                    <input class='hisui-radio' type="radio" label='工作组汇总打印' name='wtSortGrp' value='2'>
                </td>
            </tr>
        </table>
    </div>
    <div id="gridOrdExeBar" style="height:32px;padding-top:2px">
        <table>
            <tr>
                <td>
                    <span><a class="hisui-linkbutton" iconCls='icon-btn-pack' plain="true" id="btnPack">打包</a></span>
                </td>
                <td>
                    <span><a class="hisui-linkbutton" iconCls='icon-btn-pack-cancel' plain="true"
                            id="btnUnPack">取消打包</a></span>
                </td>
            </tr>
        </table>
    </div>
    <div id="gridAdmBar">
        <div>
            <div><input id="txtPatNo" type="text" class="hisui-validatebox" placeholder="登记号..." style="height:30px" />
            </div>
        </div>
    </div>
    <div id="needUserWin" class="hisui-dialog" title="选择核对人" closed="true" style="width:245px;height:210px;"
        data-options="iconCls:'icon-person',modal:true,
	buttons:[{
		text:'确定',
		handler:function(){
			NeedUserWinSave();
		}
	},{
		text:'取消',
		handler:function(){
			$('#needUserWin').window('close');
		}
	}]">
        <div class="hisui-layout" fit="true" border="false">
            <div data-options="region:'center'" class="dhcpha-hisui-container" style="padding:0px">
                <div class="hisui-layout" data-options="fit:true,border:false">
                    <div data-options="region:'center'" border="false">
                        <table style="padding-left:5px;padding-top:5px;">
                            <tr style="height:40px;display:none;">
                                <td>
                                    <label for="cmbNeedUser30">排药核对人</label>
                                </td>
                                <td>
                                    <input id="cmbNeedUser30" />
                                </td>
                            </tr>
                            <tr style="height:40px;display:none;">
                                <td>
                                    <label for="cmbNeedUser40">贴签核对人</label>
                                </td>
                                <td>
                                    <input id="cmbNeedUser40" />
                                </td>
                            </tr>
                            <tr style="height:40px;display:none;">
                                <td>
                                    <label for="cmbNeedUser50">核对核对人</label>
                                </td>
                                <td>
                                    <input id="cmbNeedUser50" />
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- 工作组窗体维护 -->
    <div id="gridPrtWayWin" class="hisui-dialog" title="打印排序方式修改" closed="true" style="width:320px;height:400px;"
        data-options="iconCls:'',modal:true,
	buttons:[{
		text:'确定',
		handler:function(){
            UpdateWorkTypePrtWay();
		}
	},{
		text:'取消',
		handler:function(){
			$('#gridPrtWayWin').window('close');
		}
	}]">
        <div class="hisui-layout" fit="true">
            <div data-options="region:'center'" border="false" style="padding:10px;padding-bottom: 0px;">
                <div class="hisui-layout" data-options="fit:true">
                    <div data-options="region:'center'" style="border:1px solid #cccccc;border-radius: 4px;">
                        <table id="gridPrtDict"></table>
                    </div>
                    <div data-options="region:'east',width:175,split:true"
                        style="border:1px solid #cccccc;border-radius: 4px;">
                        <table id="gridPrtWay"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="../scripts/pharmacy/plugins/datagrid-dnd/datagrid-dnd.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/DHCSTXMLPrint.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/print.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/labelprint.js"></script>
</body>

<server>
    d ##class(web.DHCXMLPConfig).LODOPInit()
</server>

</HTML>