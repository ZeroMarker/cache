<!-- HEAD TAG --------------------------------------------- -->

<csr:rule name="HEAD" match="TRAK:HEAD">
<csr:description>
The <b>TRAK:HEAD</b> tag defines defines standard header info and initialisation for each page. For example javascript utilties, style sheet, theme and initialisation code
</csr:description>

<csr:action>
	<SCRIPT LANGUAGE=CACHE RUNAT=SERVER>
	d ##Class(websys.Configuration).Head()
	</SCRIPT>
</csr:action>
</csr:rule>

