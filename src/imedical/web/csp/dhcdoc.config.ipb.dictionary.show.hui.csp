<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray'" style="height:52px;">
		<div class="hisui-panel" style="border:0;border-bottom:1px dashed #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label id="_HospUserListLabel" style='color:red;' class='r-label'>医院</label>
					</td>
				
					<td>
						<input id="_HospUserList" style="width:250px;"/>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',headerCls:'panel-header-gray',title:'',border:false">
		<div id="i-dicList"></div>
	</div>
</div>
<div id='i-config' class='c-config c-hidden' style="width:470px;">
	<input id="i-id" type="hidden"/>
	<input id="i-action" type="hidden" />
	<table class="search-table">
		<tr>
			<td class="r-label">
				<label>字典类型</span>
			</td>
			<td>
				<input id="i-type" class="textbox" type="text"/>
			</td>
			<td class="r-label">
				<label>是否激活</span>
			</td>
			<td>
				<input id="i-active" class='hisui-checkbox' type="checkbox" />
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label>字典代码</span>
			</td>
			<td>
				<input id="i-code" class="textbox" type="text"/>
			</td>
			<td class="r-label">
				<label>字典描述</span>
			</td>
			<td>
				<input id="i-desc" class="textbox" type="text"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label>起始日期</span>
			</td>
			<td>
				<input id="i-sdate" style="width:155px;"/>
			</td>
			<td class="r-label">
				<label>截至日期</span>
			</td>
			<td>
				<input id="i-edate" style="width:155px;"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label>备注1</span>
			</td>
			<td>
				<input id="i-note1" class="textbox" type="text"/>
			</td>
			<td class="r-label">
				<label>备注2</span>
			</td>
			<td>
				<input id="i-note2" class="textbox" type="text"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label>备注3</span>
			</td>
			<td>
				<input id="i-note3" class="textbox" type="text"/>
			</td>
			<td class="r-label">
				<label>备注4</span>
			</td>
			<td>
				<input id="i-note4" class="textbox" type="text"/>
			</td>
		</tr>
		<tr>
			<td colspan="4" style="text-align:center;">
				<a id="i-save" href="#" onclick="saveDictionary()" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
			</td>
		</tr>
	</table>
</div>