<!DOCTYPE html>
<!--药房药库 - 基药与管制药品统计
	csp:  pha.in.v3.stat.basicandpoison.csp
	js:   pha/in/v3/stat/basicandpoison.js
-->
<html>
<!-- 验证session过期 -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
</csp:method>

<head>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI />
    <PHALIBV1 />
    <style>
    	#report .kw-section-list > li{
	    	margin-right:10px;
   		}
		#report .keywords li{
			width : 132px;
		} 
	</style>
	
</head>

<body>
	<div class="hisui-panel" fit="true" border="false" style="padding:10px;">
		<div class="hisui-layout" data-options="fit:true">
			<div  data-options="region:'west',border:true,split:true,headerCls:'panel-header-gray',bodyCls:'panel-body-gray',iconCls:'icon-paper',collapsible:false" style="width:305px;" title=#(..Get("基药与管制药品统计"))# >
				<div class="hisui-layout" data-options="fit:true">
					<div data-options="region:'center',border:false">							
						<table id="div-conditions" class="pha-con-table">
							<tr>
								<td class="r-label">
									<label for="startDate"><span style="color:red">*</span>#(..Get("开始日期"))#</label>
								</td>
								<td>
									<input id="startDate" class="hisui-datebox" data-pha='class:"hisui-datebox",requied:true,query:true,clear:true' style="width:160px;"/>
								</td>
							</tr>
							<tr>
								<td class="r-label">
									<label for="endDate"><span style="color:red">*</span>#(..Get("截止日期"))#</label>
								</td>
								<td>
									<input id="endDate" class="hisui-datebox" data-pha='class:"hisui-datebox",requied:true,query:true,clear:true' style="width:160px;" />
								</td>
							</tr>
							<tr>
								<td class="r-label">
									<label for="poisonIdStr">#(..Get("管制分类"))#</label>
								</td>
								<td>
									<input id='poisonIdStr' class="hisui-combobox" data-pha='class:"hisui-combobox",query:true,clear:true'/>
								</td>
							</tr>
							<tr>
								<td class="r-label">
									<label for="inciName">#(..Get("药品别名"))#</label>
								</td>
								<td>
									<input id="inciName" placeholder="别名..." class="hisui-validatebox validatebox-text" data-pha='class:"hisui-validatebox",query:true,clear:true' style="width:153px">
								</td>
							</tr>
							<tr>
								<td class="r-label">
									<label for="inci">#(..Get("药品名称"))#</label>
								</td>
								<td>
									<input id="inci" class="hisui-combogrid" data-pha='class:"hisui-combogrid",query:true,clear:true'>
								</td>
							</tr>
						</table>
					</div>
					<div  data-options="region:'south',border:false" style="height:231px;">
						<div class="pha-line" ></div>
						<div class="pha-row" style="margin:5px 0 0 0;">
							<div class="pha-col">
								<div id="report"></div>
							</div>
						</div>
						<div class="pha-row" style="text-align:center;margin-top:5px;">
							<a href="#" id="btnFind" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">#(..Get("查询"))#</a>
							<div class="pha-col">
								<a href="#" id="btnClear" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">#(..Get("清屏"))#</a>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div data-options="region:'center',border:false">
				<csp:Include Page="pha.in.v3.stat.reptab.template.csp"></csp:Include>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/pha/in/v3/stat/com.js"></script>
	<script type="text/javascript" src="../scripts/pha/in/v3/stat/basicandpoison.js"></script>
</body>
</html>