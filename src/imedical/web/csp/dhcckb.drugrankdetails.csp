<!-- dhcckb.drugrankdetails.csp  处方审核结果分析 csp //-->
<!DOCTYPE html>
<html >
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
			<HISUI/>
			<DHCCKB/>
			<link href="../scripts/dhcnewpro/dhcckb/css/base.css" rel="stylesheet">
			<link href="../scripts/dhcnewpro/dhcckb/css/drugrankdetails.css" rel="stylesheet">
			<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>
			<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
			<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/drugrankdetails.js" charset="gb2312"></script>
	
	
</head>
<body class="background-warp">
	<div class="container">
		<div class="view-area-first">
						  <div style="padding-left:10px;color:red">当前核查药品: <span id="checkdrug"></span></div>
		</div>    		
		<div class="view-area-third">
			<div class="list-title">
				<div class="list-icon">
					<img src="../scripts/dhcnewpro/dhcckb/images/infomation.png" border=0/>
				</div>
				<label>处方明细列表</label>
				<!-- <div class="list-tools">
					<span class="icon-right icon-more"></span>
				</div> -->
   </div>
   <div class="list-order-item">
   	
   			<div id="detailgrid" toolbar="#toolbar"></div>
   		
			</div>
		</div>
	</div>
<div id="toolbar" class="datagrid-toolbar">
	<div id="queryForm" style="margin:5px;">
		<table cellspacing="0" cellpadding="0" >
			<tr>
				<td class="textLabel"><span style="margin-right:10px;margin-left:10px;">审核标记</span><input style="margin-left:10px;" class="hisui-combobox" id="sign" ></input> </td>			
				<td class="textLabel"><span style="margin-right:10px;margin-left:10px;">审核内容</span><input style="margin-left:10px;width:450px;" class="hisui-combobox" id="exares" ></input> </td>			
				<td><a href="#" style="margin-left:20px;" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" id="find">查询</a></td>
				<td><a href="#" style="margin-left:10px;" class="hisui-linkbutton" data-options="iconCls:'icon-w-update'" id="reset">重置</a></td>
			</tr>								
		</table>
	</div>
	<div  style="margin-top:2px;border-bottom-color:#cccccc;border-bottom-width:1px; border-bottom-style:dashed;">	</div>
	<table cellspacing="0" cellpadding="0">
	    <tr>
		  			<td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-save" id="save">保存</a></td>
		  			<td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-add-item" id="synchron">同步</a></td>
	    </tr>
	</table>
</div>
</body>
</html>
