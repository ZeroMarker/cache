<!-- 
 * FileName: dhcbill.conf.page.reg.csp
 * Anchor: ZhYW
 * Date: 2019-11-05
 * Description: 住院登记页面配置
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<style>
		.td-first-col-85 {
			width: 85px;
		}
		.textbox.tb250 {
			width: 250px;
		}
	</style>
	<server>
		set pageId=+%request.Get("PageId")
		set showInsuCode="SHOWINSU"
		set DOMSEQCode="DOMSEQ"
		set codeStr=showInsuCode_"^"_DOMSEQCode
		w "<script type=""text/javascript"">",!
		w "	var GV = {",!
		w "		PageId: "_pageId_",",!
		w "		CodeStr: """_codeStr_""",",!
		w "		ShowInsuCode: """_showInsuCode_""",",!
		w "		DOMSEQCode: """_DOMSEQCode_""",",!
		w "		PCSite: """",",!
		w "		PCSiteDR: """"",!
		w "	};",!
		w "</script>"
	</server>
</head>

<body>
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',border:false" style="height:50px;">
			<div class="messager-popover info" style="padding:5px;top:10px;float:left;left:10px;right:10px;">
				<span class="messager-popover-icon info"></span>
				<span>若住院登记页面元素有变动，需先删除配置记录并刷新住院登记页面缓存DOM元素后重新配置</span>
			</div>
		</div>
		<div data-options="region:'center',border:false">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false">
					<div style="padding:0 0 10px 10px;">
						<label style="padding-right:7px;">医院</label>
						<input id="hospital" class="hisui-combobox textbox"/>
					</div>
				</div>
				<div data-options="region:'west',border:false" style="width:430px;padding-right:10px;">
					<div class="hisui-layout" data-options="fit:true">
						<div data-options="region:'center',border:false" style="padding: 0 0 10px 10px;">
							<div id="dl-tb" style="padding-top:3px;">
								<table cellspacing="0" cellpadding="0">
									<tbody>
										<tr>
											<td colspan="3">
												<label style="padding-left:10px;padding-right:7px;">入院登记和医保登记是否在同一页面操作</label>
												<div id="showInsuFlag"></div>
											</td>
										</tr>
										<tr>
											<td><a href="javascript:;" class="hisui-linkbutton" iconCls="icon-arrow-top" id="btn-moveUp" plain="true">上移</a></td>
											<td><a href="javascript:;" class="hisui-linkbutton" iconCls="icon-arrow-bottom" id="btn-moveDown" plain="true">下移</a></td>
											<td><span style="color:#FF0000;padding-left:20px;">提示：上下移动可调整回车跳转顺序</span></td>
										</tr>
									</tbody>
								</table>
							</div>
							<table id="domList"></table>
						</div>
					</div>
				</div>
				<div data-options="region:'center',border:false">
					<div class="hisui-panel" data-options="border:false,fit:true" style="padding:0 10px 10px 0;">
						<table id="pageConfList"></table>
						<div id="pl-tb">
							<a href="javascript:;" id="btn-delete" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true">删除</a>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div data-options="region:'south',border:false">
			<div style="text-align:center;padding-bottom:10px;">
				<a href="javascript:;" id="btn-save" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
			</div>
		</div>
	</div>
	<div id="modeDlg" style="width:400px;height:190px;display:none;">
		<table class="search-table" style="width:100%;">
			<tr>
				<td class="r-label"><label class="clsRequired">保存模式</label></td>
				<td><input id="site" class="hisui-combobox textbox tb250"/></td>
			</tr>
			<tr>
				<td class="r-label"><label class="clsRequired">安全组/医院</label></td>
				<td><input id="siteId" class="hisui-combobox textbox tb250"/></td>
			</tr>
		</table>
	</div>
	<script type="text/javascript" src="../scripts/dhcbillconfig/dhcbill.conf.page.reg.js"></script>
</body>
</html>
