<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!--dhcem.docpatlist.csp 医生收治病人列表-->
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<link href="../scripts/dhcnewpro/dhcem/css/consultaudit.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/consultprintcom.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/doctrepatlist.js"></script>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',headerCls:'panel-header-gray',title:'急诊医生收治病人查询',collapsible:false" style="height:90px;padding:2px;">
		<div style="margin:8px;">
			<label>开始日期</label>
			<input id="StartDate" class="hisui-datebox textbox">
			<label style="margin-left:40px;">结束日期</label>
        	<input id="EndDate" class="hisui-datebox textbox">
			<label style="margin-left:40px;">急诊医生</label>
			<input id="EmDoc" class="hisui-validatebox" style="width:138px;vertical-align:bottom;"/>
			<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-search-white'" onclick="QryPatList()" style="margin-left:30px;">查询</a>
		</div>
	</div>
	<div data-options="region:'center',headerCls:'panel-header-gray',title:''">
		<table id="bmDetList"></table>
	</div>
</div>
<!-- 病人列表 -->
<div id="PupPatWin">
	<div id="PupPatList"></div>
</div>
</body>
</html>
