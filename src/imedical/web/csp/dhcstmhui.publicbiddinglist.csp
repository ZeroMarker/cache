<!--招标级别,招标轮次-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>

<HTML XMLNS=TRAK>
<HEAD>
	<TITLE><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</HEAD>
<body class="hisui-layout" data-options="fit:true">
	<div id="DetailTabs" class="hisui-tabs tabs-gray GridPadding" data-options="region:'center',tabPosition:'left',headerWidth:100,headerCls:'panel-header-gray'" style="border:0px solid #ccc;">
		<div title="招标级别" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'center'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="PbLevelGrid"></table>
				</div>
			</div>
		</div>
		<div title="招标轮次维护" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'center',headerCls:'panel-header-gray'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="PublicBiddingList"></table>
				</div>
				<div id="PublicBiddingListTB">
					<table>
						<tr>
							<td>
								<a href="#" id="AddBT" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">新增</a>
								<a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">保存</a>
							</td>
						</tr>
					</table>
				</div>
			</div>
		</div>
		
	</div>
	<script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonHosp.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/PublicBiddingList/PublicBiddingList.js"></script>
</body>

</HTML>