<!DOCTYPE html>
<!--排班管理-请假管理-请假审批
	csp: dhcpha.pivas.leaveaudit.csp
	js:  scripts/pharmacy/pivas/leaveaudit.js
-->
<HTML XMLNS=TRAK>
<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>
	<Server language="CACHE" runat="Server">
   </Server> 
    <SCRIPT Language="Javascript">
	</script>
<style>
</style>
</HEAD>
<body >
    <div class="hisui-layout"  fit="true" border="false">
        <div data-options="region:'center',split:true,border:false,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">
        	<div class="hisui-layout" fit="true" border="false">
        		<div data-options="region:'center',split:true,headerCls:'panel-header-gray',title:''" class="dhcpha-hisui-border"  style="border-radius: 4px;">
        			<table id="gridLeave"></table>
				</div>
			</div>
        </div> 
    </div>
 	<div id="gridLeaveBar">
    	<table  class="dhcpha-north-tbl" cellspacing=0 cellpadding=0>
    		<tr>
	    		<td>
	    			
		        	<input id="cmbPivaLoc" type="text" class="hisui-combobox"/>
		        </td>
	    		<td>
	    			
		        	<input id="cmbLeaveStatus" type="text" class="hisui-combobox"/>
		        </td>
	    		<td>
	    			
		        	<input id="cmbPivaUser" type="text" class="hisui-combobox"/>
		        </td>
	    		<td>
	    			
		        	<input id="cmbLeaveType" type="text" class="hisui-combobox"/>
		        </td>
				<td>
				<a id="btnFind" class="hisui-linkbutton" data-options="iconCls:'icon-w-find',plain:false">查询</a>
				</td>
				<td>
				<a id="btnPass" class="hisui-linkbutton" data-options="iconCls:'icon-w-save',plain:false">通过</a>
				</td>
				<td>
				<a id="btnRefuse" class="hisui-linkbutton" data-options="iconCls:'icon-w-back',plain:false">拒绝</a>
				</td>
		    </tr>
		</table>   
 	</div>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.store.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.tools.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/pivas/leaveaudit.js"></script>
</body>
</HTML>