<!--dhcapp.caseclass.csp -->
<html>
<head>
<meta http-equiv="Content-Type" content="text/html;">
<meta http-equiv="x-ua-compatible" content="ie=7" />
<title>病理病人病历分类字典表</title>
<DHCAPP:EASYUI/>
<script type="text/javascript" src="../scripts/dhcdoc/dhcapp/caseclass.js"></script>
</head>

<body>
<div class="easyui-layout"  fit="true">
	<div data-options="region:'center',title:'病历分类字典'" >
		<div id="caseclassdg" toolbar="#toolbar"></div>
	</div>
</div>

<div id="toolbar" class="datagrid-toolbar">
	<table cellspacing="0" cellpadding="0">
		<tr>
			<td class="textLabel">代码<input type="text" id="code" class="textParent" style="margin:0 5px"></input> </td>	
			<td class="textLabel">描述<input type="text" id="desc" class="textParent" style="margin:0 5px"></input> </td>        
			<td><a href="#" class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true" id="find">查询</a> </td>					
			<td><div class="datagrid-btn-separator"> </div> </td>
			<td ><a class="easyui-linkbutton" data-options="iconCls:'icon-reload',plain:true" id="reset">重置</a> </td>
				
		</tr>								
	</table>
	<div class="tool-bar-line">	</div>
	<table cellspacing="0" cellpadding="0">
	    <tr>
	      <td><a class="easyui-linkbutton" plain="true" iconCls="icon-add" onclick="javascript:addRow()">增加</a> </td>
	      <td><div class="datagrid-btn-separator"></div></td>
	      <td><a class="easyui-linkbutton" plain="true" iconCls="icon-remove" onclick="javascript:deleteRow()">删除</a></td>
		  <td><div class="datagrid-btn-separator"></div></td>
		  <td><a class="easyui-linkbutton" plain="true" iconCls="icon-save" onclick="javascript:save()">保存</a></td>
		  <td><div class="datagrid-btn-separator"></div></td>
	      
	    </tr>
	</table>
</div>	
</body>
</html>
