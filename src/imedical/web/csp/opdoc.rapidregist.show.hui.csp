
<div class="hisui-layout" fit="true">
	<div data-options="region:'center',collapsible:false,border:false" style="#($SELECT(winfrom="outpatlist":"padding:10px 0 10px 0;",1:"padding:10px;"))#">
		<div class="hisui-panel" fit="true" 
			data-options="title:'#($SELECT((winfrom="outpatlist")||(winfrom="opcharge"):"",1:"就诊登记"))#',headerCls:'panel-header-gray',iconCls:'icon-edit'#($SELECT(winfrom="":",border:true",1:",border:false"))#"
			>
			<!--style='#($SELECT(winfrom="outpatlist":"padding:10px 0 10px 0;",1:"padding:10px;"))#'-->
			<table class="search-table" >
				<tr>
					<td class="r-label">
						<label>#(..Get("卡类型"))#</label>
					</td>
					<td>
						<input id="CardTypeNew" disabled class="textbox" placeholder="#(..Get("卡类型..."))#">
					</td>
					<td class="r-label">
						<label>#(..Get("卡号"))#</label>
					</td>
					<td>
						<input id="CardNo" class="textbox" placeholder="#(..Get("刷卡/请输入卡号..."))#">
					</td>
					<td class="r-label" style="text-align:left;">
						<a href="#" id="BReadCard" class="hisui-linkbutton" data-options="iconCls:'icon-w-card'">读卡</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label>#(..Get("登记号"))#</label>
					</td>
					<td>
						<input id="PatNo" class="textbox" placeholder="#(..Get("请输入登记号..."))#">
					</td>
					<td class="r-label">
						<label>#(..Get("患者姓名"))#</label>
					</td>
					<td>
						<input disabled="disabled" id="PatName" class="textbox">
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label>#(..Get("患者级别"))#</label>
					</td>
					<td>
						<input id="PoliticalLevel" disabled class="textbox">
					</td>
					<td class="r-label">
						<label>#(..Get("患者密级"))#</label>
					</td>
					<td>
						<input id="SecretLevel" disabled class="textbox">
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label>#(..Get("科室"))#</label>
					</td>
					<td>
						<input id="LocList" class="hisui-combobox textbox">
					</td>
					<td class="r-label">
						<label>#(..Get("号别"))#</label>
					</td>
					<td>
						<input id="MarkDocList" class="hisui-combobox textbox">
					</td>
				</tr>
				<tr>
					<csp:if condition='RegConDisForRapidReg="1"'>
						<td class="r-label">
							<label>#(..Get("优惠"))#</label>
						</td>
						<td>
							<input id="RegConDisList" class="hisui-combobox textbox">
						</td>
					</csp:if>
					<csp:if condition='DocOPRegistBill="1"'>
						<td class="r-label">
							<label>#(..Get("支付方式"))#</label>
						</td>
						<td>
							<input id="PayMode" class="hisui-combobox textbox">
						</td>
					</csp:if>
				</tr>
				<tr>
                    <td></td>
					<td colspan="2" class="r-label" style="text-align:left;height:20px">
						<label id="AccLeft" style="color:red;font-weight:bold;"></label>
					</td>
					<td></td><td></td>
				</tr>
				<tr>
					<td colspan="5" style="text-align:center;padding-top:5px;">
						<a id="RegistSave" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
</div>