<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!--dhcmdt.conslistmain.csp //-->
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>	
	<script type="text/javascript" src="../scripts/framework/easyui-1.3.6/jquery.tabs.js" charset="utf-8"></script>
	<link href="../scripts/dhcnewpro/dhcmdt/css/base.css" rel="stylesheet">
	<link href="../scripts/dhcnewpro/dhcmdt/css/conslistmain.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/DHCWeb.OPCommonManageCard.js"></script>
    <script type="text/javascript" src="../scripts/DHCWeb.COMMHardManager.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcmdt/consfuncom.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcmdt/conslistmain.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcmdt/cadigitalsign.js"></script>
	<server>
	  D ##class(web.UDHCCardCommLinkRegister).GetCardHardJSFunction()
	  s LgHospID=%session.Get("LOGON.HOSPID")
	  /// 是否有疑难病会诊中心
	  s HasCenter=##Class(web.DHCMDTCom).GetEmSysConfig("HASCENTER",LgHospID)
	  w "<script Language=""JavaScript"">"_$c(13,10)
	  w "var HasCenter="""_$g(HasCenter)_""";"_$c(13,10)
	  w "</script>"
   </server>
</head>
<body>
	<div class="hisui-layout" fit="true">
		<div data-options="region:'center',title:''" style="border:0;padding:10px 10px 10px 10px;">
			<div class="hisui-layout" fit="true">
				<div data-options="region:'north',title:'会诊执行',headerCls:'panel-header-gray',iconCls:'icon-paper',collapsible:false" style="height:220px;">
					<div id="pf-hd">
						<label style="padding-right:5px;">#(..Get("快捷查询"))#</label>
						<a id="cstUserNum" href="#" class="hisui-linkbutton btn-select" onclick="qrySelfSend('1');">本人申请</a>
						<a id="csUserNum" href="#" class="hisui-linkbutton" onclick="qrySelfSend('2');">本人会诊</a>
					    <a id="GroupUserNum" href="#" class="hisui-linkbutton" onclick="qrySelfSend('3');">本组会诊</a>
			        </div>
			        <div class="dashline-div"></div>
			        <div id="pf-hc">
						<div>
							<label style="padding-right:5px;margin-right:10px;">#(..Get("开始日期"))#</label><input id="startDate" class="hisui-datebox textbox" style="margin-left:10px;">
			        		<label style="margin-left:53px;margin-right:10px;">#(..Get("会诊状态"))#</label>
							<input id="consStatus" class="hisui-combobox" style="margin-left:10px;width:140px;height:30px;line-height:30px;"/>
			        		<label style="margin-left:53px;margin-right:10px;">#(..Get("病人ID"))#</label><input id="regNo" class="hisui-validatebox" style="margin-left:10px;">
							<a href="#" style="margin-top:3px;margin-left:10px;" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" onclick="qryConsList()">查询</a>
						</div>
						<div class="pf-toolhc">
				        	<label style="padding-right:5px;margin-right:10px;">#(..Get("结束日期"))#</label><input id="endDate" class="hisui-datebox textbox" style="margin-left:10px;">
							<label style="margin-left:53px;margin-right:10px;">#(..Get("疑难病种"))#</label>
							<input id="mdtDisGrp" class="hisui-combobox" style="margin-left:10px;width:140px;height:30px;line-height:30px;"/>
			        		<label style="margin-left:67px;margin-right:10px;">#(..Get("姓名"))#</label><input id="PatName" class="hisui-validatebox" style="margin-left:10px;">
							<!--<a href="#" style="margin-top:3px;margin-left:10px;" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'" onclick="Print()">打印</a>-->
						</div>
			        </div>
			        <div class="dashline-div"></div>
					<div class="pf-tools">
						<a href="#" style="margin-top:3px;" class="hisui-linkbutton" plain="true" data-options="iconCls:'icon-cancel'" onclick="cancelMdtCons()" id="bt_cancel">取消会诊</a>
						<a href="#" style="margin-left:2px;" class="hisui-linkbutton" plain="true" data-options="iconCls:'icon-write-order'" onclick="modProWin()" id="bt_Revisionexp">修改专家</a>
						<a href="#" style="margin-top:3px;margin-left:2px;" class="hisui-linkbutton" plain="true" data-options="iconCls:'icon-edit'" onclick="mdtHandleWin('P')" id="bt_acc">评估</a>
						<a href="#" style="margin-top:3px;margin-left:2px;" class="hisui-linkbutton" plain="true" data-options="iconCls:'icon-edit'" onclick="mdtHandleWin('E')" id="bt_exe">执行</a>
						<a href="#" style="margin-top:3px;margin-left:2px;" class="hisui-linkbutton" plain="true" data-options="iconCls:'icon-wrench-blue'" onclick="mdtCsHandle()" id="bt_make">处理</a>
						<a href="#" style="margin-top:3px;margin-left:2px;" class="hisui-linkbutton" plain="true" data-options="iconCls:'icon-emr-cri'" onclick="PatBaseWin()" id="bt_patemr">查看病历</a>
						<a href="#" style="margin-top:3px;margin-left:2px;" class="hisui-linkbutton" plain="true" data-options="iconCls:'icon-book'" onclick="mdtHandleWin('L')" id="bt_csr">会诊病历</a>
					</div>
				</div>
				<div data-options="region:'center',headerCls:'panel-header-gray'">
					<table id="PatList"></table>
				</div>
			</div>
		</div>
	</div>
	
    <!-- mdt会诊查看窗口 -->
	<div id="mdtPopWin">
		<iframe id="mdtPopFrame" src="" width="100%" height="99%" frameborder="0"></iframe>
	</div>
	<!-- mdt病例评估 -->
	<div id="mdtPopAccWin">
		<iframe id="mdtPopAccFrame" src="" width="100%" height="99%" frameborder="0"></iframe>
		 <!-- 
		 <div style="margin:10px 0px;">
			<label style="font-weight:bold;margin-left:10px;">评估内容</label>
			<div style="padding:8px;">
				<textarea id="CstRefReason"  style="width:980px;height:200px;resize:none;"></textarea>
			</div>
		</div>
		-->
	</div>
	<!-- mdt会诊处理窗口 -->
	<div id="newRefOpWin" style="padding:0px 1px;">
		 <div style="margin:10px 0px;">
			<label style="font-weight:bold;margin-left:10px;">#(..Get("不能参加原因"))#</label>
			<div style="padding:8px 8px 0px 8px;">
				<textarea id="CstRefReason" class="textbox" style="width:573px;height:160px;resize:none;"></textarea>
			</div>
		</div>
		<div id="mdtWinTools" style="text-align: right;text-align:center">
			<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-ok'" onclick="AcceptCstNo()" style="margin-left:15px">接收</a>
			<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-back'" onclick="RefCstNo()" style="margin-left:30px">回复</a>
		</div>
	</div>
</body> 
</html>
