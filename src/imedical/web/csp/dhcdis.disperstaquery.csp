<html>
<head>
<!-- dhcdis.disperstaquery.csp -->
<title>配送人员状态查询</title>
<DHCAPP:EASYUI/>

<script type="text/javascript" src="../scripts/dhcnewpro/dhcdis/disperstaquery.js"></script>
</head>
<body>
	<div id="mainpanel" class="easyui-layout" fit="true">
		<div data-options="region:'north',title:'配送人员状态查询'" style="width:500px;height:125px;">
			<!-- 查询条件 -->
			<table style="font-size:14px">
				<tr style="height:20px">
					<td width="40"></td>
					<td></td>
					<td></td>
					<td width="30"></td>
					<td></td>
					<td></td>
					<td width="30"></td>
					<td></td>
					<td></td>
					<td width="30"></td>
					<td></td>
				</tr>
				<tr>
					<td></td>
					<td align="right" class="input-label">科室:</td>
					<td width="150px"><input id="Dept"/></td>
					<td></td>
					<td align="right" class="input-label">状态:</td>
					<td width="150px"><input id="Status"/></td>
					<td></td>
					<td ><a class="easyui-linkbutton" data-options="iconCls:'icon-search'" id="find">查询</a> </td>
					<td>&nbsp&nbsp&nbsp</td>
					<td ><a class="easyui-linkbutton" id="clear">清空查询条件</a> </td> 
				</tr>
			</table>
		</div>
		<!--数据列区域-->
		<div data-options="region:'center',title:'状态列表'">
			<div id="statuslist" toolbar="#subtoolbar"></div>
		</div>
	</div>
</body>
</html>

