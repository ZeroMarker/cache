<csp:content charset="UTF-8">
<SERVER>
s Action=$Get(%request.Data("actiontype",1))
s Start=$Get(%request.Data("start",1))
s Limit=$Get(%request.Data("limit",1))
s StkType=$Get(%request.Data("StkType",1))
;
i Action = "OperateType" d
	.s type=$Get(%request.Data("Type",1))
	.w ##class(web.DHCSTM.Util.OtherUtil).GetOperateType(type)
i Action = "ReasonForReturn" d
	.w ##class(web.DHCSTM.Util.OtherUtil).GetReasonForReturn(Start,Limit,StkType)
i Action="ReasonForAdjSpStore"  d
    .w ##class(web.DHCSTM.Util.OtherUtil).GetReasonForAdjSp(Start,Limit,StkType)
i Action = "ReasonForAdjustment" d
	.w ##class(web.DHCSTM.Util.OtherUtil).GeReasonForAdjustment(StkType)
i Action = "ReasonForScrap" d
	.w ##class(web.DHCSTM.Util.OtherUtil).GetReasonForScrap(Start,Limit,StkType)
i Action = "ReasonForCancelInPo" d
	.w ##class(web.DHCSTM.Util.OtherUtil).GetReasonForCancelInPo(Start,Limit,StkType)
</SERVER>