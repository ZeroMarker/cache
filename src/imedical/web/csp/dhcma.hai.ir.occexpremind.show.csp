<!-- 主页面-->
<div class="hisui-layout" data-options="fit:true">
	<div id="west" data-options="region:'west',border:false,collapsible:false" style="width:300px;padding:10px;">
        <div class="hisui-panel" data-options="fit:true,title:'查询条件',headerCls:'panel-header-gray',iconCls:'icon-paper',">
            <table class="search-table">
                <tr>
                    <td class="r-label">
                        <label for="cboHospital">院区</label>
                    </td>
                    <td>
                        <input class="hisui-combobox textbox" id="cboHospital" style="width:192px" />
                    </td>
                </tr>
                <tr>
                    <td class="r-label">
                        <label for="cboDateType">日期类型</label>
                    </td>
                    <td>
                        <input class="hisui-combobox textbox" id="cboDateType" style="width: 192px" />
                    </td>
                </tr>
                <tr>
					<td class="r-label">
						<label for="cboYear">快速选择</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="cboYear" style="width:93px" />
						<input class="hisui-combobox textbox" id="cboMonth" style="width:94px" />
					</td>
				</tr>
                <tr>
                    <td class="r-label">
                        <label for="DateFrom">开始日期</label>
                    </td>
                    <td>
                        <input class="hisui-datebox textbox" id="DateFrom" style="width: 192px" />
                    </td>
                </tr>
                <tr>
                    <td class="r-label">
                        <label for="DateTo">至</label>
                    </td>
                    <td>
                        <input class="hisui-datebox textbox" id="DateTo" style="width: 192px" />
                    </td>
                </tr>
                 <tr>
                    <td class="r-label">
                        <label for="cboRepType">报告类型</label>
                    </td>
                    <td>
                        <input class="hisui-combobox textbox" id="cboRepType" style="width: 192px" />
                    </td>
                </tr>
                <tr>
                    <td class="r-label">
                        <label for="cboLocation">登记科室</label>
                    </td>
                    <td>
                        <input class="hisui-combobox textbox" id="cboLocation" style="width: 192px" />
                    </td>
                </tr>
            </table>
            <div style="text-align:center">
                <a id="btnQuery" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
            </div>
        </div>
    </div>	
	
	<div data-options="region:'center',border:false,collapsible:false" style="padding:10px 10px 10px 0px;">
        <table id="gridExpReg" data-options="toolbar:'#custtb'"></table>
        <div id="custtb" style="padding:3px">
            <a class="hisui-linkbutton" id="btnExport" data-options="iconCls:'icon-export',plain:true"> 导出 </a>
        </div>
	</div>
</div>
<!--职业暴露提醒明细-->
<div id="LayerOccRepLog" style="overflow:hidden;padding:10px;display:none;">
    <table id="gridExpRepLog"></table>
</div>
<!--职业暴露操作明细-->
<div id="LayerOccRepLab" style="overflow:hidden;padding:10px;display:none;">
    <table id="gridExpLab"></table>
</div>
