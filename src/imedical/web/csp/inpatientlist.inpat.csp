	<HISUI></HISUI>
	
    <STYLE type='text/css'>
        body,table{
	       font-family: "Microsoft Yahei";
	       font-size: 14px;
	       background: #FFF;
	    }
	    body{
			padding:0px;
		}
    	.backcolorcom{
	    	padding: 2px 10px;
	    	color: #FFF;
	    	border-radius: 5px;
	    }
	    #BZCount{
		    background: #F05AD7;
		}
		#BWCount{
		    background: #F16D56;
		}
		#CurPatCount{
		    background: #27B66B;
		}
		.panel-body{
			border-color:#E7E7E7;
		}
		.inpatbtncom,.inpatdaybtncom{
			font-size: 14px !important;  
			position: relative;
			display: block;
			padding:0px 5px; 
			border-radius: 4px;
			float:left;
			/*margin:0 5px;*/
			margin-right:5px;
			color:#ff4401;
			
			height: 26px;
			line-height: 26px;
			text-align: center;
	   }
	   .inpatbtncom:focus, 
	   .inpatbtncom:hover,
	   .selinpatbtn,
	   .inpatdaybtncom:focus,
	   .inpatdaybtncom:hover,
	   .selinpatdaybtn{
			background-color: #ffe9db;
			
			cursor: pointer;
		}
		.inpatbtncom:focus, 
		.inpatbtncom:hover,
		.inpatdaybtncom:focus, 
		.inpatdaybtncom:hover{
			text-decoration: none;
		}
		.search-table{
			border-collapse:separate;
			border-spacing:0 10px;
			/*padding: 0 10px;*/
		}
		.r-label{
			padding-left: 10px;
		}
		.r-label-min{
			padding:0 5px;
			text-align:right;
		}
    </STYLE>
</head>
<body>
    <server>
    	s PatListType=%request.Get("PatListType")
    	s isNurseLogin =##class(web.DHCDocMain).isNurseLogin()
    	;s %session.Get("LOGON.USERID")=
   		s DefindeListShow=""
    	s DefaultCurrentGroupe=##class(web.DHCDocConfig).GetConfigNode1("layoutInPatList_Group",%session.Get("LOGON.GROUPID"))
    	if (DefaultCurrentGroupe'="") s DefindeListShow=DefaultCurrentGroupe
    	s DefaultCurrentUser=##class(web.DHCDocConfig).GetConfigNode1("layoutInPatList_User",%session.Get("LOGON.USERID")_"Z"_%session.Get("LOGON.GROUPID"))
   		if (DefaultCurrentUser'="") s DefindeListShow=DefaultCurrentUser
   		s LogonWardId=%session.Get("LOGON.WARDID")
   		s ShowFromPage=%request.Get("ShowFromPage")
   		s DefaultCurrentWard=##class(web.DHCDocConfig).GetConfigNode1("DefaultCurrentWard",%session.Get("LOGON.USERID")_"Z"_%session.Get("LOGON.GROUPID"))
   </server>
    <!--在院患者-->
