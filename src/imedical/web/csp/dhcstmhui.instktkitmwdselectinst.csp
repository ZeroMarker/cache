<!--盘点录入-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false" style="height:50px" >
					<table class="Condition" id="Conditions">
						<input id="FInstComp" name="FInstComp" type="hidden">
						<input id="FStkTkComp" name="FStkTkComp" type="hidden">
						<input id="FAdjComp" name="FAdjComp" type="hidden">
						<tr>
							<td>科室</td>
							<td><input id="FLocId" name="FLocId" class="textbox hisui-combobox"></td>
							<td>起始日期</td>
							<td><input id="FStartDate" name="FStartDate" class="hisui-datebox textbox"></td>
							<td>截止日期</td>
							<td><input id="FEndDate" name="FEndDate" class="hisui-datebox textbox"></td>
							<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
							<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
							<td><a href="#" id="CompleteBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">选取</a></td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false">
					<table id="MasterGrid"></table>
				</div>
	<div id="SelectModel" class="hisui-dialog SelectModel" data-options="closed:'true',modal:true" title="实盘录入模式选择" style="width:500px;height:300px;top:100px;left:300px;padding:10px;">
		<div>
			<table id="SelectModelCondition" class="Condition" style="text-align:left">
					<tr>
						<td>
							<input id="systel1" class='hisui-radio' type="radio" value='1' name='InputType' label='录入方式一:填充批次表格式（根据账盘数据按批次填充实盘数）'>
						</td>
					</tr>
					<tr>
						<td>
							<input id="systel2" class='hisui-radio' type="radio" value='2' name='InputType' label='录入方式二:按库存批次录入方式'>
						</td>
					</tr>
					<tr>
						<td>
							<input id="systel3" class='hisui-radio' type="radio" value='3' name='InputType' label='录入方式三:按品种填充表格式（根据账盘数据按品种填充实盘数）'>
						</td>
					</tr>
					<tr>
						<td>
							<input id="systel4" class='hisui-radio' type="radio" value='4' name='InputType' label='录入方式四:高值扫码盘点'>
						</td>
					</tr>
					<tr>
						<td>实盘窗口<input id="InStkTkWin" name="InStkTkWin" class="hisui-combobox" style="width:180px;" label='录入方式一:填充批次表格式（根据账盘数据按批次填充实盘数）'></td>
					</tr>
					<tr>
						<td>
							<a href="#" id="FCancelBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-cancel'" style="margin : 0px 10px 0px 0px;">取消</a>
							<a href="#" id="FSuerBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">确认</a>
						</td>
					</tr>
			</table>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InStkTk/INStkTkItmWdSelectInst.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InStkTk/SelectInputModel.js"></script>

</body>
</html>