<!-- dhcem.wirtebedshift.csp  交班日志页面-->
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<link href="../scripts/dhcnewpro/dhcem/css/base.css" rel="stylesheet">
	<link href="../scripts/dhcnewpro/dhcem/css/wirtebedshift.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/bedsideshiftscom.js"></script>
	<script Language="Javascript">
	/// 引用内容
	function InsQuote(resQuote, flag){

		frames[1].InsQuote(resQuote, flag); /// 插入引用内容
	}
   </script>
</head>
<body class="background-warp">
	<div class="container">
		<server>
			s EpisodeID=$Get(%request.Data("EpisodeID",1)) /// 就诊ID
			s EmType=$Get(%request.Data("EmType",1))       /// 交班类型
			s bsID=$Get(%request.Data("bsID",1))           /// 交班ID
			s Pid=$Get(%request.Data("Pid",1))             /// 进程号
			s bsItemID=$Get(%request.Data("bsItemID",1))   /// 交班子表ID
			s WriUrl="dhcem.shiftwrite.csp?EpisodeID="_EpisodeID_"&bsID="_bsID_"&bsItemID="_bsItemID_"&Pid="_Pid
			s HisUrl="dhcem.pattimeaxis.csp?EpisodeID="_EpisodeID_"&EmType="_EmType
	   </server>
		<div class="page-east">
			<iframe src="#(HisUrl)#" width="100%" height="100%" frameborder="0" style="display:block"></iframe>
    	</div>
    	<div class="page-center">
    		<iframe src="#(WriUrl)#" width="100%" height="100%" frameborder="0" style="display:block"></iframe>
		</div>
	</div>
</body>
</html>
