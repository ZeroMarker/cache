<!-- 
 * FileName: dhcbill.ipbill.inpatlist.csp
 * Anchor: ZhYW
 * Date: 2019-03-06
 * Description: 住院患者查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.inpatlist.css'/>
</head>

<body>
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',border:false,collapsible:false" style="height:130px;">
			<table class="search-table" style="width:100%">
				<tr>
					<td class="r-label td-first-col"><label>开始日期</label></td>
					<td><input id="stDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>结束日期</label></td>
					<td><input id="endDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>患者姓名</label></td>
					<td><input id="patName" class="textbox"/></td>
					<td class="r-label">
						<input class="hisui-radio" type="radio" name="status" label="医生确认" id="Medical">
						<input class="hisui-radio" type="radio" name="status" label="护士确认" id="Final">
					</td>
					<td class="r-label"><a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a></td>
				</tr>
				<tr>
					<td class="r-label"><label>科室</label></td>
					<td><input id="dept" class="hisui-combobox textbox"/></td>
					<td class="r-label"><label>病区</label></td>
					<td><input id="ward" class="hisui-combobox textbox"/></td>
					<td class="r-label"><label>费别</label></td>
					<td><input id="admReason" class="hisui-combobox textbox"/></td>
					<td class="r-label">
						<input class="hisui-radio" type="radio" name="status" label="财务结算" id="PayFlag">
						<input class="hisui-radio" type="radio" name="status" label="当前在院" id="CurAdm" data-options="checked:true">
					</td>
					<td class="r-label"><a href="javascript:;" class="hisui-linkbutton" id="btn-clear" data-options="iconCls:'icon-w-clean'">清屏</a></td>
				</tr>
				<tr>
					<td class="r-label"><label>发票号</label></td>
					<td><input id="receiptNo" class="textbox"/></td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',border:false" style="border-top:1px solid #cccccc;">
			<table id="patList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.inpatlist.js"></script>
</body>
</html>