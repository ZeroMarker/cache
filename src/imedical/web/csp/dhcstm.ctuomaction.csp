<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

	s listData=$Get(%request.Data("data",1))
	s RowId=$Get(%request.Data("rowid",1))
	s CtUomId=$Get(%request.Data("CtUomId",1))
	s Desc=$Get(%request.Data("desc",1))

	//查询包装单位
	i Action = "selectAll" d
	.
	.s ^zhwh(999)=Start_"^"_Limit_"^"_Sort_"^"_Dir_"^"_Desc
	.s result = ##class(web.DHCSTM.CTUOM).SelectAll(Start,Limit,Sort,Dir,Desc)
	.i result ="" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result

	//保存包装单位
	i Action = "save" d
	.s result = ##class(web.DHCSTM.CTUOM).Save(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//删除包装单位
	i Action = "delete" d
	.s result = ##class(web.DHCSTM.CTUOM).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//根据包装单位查询包装单位转换单位
	i Action = "selectCtUomConFacByCU" d
	.s result = ##class(web.DHCSTM.CTUOM).SelectConUom(CtUomId)
	.i result = "" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result

	//保存包装单位与包装单位转换的关系记录
	i Action = "addRelation" d
	.s result = ##class(web.DHCSTM.CTUOM).AddConFac(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//保存包装单位与包装单位转换的关系记录
	i Action = "updateRelation" d
	.s result = ##class(web.DHCSTM.CTUOM).UpdConFac(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//删除包装单位转换
	i Action = "deleteFac" d
	.s result = ##class(web.DHCSTM.CTUOM).DeleteConFac(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

</script>