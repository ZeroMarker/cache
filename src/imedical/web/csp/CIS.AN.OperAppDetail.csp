+<html>
<head>
<!-- Put your page Title here -->
<title>手术申请</title>
<csp:content charset="UTF-8">
<HISUI></HISUI>
<!--<dhcclinic:easyui></dhcclinic:easyui>-->
<dhcclinic:head></dhcclinic:head>

</head>
<body class="hisui-layout">
    <div data-options="region:'center',border:false" style="padding:10px;">
    	<form id="appDetailForm">
    		<div class="hisui-panel" title="患者信息" style="padding:5px 0;width:1150px;margin-bottom:10px;" data-options="headerCls:'panel-header-card'">
    			<div>
	                <div class="form-row">
	                    <div class="form-title-right4">姓名</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PatName" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">性别</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PatGender" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">年龄</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PatAge" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">科室</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PatDeptDesc" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
	            <div>
	                <div class="form-row">
	                    <div class="form-title-right4">病区</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PatWardDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">床位</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PatBedCode" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">费别</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="AdmReason" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">密级</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="SecretLevel" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
    		</div>
    		<div class="hisui-panel" title="手术信息" style="padding:5px 0;width:1150px;margin-bottom:10px;" data-options="headerCls:'panel-header-card'">
    			<div>
	                <div class="form-row">
	                    <div class="form-title-right4">申请科室</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="AppDeptDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">申请医生</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="AppCareProvDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">手术室</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="OperDeptDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">手术类型</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="SourceTypeDesc" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
	            <div>
	                <div class="form-row">
	                    <div class="form-title-right4">手术日期</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="OperDate" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">开始时间</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="OperTime" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">预计时长</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="OperDuration" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">重返手术</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="ReentryOperationDesc" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
	            <div>
	                <div class="form-row">
	                    <div class="form-title-right4">麻醉科室</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="AnaDeptDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">拟施麻醉</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PrevAnaMethodDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">计划台次</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PlanSeq" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">台次类型</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="SeqTypeDesc" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
	            <div>
	                <div class="form-row">
	                    <div class="form-title-right4">术前诊断</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="PrevDiagnosisDesc" class="hisui-validatebox" style="width:1045px;">
	                    </div>
	                    
	                </div>
	            </div>
	            <div>
	                <div class="form-row">
	                	<div class="form-title-right4">拟施手术</div>
	                    <div class="form-item-normal">
	                        <table id="operationBox"></table>
	                    </div>
	                    
	                </div>
	            </div>
	            <div>
	                <div class="form-row">
	                    <div class="form-title-right4">手术体位</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="OperPositionDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">实习进修</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="SurIntership" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">参观人员</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="SurVisitors" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
    		</div>
    		<div class="hisui-panel" title="手术要求" style="padding:5px 0;width:1150px;margin-bottom:10px;" data-options="headerCls:'panel-header-card'">
	            <div>
	            	<div class="form-row">
	            		<div class="form-title-right4">仪器器械</div>
	            		<div class="form-item-normal">
	            			<input type="text" class="hisui-validatebox" id="SurgicalMaterials" name="SurgicalMaterials" style="width:460px">
                        </div>
                        <div class="form-title-right5">特殊情况</div>
                        <div class="form-item-normal">
                        	<input type="text" class="hisui-validatebox" id="SpecialConditions" name="SpecialConditions" style="width:459px">
                        </div>
                    </div>
                </div>
                <div>
                	<div class="form-row">
                		<div class="form-title-right4">高值耗材</div>
                		<div class="form-item-normal">
                			<input type="text" class="hisui-validatebox" id="HighConsume" name="HighConsume" style="width:460px">
                    	</div>
                    	<div class="form-title-right5">手术要求</div>
                    	<div class="form-item-normal">
                    		<select id="OperRequirement" name="OperRequirement" class="hisui-combobox" style="width:466px">
                        	</select>
                   	 	</div>
                	</div>
                </div>
            </div>
    		<div class="hisui-panel" title="检验信息" style="padding:5px 0;width:1150px;margin-bottom:10px;" data-options="headerCls:'panel-header-card'">
	            <div>
	                <div class="form-row">
	                    <div class="form-title-right4">血型</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="BloodTypeDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">Rh(D)</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="RHBloodTypeDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">HbsAg</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="HbsAgDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">HcvAb</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="HcvAbDesc" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
	            <div>
	                <div class="form-row">
	                    <div class="form-title-right4">HivAb</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="HivAbDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right4">梅毒</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="SyphilisDesc" class="hisui-validatebox">
	                    </div>
	                    <div class="form-title-right5">多重耐药菌</div>
	                    <div class="form-item-normal">
	                        <input type="text" name="MDROSDesc" class="hisui-validatebox">
	                    </div>
	                </div>
	            </div>
    		</div>
    	</form>
    </div>
	<script type="text/javascript" src="../service/dhcanop/js/CIS.AN.OperAppDetail.js"></script>
</body>
</html>
