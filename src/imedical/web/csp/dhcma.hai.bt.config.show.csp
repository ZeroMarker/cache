<!-- 主页面-->
<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'center',border:false" fit="true" class="ma-panel" style="padding:10px">
		<table id="gridConfig" data-options="toolbar:'#ToolBar'"></table>
		<div id="ToolBar" style="padding:3px" >
			<div style="padding: 7px 0 7px 7px;">
                <label>模块</label>
                <input class="textbox hisui-combobox" id="cboModList" style="width:250px" ><label style="padding-right:5px"></label> 
                <input class="hisui-searchbox" id="searchbox" style="width:260px;" data-options="prompt:'搜索'"  />
            </div>
            <span class="line" style="display: block;border-bottom: 1px dashed rgb(226, 226, 226);margin: 3px 0px;clear: both;/* padding-bottom: 5px; */"></span>
			<a class="hisui-linkbutton" id="btnAdd"  data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true"> 新增 </a>
			<a class="hisui-linkbutton" id="btnEdit" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"> 修改 </a>
			<a class="hisui-linkbutton" id="btnDelete"  data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true" > 删除 </a>
		</div>
	</div>
</div>
<div id="winEdit" style="overflow:hidden;padding-right:10px;display:none">
	<table class="search-table">
		<tr>
			<td class="r-label">
				<label for="txtCode">参数代码</label>
			</td>
			<td>
				<input class="hisui-validatebox textbox" id="txtCode" data-options="required:true" style="width:270px" >
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="txtDesc">参数描述</label>
			</td>
			<td>
				<input class="hisui-validatebox textbox" id="txtDesc" data-options="required:true" style="width:270px">
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="txtValue">值</label>
			</td>
			<td>
				<input class="hisui-validatebox textbox" id="txtValue" data-options="required:true" style="width:270px" >
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="cboHospital">医院</label>
			</td>
			<td>
				<input class="textbox hisui-combobox" id="cboHospital" style="width:277px" >
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="cboAddMod">模块</label>
			</td>
			<td>
				<input class="hisui-validatebox hisui-combobox" id="cboAddMod" style="width:277px" data-options="required:true,"/> 
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="txtResume">备注</label>
			</td>
			<td>
				<input class="hisui-validatebox textbox" id="txtResume" style="width:270px" >
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="txtIndNo">排序码</label>
			</td>
			<td>
				<input class="hisui-validatebox textbox" id="txtIndNo" style="width:270px" >
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="chkIsActive">是否有效</label>
			</td>
			<td>
				<input class='hisui-checkbox' type="checkbox" id="chkIsActive" checked >
			</td>
		</tr>
		</tr>
			<td colspan="2" class="search-btnlist" Style="text-align:center">
				<a id="winBtnEdit" class="hisui-linkbutton" >保存</a>
				<a id="winBtnClose" class="hisui-linkbutton">关闭</a>
			</td>
		</tr>
	</table>
</div>	