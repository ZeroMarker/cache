<!--调价单录入(批次)-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="BorderLine" data-options="region:'north'" > 
		<table class="Condition" id="Conditions">
			<input id="Inci" name="Inci" type="hidden">
			<tr>
				<td>起始日期</td>
				<td><input id="StartDate" name="StartDate" class="hisui-datebox textbox"></td>
				<td></td>
				<td>类组</td>
				<td><select id="ScgId" name="ScgId" class="hisui-stkscgcombotree" style="width:160px"></select></td>
				<td></td>
				<td>物资名称</td>
				<td><input id="InciDesc" name="InciDesc" class="textbox"></td>
				<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>	
			</tr>
			<tr>
				<td>截止日期</td>
				<td><input id="EndDate" name="EndDate" class="hisui-datebox textbox"></td>
				<td></td>
				<td>调价单号</td>
				<td><input id="AdjspNo" name="AdjspNo" class="textbox" style="width:153px;"></td>
				<td></td><td></td><td></td>
				<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" >清屏</a></td>
			</tr>
		</table>
	</div>
	<div data-options="region:'center',border:false,headerCls:'panel-header-gray'">
		<table id="DetailGrid"></table>
	</div>
	<div id="SaveRpSp" class="hisui-dialog SaveRpSp" data-options="closed:'true'" title="填充进售价" style="width:250px;">
		<table class="Condition">
			<tr>
				<td>进价</td>
				<td><input id="Rp" name="Rp" class="textbox"></td>
			</tr>
			<tr>
				<td>售价</td>
				<td><input id="Sp" name="Sp" class="textbox"></td>
			</tr>
			<tr>
				<td>调价原因</td>
				<td><input id="AdjReason" name="AdjReason" class="hisui-combobox"></td>
			</tr>
			<tr>
				<td colspan="2">
					<input class="hisui-radio" type="radio" label="跳过已填" name="Fill" value="1" data-options="checked:true">
					<input class="hisui-radio" type="radio" label="覆盖已填" name="Fill" value="2">
				</td>
			</tr>
			<tr>
				<td colspan="2" style="text-align:center;">
					<a href="#" id="RpSpSureBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
				</td>
			</tr>
		</table>
	</div>
	<!--批次弹窗-->
	<div id="IncItmBatWindowAll" class="hisui-window" title="科室库存项批次信息" style="height:500px;width:800px" data-options="minimizable:false,closed:true">
	</div>
	
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/IncItmBatWindowAll.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/AdjPriceBatch/AdjPriceCommonBatch.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/AdjPriceBatch/CreateAdjPriceBatch.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/AdjPriceBatch/SaveRpSp.js"></script>	
</body>
</html>