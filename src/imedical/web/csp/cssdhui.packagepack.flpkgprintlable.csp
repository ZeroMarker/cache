<!--辅料包打印-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<server>
	d ##class(web.DHCXMLPConfig).LODOPInit()
</server>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'center',border:false">
		<div id="UomTB">
			<input ID="InvPrintEncrypt" type="hidden" value="#(##Class(%CSP.Page).Encrypt($lb("web.DHCXMLIO.ReadXML")))#">
			<table class="Condition" id="cleantable">
				<input id="txtChkerv" name="txtChkerv" type="hidden">
				<input id="txtActorv" name="txtActorv" type="hidden">
				<input id="cleanStrov" name="cleanStrov" type="hidden">
				<input id="Iscmt" name="Iscmt" type="hidden">
				<tr>
					<td>敷料包</td>
					<td><select id="package" name="package" class="hisui-combobox" style="width:150px;"></select></td>
					<td>灭菌日期</td>
					<td><input id="cleandate" name="cleandate" class="hisui-dateboxq textbox"></input></td>
					<td>有效期</td>
					<td>
						<select id="PackType" name="PackType" class="hisui-combobox" style="width:155px;">
							<option value="5">常规</option>
							<option value ="0">7天</option>
							<option value ="1">14天</option>
							<option value="2">30天</option>
							<option value="3">90天</option>
							<option value="4">180天</option>
						</select>
					</td>
					<td>
						<a href="#" id="query" data-options="iconCls:'icon-w-find'" class="hisui-linkbutton" style="width:100px">查询</a>
					</td>
					<td>
						<a href="#" id="Print" data-options="iconCls:'icon-w-print'" class="hisui-linkbutton" class="hisui-linkbutton" style="width:130px">打印</a>
					</td>
					<td colspan="2"> 
						<a href="#" id="LabelGen" class="hisui-linkbutton" class="hisui-linkbutton" style="width:150px">标签生成</a>		
					</td>
				</tr>
				<tr>
					<td>科室</td>
					<td>
						<select id="Loc" name="Loc" class="hisui-combobox" style="width:150px;"></select>
					</td>
					<td>审核人</td>
					<td>
						<input id="txtChker" name="txtChker" class="textbox"></input>
					</td>
					<td>包装人</td>
					<td>
						<input id="txtActor" name="txtActor" class="textbox"></input>
					</td>
					<td>
						<label>数量</label><input id="num" name="num" class="hisui-numberbox textbox" style="width:100px"></input>
					</td>
					<td>
						<a href="#" id="PrintNot" data-options="iconCls:'icon-w-print'" class="hisui-linkbutton" class="hisui-linkbutton" style="width:130px">无明细打印</a>
					</td>
					<td>
						<a href="#" id="GenLableAndPrint" class="hisui-linkbutton" style="width:150px">标签生成并打印</a>
					</td>
				</tr>
				</table>	
		</div>
		<table id="GridList"></table>
</div>
<script type="text/javascript" src="../scripts/DHCPrtComm.js"></SCRIPT>
<script type="text/javascript" src="../scripts/cssd/Pack/LabelPrint.js"></script>
<script type="text/javascript" src="../scripts/cssd/Pack/FlPkgPrintLable.js"></script>
</body>
</html>
