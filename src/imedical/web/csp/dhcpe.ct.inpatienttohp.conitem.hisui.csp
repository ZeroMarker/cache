<!-- 
 * FileName: dhcpe.ct.inpatienttohp.conitem.hisui.csp
 * Anchor: sxt
 * Date: 2021-08-12
 * Description: HISUI 会诊费设置 多院区
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 30px;
}
</style>
</head>
<body>
<div class="hisui-layout" data-options="fit:true" border="false" id="layout">
        <div data-options="region:'north',split:false,collapsible:false,headerCls:'panel-header-gray'"  title="" border="false">
	    	 <table class="search-table">
	    		 <tr rowspan="3">
	    	 		<td class="r-label"><label>科室</label></td>
	    	 		<td >
	    	 			<input id="LocList" class="hisui-combobox textbox" type="text" style="width:154px;">
	    	 		</td>
				
		 			<!--td class="r-label"><label for="DefaultItemDesc">默认会诊医嘱</label></td>
					<td>
						<input class="hisui-combogrid" type="text" id="DefaultItemDesc">
					</td-->
					
					<td class="r-label r-label-1"><label for="HPBUser">医生</label></td>
					<td>
						<input class="hisui-combogrid" type="text" id="HPBUser" style="width:154px;">
					</td>
					
					<td class="r-label r-label-1">
						<input class="hisui-checkbox" type="checkbox" id="ActiveFlag" checked><label for="ActiveFlag">激活</label>
					</td>
					<td class="r-label r-label-1">
						<a id="BSaveItem" href="#" class="hisui-linkbutton"  data-options="iconCls:'icon-w-save'">保存</a>
					</td>
					
				</tr>	
				<tr>
					
					<td class="r-label"><label for="ItemDesc">项目</label></td>
					<td>
						<input class="hisui-combogrid" type="text" id="ItemDesc" style="width:154px;">
					</td>
					
					<td class="r-label r-label-1"><label for="CarPrvTpDesc">医护人员类型</label></td>
					<td>
						<input class="hisui-combogrid" id="CarPrvTpDesc" style="width:154px;" >
					</td>
				
					<td></td>
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
					
					<td><input type="hidden" id="ID"/></td>
					
				</tr>
				
			</table>
		
	</div>
	  <div data-options="region:'center',headerCls:'panel-header-gray'"  border="false" style="border-top:1px solid #ddd;" >
	   		<table data-options="fit:true"  id="IPToHPConItemQueryTab"></table>
		</div>	
</div>
<div id="LocListWin"  style="display:none;">
        <div data-options="region:'north',split:false,border:true" style="height:290px;padding:10px;">
        <div style="height:100%;border:1px solid #ccc;border-radius:4px;">
                    <table data-options="bodyCls:'panel-header-gray'" id="LocListGrid"></table>
            </div>
        </div>
</div>

<script type="text/javascript" src="../scripts/dhcpe/ct/inpatienttohp.conitem.hisui.js"></script>
<script type="text/javascript" src="../scripts/dhcpe/ct/dhcpe.ct.common.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
</body>		
</html>
			
		
 


