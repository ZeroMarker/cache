<csr:rule name="DHCTTMENU" match="DHCTT:MENU">
<csr:description>
	cache tip tool create menu
</csr:description>
<!--<csr:attribute name="selectedIndex" description="selected index; [0,len-1]" type="Label:STRING">-->
<csr:action>
	<table>	
	<tbody>	
		<tr>
			<td>
				<div>&nbsp;			
					<script runat=server language=cache>
						s cspurl = ..#CSPURL					
						s cspurl = ##class(%File).GetFilename(cspurl)
						k ^DHCTTMenu("Menu")
						s ^DHCTTMenu("Menu",1,"Home")="#"
						s ^DHCTTMenu("Menu",2,"XMLPrintEdit")="dhctt.xmlprintedit.csp"
						s ^DHCTTMenu("Menu",3,"FindGloabl")="dhctt.findglobal.csp"
						s ^DHCTTMenu("Menu",4,"FindTableStructure")="dhctt.findtablestructure.csp"
						s ^DHCTTMenu("Menu",5,"XMLExport")="dhctt.xmlexport.csp"
						s ^DHCTTMenu("Menu",6,"XMLImport")="dhctt.xmlimport.csp"
						s ^DHCTTMenu("Menu",7,"SQL")="dhctt.sql.csp"
						s ^DHCTTMenu("Menu",8,"Config")="dhctt.config.csp"
						s menulenght=$o(^DHCTTMenu("Menu",""),-1)
						f i=1:1:menulenght d
						.s menuname=$O(^DHCTTMenu("Menu",i,""))
						.w "<a target=""_top"" href="""_^DHCTTMenu("Menu",i,menuname)_""""
						.w:cspurl=^DHCTTMenu("Menu",i,menuname) " class=MenuSelected"
						.w ">"_menuname_"</a> &nbsp;" 
					</script>	
				</div>	
			</td>
		</tr>	
	</tbody>
	</table>
</csr:action>
</csr:rule>

