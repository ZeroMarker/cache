<!DOCTYPE html>
<!--药房药库-处方点评-住院医嘱点评
	csp:pha.prc.v2.create.ipgeneral.csp
	js: pha/prc/v2/create.ipgeneral.js
-->
<html>
<!-- 验证session过期 -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
</csp:method>

<head>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>
    </TRAK:HEAD>
    <HISUI />
    <PHALIBV1 />
</head>

<body>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',border:false" class="pha-body">
            <div class="hisui-layout" fit="true">
				
				<div data-options="region:'north',height:95,border:false,split:true">
                    <div class="hisui-panel" title="文件导入生成点评单"
                        data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true,bodyCls:''" >
                        <div class="pha-row">							
							<div class="pha-col" >
								<label for="conFile" >文件选择</label>
							</div>
							<div class="pha-col">
								<input id='conFileBox' data-pha />
							</div>
							<a  id='btnFileBox' class="hisui-linkbutton">导入</a>
							<div class="pha-col">
								<a class="hisui-linkbutton" data-options="iconCls:'icon-w-download'" id="btnDownLoad">导入模板下载</a> 
							</div>
							<div class="pha-col">
				                 点评单号
				            </div>
				            <div class="pha-col">
				                <input id="importConComNo" class='hisui-validatebox' disabled="true" data-pha="class:'hisui-validatebox'">
				            </div>
				        </div>
                   </div>
                </div>
				
				<div data-options="region:'center',border:false">
                    <div class="hisui-panel" title="手工录入生成点评单" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true,bodyCls:''">
						<div class="hisui-layout" fit="true">
							<div data-options="region:'center',border:false" style="padding:10px;padding-bottom:0px;">
								<div class="hisui-panel" title="统计出院人数"  data-options="headerCls:'panel-header-card-gray',fit:true" style="padding-top:10px;overflow:hidden">
									<div class="pha-row">
										<div class="pha-col" >
											<label for="conStartDate">开始日期</label>
										</div>
										<div class="pha-col">
											<input id='conStartDate' data-pha />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conMultiDocLoc">医生科室</label>
										</div>
										<div class="pha-col">
											<input id='conMultiDocLoc' data-pha="" />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conMultiBillType">费用类型</label>
										</div>
										<div class="pha-col">
											<input id='conMultiBillType' data-pha="" />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											  <div class="pha-drug-gene-label">
												  <label for="genePHCCat"></span>药学分类</label>
											  </div>
										  </div>
										<div class="pha-col">
											 <div class="pha-drug-gene-text">
												 <input id="genePHCCat"
													 data-pha='class:"hisui-triggerbox",requied:true,save:0,clear:true'>
											 </div>
										 </div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conMultiAntDrugLevel">抗菌药物级别</label>
										</div>
										<div class="pha-col" >
											<input id='conMultiAntDrugLevel' data-pha="" />
										</div>
									</div>
									<div class="pha-row">
										<div class="pha-col">
											<label for="conEndDate">截止日期</label>
										</div>
										<div class="pha-col">
											<input id='conEndDate' data-pha />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conMultiPhaLoc">药房名称</label>
										</div>
										<div class="pha-col">
											<input id='conMultiPhaLoc' data-pha="" />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conMultiPosion">管制分类</label>
										</div>
										<div class="pha-col">
											<input id='conMultiPosion' data-pha="" />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conMultiArcDesc">医嘱名称</label>
										</div>
										<div class="pha-col">
											<input id='conMultiArcDesc' data-pha />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											 <label for="conAgeMax">病人年龄下限</label>
										</div>
										<div class="pha-col">
											<input id="conAgeMax" class='hisui-validatebox' data-pha="class:'hisui-validatebox'">
										</div>										
									</div>
									<div class="pha-row">
										<div class="pha-col" style="margin-left:27px;">
											<label for="conDoctor">医生</label>
										</div>
										<div class="pha-col">
											<input id='conDoctor' data-pha />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conMultiBldType">切口类型</label>
										</div>
										<div class="pha-col">
											<input id='conMultiBldType' data-pha="" />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conOperation">手术名称</label>
										</div>
										<div class="pha-col">
											<input id='conOperation' data-pha="" />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="chkBasicFlag">基本药物</label>
										</div>
										<div class="pha-col" style="margin-left:-10px;">
											<input id="chkBasicFlag" type="checkbox" class="hisui-checkbox">
										</div>
										<div class="pha-col" style="margin-left:184px;">
											 病人年龄上限
										</div>
										<div class="pha-col">
											<input id="conAgeMax" class='hisui-validatebox' data-pha="class:'hisui-validatebox'">
										</div>
									</div>
									<div class="pha-row" style="margin-top:20px;">
										<div class="pha-col">
											<a class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:160px;margin-left:348px;" id="btnQuery">统计出院人数</a> 
										</div>
										<div class="pha-col" style="margin-left:42px;">
											 出院人数
										</div>
										<div class="pha-col">
											<input id="conLeaveNum" class='hisui-validatebox' disabled="true" style="width:155px;" data-pha="class:'hisui-validatebox'">
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<a class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="btnClean">清屏</a> 
										</div>
									</div>	
								</div>	
							</div>
						
							<div data-options="region:'south',height:250,border:false,split:false" style="padding:0px 10px;">
								<div class="hisui-panel" title="生成点评单" data-options="headerCls:'panel-header-card-gray',fit:true" style="padding-top:10px;overflow:hidden">	
									<div style="height:205px">
										<div class="pha-row">
											<div class="pha-col">
												<label for="conSaveType">抽取类型</label>
											</div>
											<div class="pha-col">
												<input id='conSaveType' data-pha="" />
											</div>
											<div class="pha-col" style="margin-left:-10px;">
												<input id='conSaveTxt' class='hisui-validatebox' style="width:62px;" data-pha="" />
											</div>
											<div class="pha-col" style="margin-left:56px;">
												 间隔数
											</div>
											<div class="pha-col">
												<input id="conSpaceQty" class='hisui-validatebox' data-pha="class:'hisui-validatebox'" style="width:153px;" data-options="prompt:'录入后回车校验'">
											</div>  
											<div class="pha-col" style="margin-left:56px;">
												 抽查数
											</div>
											<div class="pha-col">
												<input id="conWriteQty" class='hisui-validatebox' style="width:155px;" disabled="true" data-pha="class:'hisui-validatebox'">
											</div> 							
											<div class="pha-col" style="margin-left:30px;" >
												 建议间隔数
											</div>
											<div class="pha-col">
												<input id="conASpaceQty" class='hisui-validatebox' data-pha="class:'hisui-validatebox'" style="width:154px;" disabled="true">
											</div>
											<div class="pha-col" style="margin-left:40px;">
												 理论所需总数
											</div>
											<div class="pha-col">
												 <input id="conTheoryQty" class='hisui-validatebox' disabled="true" data-pha="class:'hisui-validatebox'">
											</div>
										</div>
										<div class="pha-row" style="margin-top:20px;">
											<div class="pha-col">
												<a class="hisui-linkbutton" data-options="iconCls:'icon-w-save'" style="margin-left:347px;width:159px;" id="btnSave">抽取处方样本</a> 
											</div>
											<div class="pha-col" style="margin-left:42px;">
												 点评单号
											</div>
											<div class="pha-col">
												<input id="conComNo" class='hisui-validatebox' disabled="true" style="width:155px;" data-pha="class:'hisui-validatebox'">
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>     
                    </div>
                </div>
            </div>
        </div>
    </div>
	<!-- 导入excel文件读取插件 -->
    <script type="text/javascript" src="../scripts/pha/plugins/xlsx/xlsx.full.min.js"></script>
    <!-- 本页面js -->
    <script type="text/javascript" src="../scripts/pha/prc/com/v2/js/store.js"></script>
    <script type="text/javascript" src="../scripts/pha/prc/v2/create.ipgeneral.js"></script>
</body>

</html>