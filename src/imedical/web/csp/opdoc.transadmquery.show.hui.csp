<div class="hisui-panel" data-options="fit:true,title:'#(..Get("转诊查询"))#',headerCls:'panel-header-gray',iconCls:'icon-apply-check'">
	<div class="hisui-layout" fit="true">
		<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="height:auto;padding:0 10px;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="FDate">#(..Get("开始日期"))#</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="FDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label r-label-1">
						<label for="TDate">#(..Get("结束日期"))#</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="TDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label r-label-1">
						<label for="TLocDesc">#(..Get("转诊科室"))#</label>
					</td>
					<td>
						<input class="textbox" id="TLocDesc"/>
					</td>
					<td class="r-label r-label-1">
						<label for="TUserName">#(..Get("转诊人"))#</label>
					</td>
					<td>
						<input class="textbox" id="TUserName"/>
					</td>
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">#(..Get("查询"))#</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="RegNo">#(..Get("登记号"))#</label>
					</td>
					<td>
						<input class="textbox" id="RegNo"/>
					</td>
					<td class="r-label r-label-1">
						<label for="PName">#(..Get("姓名"))#</label>
					</td>
					<td>
						<input class="textbox" id="PName"/>
					</td>
					<td>
					</td>
					<td>
					</td>
					<td>
					</td>
					<td>
					</td>
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">#(..Get("清屏"))#</a>
					</td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',title:'',bodyCls:'panel-body-gray',collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="border-width: 1px 0 0 0 ; border-radius: 0;">
			<table id="TransAdmTab"></table>
		</div>
	</div>
</div>
