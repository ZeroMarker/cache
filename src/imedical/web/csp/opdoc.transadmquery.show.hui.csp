<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="height:135px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'转诊查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-top:0;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="FDate">开始日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="FDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label r-label-1">
						<label for="TDate">结束日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="TDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label r-label-1">
						<label for="TLocDesc">转诊科室</label>
					</td>
					<td>
						<input class="textbox" id="TLocDesc"/>
					</td>
					<td class="r-label r-label-1">
						<label for="TUserName">转诊人</label>
					</td>
					<td>
						<input class="textbox" id="TUserName"/>
					</td>
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="RegNo">登记号</label>
					</td>
					<td>
						<input class="textbox" id="RegNo"/>
					</td>
					<td class="r-label r-label-1">
						<label for="PName">姓名</label>
					</td>
					<td>
						<input class="textbox" id="PName"/>
					</td>
					<td>
					</td>
					<td>
					</td>
					<td>
					</td>
					<td>
					</td>
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="TransAdmTab"></table>
		</div>
	</div>
