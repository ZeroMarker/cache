<!-- 
 * FileName: dhcbill.ipbill.chequeback.csp
 * Anchor: ZhYW
 * Date: 2020-01-02
 * Description: 支票到账
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.chequeback.css'/>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',border:false,collapsible:false" style="height:124px;">
			<div class="hisui-panel" data-options="title:'支票到账',fit:true,headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border-bottom-style:none;border-radius:0;">
				<table class="search-table">
					<tr>
						<td class="r-label"><label>开始日期</label></td>
						<td><input id="stDate" class="hisui-datebox textbox"></td>
						<td class="r-label r-label-1"><label>结束日期</label></td>
						<td><input id="endDate" class="hisui-datebox textbox"/></td>
						<td class="r-label r-label-1"><label>支付方式</label></td>
						<td><input id="paymode" class="hisui-combobox textbox"/></td>
						<td class="r-label r-label-1"><label>到账标志</label></td>
						<td><input id="backFlag" class="hisui-combobox textbox"></td>
						<td class="r-label r-label-1"><a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a></td>
					</tr>
					<tr>
						<td class="r-label"><label>账号</label></td>
						<td><input id="cardNo" class="textbox"></td>
						<td class="r-label r-label-1"><label>备注</label></td>
						<td colspan="5"><input id="remark" class="textbox tb-colspan"/></td>
						<td class="r-label r-label-1"><a href="javascript:;" class="hisui-linkbutton" id="btn-insert" data-options="iconCls:'icon-w-save'">到账</a></td>
						<td class="r-label"><a href="javascript:;" class="hisui-linkbutton" id="btn-update" data-options="iconCls:'icon-w-update'">修改</a></td>
					</tr>
				</table>
			</div>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
			<table class="hisui-datagrid" id="chequeList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.chequeback.js"></script>
</body>
</html>
