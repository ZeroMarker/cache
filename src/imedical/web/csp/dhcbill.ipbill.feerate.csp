<!-- 
 * FileName: dhcbill.ipbill.feerate.csp
 * Anchor: ZhYW
 * Date: 2019-12-30
 * Description: 住院患者费用比例
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.feerate.css'/>
	<server>
		w "<input id='EpisodeID' name='EpisodeID' type='hidden'>",!
		w "<input id='BillRowId' name='BillRowId' type='hidden'>"
	</server>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',collapsible:false,title:'住院患者费用比例',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:167px;">
			<table class="search-table" style="width:90%">
				<tr>
					<td class="r-label td-first-col-58"><label>登记号</label></td>
					<td><input id="patientNo" class="textbox"/></td>
					<td class="r-label"><label>账单号</label></td>
					<td><input id="billList" class="textbox"/></td>
					<td class="r-label"><label>科室</label></td>
					<td><input id="dept" class="textbox" disabled/></td>
					<td class="r-label"><label>总金额</label></td>
					<td><input id="totalAmt" class="hisui-numberbox textbox" data-options="disabled:true,precision:2"/></td>
				</tr>
				<tr>
					<td class="r-label"><label>姓名</label></td>
					<td><input id="patName" class="textbox" disabled/></td>
					<td class="r-label"><label>床号</label></td>
					<td><input id="bed" class="textbox" disabled/></td>
					<td class="r-label"><label>住院天数</label></td>
					<td><input id="admDays" class="textbox" disabled/></td>
					<td class="r-label"><label>入院日期</label></td>
					<td><input id="admDate" class="textbox" disabled/></td>
				</tr>
				<tr>
					<td class="r-label"><label>出院日期</label></td>
					<td><input id="dischDate" class="textbox" disabled/></td>
					<td class="r-label"><label>费别</label></td>
					<td><input id="admReason" class="textbox" disabled/></td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
			<table class="hisui-datagrid" id="rateList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.feerate.js"></script>
</body>
</html>
