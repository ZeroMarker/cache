<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!--ma.ipmr.ss.workflow.retrieve.csp-->
<!DOCTYPE html>
<html lang="en">
	<head>
		<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
		<HISUI/>
		<IPMR:LOGON/>
		<!--
		<ADDINS/>
		<ADDINS require="LODOP,CmdShell"/>-->
		<link type="text/css" rel="styleSheet"  href="../scripts/ipmr/css/base.css" />
		<style type="text/css">
			.datagrid-btn-separator.small {
				float: none;
				height: 35px;
				border-left: 1px solid #ccc;
				margin: 0px 4px;
				display: inline-block;
				vertical-align: middle;
			}
			#CheckUserDialog .dialog-button {
			    padding: 0px 10px 20px 10px;
			    text-align: center;
			    border: none;
			    background-color: #fff;
			}
		</style>
	</head>
	<body style="padding:10px;">
		<csp:Include Page="ma.ipmr.ss.workflow.retrieve.show.csp">
		<script language="cache" runat="server">
			Set MrClass=$Get(%request.Data("MrClass",1),"")
			Set:MrClass="" MrClass="I"			// 如果为空，设置为住院病案
			d ##class(web.DHCXMLPConfig).LODOPInit()
			Set AutoPrintMBar=##class(CT.IPMR.BT.Config).GetValByCode("AutoPrintMBar")	
			Set AutoPrintVBar=##class(CT.IPMR.BT.Config).GetValByCode("AutoPrintVBar")	
			Set EnsSysConfig = ##Class(MA.IPMR.ImedicalSrv).getEnsSysConfig()
			Set FillNoConfig = ##class(CT.IPMR.BT.Config).GetValByCode("FillNoConfig","")
			Set FillNoAct = $p(FillNoConfig,"|",1)
			Set FillNoType = $p(FillNoConfig,"|",2)
			Set CreatFillNo=0
			If (FillNoAct=1)&&(FillNoType=1) { // 回收生成卷归档号
				Set CreatFillNo= 1
			}
			Set IsAutoBack = ##class(CT.IPMR.BT.Config).GetValByCode("IsAutoBack","")   // 是否扫码自动回收
	  	</script>
	  	<input id="InvPrintEncrypt" name="InvPrintEncrypt" type=hidden value="#(##Class(%CSP.Page).Encrypt($lb("web.DHCXMLIO.ReadXML")))#">
		<SCRIPT language = 'javascript' >
			//全局请求后台服务对象
			var ServerObj={
				MrClass		: '#(MrClass)#',
				AutoPrintMBar	: '#(AutoPrintMBar)#',
				AutoPrintVBar	: '#(AutoPrintVBar)#',
				EnsSysConfig	: '#(EnsSysConfig)#',
				CreatFillNo	: '#(CreatFillNo)#',
				IsAutoBack	: '#(IsAutoBack)#'
			};
		</SCRIPT>
		<script type="text/javascript"  src="../scripts/websys.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/print.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/common.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/extend.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/plugins/datagrid-export.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/ss/workflow/retrieve.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/ss/workflow/volselect.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/ss/workflow/checkuser.js"></script>
		<script type="text/javascript"  src="../scripts/DHCPrtComm.js"></script>
		
	</body>
</html>