<!doctype html>
<!--dhcdoc.deldiaglist.csp 诊断删除记录-->
<HTML lang="zh-CN">
<HEAD>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI></HISUI>
</head>
<body>
	<div class="hisui-layout" data-options="fit:true,border:false">
		<div data-options="region:'center',title:'',border:false,split:true,headerCls:'panel-header-gray'" style="padding:10px">
			<div class="hisui-panel" fit="true" data-options="" style="border:1px solid #e2e2e2;border-radius:4px;">
				<table id="tabDelList"></table>
			</div>
		</div>
	</div>
	<script type="text/javascript">
		var mradm='#(%request.Get("mradm"))#';
		$(function(){
			var Columns=[[
				{field:'delDate',title:'删除时间'},
				{field:'delUserName',title:'删除人'},
				{field:'DiagnosCat',title:'分类'},
				{field:'MRCIDDesc',title:'诊断',width:110},
				{field:'DiagNotes',title:'备注',width:60},
				{field:'DiagStat',title:'诊断状态'},
				{field:'MainDiagFlag',title:'主诊断'},
				{field:'DIAOnsetDate',title:'发病日期'},
				{field:'MRDIADate',title:'诊断日期'},
				{field:'DiagnosDoctor',title:'诊断录入人'}
			]]
			tabDelListDataGrid=$("#tabDelList").datagrid({  
				fit : true,
				border : false,
				rownumbers:true,
				striped : false,
				singleSelect : false,
				fitColumns : false,
				autoRowHeight : false,
				pagination : true, 
				url:$URL+"?ClassName=web.DHCDocDiagnosEntryV8&QueryName=GetDelDiagList&mradm="+mradm, 
				pageSize: 20,
				pageList : [20,100,200],
				idField:'index',
				columns :Columns,
			})
		})
	</script>
</body>
</html>
