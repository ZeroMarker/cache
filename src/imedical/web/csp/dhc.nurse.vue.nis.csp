    <script language="cache" runat="SERVER">
      s flag2 = ##class(websys.Conversions).IsValidMethodName("Nur.NIS.Common.SystemConfig","GetChromePrintFlag")
      s chromePrintFlag = "false"
      if (flag2 = "1") {
        s chromePrintFlag = ##class(Nur.NIS.Common.SystemConfig).GetChromePrintFlag()
      }
      s pageNameNew = $G(%request.Data("pageName", 1))
      s emrPrintFlag = 1 //需要加载病历打印插件
      if (pageNameNew["bedChart") { //床位图不加载病历打印插件
          s emrPrintFlag = 0
      }
    </script>
    <csp:if condition='chromePrintFlag="true"'>
      <script type="text/javascript" src="../scripts/nurse/hisui-0.1.0/dist/js/PrintProvider.js">
