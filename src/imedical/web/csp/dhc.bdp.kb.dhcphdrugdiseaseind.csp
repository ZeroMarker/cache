<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired()
 //i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
 q 1
 /*
	@CSP:dhc.bdp.kb.dhcphdrugdiseaseind.csp
	@JS:bdp/App/KB/NewBus/DHC_PHDrugDiseaseInd.js
	@Title:药品说明书编辑器――适应证
	@Description:适应证的增删改
	@Creator:基础数据平台组 - 丁亚男
	@CreateDate:2018-09-05
*/
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
 <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
 <TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
 <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
 <HISUI/>
 <link rel="stylesheet" type="text/css" href="../scripts/bdp/Framework/css/styleUI.css" />
 <script type="text/javascript" src="../scripts/bdp/Framework/scripts/FunLibUI.js"></script> 
<style type="text/css">
/*多选框*/
.mulitiDiv {
	width:560px;
	height:360px;
	border: 1px solid #40a2de;
}

.webui-popover-title {
	padding:8px 14px;
	display: block;
    background-color: #f9f9fa;
    border-bottom: 1px solid #cccccc;
    border-radius: 5px 5px 0 0;
    color: black
}
.webui-popover .webui-popover-content {
    padding: 1px 5px;
}
.panel-header-noborder {
    border-width: 0 0 0px 0;
}
</style>
<script type="text/javascript"  >
    $(function(){
              BodyLoadHandler();

    });
</script>
</HEAD>
<BODY >
    <server>
    	s GlPGenDr=%request.Get("GlPGenDr")
    	s GlPPointer=%request.Get("GlPPointer")
        s GlPPointerType=%request.Get("GlPPointerType")
   </server> 
    <div class="hisui-layout" data-options="fit:true" border="false" >
    	<div data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',region:'north',split:true"  style="height:148px;" border="false" title="适应证" > 
			<table data-options="fit:true,nowrap:false"  id="mygrid" border="false"></table> 
		</div> 
       	<div data-options="headerCls:'panel-header-gray',region:'center'" border="false"> 
       		<div class="hisui-layout" data-options="fit:true" border="false" >
       			<div id="mytbar" data-options="region:'north',split:false"  style="height:34px;text-align:left;border-top:1px solid #C0C0C0;border-bottom:1px dashed #C0C0C0;overflow:hidden" border="false">
	 			   	 <a   class="hisui-linkbutton" iconCls='icon-add' plain="true" id="btnAdd">新增</a>
	    	         <a   class="hisui-linkbutton" iconCls='icon-write-order' plain="true" id="btnUpd">修改</a>
	    	         <a   class="hisui-linkbutton" iconCls='icon-cancel' plain="true" id="btnDel">删除</a>
	    	         <a   class="hisui-linkbutton" iconCls='icon-refresh' plain="true" id="btnRel">清屏</a>
				</div>	
	       		<div data-options="region:'center',split:false" style="padding:4px 30px 20px 10px" border="false">
			    	 <form id="form-save" method="post" data-options="fit:true,border:false,plain:true"> 
			    	 <table cellpadding="5">
					    <tr>
				               <td class="tdleftlabel">级别</td>
				               <td>
				               		<table>
						    		  <tr>
				               				<td><input id="PHINSTModeF" name="PHINSTMode"  style="width:180px;"></td>
				               				<td class="tdlabel" width="30px">年龄</td>
				               				<td><input id="PDAAgeDrF" name="PDAAgeDr"  style="width:160px;"></td>
					    		   			<td class="tdlabel" width="70px">年龄限制</td>
					    		   			<td><input id="PDAMinValF" name="PDAMinVal" type="text" class="hisui-validatebox" style="width:60px" ></td>
								            <td>-</td>
								            <td><input id="PDAMaxValF" name="PDAMaxVal" type="text" class="hisui-validatebox" style="width:60px" ></td>
								            <td><input id="PDAUomDrF" name="PDAUomDr"  style="width:120px;"></td>
					    				</tr>
					    			</table>
					    		</td>
					    </tr>	
					    <tr>
				    		   
				    		   <td class="tdleftlabel">检验项目</td>
				    		   <td>
				               		<table>
						    		  <tr>
						    		  		<td><input id="LABILabDrF" name="LABILabDr"  style="width:180px;"></td>
						    		  		<td class="tdlabel" width="30px">逻辑</td>
						    		  		<td><input id="LABIRelationF" name="LABIRelation"  style="width:160px;" disabled="true"></td>
											<td class="tdlabel" width="70px">指标值范围</td>
											<td><input id="LABIMinValF" name="LABIMinVal" type="text" class="hisui-validatebox" style="width:60px" disabled="true" ></td>
											<td>-</td>
											<td><input id="LABIMaxValF" name="LABIMaxVal" type="text" class="hisui-validatebox" style="width:60px" disabled="true" ></td>
											<td><input id="LABIUomDrF" name="LABIUomDr"  style="width:120px;" disabled="true"></td>
					   				 </tr>
					   				</table>
				              </td>
					    </tr>
					    <tr>
				               <td class="tdleftlabel">性别</td>
				               <td>
				               		<table>
						    		  <tr>
											<td id="PHINSTSexF">
					           					 <input class='hisui-radio' type="radio" label='男' name='PHINSTSex' value='M' id="sexM">
					            				 <input class='hisui-radio' type="radio" label='女' name='PHINSTSex' value='F' id="sexF">
					           					 <input class='hisui-radio' type="radio" label='全部' name='PHINSTSex' value='A' data-options="checked:true" id="sexA">
					        				</td>
						              </tr>
					              </table>
				              </td>
					    </tr>
					   
					    <tr>
				    		  <td class="tdleftlabel">提示(医生)</td>
				              <td><textarea id="PHINSTDocUseTipsF" name="PHINSTDocUseTips" type="text" class="hisui-validatebox" style="width:750px;height:30px;overflow:hidden" ></textarea></td>
					    </tr>    
					    <tr>
				    		  <td class="tdleftlabel">备注</td>
				              <td><textarea id="PHINSTNoteF" name="PHINSTNote" type="text" class="hisui-validatebox" style="width:750px;height:30px;overflow:hidden"  ></textarea></td>
					    </tr> 
					    <tr>
				    		  <td class="tdleftlabel">描述</td>
				              <!--<td><input id="PHINSTTextF" name="PHINSTText" class="hisui-textbox" data-options="multiline:true" style="width:260px;height:60px" ></td>-->
				              <td><textarea id="PHINSTTextF" name="PHINSTText" type="text" class="hisui-validatebox" style="width:750px;height:90px;overflow:hidden" data-options="required:true"></textarea></td>
					    </tr> 
					    <tr>
					       <table>
					        <tr>
	                        <td>
		                        <div class="hisui-panel" title="病症" style="width:415px;height:280px;" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',">   
									
										<table style="width:380px;height:260px;"  id="diaGrid" border="false" toolbar="#myDiatbar"></table>
								</div>	
	                        </td>
	                        <td   style="padding: 2px 0px 2px 10px;">
		                        <div class="hisui-panel" title="治疗手术" style="width:420px;height:280px;" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',"> 
										<table style="height:auto" width="100%" id="OperaGrid" border="false" toolbar="#myOperatbar" ></table>
								</div>	
	                        </td>
	                        </tr>
	                        </table>
	                    </tr>	
					    <tr>
					       <table>
					        <tr>
	                        <td>
		                        <div class="hisui-panel" title="症状体征" style="width:415px;height:280px;" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',">   
									<table style="width:380px;height:260px;"  id="SymGrid" border="false" toolbar="#mySymtbar"></table>
								</div>	
	                        </td>
	                        <td   style="padding: 2px 0px 2px 10px;">
		                        <div class="hisui-panel" title="细菌" style="width:420px;height:280px;" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',"> 
										
										<table style="height:auto" width="100%" id="BtrGrid" border="false"toolbar="#myBtrtbar" ></table>
								</div>	
	                        </td>
	                        </tr>
	                        </table>
	                    </tr>
	                    <tr>
	                      <table>
					        <tr>
	                       <td>
		                        <div class="hisui-panel" title="菌属" style="width:415px;height:280px;" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',">   
									
										<table style="width:380px;height:260px;"  id="GenusGrid" border="false"toolbar="#myGenustbar" ></table>
								</div>	
	                        </td>
	                        </table >
	                    </tr>                	                    		                   		    
					 </table >
					 </form>
				</div>
	    	</div>
		</div> 	 
    </div>
    <!--已选病症列表工具条-->
    <div id="myDiatbar"  style="padding: 1px; height: auto; "  >
		<a class="hisui-linkbutton" iconCls='icon-add' plain="true" id="btnAddDia">新增病症</a>								    
	 </div> 
	 <!--已选手术列表工具条-->
	 <div id="myOperatbar" style="padding: 1px; height: auto; "  >
		<a class="hisui-linkbutton" iconCls='icon-add' plain="true" id="btnAddOpera">新增治疗手术</a>									    
 	</div>
	 <!--已选症状体征列表工具条-->									
	 <div id="mySymtbar" style="padding: 1px; height: auto; "  >
		<a class="hisui-linkbutton" iconCls='icon-add' plain="true" id="btnAddSym">新增症状体征</a>								    
	 </div>
	 <!--已选细菌列表工具条-->	
	<div id="myBtrtbar" style="padding: 1px; height: auto; "  >
		<a class="hisui-linkbutton" iconCls='icon-add' plain="true" id="btnAddBtr">新增细菌</a>									    
 	</div>
	<!--已选菌属列表工具条-->	
	 <div id="myGenustbar" style="padding: 1px; height: auto; "  >
		<a class="hisui-linkbutton" iconCls='icon-add' plain="true" id="btnAddGenus">新增菌属</a>								    
	 </div>
	<!--未选病症列表工具条-->
    <div id="myunDiatbar" data-options="split:false"  style="text-align: left; display:block;" >
        <a href="#" class="hisui-searchbox" data-options="stopAllEventOnDisabled:true" id="btnDiaSearch"></a>
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnDiaReset">清屏</a> 
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnDiaSave">保存</a>        
    </div> 
    
    <!--未选治疗手术列表工具条-->
    <div id="myunOperatbar" data-options="split:false"  style="text-align: left; display:block;" >
        <a href="#" class="hisui-searchbox" data-options="stopAllEventOnDisabled:true" id="btnOperaSearch"></a>
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnOperaReset">清屏</a> 
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnOperaSave">保存</a>        
    </div> 
    
	<!--未选症状体征列表工具条-->
    <div id="myunSymtbar" data-options="split:false"  style="text-align: left; display:block;" >
        <a href="#" class="hisui-searchbox" data-options="stopAllEventOnDisabled:true" id="btnSymSearch"></a>
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnSymReset">清屏</a> 
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnSymSave">保存</a>        
    </div> 
    
	<!--未选细菌列表工具条-->
    <div id="myunBtrtbar" data-options="split:false"  style="text-align: left; display:block;" >
        <a href="#" class="hisui-searchbox" data-options="stopAllEventOnDisabled:true" id="btnBtrSearch"></a>
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnBtrReset">清屏</a> 
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnBtrSave">保存</a>        
    </div> 
	
	<!--未选菌属列表工具条-->
    <div id="myunGenustbar" data-options="split:false"  style="text-align: left; display:block;" >
        <a href="#" class="hisui-searchbox" data-options="stopAllEventOnDisabled:true" id="btnGenusSearch"></a>
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnGenusReset">清屏</a> 
        <a href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true" id="btnGenusSave">保存</a>        
    </div> 
    
    <script type="text/javascript">
     var  GlPGenDr="#(GlPGenDr)#"
     var  GlPPointer="#(GlPPointer)#"
     var  GlPPointerType="#(GlPPointerType)#"
  </script>
  <script type="text/javascript" src="../scripts/bdp/App/KB/NewBus/DHC_PHDrugDiseaseInd.js"></script>
</BODY>
</HTML>