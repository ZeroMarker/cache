<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<HTML>
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>
<style type="text/css">
.eq-table-report-label{text-align:right;padding-right:10px;}
.eq-table-report-td{display:table-cell;height:100%;padding-top:10px;}
</style>

</HEAD>
<body class="hisui-layout"> 
	<div data-options="region:'north',border:false,split:true" style="height:100px;padding:10px 10px 0 10px;"> 
		<div class="hisui-panel" title="Table列显示编辑界面" data-options="fit:true,border:true,'eqtitle':'maintitle'">
				<div class="eq-table" style="width:100%">
					<div class="eq-table-tr">
						<div class="eq-table-report-td eq-table-report-label"><label id="cSourceType" for="SourceType">来源类型:</label></div>
						<div class="eq-table-report-td eq-table-input"><select class="hisui-combobox hisui-validatebox" style="width:170px" id="SourceType" data-required="true" data-options=" prompt:'必填项'">  <!--modify by wl 2019-9-3 1015165 增加参数-->
						<option value></option>
						<option value="0">系统</option>
						<option value="1">院区</option>
						<option value="2">安全组</option>
						<option value="3">人员</option></select>
						</div>					
						<div class="eq-table-report-td eq-table-report-label"><label id="cSourceID" for="SourceID">来源:</label></div>
						<div class="eq-table-report-td eq-table-input"><input class="hisui-validatebox" id="SourceID" ></div>					
						<div class="eq-table-report-td eq-table-report-label"><label id="cComponent" for="Component">组件名:</label></div>
						<div class="eq-table-report-td eq-table-input"><input class="hisui-validatebox" id="Component" data-options="'component':'PLAT.L.Component'"></div>					
						<div class="eq-table-report-td eq-table-button"><a id="BAddComponent" href="#" class="hisui-linkbutton" iconCls="icon-edit">组件定义</a></div>
						<div class="eq-table-report-td eq-table-button"><a id="BFind" href="#" class="hisui-linkbutton" iconCls="icon-search">查找</a></div>
						<div class="eq-table-report-td eq-table-button"><a id="BSvae" href="#" class="hisui-linkbutton" iconCls="icon-save">保存</a></div>
			    	</div>
					<div class="eq-table-tr">
		                <td><input id="ComponentDR" type="hidden" value=""></td>
		                <!--modify by lmm 2019-11-19-->
		                <td><input id="SourceIDDR" type="hidden" value=""></td>
			    	</div>
			    </div>		
		
		</div>	
	</div>
	<div data-options="region:'center',border:false,split:true" style="width:260px;padding:5px 5px 10px 10px;">
		<div class="hisui-panel" title="名称" data-options="fit:true,border:true,'eqtitle':'listtitle'">
			<table id="ComponentSet"></table>
			
		</div>
	</div>
	<div data-options="region:'east',border:false,split:true" style="width:800px;padding:5px 10px 10px 0;">
		<div class="hisui-panel" title="明细" data-options="fit:true,border:true,'eqtitle':'listtitle'">
			<table id="ComponentSetItem"></table>
		</div>
	</div>
	<!--模态窗口-->
	<div id="modelDlg"></div>
<script type="text/javascript" src="../scripts/dhceq/plat/componentset.js"></script>
</body>

</html>