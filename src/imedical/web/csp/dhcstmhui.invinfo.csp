<!--发票信息维护-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'west',border:false,collapsible:false" style="width:650px;">
		<div class="hisui-panel" title="发票信息列表" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false,collapsible:false" style="border-bottom:none;">
					<table id="MainConditions" class="Condition">
						<tr>
							<td><label>发票ID</label><input id="InvId_1" name="InvId" class="textbox"></td>
							<td><label>供应商</label><input id="Vendor" name="Vendor" class="textbox hisui-combobox"></td>
							<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
						</tr>
						<tr>
							<td><label>发票代码</label><input id="InvCode" name="InvCode" class="textbox"></td>
							<td><label>发票号</label><input id="InvNo" name="InvNo" class="textbox"></td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false,collapsible:false">
					<table id="InvGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div class="hisui-tabs tabs-gray GridPadding" data-options="region:'center'" style="padding-left:0;border:none;"> 
		<div title="发票信息">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false" style="border-bottom:1px dashed #CCCCCC;padding:5px;">
					<a href="#" id="AddBT" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">新增</a>
					<a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">保存</a>
				</div>
				<div data-options="region:'center',border:false">
					<table id="Conditions" class="Condition">
						<input id="RowId" name="RowId" type="hidden">
						<tr>
							<td><span class="required">*</span><label>供应商</label><input id="Vendor_1" name="Vendor" class="hisui-combobox textbox" data-options="required:true"></td>
						</tr>
						<tr>
							<td><span class="required">*</span><label>发票ID</label><input id="InvId" name="InvId" class="hisui-validatebox textbox" data-options="required:true"></td>
						</tr>
						<tr>
							<td><span class="required">*</span><label>发票代码</label><input id="InvCode_1" name="InvCode" class="hisui-validatebox textbox" data-options="required:true"></td>
						</tr>
						<tr>
							<td><span class="required">*</span><label>发票号</label><input id="InvNo_1" name="InvNo" class="hisui-validatebox textbox" data-options="required:true"></td>
						</tr>
						<tr>
							<td><span class="required">*</span><label>发票金额</label><input id="InvAmt" name="InvAmt" class="hisui-validatebox textbox" data-options="required:true"></td>
						</tr>
						<tr>
							<td><label>发票日期</label><input id="InvDate" name="InvDate" class="hisui-dateboxq textbox"></td>
						</tr>
					</table>
				</div>
			</div>
		</div>
		<!--<div title="图片" data-options="closable:false">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false" style="padding:5px;">
					<a href="#" id="UpLoadBT" class="hisui-linkbutton" data-options="iconCls:'icon-upload-cloud',plain:true">上传</a>
					<a href="#" id="TakePhotoBT" class="hisui-linkbutton" data-options="iconCls:'icon-camera',plain:true">拍照</a>
				</div>
			</div>
		</div>-->
	</div>
	<div id="UpLoadWin" class="hisui-dialog" data-options="closed:'true',modal:true" style="width:600px;height:480px" title="图片上传"> 
		<div class="hisui-layout" data-options="fit:true">   
			<div class="BorderLine" data-options="region:'north',border:false" style="height:58px;">
				<table class="Condition">
					<tr>
						<td><div id="AddPicBT">添加图片</div></td>
						<td><div id="UpLoadPicBT" class="hisui-linkbutton">上传图片</div></td>
					</tr>
				</table>
			</div> 
			<div data-options="region:'center',border:false">   
				<div id="PicList" class="uploader-list" style="padding:10px;"></div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/jquery.tmpl.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/Viewer/viewer.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/webuploader/webuploader.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonuploader.js"></script>	
	<script type="text/javascript" src="../scripts/dhcstmhisui/InvManage/InvInfo.js"></script>
</body>
</html>	