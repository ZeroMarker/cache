<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<DHCEQ:HISUI></DHCEQ:HISUI>
	<HISUI/>
</head>
<body class="hisui-layout">
	<script language=Cache runat=server>
		s EquipDR=$Get(%request.Data("EquipDR",1))
		s ReadOnly=$Get(%request.Data("ReadOnly",1))
	</script>
	<div data-options="region:'center',border:false">
		<div class="hisui-panel" data-options="border:false" style="padding:10px;">
			<div>
				<div class="eq-table">
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-item">基本信息</div>
					</div>
				</div>
				<div class="eq-table">
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label"><label id="cBDEquipDR_EQName" for="BDEquipDR_EQName">房屋名称</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDEquipDR_EQName" disabled="true"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDBuildingArea" for="BDBuildingArea">建筑面积</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDBuildingArea"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDSubArea" for="BDSubArea">地下面积</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDSubArea"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDPlace" for="BDPlace">位置</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDPlace"></div>
					</div>
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label"><label id="cBDUtilizationArea" for="BDUtilizationArea">使用面积</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDUtilizationArea"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDSelfUseArea" for="BDSelfUseArea">业务用面积</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDSelfUseArea"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDWorkArea" for="BDWorkArea">办公用面积</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDWorkArea"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDOtherArea" for="BDOtherArea">其他用面积</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDOtherArea"></div>
					</div>
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label"><label id="cBDOwnerFlag" for="BDOwnerFlag">是否有产权</label></div>
						<div class="eq-table-td eq-table-input"><input type="checkbox" class='hisui-checkbox' id="BDOwnerFlag"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDOwnerCertificate" for="BDOwnerCertificate">权属证明</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDOwnerCertificate"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDCertificateNo" for="BDCertificateNo">权属证号</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDCertificateNo"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDCertificateDate" for="BDCertificateDate">发证日期</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="BDCertificateDate"></div>
					</div>
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label"><label id="cBDFloorNum" for="BDFloorNum">地上楼层数</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDFloorNum"></div>
						<div class="eq-table-td eq-table-label"><label id="cBDUnderFloorNum" for="BDUnderFloorNum">底下楼层数</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BDUnderFloorNum"></div>
						<div class="eq-table-td eq-table-label"></div>
						<div class="eq-table-td eq-table-input"></div>
						<div class="eq-table-td eq-table-label"></div>
						<div class="eq-table-td eq-table-input"></div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div data-options="region:'south',border:false" class="footer">
		<div class="footer-box">
			<div style="float:left;width:100%;"><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">保存</a></div>
		</div>
	</div>

	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="BDEquipDR" value="#(EquipDR)#">
			<INPUT type="hidden" id="ReadOnly" value="#(ReadOnly)#">
			<INPUT type="hidden" id="BDRowID" value="">
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/em/building.js'></script>
</body>
</html>