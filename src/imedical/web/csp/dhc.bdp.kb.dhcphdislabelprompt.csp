
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
/*
    @CSP：dhc.bdp.kb.dhcphdislabelprompt.csp
    @JS:DHC_PHDisLabelPrompt.js
    @Title：推导目录提示表
    @Creator:石萧伟
    @Description:推导目录提示表
    @CreateDate：2018-08-16
*/
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
 <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
 <TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
 <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI/>
<link rel="stylesheet" type="text/css" href="../scripts/bdp/Framework/css/styleUI.css" />
<script type="text/javascript" src="../scripts/bdp/Framework/scripts/FunLibUI.js"></script>
<script type="text/javascript" src="../scripts/bdp/App/KB/NewDic/DHC_PHDisLabelPrompt.js"></script>
<style>
</style>
</HEAD>
<BODY>
    <server>
    	s parref=%request.Get("parref")
   </server> 
     <div class="hisui-layout" data-options="fit:true" border="false" id="layout">
		<div data-options="region:'center',split:true,headerCls:'panel-header-gray'" style="padding:10px" border="false">
			<table data-options="fit:true,bodyCls:'panel-header-gray'" id="Promptgrid" border="true" toolbar="#allbar"></table> <!--toolbar="#allbar"-->
		</div>      
 	</div> 
     <!--增改弹窗-->
     <div id="myWin" style="width:400px;height:300px;top:30px;padding:10px;display:none;">   
		<form id="form-save" method="post" data-options="fit:true,border:false,plain:true"> 
			<table id="addtable" align="center">
				 <tr>
				 	<td><input id="PDPLastId" name="PDPLastId" type="hidden"></td>
				 </tr>			
				 <tr>
				   <td class="tdlabel"><font color=red>*</font>关联目录</td>
				   <td><input id="PDPLabelDr" name="PDPLabelDr" type="text" class="hisui-validatebox" style="width:250px"></td>
				 </tr>	
				 <tr>
				   <td class="tdlabel"><font color=red>*</font>关联项目</td>
				   <td><input id="PDPPrompt" name="PDPPrompt" type="text" class="hisui-validatebox" style="width:250px"></td>
				 </tr>					 
				 <tr>
				   <td class="tdlabel">结果</td>
				   <td><input id="PDPPromptVal" name="PDPPromptVal" type="text" class="hisui-validatebox" style="width:243px"></td>
				 </tr>	
	    		 <tr>
	    		   <td class="tdlabel">系统标识</td>
				   <td><input class='hisui-checkbox' type="checkbox" label="" id="PDPSysFlag" name="PDPSysFlag" data-options="checked:true" value="Y"></td>
				 </tr>				 							
			</table >
		</form>
	</div>
    <!--数据面板工具栏-->
    <div id="allbar" style='padding-top:3px'>
    	<span style="padding-left:10px;"><h style="padding-right:10px">关联目录</h><input style="width:165px;" id="PromptId"></span>
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find',stopAllEventOnDisabled:true" id="btn_search" style="margin-bottom:2px;margin-left:10px">查询</a>
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean',stopAllEventOnDisabled:true" id="btnRefresh" style="margin-bottom:2px;margin-left:10px">清屏</a><br/>
    	<div style='border-top: 1px dashed #C0C0C0;font-size: 14px;margin-top:3px'></div>
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true" id="add_btn">新增</a>
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true,stopAllEventOnDisabled:true" id="update_btn">保存</a> 
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true" id="del_btn">删除</a>
    </div>
    <script type="text/javascript">
     var  parref="#(parref)#"
  </script>    
</BODY>
</HTML>

