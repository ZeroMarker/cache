<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>
<style type="text/css">
i{display:block;width:30px;}
</style>
</HEAD>
<SCRIPT language="Cache" RUNAT="SERVER">
	s BussType=$Get(%request.Data("BussType",1))
	s BussNo=$Get(%request.Data("BussNo",1))
</Script>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="eqtitle:'maintitle',fit:true,title:'单据修改'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="height:60px;border-bottom:dashed 1px #ccc;">
					<div class="eq-table">
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cBussType" for="BussType">业务类型</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-combobox textbox" id="BussType" value="#(BussType)#"></div>
							<div class="eq-table-td eq-table-label"><label id="cBussNo" for="BussNo">业务单号</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BussNo" value="#(BussNo)#"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td"><a id="BFind" href="#" class="hisui-linkbutton" iconCls="icon-w-find" data-options="stopAllEventOnDisabled:true">查询</a></div>
						</div>
					</div>
				</div>
				<div data-options="region:'center',border:false" style="padding:10px;">
					<div class="hisui-layout" data-options="fit:true,border:false" id="BusinessContent">
						<div data-options="region:'north',border:false,fit:true" style="padding:10px;"><!--主单区域-->
							<div style="background:url(../images/no_data.png) no-repeat center;height:100%;width:100%" id="BusinessMain"></div>
						</div>
						<div data-options="region:'center',border:false" style="padding:10px;"><!--明细区域-->
							<div class="hisui-panel" data-options="eqtitle:'edittitle',title:'主单信息'" style="padding:10px;margin-bottom:10px;">
								<div class="eq-table">
									<div class="eq-table-tr">
										<div class="eq-table-td eq-table-label"><label id="cISInStockNo" for="ISInStockNo">入库单号</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISInStockNo"></div>
										<div class="eq-table-td eq-table-label"><label id="cISLocDR_CTLOCDesc" for="ISLocDR_CTLOCDesc">库房</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLocDR_CTLOCDesc"></div>
										<div class="eq-table-td eq-table-label"><label id="cISBuyLocDR_CTLOCDesc" for="ISBuyLocDR_CTLOCDesc">采购部门</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISBuyLocDR_CTLOCDesc" data-options="'itype':'9','property':'PLAT.L.Loc'"><i class="icon-blue-edit"></i></div>
										<div class="eq-table-td eq-table-label"><label id="cISBuyUserDR_SSUSRName" for="ISBuyUserDR_SSUSRName">采购人</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISBuyUserDR_SSUSRName" data-options="'itype':'9','property':'PLAT.L.EQUser'"><i class="icon-blue-edit"></i></div>
									</div>
									<div class="eq-table-tr">
										<div class="eq-table-td eq-table-label"><label id="cISEquipTypeDR_ETDesc" for="ISEquipTypeDR_ETDesc">管理类组</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISEquipTypeDR_ETDesc" data-options="'itype':'9','property':'PLAT.L.EquipType'"><i class="icon-blue-edit"></i></div>
										<div class="eq-table-td eq-table-label"><label id="cISProviderDR_VDesc" for="ISProviderDR_VDesc">供货商</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISProviderDR_VDesc" data-options="'itype':'9','property':'PLAT.L.Vendor'"><i class="icon-blue-edit"></i></div>
										<div class="eq-table-td eq-table-label"><label id="cISOriginDR_ODesc" for="ISOriginDR_ODesc">来源</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISOriginDR_ODesc" data-options="'itype':'9','property':'PLAT.L.Origin'"><i class="icon-blue-edit"></i></div>
										<div class="eq-table-td eq-table-label"><label id="cISInDate" for="ISInDate">制单日期</label></div>
										<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISInDate" data-options="'itype':'5','property':''"><i class="icon-blue-edit"></i></div>
									</div>
								</div>
							</div>
							<table id="DHCEQBusinessList" data-options="border:false,title:'明细信息',headerCls:'panel-header-gray',iconCls:'icon-add-note'" class="hisui-datagrid"></table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<INPUT type="hidden" id="BussID">
	<INPUT type="hidden" id="MainFlag" value="Y">
	<script type='text/javascript' src='../scripts/dhceq/plat/businessmodify.js'></script>
	<script type='text/javascript' src='../scripts/dhceq/plat/businessmodifycommon.js'></script>
</body>
</HTML>