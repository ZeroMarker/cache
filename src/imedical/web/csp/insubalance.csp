<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<script type="text/javascript" src="../scripts/insubalance.js"></script>
</head>
<body>
	<div class="hisui-layout" fit="true">   
		<div data-options="region:'north',title:'院内门诊对账',headerCls:'panel-header',iconCls:'icon-search'" style="height:84px;">
			<table style="margin:5px">
			<tr>
				<td>开始日期<input id="stdate" class="hisui-datebox"  name="stdate"/></td>
				<td>结束日期<input id="endate" class="hisui-datebox"  name="endate"/></td>
				<td><a href="#" id="Find" class="hisui-linkbutton" data-options="iconCls:'icon-search'" onClick="Inquriy()">查询</a></td>
			</tr>
		</table>
		</div>
		<div data-options="region:'east',iconCls:'icon-reload',title:'医保异常结算数据',split:true,headerCls:'panel-header-gray',collapsed:false" style="width:850px;">
			<table id="InsuError"></table>
		</div>   
		<div data-options="region:'center',title:'His异常结算数据',headerCls:'panel-header-gray',iconCls:'icon-emr-cri'">
			<div id="HisTabs" class="hisui-tabs tabs-gray" fit="true">
				<div title="普通门诊" data-options="iconCls:'icon-add-note',selected:true" fit="true">
					<table id='INVDiv'></table>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
