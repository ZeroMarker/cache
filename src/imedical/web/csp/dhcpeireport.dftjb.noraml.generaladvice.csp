      function DateDemo()
      {
	  var d, s =""
      d = new Date(); 
      s += d.getYear()+ "-";                            // 创建 Date 对象C
      s += (d.getMonth() + 1) + "-";            // 获取月份C
      s += d.getDate() ;                   // 获取日C
