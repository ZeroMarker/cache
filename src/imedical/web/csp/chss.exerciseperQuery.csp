	.e  d
	..w "{success:'false',info:'"_result_"'}"
	i Action = "add" d
	.s result = ##class(CHSSWeb.DictDateOfExercisePer).insert(Code,Description,StartDate,EndDate,Active,Sort)
	.i result = 0 d
	..w "{success:'true',info:'保存成功'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
</script>

