
<script language="cache" runat="server">

 	s Action=$Get(%request.Data("actiontype",1))
 	
 	s Username=$Get(%request.Data("user",1))
 	s Pageid=$Get(%request.Data("pageid",1))

 	s Start=$Get(%request.Data("start",1))
 	s Limit=$Get(%request.Data("limit",1))
 	s Dir=$Get(%request.Data("dir",1))
	s Sort=$Get(%request.Data("sort",1))
	
	s Rowid=$Get(%request.Data("RowId",1))
	s Code=$Get(%request.Data("Code",1))
	s Description=$Get(%request.Data("Description",1))
	s StartDate=$Get(%request.Data("StartDate",1))
	s EndDate=$Get(%request.Data("EndDate",1))
	s Active=$Get(%request.Data("Active",1))
	s Sort=$Get(%request.Data("Sort",1))
	
	i Action = "edit" d
	.s result = ##class(CHSSWeb.DictDateOfExercisePer).update(Rowid,Code,Description,StartDate,EndDate,Active,Sort)
	.i result = 0 d
	..w "{success:'true',info:'保存成功'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	i Action = "list" d
	.w ##class(CHSSWeb.DictDateOfExercisePer).ListPage(Start,Limit)
	i Action = "del" d
	.s result = ##class(CHSSWeb.DictDateOfExercisePer).delete(Rowid)
	.i result = 0 d
