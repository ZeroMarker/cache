
<!--
 * FileName: dhcpehighrisk.hisui.csp
 * Anchor: xy
 * Date: 2019-07-05
 * Description: HISUI高危信息查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<ADDINS require="CmdShell"/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.panel-header{
	border-bottom: 0;
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 14px;
}
.r-label-11{
	padding-left: 15px;
}
</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'center',border:false,collapsible:false" style="padding:10px;">
		<div data-options="region:'north',border:false,collapsible:false" style="height:400px;padding:0px;">
			<div class="hisui-panel" fit="true" data-options="title:'高危信息维护',headerCls:'panel-header-gray',iconCls:'icon-write-order'" style="border:1px solid #ccc;border-radius:0 0 4px 4px;">
		 		<div class="hisui-layout" fit="true">
		 			<div data-options="region:'north',border:false,collapsible:false" style="height:172px;border-bottom:1px solid #C0C0C0;">
		 				<table class="search-table">
		 					<tr>
		 						<td class="r-label" ><label for="StartDate">开始日期</label></td>
								<td>
									<input id="StartDate" class="hisui-datebox textbox"/>
								</td>
								<td class="r-label r-label-1" ><label for="EndDate">结束日期</label></td>
								<td>
									<input id="EndDate" class="hisui-datebox textbox"/>
								</td>
								<td class="r-label r-label-1">
									<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
								</td>
							</tr>
							<tr>
		 						<td class="r-label"><label for="GroupDesc">团体</label></td>
          						<td> 
              						<input class="hisui-combogrid" type="text" Id="GroupDesc" style="width:154px;"/>
         			 			</td>
         			 			
								<td class="r-label r-label-1" ><label for="Contract">合同</label></td>
								<td>
									<input class="hisui-combogrid" type="text" Id="Contract" style="width:154px;"/>
								</td>
								<td alight='left' style="padding-left:10px">
									<label for="CRMFlag">需随访</label><input class='hisui-checkbox' type="checkbox" id="CRMFlag"/>
								</td>

							</tr>
							<tr>
		 						<td class="r-label"><label for="VIPLevel">VIP等级</label></td>
          						<td> 
              						<input class="hisui-combobox" type="text" Id="VIPLevel" style="width:154px;"/>
         			 			</td>
         			 			
								<td class="r-label r-label-1" ><label for="IfSolve">是否处理</label></td>
								<td>
									<input class="hisui-combobox" type="text" Id="IfSolve" style="width:154px;"/>
								</td>
							</tr>
							<tr>
								<td class="r-label"><label for="Name">姓名</label></td>
								<td>
									<input class="textbox" id="Name"/>
								</td>
								
								<td class="l-label r-label-1"><label for="ResultFlag">包含上报</label></td>
								<td>
									<input class='hisui-checkbox' id="ResultFlag" type="checkbox"/>
								</td>
							</tr>			
                  	 	</table>
		 			</div>
		 			<div data-options="region:'center',border:false">
		 				<table id="HighRiskGrid"></table>
		 			</div>
				</div>							
			</div>
		</div>
		
		<div data-options="region:'center',border:false,collapsible:false" style="height:335px;padding:10px 0 0 0;">
			<div class="hisui-panel" id="express" fit="true" data-options="title:'表达式',headerCls:'panel-header-gray',iconCls:'icon-write-order'" style="border:1px solid #ccc;border-radius:0 0 4px 4px;">
		 		<div class="hisui-layout" fit="true">
		 			<div data-options="region:'north',border:false,collapsible:false" style="height:77px;border-bottom:1px solid #C0C0C0;">
		 				<table class="search-table">
		 					<input type="hidden" id="ParrefRowId"/></td>
		 					<tr>
        						<td class="r-label">
              						<textarea id="NorInfo"  style="width:445px;height:52px;"></textarea>	
          						</td>
          						<td class="r-label">
									<a id="BSave" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'">保存</a>
								</td>
							</tr>
							
						
                  	 </table>
		 			</div>
		 			<div data-options="region:'center',border:false">
		 				<table class="hisui-datagrid" id="DHCPEEDCondition" data-options="bodyCls:'panel-header-gray',singleSelect:true"></table>

		 			</div>
				</div>							
			</div>
		</div>
		
	</div>
	<div data-options="region:'east',border:false" style="width:785px;padding:10px 10px 10px 0;border:0;">
		<div class="hisui-panel" fit="true" data-options="title:'高危明细',headerCls:'panel-header-gray',iconCls:'icon-paper'" style="height:100%;border:1px solid #ccc;border-radius:0 0 4px 4px;">
			<div class="hisui-layout" fit="true">
		 		<div data-options="region:'north',border:false,collapsible:false" style="height:125px;border-bottom:1px solid #ccc;">
		 			<table class="search-table">
		 				<tr>
							<td class="r-label"><label for="Tel">电话</label></td>
							<td>
								<input class="textbox" id="Tel">
							</td>
							<td class="r-label r-label-1">
								<a id="BTel" href="#" class="hisui-linkbutton">电话通知</a>
							</td>	
							<td class="r-label r-label-1">
								<a id="BSendMessage" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-msg'">短信通知</a>
							</td>
							<td class="r-label r-label-1">
								<a id="BExport" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">导出</a>
							</td>
							<td class="r-label r-label-1">
								<a id="BtnExportWord" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">导出word</a>
							</td>
							
						</tr>
						<tr>
							<td class="r-label"><label for="DealRemark">处理情况</label></td>
							<td colspan="7">
								<textarea id="DealRemark" class="textbox" style="width:648px; height:60px;"> </textarea>
							</td>
							<td><input type="hidden" id="PAADM"/></td>
						</tr>
						
		 				
                   </table>
		 		</div>
		 		<div data-options="region:'center',border:false">
		 			<table id="HighRiskListGrid"></table>
		 		</div>
			</div>		
		</div>
	</div>
</div>
 
<script type="text/javascript" src="../scripts/DHCPEHighRiskFind.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
</body>		
</html>
			
		
 








