<div class="hisui-panel" data-options="border:false" style="border-radius:0;">
	<table class="search-table">
		<tr>
			<td class="r-label">
				<label>登记号</label>
			</td>
			<td>
				<input id="PatNo" class="textbox" style="width:113px;">
			</td>
			<td class="r-label">
				<label>卡类型</label>
			</td>
			<td>
				<input id="CardTypeNew" disabled="disabled" class="textbox" placeholder="卡类型..." style="width:113px;">
			</td>
			<td class="r-label">
				<label>卡号</label>
			</td>
			<td>
				<input id="CardNo" class="textbox" style="width:113px;">
			</td>
			<td class="r-label">
				<a id="ReadCard" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-card'">读卡</a>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label>姓名</label>
			</td>
			<td>
				<input id="PatName" disabled="disabled" class="textbox" style="width:113px;">
			</td>
			<td class="r-label">
				<label>性别</label>
			</td>
			<td>
				<input id="PatSex" disabled="disabled" class="textbox" style="width:113px;">
			</td>
			<td class="r-label">
				<label>补打日期</label>
			</td>
			<td>
				<input id="SearchDate" class="textbox hisui-datebox" data-options="formatter:myformatter,parser:myparser" style="width:120px;">
			</td>
			<td class="r-label">
				<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label>病案号</label>
			</td>
			<td>
				<input id="Patmed" class="textbox" style="width:113px;">
			</td>
			<td class="r-label">
				<label>就诊记录</label>
			</td>
			<td>
				<input id="admList" class="textbox" style="width:120px;">
			</td>
			<td></td>
			<td></td>
			<td class="r-label">
				<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
			</td>
		</tr>
	</table>
</div>
