
<!-- 
 * FileName: dhcpedoctorbatchaudit.hisui.csp
 * Anchor: xy
 * Date: 2020-05-09
 * Description: HISUI 批量总检初审
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 20px;
}


</style>
</head>
<body>

<SCRIPT language="Cache" RUNAT="SERVER"> 
 s MainDoctor=%request.Get("MainDoctor")  
</SCRIPT> 
 
 <script type="text/javascript">
      var  MainDoctor="#(MainDoctor)#" 
      if(MainDoctor=="N") var title="批量总检初审"
      if(MainDoctor=="Y") var title="批量总检复审"
</script>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:95px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:title,headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border-radius:0;border-bottom:1px solid #C0C0C0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="ArrivedDate">到达日期</label></td>
					<td>
						<input id="ArrivedDate" class="hisui-datebox textbox"/>
					</td>
					<!--td class="r-label"><label for="AuditDate">总检日期</label></td>
					<td>
						<input id="AuditDate" class="hisui-datebox textbox"/>
					</td-->
					
					<td class="r-label r-label-1"></td>
					<td>
						<input class='hisui-checkbox' type="checkbox" id="NoResultFlag"/><label id="tNoResultFlag">存在未完成项目</label>
					</td>

					<td class="r-label r-label-1"></td>
					<td>
						<input class='hisui-checkbox' type="checkbox" id="ShowErr"/><label for="ShowErr">显示错误</label>
					</td>
					
	
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BBatchAudit" href="#" class="hisui-linkbutton" style="width:130px;" data-options="iconCls:'icon-w-edit'">批量总检</a>
					</td>		
				</tr>
						
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="DoctorBatchAuditGrid"></table>
			</div>
        </div>
	      
</div>
   
<script type="text/javascript" src="../scripts/DHCPEDoctorBatchAudit.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
</body>		
</html>
			
		
 





