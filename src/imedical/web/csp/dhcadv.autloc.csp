<html>
<head>
<FORM:EASYUI/>
	<script type="text/javascript" src="../scripts/dhcnewpro/skin/js/autloc.js"></script>
</head> 
<body>
	<div id="mainpanel" class="easyui-layout"  fit="true">
		<div data-options="region:'center'">
			<table id="autlocdg" toolbar="#atb"></table>
		</div>
		<div data-options="region:'east'" style='width:600px;'>
			<table id="autlocitmdg" toolbar="#aitb"></table>
		</div>
		
	</div>
	<div id="atb" style="padding:5px;height:auto">
		<div style="margin-bottom:5px">
			<tr>
			<td class="textLabel">代码<span style="margin-left:5px;"></span><input type="text" id="code" class="textParent"></input> </td>
			<td class="textLabel">描述<span style="margin-left:5px;"></span><input type="text" id="desc" class="textParent"></input> </td>
			<td ><a class="easyui-linkbutton" style="width:90"  data-options="iconCls:'icon-search'" id="find">查询</a> </td>					
			</tr>								
			<div class="tool-bar-line">	</div>
		</div>
		<table cellspacing="0" cellpadding="0" >
			<tr>
		      <td><a class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true" id="insert">增加</a></td>
		      <td><div class="datagrid-btn-separator"> </div> </td>
		      <td><a class="easyui-linkbutton" data-options="iconCls:'icon-remove',plain:true" id="delete">删除</a> </td>
		      <td><div class="datagrid-btn-separator"> </div> </td>	
		      <td><a class="easyui-linkbutton" data-options="iconCls:'icon-save',plain:true" id="save">保存</a> </td>
		      <td><div class="datagrid-btn-separator"> </div> </td>	
		    </tr>
		</table>
	</div>
	<div id="aitb" style="padding:5px;height:auto">
		<table cellspacing="0" cellpadding="0" >
			<tr>
		      <td><a class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true" id="insali">增加</a></td>
		      <td><div class="datagrid-btn-separator"> </div> </td>
		      <td><a class="easyui-linkbutton" data-options="iconCls:'icon-remove',plain:true" id="delali">删除</a> </td>
		      <td><div class="datagrid-btn-separator"> </div> </td>	
		      <td><a class="easyui-linkbutton" data-options="iconCls:'icon-save',plain:true" id="savali">保存</a> </td>
		      <td><div class="datagrid-btn-separator"> </div> </td>	
		    </tr>
		</table>
	</div>
</body>
</html>
