<!--下收下送路线维护-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
	<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'west',border:false,collapsible:false" style="width:750px;">
		<div class="hisui-panel" title="下收下送线路维护"  data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order',fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false,collapsible:false" >
					<div data-options="region:'north',collapsible:false,border:false" class="BorderLine">
						<table id="LineTB" class="Condition" >
							<tr>
								<td>系统</td>
								<td>
									<select id="System" name="System" class="hisui-combobox"  style="width:150px;"></select>
								</td>
								<td>
									<a href="#" id="SearchBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
								</td>
								<td>
									<a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-clean'">清屏</a>
								</td>
							</tr>
						</table>
					</div>
					<table style="padding-top:1px;padding-bottom:1px">
						<tr>
							<td>
								<a href="#" id="AddBT" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">新增</a>
							</td>
							<td>
								<a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">保存</a>
							</td>
							<td>
								<a href="#" id="DeleteBT" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true">删除</a>
							</td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false">
					<table id="SendLineGrid"></table>
				</div>
			</div>
		</div>
	</div>	
	<div class="GridPadding" data-options="region:'center',border:false" style="padding-left:0;">
		<div class="hisui-panel" title="科室维护" data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order',fit:true">
			<div class="hisui-layout" fit="true">
				<div data-options="region:'center',border:false">
					<table id="LocGrid"></table>
				</div>
			</div>
		</div>
	</div>	
<script type="text/javascript" src="../scripts/cssd/System/SendRoadLine.js"></script>
</body>
</html>