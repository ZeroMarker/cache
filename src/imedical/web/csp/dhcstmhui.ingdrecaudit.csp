<!--入库单审核-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
 <SERVER>
 	d ##class(web.DHCSTMHUI.Util.RQServerConfig).GetApplet()  //润乾打印
 	d ##class(web.DHCXMLPConfig).LODOPInit()  //Lodop打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',border:false,collapsible:false">
		<div class="hisui-panel" data-options="title:'入库单',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'"> 
			<div class="hisui-layout" data-options="fit:true">
				<div class="BorderLine" data-options="region:'north',border:false,collapsible:false">
					<table id="FindConditions" class="Condition">
						<input id="FStatusBox" name="FStatusBox" value="Y" type="hidden">
						<tr>
							<td>入库科室</td>
							<td><input id="FRecLoc" name="FRecLoc" class="hisui-combobox" style="width:130px;"></td>
							<td>起始日期</td>
							<td><input id="StartDate" name="StartDate" class="hisui-datebox" style="width:160px;"></td>
							<td>供应商</td>
							<td><select id="FVendorBox" name="FVendorBox" class="hisui-combobox" style="width:160px;"></select></td>
							<td align="right"><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
							<td align="left">
								<a href="#" id="AuditBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-stamp'">审核</a>
							</td>
							<td>
								<a href="#" id="SaveInvBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存发票信息</a>
							</td>
						</tr>
						<tr>
							<td>制单人</td>
							<td><input id="CreateUserId" name="CreateUserId" class="hisui-combobox" style="width:130px;"></td>
							<td>截止日期</td>
							<td><input id="EndDate" name="EndDate" class="hisui-datebox" style="width:160px;"></select></td>
							<td>赠送或调价换票</td>
							<td>
								<select id="Findtype" name="Findtype" class="hisui-combobox" style="width:160px;">
									<option value="All">全部</option>
									<option value="G">赠送</option>
									<option value="A">调价换票</option>
								</select>
							</td>
							<td align="right"><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
							<td align="left">
								<a href="#" id="PrintBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">打印</a>
							</td>
							<td>
								<a href="#" id="PrintHVColBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">高值汇总打印</a>
							</td>
						</tr>
						<tr>
							<td>接收科室</td>
							<td><input id="RequestLoc" name="RequestLoc" class="hisui-combobox" style="width:130px;"></td>
							<td>入库单号</td>
							<td><input id="FInGrNo" name="FInGrNo" class="textbox" style="width:153px;"></td>
							<td>是否审核</td>
							<td>
								<select id="AuditFlag" name="AuditFlag" class="hisui-combobox" style="width:160px;">
									<option value="N">未审核</option>
									<option value="Y">已审核</option>
								</select>
							</td>
							<td align="right">是否打印</td>
							<td align="left">
								<select id="PrintFlag" name="PrintFlag" class="hisui-combobox" style="width:88px;">
									<option value=1>全部</option>
									<option value=2>已打印</option>
									<option value=3>未打印</option>
								</select>
							</td>
						</tr>
						<tr>
							<td>类组</td>
							<td><select id="ScgId" name="ScgId" class="hisui-stkscgcombotree" style="width:130px"></select></td>
							<td>物资名称</td>
							<td><input id="InciDesc" name="InciDesc" class="textbox" style="width:160px"></td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false,collapsible:false">
					<table id="InGdRecMainGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div class="GridPadding" data-options="region:'south',border:false,collapsible:false" style="padding-top:0;height:250px;">
		<div class="hisui-panel" title="入库单明细" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
			<table id="InGdRecDetailGrid"></table>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonPrint.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/ItmTrack/ItmTrackCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecPrint.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecAudit.js"></script>
</body>
</html>