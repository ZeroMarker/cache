<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

    s Action=$Get(%request.Data("actiontype",1))
    s Start=$Get(%request.Data("start",1))
    s Limit=$Get(%request.Data("limit",1))
    s Sort=$Get(%request.Data("sort",1))
    s Dir=$Get(%request.Data("dir",1))

   i Action = "GetParamProp" d
	.s GroupId=$g(%request.Data("GroupId",1))
	.s LocId=$g(%request.Data("LocId",1))
	.s UserId=$g(%request.Data("UserId",1))
	.s ret=##class(web.DHCSTM.Common.TipsWin).GetParamProp(GroupId,LocId,UserId)
	.w "{success:'true',info:'"_ret_"'}"
	.
   
   
	i Action = "GetTips" d
	.s GroupId=$g(%request.Data("GroupId",1))
	.s LocId=$g(%request.Data("LocId",1))
	.s UserId=$g(%request.Data("UserId",1))
	.d ##class(web.DHCSTM.Common.TipsWin).GetTips(GroupId,LocId,UserId)	
	
	i Action="GetVenderAptness" d
	.s VenderDr=$g(%request.Data("VenderDr",1))
	.d ##class(web.DHCSTM.Common.TipsWin).GetVenderAptness(VenderDr)
	i Action="GetManfAptness" d
	.s ManfId=$g(%request.Data("ManfId",1))
	.d ##class(web.DHCSTM.Common.TipsWin).GetManfAptness(ManfId)
	
	i Action="GetVenderAptnessNew" d
	.s VenderDr=$g(%request.Data("VenderDr",1))
	.d ##class(web.DHCSTM.Common.TipsWin).GetVenderAptnessNew(VenderDr)
	i Action="GetManfAptnessNew" d
	.s ManfId=$g(%request.Data("ManfId",1))
	.d ##class(web.DHCSTM.Common.TipsWin).GetManfAptnessNew(ManfId)
</script>