<!DOCTYPE html>
<!--配置费用统计
	csp:dhcpha.pivas.report.feestat.csp
	js: scripts/pharmacy/pivas/report.feestat.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>

</HEAD>

<body style="overflow:hidden;">
	<div class="dhcpha-win-mask">
		<img src="../scripts/pharmacy/common/image/page-loading.gif"></img>
	</div>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',split:true,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">

            <div class="hisui-layout" fit="true">
                <div data-options="region:'north',title:'配置费用统计',iconCls:'icon-sum',split:true,headerCls:'panel-header-gray',collapsible:false" class='dhcpha-north-r1'>
                    <table class="dhcpha-north-tbl">
                        <tr>
                            <td>
                                <label for="cmbPivaLoc">配液中心</label>
                                <input id="cmbPivaLoc" type="text" />
                            </td>
                            <td>
                                <label for="dateStart">开始日期</label>
                                <input id="dateStart" class="hisui-datebox dhcpha-date" type="text" />
                            </td>
                            <td>
                                <label for="dateEnd">结束日期</label>
                                <input id="dateEnd" class="hisui-datebox dhcpha-date" type="text" />
                            </td>
                            <td style="display:none">
                                <input class='hisui-radio' type="radio" label='按病区汇总' name='radioFee' value='feeSum' data-options="checked:true">
                                <!--input class='hisui-radio' type="radio" label='按明细' name='radioFee' value='feeDetail'-->
                            </td>
                            <td>
                                <a class="hisui-linkbutton" iconCls="icon-w-find" plain="false" id="btnFind">查询</a>
                            </td>
                        </tr>
                    </table>
                </div>
                <div data-options="region:'center',title:'',headerCls:'panel-header-gray',border:false" style="overflow:hidden;border-radius:4px;">
                    <iframe></iframe>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/print.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/report.feestat.js"></script>
</body>

</HTML>