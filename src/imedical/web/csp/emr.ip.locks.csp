﻿<html>
<head>
    <HISUI/>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<link rel="stylesheet" type="text/css" href="../scripts/emr/css/layout.css"/>
	<script type="text/javascript" src="../scripts/emr/js/tools.js"></script> 
	<script language="cache" runat="server">
		d ##Class(EMRservice.SystemParameter).GetImportResource()
		s userID = %session.Get("LOGON.USERID")
		s userName = %session.Get("LOGON.USERNAME")
		s IsSetToLog = ##Class(EMRservice.BL.BLSysOption).GetOptionValueByName("IsSetToLog")
  </script>
	<script language="javascript">
	  var userId = '#(userID)#';
	  var IsSetToLog = '#(IsSetToLog)#';
	  var SecCode = "";  //病人密级代码
	  var userName = '#(userName)#';
   </script>
	<title>病历内容</title>
</head>
<body class="hisui-layout" data-options="fit:true,border:false">
	<div data-options="region:'north'" style="height:44"> 
		<table class="tableStyle01" style="width:100%; height:100%">
			<tr style="margin:0 5px 0 5px;">
				<td style="width:200px">
					#(..Get("用户名称"))# <input class="hisui-validatebox" id="UserName" style="width:100px"/>
				</td>
				<td style="width:300px">
					#(..Get("计算机IP"))# <input class="hisui-validatebox" id="IPAddress" style="width:200px"/>
				</td>
				<td style="width:300px">
					#(..Get("目录名称"))# <input class="hisui-validatebox" id="DocName" style="width:200px"/>
				</td>
				<td>
				    <a href="#" id="LockQuery" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" >#(..Get("查询"))#</a>
				</td>
			</tr>
	    </table>	
	</div>
	<div data-options="region:'center',border:false">
		<table id="lockList"></table>
	</div>
    <script type="text/javascript" src="../scripts/emr/js/ip.locks.js"></script> 
</body>
</html>