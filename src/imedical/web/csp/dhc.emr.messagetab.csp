<HTML>
<HEAD>
	<meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
	<TITLE>MessageTab</TITLE>
 		<script language="cache" runat="server">
	 	s tabId=$G(%request.Data("tabId",1))
	 	s userId=$G(%request.Data("userId",1))
	 	s groupId=$G(%request.Data("groupId",1))
	 	s ctlocId=$G(%request.Data("ctlocId",1))
	 	s InstanceId = $G(%request.Data("InstanceId",1))
	 	s emrDocId = $G(%request.Data("emrDocId",1))
		s:(userId = "") userId = %session.Get("LOGON.USERID")
		s:(groupId = "") groupId = %session.Get("LOGON.GROUPID")
		s:(ctlocId = "") ctlocId = %session.Get("LOGON.CTLOCID")


	</script>
	<script type="text/javascript">
		var tabId = '#(tabId)#';
		var userId = '#(userId)#';
		var groupId = '#(groupId)#';
		var ctlocId = '#(ctlocId)#';
		var InstanceId = '#(InstanceId)#';
		var emrDocId = '#(emrDocId)#';
	</script>
	<link rel="stylesheet" type="text/css" href="../scripts_lib/hisui-0.1.0/dist/css/hisui.css" />
        <script type="text/javascript" src="../scripts_lib/hisui-0.1.0/dist/js/jquery-1.11.3.min.js" charset="utf-8"></script>
        <script type="text/javascript" src="../scripts_lib/hisui-0.1.0/dist/js/jquery.hisui.js" charset="utf-8"></script>
        <script type="text/javascript" src="../scripts_lib/hisui-0.1.0/dist/js/locale/hisui-lang-zh_CN.js" charset="utf-8"></script>
        <script type="text/javascript" src="../scripts/websys.jquery.js" charset="utf-8"></script>
        <script type="text/javascript" src="../../dist/plugin/datagrid-scrollview.js"></script>
        <script src="../scripts/epr/JQuery/datagrid-detailview.js"></script>
        <script type="text/javascript" src="../scripts/emr/lib/tool/json2.js"></script>
	
    
    <script src="../scripts/epr/JQuery/datagrid-detailview.js"></script>
	
	<script charset="utf-8" type="text/javascript" src="../scripts/DHC.EMR.Quality.MessageTab.js"></script>
	
</HEAD>

<body class="hisui-layout" data-options="fit:true,border:true" style="border:none;">
	<div data-options="region:'center'" style="padding:5px;">		
		<div id="messageListTableTBar" style="padding:10px 0px 10px 0px;height:65px">	
			<div>
				<span style="margin-left:10px;margin-right:10px">消息状态 </span><select class="hisui-combobox" id="inputMessageType" name="inputMessageType" style="width:130px;"></select>
				<input id="inputCreateDateStart" class="hisui-datebox" style="width:120px;" />
	 			<input id="inputCreateDateEnd" class="hisui-datebox" style="width:120px;" />
 			</div>
 			<div style="padding:10px 0px 10px 0px">
 			 	<div style="padding:0px 0px 0px 0px;border:1px #cccccc dashed;border-left:none;border-right:none;border-top:none;"> </div>
 			 	<a style="padding:10px 0px 0px 0px;"href="javascript:void(0)" id="doneBtn" class="hisui-linkbutton" data-options="iconCls:'icon-redo',plain:true" >处理</a>
 			 	<a style="padding:10px 0px 0px 0px;"href="javascript:void(0)" id="appealBtn" class="hisui-linkbutton" data-options="iconCls:'icon-undo',plain:true" >申诉</a>
				<div id="content" class="hisui-dialog" closed="true" >
					<div id="appealwrap" style="padding:20px 0px 0px 35px;">
						<div>
						<h3 float="left">
						申诉原因：
						</h3>
						<input id="appealText" style="width:230px;height:35px" />
						</div>
						
					</div>	
					<div id="dlg-buttons1" style="float: left;padding:30px 0px 0px 50px;"> 
						<a href="#" id="appealOk" class="hisui-linkbutton" iconCls="icon-ok" click="confirm()">确认</a>
					</div>
					<div id="dlg-buttons2" style="float: left;padding:30px 0px 0px 20px;"> 
					<a href="#" id="appealCancel" class="hisui-linkbutton" iconCls="icon-cancel" click="cancle()">取消</a>
					</div>
				</div>	
 			</div>
		</div>
		<table id="messageListTable" title="质控消息待处理列表" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper'" style="padding:5px 15px 5px 15px"></table>
	    

	</div>
</body>

</HTML>

