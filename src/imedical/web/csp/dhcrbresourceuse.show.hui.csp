<div id="layout_main" class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray'" style="height:41px;">
		<table class="search-table">
				<tr>
					<td class="r-label">
						<label id="_HospListLabel" style='color:red;'>医院</label>
					</td>
				
					<td>
						<input id="_HospList"/>
					</td>
				</tr>
		</table>
	</div>
	<div data-options="region:'west',border:false,collapsible:false,headerCls:'panel-header-gray'" style="width:370px;padding:10px 10px 10px 10px;">
		<div class="hisui-panel" title="科室列表"
		data-options="headerCls:'panel-header-gray',iconCls:'icon-select-grant',fit:true,closable:false,collapsible:false,minimizable:false,maximizable:false">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label class="left_label" for="GHUser">排班人员</label>
					</td>
				
					<td>
						<!--<input class="hisui-combobox" id="GHUser" style="width:250px"/>-->
						<input class="hisui- textbox" id="GHUser" style="width:250px"/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label class="left_label" for="LocNameDesc">科室描述</label>
					</td>
					<td>
						<input class="hisui- textbox" id="LocNameDesc" style="width:243px"/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label class="left_label" for="List_AllDept">
							<!--<a href="#" title="双击添加到已选科室列表" class="hisui-tooltip" data-options="position:'right'">待选科室</a>-->
							待选科室
						</label>
					</td>
					<td>
						<div class="hisui-panel" style="border-radius:0px;padding:0px 0px;width:250px;height:400px"
						data-options="headerCls:'panel-header-card',closable:false, collapsible:false,minimizable:false,maximizable:false"> 
							<select multiple='multiple' id="List_AllDept" style="border-style:none;height:100%;width:99%;padding:5px 0px 5px 5px"></select>
						</div>
					</td>
				</tr>
				<tr>
					<td class="f-label" colspan=2>
						<a id="BtnAdd" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-add'">增加</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray'" style="padding:10px 10px 10px 0px">
		<div id="pan_center" class="hisui-panel" title="排班权限"
		data-options="headerCls:'panel-header-gray',iconCls:'icon-set-paper',fit:true,closable:false,collapsible:false,minimizable:false,maximizable:false">
			<div style="position:relative;clear:both;padding:10px;">
				<div id="div_popover" class="messager-popover error" style="border-radius:0px;">
					<span class="messager-popover-icon error"></span>
					<span class="content">双击已选科室删除科室数据 科室医生不选默认科室下全部</span>
				</div>
			</div>
			<table class="search-table">
				<tr>
					<td colspan=4>
						<label for="">&nbsp;</label>
					</td>
					
				</tr>
				<tr>
					<td class="f-label" style="height:20px">
						<label for="">&nbsp;</label>
					</td>
					<td class="f-label" style="height:20px">
						<label for="">&nbsp;</label>
					</td>
				</tr>
				<tr>
					<td class="f-label" style="height:30px">
						<label for="List_UDept">
							已选科室
							<!--<a href="#" title="双击删除已选科室" class="hisui-tooltip" data-options="position:'top'">已选科室</a>-->
						</label>
					</td>
					<td class="r-label">
						<div class="hisui-panel" style="border-radius:0px;padding:0px 0px;width:250px;height:400px" onmouseover=""
						data-options="headerCls:'panel-header-card',closable:false, collapsible:false,minimizable:false,maximizable:false"> 
							<select multiple="multiple" id="List_UDept" style="border-style:none;height:100%;width:99%;padding:5px 0px 5px 5px"></select>
						</div>
					</td>
					<td class="f-label" style="height:30px">
						<label for="List_UDoc">
							科室医生
							<!--<a href="#" title="科室医生不选默认科室下全部" class="hisui-tooltip" data-options="position:'top'">科室医生</a>-->
						</label>
					</td>
					<td class="r-label">
						<div class="hisui-panel" style="border-radius:0px;padding:0px 0px;width:250px;height:400px"
						data-options="headerCls:'panel-header-card',closable:false, collapsible:false,minimizable:false,maximizable:false"> 
							<select multiple='false' id="List_UDoc" style="border-style:none;height:100%;width:99%;padding:5px 0px 5px 5px"></select>
						</div>
					</td>
				</tr>
				<tr>
					<td class="f-label" colspan=4>
						<a id="BtnUpdate" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
</div>

