<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))


	s UserId=$Get(%request.Data("userId",1))
	s Ret=$Get(%request.Data("ret",1))

	s StrPar=$Get(%request.Data("strPar",1))
	s RowId=$Get(%request.Data("rowid",1))
	s Ingrt=$Get(%request.Data("ingrt",1))
	
	s LocId=$Get(%request.Data("locId",1))
	s RetNo=$Get(%request.Data("retNo",1))
	s VendorId=$Get(%request.Data("vendorId",1))
	
	s Scg=$Get(%request.Data("scg",1))
	s StkType=$Get(%request.Data("stkType",1))
	s AdjChequeFlag=$Get(%request.Data("adjChequeFlag",1))

	//根据条件查找退货单
	i Action = "selectBatch" d
	.s result = ##class(web.DHCSTM.DHCINGdRet).jsDHCINGdRet(Start,Limit,Sort,Dir,StrPar)
	.i result ="" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result

	

	//查询批次
	i Action = "selectBatch2" d
	.s result = ##class(web.DHCSTM.DHCINGdRet).jsGdRecItmToRet(StrPar,Start,Limit)
	.i result ="" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result

	//保存退货主表信息
	i Action = "save" d
	.s result = ##class(web.DHCSTM.DHCINGdRet).Update(Ret,LocId,RetNo,VendorId,UserId,Scg,StkType,AdjChequeFlag)
	.i result > 0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//保存退货明细
	i Action = "saveDetail" d
	.s result = ##class(web.DHCSTM.DHCINGrtItm).Save(Ingrt,Rows)
	.i result'= -1 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//根据退货主表的Id查询退货单号
	i Action = "getOrder" d
	.s result = ##class(web.DHCSTM.DHCINGrtItm).GetOrderByRowId(RowId)
	.i result'="" d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//根据退货主表Id查询退货明细记录
	i Action = "select" d
	.s result = ##class(web.DHCSTM.DHCINGrtItm).jsDHCINGdRetItm(Start,Limit,Sort,Dir,Ret)
	.i result ="" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result	

	//根据退货明细Id删除退货明细记录
	i Action = "delete" d
	.s result = ##class(web.DHCSTM.DHCINGrtItm).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	

	//退货单完成设置
	i Action = "complet" d
	.s result = ##class(web.DHCSTM.DHCINGdRet).SetCompleted(Ret)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
</script>