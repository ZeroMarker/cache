<!--灭菌不合格登记-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:0px">
		<div class="hisui-panel" data-options="title:'灭菌不合格登记',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="UomTB">
				<div> 
					<table class="Condition" id="MainCondition">
						<tr>
							<td><label>开始日期</label><input id="FStartD" name="FStartD" class="textbox hisui-dateboxq" style="width:143px"></td>
							<td style="padding-left:30px"><label>结束日期</label><input id="FEndD" name="FEndD" class="textbox hisui-dateboxq" style="width:143px"></td>
							<td style="padding-left:30px"><a href="#" id="SearchBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:100px">查询</a></td>
						</tr>
						<tr>
							<td><label>科室</label><input class="textbox hisui-combobox" id="DeptLocID" name="DeptLocID" readonly="readonly" style="width:150px"></td>
							<td></td>
							<td style="padding-left:30px"><a href="#" id="SaveBT" class="textbox hisui-linkbutton" data-options="iconCls:''" style="width:100px">新增单据</a></td>
						</tr>
					</table>
				</div>
			</div>
				<table id="MainList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:520px;padding-left:10px">
		<div class="hisui-panel" data-options="title:'灭菌不合格明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<table id="ItemList"></table>
		</div>
	</div>
<script type="text/javascript" src="../scripts/cssd/Sterilize/SterFail.js"></script>
<script type="text/javascript" src="../scripts/cssd/Sterilize/SterCommon.js"></script>
</body>
</html>