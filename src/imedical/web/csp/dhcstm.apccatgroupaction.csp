<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))
	s StrParam=$Get(%request.Data("StrParam",1))

i Action = "GetAllVendor" d
	.d ##class(web.DHCSTM.ApcCatGroup).GetAllVendor(Start,Limit,Sort,Dir,StrParam)

//获取供应商已授权的类组
i Action = "GetApcScg" d
	.d ##class(web.DHCSTM.ApcCatGroup).GetApcScg(Start,Limit,StrParam)

//获取供应商未授权的类组
i Action = "GetUnApcScg" d
	.d ##class(web.DHCSTM.ApcCatGroup).GetUnApcScg(Start,Limit,StrParam)

i Action = "GetAllScg" d
	.d ##class(web.DHCSTM.ApcCatGroup).GetAllScg(Start,Limit)

//获取某类组已授权的供应商
i Action = "GetScgApc" d
	.d ##class(web.DHCSTM.ApcCatGroup).GetScgApc(Start,Limit,Sort,Dir,StrParam)

//获取某类组未授权的供应商
i Action = "GetUnScgApc" d
	.d ##class(web.DHCSTM.ApcCatGroup).GetUnScgApc(Start,Limit,Sort,Dir,StrParam)

i Action = "Save" d
	.s ScgStr=$Get(%request.Data("ScgStr",1))
	.s VendorIdStr=$Get(%request.Data("VendorIdStr",1))
	.s ret=##class(web.DHCSTM.ApcCatGroup).Save(VendorIdStr,ScgStr)
	.i ret="" d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"

i Action = "Update" d
	.s ListData=$Get(%request.Data("ListData",1))
	.s ret=##class(web.DHCSTM.ApcCatGroup).Update(ListData)
	.i ret=0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"

i Action = "Delete" d
	.s ScgStr=$Get(%request.Data("ScgStr",1))
	.s VendorIdStr=$Get(%request.Data("VendorIdStr",1))
	.s result = ##class(web.DHCSTM.ApcCatGroup).Delete(VendorIdStr,ScgStr)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

</script>