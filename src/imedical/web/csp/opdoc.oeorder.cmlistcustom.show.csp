<div id="Ordlayout_main"class="hisui-layout" data-options="fit:true,border:false">
	<div id="OrdSearch" data-options="region:'north',border:false" style="height:246px;padding:10px 10px 0 10px;">
		<csp:if condition='PAAdmType="I"'>
		<csp:else>
			<div class="hisui-tabs tabs-gray" id="PrescriptTypeBillList"></div>
		</csp:if>
		<!--csp:if condition='PAAdmType="O"'>
	    	<a href="#" id="ordEntryType_change_Btn" class="hisui-linkbutton" data-options="iconCls:'icon-w-switch'">西药录入</a>
		</csp:if-->
    	<table class="search-table">
    		<tr>
    			<td class="r-label" colspan="4">
    				<div id="CMPrescTypeKW"></div>
    			</td>
    			<td class="r-label" colspan="4">
    				<label id="Prompt"></label>
    			</td>
    			<td class="r-label">
    				<label for="PrescUrgent" style="color:red;">加急</label>
    			</td>
    			<td>
    				<input class='hisui-checkbox' type="checkbox" id="PrescUrgent" data-options="checked:false">
    			</td>
    		</tr>
    		<tr>
    			<td class="r-label">
    				<label>用药频次</label>
    			</td>
    			<td>
    				<input id="PrescFrequence" class="textbox">
    			</td>
    			<td class="r-label">
    				<label>使用方式</label>
    			</td>
    			<td>
    				<input id="PrescInstruction" class="textbox">
    			</td>
    			
    			<td class="r-label">
    				<label>用药副数</label>
    			</td>
    			<td>
    				<input id="PrescDuration" class="textbox">
    			</td>
    			<csp:if condition='PAAdmType="I"'>
    				<td class="r-label">
    					<label>长期医嘱</label>
    				</td>
    				<td>
    					<input id="AddLongOrderList" class="textbox">
    				</td>
				<csp:else>
					<td class="r-label">
    					<label>处方列表</label>
    				</td>
    				<td>
    					<input id="PrescList" class="textbox">
    				</td>
				</csp:if>
    			<td class="r-label">
    				<!--label for="PrescCookDecoction">代煎</label-->
    			</td>
    			<td>
    				<!--input class='hisui-checkbox' type="checkbox" id="PrescCookDecoction" data-options="checked:false"-->
    			</td>
    		</tr>
    		<tr>
    			<!--td class="r-label">
    				<label>处方剂型</label>
    			</td>
    			<td>
    				<input id="CMPrescType" class="textbox">
    			</td-->
    			<td class="r-label">
    				<label>一次用量</label>
    			</td>
    			<td>
    				<input id="PrescOrderQty" class="textbox">
    			</td>
    			<td class="r-label">
    				<label>医嘱类型</label>
    			</td>
    			<td>
    				<input id="PrescPrior" class="textbox">
    			</td>
    			<td class="r-label">
    				<label>金额</label>
    			</td>
    			<td>
    				<input id="ScreenBillSum" class="textbox" disabled="disabled" style="width:68px;"><input id="TotailBillSum" class="textbox" disabled="disabled" style="width:68px;margin-left:5px;">
    			</td>
    			<td class="r-label">
    				<label id="CPrescAppenItemQty">药箅子数</label>
    			</td>
    			<td>
    				<csp:if condition='CMOpenForAllHosp="1"'>
    					<input id="PrescAppenItemQty" class="textbox" style="width:90px;">
    					<input id="CMOrderOpenForAllHosp" class='hisui-checkbox' type="checkbox" label="跨院">
    				<csp:else>
    					<input id="PrescAppenItemQty" class="textbox">
    				</csp:if>
    			</td>
    			<td class="r-label">
    				<label for="DrugAndQtyYQ">药量齐开</label>
    			</td>
    			<td>
    				<input class='hisui-checkbox' type="checkbox" id="DrugAndQtyYQ" data-options="name:'DrugAndQtyEntryMode',checked:false">
    			</td>
    		</td>
