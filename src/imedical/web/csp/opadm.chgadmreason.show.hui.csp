<div data-options="region:'north',split:false,border:false,collapsible:false," style="border:0;padding:10px 10px 0 10px;">
	<div class="hisui-panel" data-options="title:'',headerCls:'panel-header-gray',iconCls:'icon-exe-order'" style="height:91px;border:1px solid #ccc;border-bottom:0;border-radius:4px 4px 0 0;">
			<table class="search-table">
				<tr>
					<td class="r-label" colspan='2'>
						<input class="textbox" id="PatInfo" style="width:300px;"/>
					</td>
					<td class="r-label">
						<label for="InsuranceNo">医保号</label>
					</td>
					<td>
						<input class="textbox" id="InsuranceNo"/>
					</td>
				</tr>
				<tr>
					<td class="r-label" colspan='2'>
						<input class="textbox" id="AdmInfo" style="width:300px;"/>
					</td>
					<td colspan='2'>
						<span class="r-label">
							<input id="UpAdm" class="hisui-checkbox" type="checkbox" checked label="就诊费别">
						</span>
						<span class="r-label">
							<input id="UpPat" class="hisui-checkbox" type="checkbox" label="患者类型">
						</span>
						<!--有患者批量修改医嘱费别界面，此处修改医嘱费别看不到医嘱明细暂时屏蔽-->
						<span hidden class="r-label">
							<input id="UpOrd" class="hisui-checkbox" type="checkbox" label="医嘱费别" data-options="onChecked:ShowWarning">
						</span>
					</td>
					
				
				<tr>
					<td class="r-label">
						<label id="cEmployeeNo" for="EmployeeNo" hidden>职工工号</label>
					</td>
					<td>
						<input class="textbox" id="EmployeeNo" hidden style="width:230px;"/>
					</td>
				</tr>				
			</table>
	</div>
</div>
<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-add-note'" style="padding:0 10px 10px 10px;">
	<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-radius:0 0 4px 4px;">
		<table id="AdmReasonListTab"></table>
	</div>
</div>
<div data-options="region:'south',border:false,collapsible:false" style="padding:0px 10px 10px 10px;border:1px solid #cc;border-radius:4px;">
	<div style="text-align:center;">
		<a id="Confirm" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
		<a id="Quit" href="#" class="hisui-linkbutton" style="margin-left:7px;" data-options="iconCls:'icon-w-back'">关闭</a>
	</div>
</div>