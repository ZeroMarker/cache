<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

	//查询项目组
	i Action = "query" d
	.s code=$g(%request.Data("Code",1))
	.s desc=$g(%request.Data("Desc",1))
	.s active=$g(%request.Data("Active",1))
	.s result = ##class(web.DHCSTM.LocItemGrp).Query(Start,Limit,Sort,Dir,desc,code,active)
	.i result = "" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result
	.
	//保存项目组
	i Action = "save" d
	.s listData=$g(%request.Data("ListData",1))
	.s result = ##class(web.DHCSTM.LocItemGrp).Save(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	.
	//删除项目组
	i Action = "delete" d
	.s RowId=$g(%request.Data("Rowid",1))
	.s result = ##class(web.DHCSTM.LocItemGrp).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	.
</script>