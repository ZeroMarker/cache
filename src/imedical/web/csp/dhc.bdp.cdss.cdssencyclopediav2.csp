    /*这是百科右侧导航代码的样式表*/
    .directory-nav{
        position: fixed; 
        right:70px; 
        bottom:40px; 
        padding: 12px 0 20px 32px; 
        display:none;overflow-y:hidden;
        max-height:450px
     }
    /*.directory-scroll{position:relative;left:0px; top: 50%;background-color:#333;overflow-y:auto;max-height:440px}*/
    .directory-nav li{
        height: 24px;
        line-height: 24px; 
        position: relative;
     }
    .directory-nav li a{
     }
    .directory-nav .l1{
     }
    .directory-nav .l2{ 
    text-indent:0.6em;
    }
    .directory-nav .l1 a{
        font-size: 14px;
     }
    .directory-nav .l2 a{
        font-size: 12px;
    }
    .directory-nav,.directory-nav a{ 
        color: #666;
     }
    .directory-nav .cur a{
    }
    .directory-nav .line{ 
        position: absolute; 
        left:4px; top: 5px; 
        bottom: 5px; z-index: 1; 
        width: 2px; 
        /*background: #ddd;*/
        border-left: 1px dashed #c0c0c0;
    }
    .directory-nav .c-top,.directory-nav .c-bottom{ 
        position: absolute; 
        left: 0; 
        z-index: 2;
        display: block; 
        width: 10px; 
        height: 10px; 
        font-size: 0; 
        line-height: 0;
        background: url(../scripts/bdp/framework/imgs/directory-nav.png) no-repeat 0 -69px;
    }
    .directory-nav .c-dot{
        position:absolute; 
        left:-32px; 
        top: 50%; 
        z-index: 2; 
        margin-top: -5px;
        width: 10px; 
        height: 10px; 
        font-size:0; 
        line-height:0;
        background: url(../scripts/bdp/framework/imgs/directory-nav.png) no-repeat -275px 0;
    }

    .directory-nav li:hover{
        background-color:#f4f6f5;
    }       
    .directory-nav li:hover .c-dot{
        background: url(../scripts/bdp/framework/imgs/directory-nav.png) no-repeat 2px -115px;
    }
    .directory-nav .c-top{ 
        top: 0
    }
    .directory-nav .c-bottom{ 
        bottom: 0
    }
    .directory-nav .cur-tag{
        position: absolute; 
        left: -1px; 
        top:25px; 
        z-index: 5; 
        margin-top: -6px;
        display: block; 
        width: 19px; 
        height: 13px; 
        font-size: 0; 
        line-height: 0;
        background: url(../scripts/bdp/framework/imgs/directory-nav.png) no-repeat -271px -37px;
        -webkit-transition:top .3s ease 0s;
        transition:top .3s ease 0s;
    }
    .para{
