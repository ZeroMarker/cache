<body >
<server>
	s termdr=%request.Get("id")
	s base=%request.Get("base")
	if (termdr'="" & base = ""){
		s base = $lg($g(^CT.WDT.CDSS.TermD(termdr)),4)
	}
	s proid=%request.Get("proid")
	s baseDesc = %request.Get("baseDesc") //供电子病历调用，传描述获取id
	s termDesc = %request.Get("termDesc") //供电子病历调用，传描述获取id
	if (baseDesc'=""){
		s base = $o(^CT.WDT.CDSS.TermI("DescIndex"," "_$zconvert(baseDesc,"U"),0))
	}
	if ((termdr="")&&(termDesc'="")&&(base'=""))
	{
		s termdr = ##class(web.CDSS.CMKB.Term).GetTermDRByDesc(base,termDesc)	//$o(^User.MKBTermI("DescIndex",base," "_$zconvert(termDesc,"U"),""))
	}
	
</server>
