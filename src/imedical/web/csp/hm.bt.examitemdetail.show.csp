<!-- 主页面 -->
<div class="hisui-layout" data-options="fit:true">
	
	<!-- 中心页面 -->
	<div data-options="region:'center',border:false" style="padding:0px;">
		<div class="hisui-panel" fit="true" data-options="border:true,iconCls:'icon-resort',fit:true,headerCls:'panel-header-gray'">
	        <input type="hidden" id="gridBTExamItemDetail_ID"/>
			<table id="gridBTExamItemDetail" data-options="border:false,toolbar:'#gridBTExamItemDetail_custtb'"></table>
			<div id="gridBTExamItemDetail_custtb" style="padding:3px;" >
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true" id="btnAdd">新增</a>
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true" id="btnUpdate">修改</a>
				<div style="float: right;">
					<input id="gridBTExamItemDetail_search" data-options="prompt:'搜索'" class="hisui-searchbox"/>
				</div>
			</div>
		</div>
	</div><!-- 中心页面 End-->
	
	<!-- 站点细项编辑页面 -->
	<div id="winBTExamItemDetailEdit" class="hisui-dialog" style="border:none;overflow:hidden;padding-right:10px;">
		<table class="search-table">
			<tr>
				<td class="r-label"><label for="txtCode">细项代码</label></td>
				<td>
					<input class="textbox" id="txtCode" style="width:260px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="txtDesc">细项名称</label></td>
				<td>
					<input class="textbox" id="txtDesc" style="width:260px" />
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="txtDesc2">别名</label></td>
				<td>
					<input class="textbox" id="txtDesc2" style="width:260px" />
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="cboItemCat">站点项目类别</label></td>
				<td>
					<input class="hisui-combobox" id="cboItemCat" style="width:267px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="cboBaseType">基本分类</label></td>
				<td>
					<input class="hisui-combobox" id="cboBaseType" style="width:267px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="cboDataFormat">数据格式</label></td>
				<td>
					<input class="hisui-combobox" id="cboDataFormat" style="width:267px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="txtUnit">单位</label></td>
				<td>
					<input class="textbox" id="txtUnit" style="width:260px" />
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="txtExpress">计算公式</label></td>
				<td>
					<input class="textbox" id="txtExpress" style="width:260px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="txtExplain">说明</label></td>
				<td>
					<input class="textbox" id="txtExplain" style="width:260px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="cboSex">性别</label></td>
				<td>
					<input class="hisui-combobox" id="cboSex" style="width:267px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="txtSort">顺序号</label></td>
				<td>
					<input class="textbox" id="txtSort" style="width:260px"/>
				</td>
			</tr>
			<tr>
				<td class="r-label" colspan="1"><label for="chkActive" >是否有效</label></td>
				<td>
					<input class='hisui-checkbox' type="checkbox" id="chkActive" checked/>
				</td>
			</tr>
		</table>
	</div><!-- 编辑页面 End-->
	
</div><!-- 主页面 End-->