 * Description: HISUI   高危上报
-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
    <TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>

<SCRIPT language="Cache" RUNAT="SERVER"> 
set PAADM=$g(%request.Data("PAADM",1))
set OrderItemID=$g(%request.Data("OrderItemID",1))
</SCRIPT> 
<script type="text/javascript">
 var PAADM="#(PAADM)#"
 var OrderItemID="#(OrderItemID)#"
    
 
 </script>
 
  <style>
.search-table{
    border-collapse:separate;
    border-spacing:0 10px;
    /*padding: 0 10px;*/
}

.panel-header{
    border-bottom: 0;
}
.r-label{
    padding-left: 10px;
}
.r-label-1{
    padding-left: 20px;
}

</style>
 
</head>
<body>
<div class="hisui-layout" fit="true">
    <div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:265px;padding:10px 10px 0 10px;">
        <div class="hisui-panel" fit="true" data-options="title:'',headerCls:'panel-header-gray',bodyCls:'panel-body-gray',iconCls:'icon-apply-check'" style="border-radius:4px 4px 0px 0px;">
            <table class="search-table">
        <tr>
            <input type="hidden" id="ID"/>
          <td  class="r-label"><label for="RegNo">#(..Get("登记号"))#</label></td>
          <td style="width:200px;">
            <div>
              <input class="hisui-validatebox" type="text" Id="RegNo" style="width:160px;" /></div>
          </td> 
          
          <td class="r-label"><label for="Name">#(..Get("姓名"))#</label></td>
          <td>
           
              <input class="hisui-validatebox" type="text" Id="Name" style="width:160px;" />
          </td> 
          <td class="r-label-1">
            <a id="BSave" href="#" class="hisui-linkbutton" style="width:100px;" data-options="iconCls:'icon-w-edit'">保存</a>
          </td> 
          
          <td class="r-label-1">
              <a id="BSend" href="#" class="hisui-linkbutton" style="width:140px;" data-options="iconCls:'icon-w-export'">保存并发送</a>
          </td> 
         </tr>
         
        <tr>
          <td  class="r-label"><label for="Tel">#(..Get("电话"))#</label></td>
          <td style="width:200px;">
            <div>
              <input class="hisui-validatebox" type="text" Id="Tel" style="width:160px;" /></div>
          </td> 
          
          <td class="r-label"><label for="Sex">#(..Get("性别"))#</label></td>
          <td>
            <div>
              <input class="hisui-validatebox" type="text" Id="Sex" style="width:160px;"/></div>
          </td> 
          <td class="r-label-1">
                <a id="BClear" href="#" class="hisui-linkbutton"  style="width:100px;" data-options="iconCls:'icon-w-clean',stopAllEventOnDisabled:'true'">清屏</a>
            </td>
         </tr>   
          <tr>
          <td class="r-label"><label for="HighRisk">#(..Get("高危信息"))#</label></td>
          <td colspan="3">
            <div>
              <input class="hisui-combobox" type="text" Id="HighRisk" style="width:415px;" />
              </div>
          </td> 
         <td class="r-label-1">
            <input class='hisui-radio' type="checkbox" id="CRMFlag" label="需随访" />
         </td>
 
         </tr>
         
         <tr>
          <td  class="r-label"><label for="Content">#(..Get("短信信息"))#</label></td>
          <td colspan="3">
            <div>
              <textarea id="Content"  style="width:409px;height:100px;"></textarea></div>
          </td> 
          
          
         </tr>
         
         
        </table>
    </div>
    
    </div>
        <div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;border-radius:0px;">
            <div class="hisui-panel" data-options="bodyCls:'panel-body-gray'" fit="true" style="height:100%;border-top:0;border-radius:0 0 4px 4px;">
                    <table data-options="bodyCls:'panel-header-gray'" id="GWList"></table>
                </div>
            </div>
</div>


  <script type="text/javascript" src="../scripts/DHCPESendMessageNewHISUI.MainDoctor.js"></script>
  <script type="text/javascript" src="../scripts/DHCPECommon.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPCommon.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPOEData.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPCommonManageCard.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.COMMHardManager.js"></script>
  <script type="text/javascript" src="../scripts/DHCCPM_DOMComm.js"></script>
  <script type="text/javascript" src="../scripts/websys.jquery.js"></script>
  <script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
  
  <script type="text/javascript" src="../scripts/websys.js"></script>
  <script type="text/javascript" src="../scripts/websys.listrows.js"></script>
  <script type="text/javascript" src="../scripts/websys.List.js"></script>
  <script type="text/javascript" src="../scripts/websys.List.Tools.js"></script>
  <script type="text/javascript" src="../scripts/DHCPrtComm.JS"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPCommon.JS"></script>
  <script type="text/javascript" src="../scripts/dhtmlXCombo.js"></script>
  <script type="text/javascript" src="../scripts/dhtmlXCommon.js"></script>
  
</body>

 
</html>



