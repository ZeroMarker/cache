<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>

<html lang="zh-CN">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>
        <EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE>
    </title>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>
    
    <script type="text/javascript" src="../scripts/DHC.WMR.PinYinHead.js"></script>
    <script type="text/javascript" src="../scripts/nurse/hisui/nur.hisui.mobilecare.common.js" charset='gbk'></script>
    <link rel="stylesheet" type="text/css" href="../scripts/nurse/hisui/css/nur.hisui.mobilecare.common.css">
    <script type="text/javascript" src="../scripts/nurse/hisui/nur.hisui.mnis.config.standardization.js" charset='gbk'></script>
	
	<style type="text/css">
		.layout>.panel>.panel-body-noheader {
			border-color: #ddd;
			-moz-border-radius: 4px;
		    -webkit-border-radius: 4px;
		    border-radius: 4px;
		}
	</style>
	
</head>

<body class="hisui-layout">
	<div id="main" data-options="region:'center'">
    	<div class="hisui-layout" data-options="fit:true">
	    	<div id="standardizationPanel" data-options="region:'center'">
				<div id="standardizationGridTbar" class="div-tbar">
					<table cellspacing="0" cellpadding="0"><tbody><tr>
						<td><a id="standardizationGridSaveBtn" group="standardizationGridBtn" href="javascript:void(0)" class="l-btn l-btn-small l-btn-plain"><span class="l-btn-left l-btn-icon-left">
							<span class="l-btn-text">保存</span>
							<span class="l-btn-icon icon-save">&nbsp;</span></span></a></td>
						<td><a id="standardizationGridAddBtn" group="standardizationGridBtn" href="javascript:void(0)" class="l-btn l-btn-small l-btn-plain"><span class="l-btn-left l-btn-icon-left">
							<span class="l-btn-text">新增</span>
							<span class="l-btn-icon icon-add">&nbsp;</span></span></a></td>
						<td><a id="standardizationGridAppendBtn" group="standardizationGridBtn" href="javascript:void(0)" class="l-btn l-btn-small l-btn-plain"><span class="l-btn-left l-btn-icon-left">
							<span class="l-btn-text">追加</span>
							<span class="l-btn-icon icon-barbell">&nbsp;</span></span></a></td>
						<td><a id="standardizationGridDeleteBtn" group="standardizationGridBtn" href="javascript:void(0)" class="l-btn l-btn-small l-btn-plain"><span class="l-btn-left l-btn-icon-left">
							<span class="l-btn-text">删除</span>
							<span class="l-btn-icon icon-cancel">&nbsp;</span></span></a></td>
					</tr></tbody></table>
				</div>
				<table id='standardizationGrid' class="hisui-datagrid" border=false fit=true data-options="headerCls:'panel-header-gray',toolbar:'#standardizationGridTbar'">
				</table>
			</div>
        </div>
    </div>
</body>

</html>
