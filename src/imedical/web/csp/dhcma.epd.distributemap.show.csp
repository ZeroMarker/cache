<!-- 主页面-->
<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'west'" style="width:286px;border:none;padding:10px 0px 10px 10px;">
		<div class="hisui-panel" data-options="fit:'true',title:'筛查条件',headerCls:'panel-header-gray',iconCls:'icon-paper',">	  
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="cboHospital">院区</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="cboHospital" style="width:190px" />
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="dtDateFrom">开始日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="dtDateFrom" style="width:190px"/>	
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="dtDateTo">结束日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="dtDateTo" style="width:190px"/>	
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="cboRepType">报告状态</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="cboRepStatus" style="width:190px" />
					</td>
				</tr>
				<tr>
					<td class="r-label" colspan="2" style="text-align:center;padding-right: 0px;">
						<a id = "btnSearchTable" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-paper'">查询</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center'" style="border:none;padding:10px;">	 
		<div class="hisui-panel" data-options="fit:'true',bodyCls:'panel-body-gray'," style="overflow:hidden;overflow-y:auto;">
			<div id="Case01" style="width:70%;height:48%;margin-top:10px;margin-left:auto;margin-right:auto;"></div>
			<div id="Case02" style="width:70%;height:48%;margin-top:5px;margin-left:auto;margin-right:auto;"></div>
		</div>
	</div>
</div>