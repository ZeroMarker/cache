$.each(datas,function(i,dataNew)
{
if(dataNew.Promptly == "False" && WindowCompleted) return;
if(dataNew.IsPrompt == "True")
{
$.messager.confirm($g("提示"), dataNew.PromptMsg, function (r) {
if (r) {
ChangeData(dataNew,currentvalue);
}
});
}
else{
ChangeData(dataNew,currentvalue);
}
});

}
