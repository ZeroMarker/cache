<!-- 
 * FileName: dhcbill.preipadmtrans.csp
 * Anchor: ZhYW
 * Date: 2018-06-30
 * Description: 预住院转门诊/住院
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<ADDINS/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.preipadmtrans.css'/>
	<script type='text/javascript' src='../scripts/DHCWeb.OPCommonManageCard.js'></script>
	<server>
		do ##class(web.UDHCCardCommLinkRegister).GetCardHardJSFunction()
		w "<input id='ReadAccExpEncrypt' type='hidden' value='"_##class(%CSP.Page).Encrypt($lb("web.UDHCAccManageCLSIF.getaccinfofromcardno"))_"'>",$c(13,10)
		w "<input id='patientId' type='hidden' value=''/>",$c(13,10)
		w "<input id='episodeId' type='hidden' value=''/>",$c(13,10)
	</server>
</head>
<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',collapsible:false,title:'预住院转门诊/住院',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:127px;border-bottom-style:dashed;">
			<table class="search-table" style="width:90%">
				<tr>
					<td class="r-label td-first-col-43"><label>卡类型</label></td>
					<td><input id="cardType" class="textbox"/></td>
					<td class="r-label"><label>卡号</label></td>
					<td><input id="cardNo" class="textbox"/></td>
					<td class="r-label"><label>开始日期</label></td>
					<td><input id="stDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>结束日期</label></td>
					<td><input id="endDate" class="hisui-datebox textbox"/></td>
					<td><a href="javascript:;" class="hisui-linkbutton" id="btn-readCard" data-options="iconCls:'icon-w-card'">读卡</a></td>
				</tr>
				<tr>
					<td class="r-label td-first-col-43"><label>登记号</label></td>
					<td><input id="patientNo" class="textbox"/></td>
					<td class="r-label"><label>当前病区</label></td>
					<td><input id="curWard" class="textbox"/></td>
					<td class="r-label"><label>转入科室</label></td>
					<td><input id="dept" class="textbox"/></td>
					<td class="r-label"><label>转入病区</label></td>
					<td><input id="ward" class="textbox"/></td>
					<td><a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a></td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
			<table id="preIPAdmList"></table>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/common/dhcbill.preipadmtrans.js"></script>
</body>
</html>