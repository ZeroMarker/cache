<div class="hisui-layout" data-options="fit:true,border:false" >
	<div data-options="region:'north',border:false" style="height:300px;padding:10px 10px 10px 10px">
		<table id="gridDiag" data-options="toolbar: '#tbDiag',border:true"></table>
	</div>
	<div data-options="region:'center',border:false" style="">
		<div  class="hisui-panel" fit="true" style="border:0px;padding:0px 10px 10px 10px">
			<table id="gridOper" data-options="toolbar: '#tbOper',border:true"></table>
		</div>
	</div>
</div>
<div id="tbDiag" style="">
	<table class="search-table">
		<tr>
			<td class="r-label"><label>#(..Get("编目版本"))#</label></td>
			<td><input class="hisui-combobox textbox" id="cboMultiVer" style="width:137px;"></td>
			<td class="r-label"><a class="hisui-linkbutton hover-dark" id="btnSave" data-options="iconCls:'icon-w-save',stopAllEventOnDisabled:true">保存</a></td>
		</tr>
	</table>
	<div class="tool-bar-line" style="border-bottom-color:#cccccc; border-bottom-width: 1px; border-bottom-style: dashed;">	</div>
	<table cellspacing="0" cellpadding="0" style="padding-bottom: 7px;padding-top: 7px;">
		<tr>
			<td><a href="javascript:void(0)" id = "DiagNew" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">新增</a></td>
			<td><a href="javascript:void(0)" id = "DiagUp" class="hisui-linkbutton" data-options="iconCls:'icon-up',plain:true">上移</a></td>
			<td><a href="javascript:void(0)" id = "DiagDown" class="hisui-linkbutton" data-options="iconCls:'icon-down',plain:true">下移</a></td>
			<td><a href="javascript:void(0)" id = "DiagCopy" class="hisui-linkbutton" data-options="iconCls:'icon-copy',plain:true">复制</a></td>
			<td><a href="javascript:void(0)" id = "DiagCancel" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true">删除</a></td>
		</tr>
	</table>
</div> 
<div id="tbOper" style="">
	<table cellspacing="0" cellpadding="0" style="padding-bottom: 7px;padding-top: 7px;">
		<tr>
			<td><a href="javascript:void(0)" id = "OperNew" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">新增</a></td>
			<td><a href="javascript:void(0)" id = "OperUp" class="hisui-linkbutton" data-options="iconCls:'icon-up',plain:true">上移</a></td>
			<td><a href="javascript:void(0)" id = "OperDown" class="hisui-linkbutton" data-options="iconCls:'icon-down',plain:true">下移</a></td>
			<td><a href="javascript:void(0)" id = "OperCopy" class="hisui-linkbutton" data-options="iconCls:'icon-copy',plain:true">复制</a></td>
			<td><a href="javascript:void(0)" id = "OperCancel" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true">删除</a></td>
		</tr>
	</table>
</div>  