<!--入库单红冲-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',border:false,headerCls:'panel-header-gray'" class="GridPadding" style="padding-top:0;padding-bottom:0;">
			<table id="FindConditions" class="Condition">
				<tr>
					<td>入库科室</td>
					<td><input id="FRecLoc" name="FRecLoc" class="hisui-combobox" style="width:160px;"></td>
					<td>起始日期</td>
					<td><input id="StartDate" name="StartDate" class="hisui-datebox" style="width:160px;"></td>
					<td>入库单号</td>
					<td><input id="FInGrNo" name="FInGrNo" class="textbox" style="width:153px;"></td>
					<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
					<td><a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:''" style="width:70px">红冲</a></td>
					<td><input id="AuditFlag" name="AuditFlag" type="hidden" value="Y"></td>
				</tr>
				<tr>
					<td>供应商</td>
					<td><select id="FVendorBox" name="FVendorBox" class="hisui-combobox" style="width:160px;"></select></td>
					<td>截止日期</td>
					<td><input id="EndDate" name="EndDate" class="hisui-datebox" style="width:160px;"></select></td>
					<td>发票号</td>
					<td><input id="InvNo" name="InvNo" class="textbox" style="width:153px;"></td>
					<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
				</tr>
			</table>
		</div>
		<div data-options="region:'west',border:false" class="GridPadding" style="width:500px;padding-top:0;">   
	    	<div class="hisui-panel" data-options="title:'入库单',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">   
				<table id="InGdRecMainGrid"></table>
	    	</div>
    	</div>
    	<div class="hisui-panel" data-options="region:'center',border:false" class="GridPadding" style="padding-left:0;padding-top:0;">   
        	<div class="hisui-panel" data-options="title:'入库单明细',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">   
				<table id="InGdRecDetailGrid"></table>
        	</div>   
        </div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecRed.js"></script>
</body>
</html>
