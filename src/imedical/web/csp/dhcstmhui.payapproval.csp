<!--付款审批-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'center',border:false,collapsible:false" style="padding:10px;height:450px;">
		<div class="hisui-layout" fit="true">
			<div data-options="region:'north',border:false,collapsible:false" style="height:175px;">
				<div class="hisui-layout" data-options="fit:true">
					<div data-options="region:'west',border:false" style="width:850px;"> 
						<div class="hisui-panel" title="入库退货单" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">    		
							<table id="MainConditions" class="Condition">
								<tr>
									<td><label>入库科室</label><input id="IngrLoc" name="IngrLoc" class="hisui-combobox" style="width:150px;"></td>
									<td><label>起始日期</label><input id="StartDate" name="StartDate" class="hisui-datebox textbox" style="width:115px;"></td>
									<td><label>审核状态</label><select id="AuditFlag" name="AuditFlag" class='hisui-simplecombobox' style="width:90px;"></select></td>
									<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
								</tr>
								<tr>
									<td><label>供应商</label><select id="Vendor" name="Vendor" class="hisui-combobox" style="width:150px;"></select></td>
									<td><label>截止日期</label><input id="EndDate" name="EndDate" class="hisui-datebox textbox" style="width:115px;"></td>
									<td><label>发票状态</label><select id="InvFlag" name="InvFlag" class='hisui-combobox' style="width:90px;">
										<option value="" selected>全部</option>
										<option value="Y">有发票</option>
										<option value="N">无发票</option>
										</select>
									</td>
									<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
								</tr>
								<tr>
									<td><label>接收科室</label><input id="ReqLoc" name="ReqLoc" class="hisui-combobox" style="width:150px;"></td>
									<td><label>发票号</label><input id="InvNo" name="InvNo" class="textbox" style="width:110px;"></td>
									<td><label>高值状态</label><select id="HvFlag" name="HvFlag" class='hisui-combobox' style="width:90px;">
										<option value="" selected>全部</option>
										<option value="Y">高值</option>
										<option value="N">非高值</option>
									</select></td>
								</tr>
							</table>
						</div>
					</div>
					<div data-options="region:'center',border:false" style="padding-left:0;">
						<div class="hisui-panel" title="保存条件" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
							<table id="SaveConditions" class="Condition">
								<tr>
									<td><label>凭证月份</label><select id="VoucherMonthBox" name="VoucherMonthBox" class="hisui-combobox" style="width:150px;"></select></td>
								</tr>
							</table>
						</div>
					</div>
				</div>
			</div>
			<div data-options="region:'center',border:false,collapsible:false">
				<div class="hisui-panel" data-options="region:'center',fit:true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table id="GRMainGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div data-options="region:'south',border:false,collapsible:false" style="padding:0 10px 10px 10px;height:350px;">
		<div class="hisui-panel" title="入库退货单明细" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
			<table id="GRDetailGrid"></table>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Pay/PayCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/PayApproval/PayApproval.js"></script>
</body>
</html>