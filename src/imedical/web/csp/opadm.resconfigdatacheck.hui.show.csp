<div class="hisui-layout" data-options="fit:true,border:false">
    <div data-options="region:'north',border:false" style="height:91px;border-bottom:1px dashed #ccc;">
    	<table class="search-table">
    		<tr>
				<td class="r-label">
					<label id="_HospUserListLabel" style='color:red;' class='r-label'>医院</label>
				</td>
			
				<td colspan="3">
					<input id="_HospUserList"/>
				</td>
			</tr>
			<tr>
				<td class="r-label"><label for="StartDate">开始日期</label></td>
				<td>
					<input class="hisui-datebox textbox" id="StartDate" data-options="formatter:myformatter,parser:myparser"/>
				</td>
				<td class="r-label">
					<label for="EndDate">结束日期</label>
				</td>
				<td>
					<input class="hisui-datebox textbox" id="EndDate" data-options="ormatter:myformatter,parser:myparser"/>
				</td>
				<td class="r-label">
					<label for="Dept">科室</label>
				</td>
				<td>
					<input class="textbox" id="Dept"/>
				</td>
				<td class="r-label">
					<label for="Mark">号别</label>
				</td>
				<td>
					<input class="textbox" id="Mark"/>
				</td>
				<td class="r-label">
					<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
				</td>
			</tr>
		</table>
	</div>
    <div data-options="region:'center'" style="border:0px;">
    	<table id="ResConfDataCheckTab"></table>
    </div>   
</div>