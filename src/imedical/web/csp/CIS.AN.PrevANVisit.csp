							<input type="checkbox" id="FamilyHistoryCHK1" data-formitem="FamilyHistory" class="hisui-checkbox" label="否">
							<input type="checkbox" id="FamilyHistoryCHK2" data-formitem="FamilyHistory" class="hisui-checkbox" label="是">
