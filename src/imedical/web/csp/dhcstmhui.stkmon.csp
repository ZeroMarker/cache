<!--生成月报-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',border:false" class="GridPadding" style="height:270px">
		<div class="hisui-panel" data-options="title:'生成月报',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">
			<table id="StkMonGrid"></table>
		</div>
	</div>
	<div data-options="region:'center',border:false" class="GridPadding" style="padding-top:0;">
		<div class="hisui-panel" data-options="title:'历史月报',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">
		<div class="hisui-layout" data-options="fit:true">
			<div data-options="region:'north',border:false" style=""> 
				<table id="HistoryStkMonConditions" class="Condition">
					<tr>
						<td>科室</td>
						<td><input id="HistoryStkMonLoc" name="HistoryStkMonLoc" class="hisui-combobox" style="width:150px;"></td>
						<td>月报范围</td>
						<td><input id="StYear" name="StYear" class="textbox" style="width:60px;"></td>
						<td>年</td>
						<td><input id="StMonth" name="StMonth" class="textbox" style="width:60px;"></td>
						<td>月</td>
						<td>--</td>
						<td><input id="EdYear" name="EdYear" class="textbox" style="width:60px;"></td>
						<td>年</td>
						<td><input id="EdMonth" name="EdMonth" class="textbox" style="width:60px;"></td>
						<td>月</td>
						<td><a href="#" id="SearchBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
						<td><a href="#" id="DeleteBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-close'">删除</a></td>
						<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
					</tr>
				</table>
			</div>
			<div data-options="region:'center',border:false">
				<table id="HistoryStkMonGrid"></table>
			</div>
		</div>
		</div>
	</div>
	
	<script type="text/javascript" src="../scripts/dhcstmhisui/StkMonRep/DHCStkMon.js"></script>
</body>
</html>
