<!--各种原因维护-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head> 
<body class="hisui-layout" data-options="fit:true">
	<div id="DetailTabs" class="hisui-tabs tabs-gray GridPadding" data-options="region:'center',tabPosition:'left',headerWidth:150,headerCls:'panel-header-gray'" style="border:0px solid #ccc;">
		<div title="调整原因维护" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div  data-options="region:'center'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="IncReasonForAdjGrid"></table>
				</div>
			</div>
		</div>
		<div title="调价原因维护" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true" >
				<div data-options="region:'center'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="ReasonForAdjustPriceGrid"></table>
				</div>
			</div>
		</div>
		<div title="报损原因维护" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true" >
				<div data-options="region:'center'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="IncReasonForScrapGrid"></table>
				</div>
			</div>
		</div>
		<div title="退货原因维护" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true" >
				<div data-options="region:'center'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="IncReasonForRetGrid"></table>
				</div>
			</div>
		</div>
		<div title="订单取消原因维护" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true" >
				<div data-options="region:'center'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="InPoReasonForCancleGrid"></table>
				</div>
			</div>
		</div>
		<div title="请求单拒绝原因维护" style="padding-left:10px;" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'center'" style="border-top:0px solid #ccc;border-left:1px solid #ccc;border-bottom:0px solid #ccc;border-right:0px solid">
					<table id="ReasonForRefuseRequestGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonHosp.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/ReasonManage/ReasonManage.js"></script>
</body>
</html>