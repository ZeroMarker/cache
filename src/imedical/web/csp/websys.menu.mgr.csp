<head>
	<server>
		set padding=%request.Get("padding")
		if padding="" set padding=10
		set padding=##class(ext.util.String).Replace(padding,"-","px ")_"px"
	</server>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<style>
		html,body{
			height:100%;
			width:100%;
			padding:0;
			margin:0;
			background-color:#fff;
		}
		span.label{
			padding:0 10px 0 15px;
			line-height:30px;	
			display:inline-block;
		}
		span.label.first{
			padding-left:5px;	
		}
		span.label.w42{
			display:inline-block;
			width:42px;	
			text-align:right;
		}
		.i-searchbox td{
			padding:5px 10px;
		}
		.i-searchbox td.i-label{
			padding:5px 0px 5px 10px;
			text-align:right;
			width:150px;
		}
		span.d-label{
			text-align:right;
			padding:0 10px 0 0;
			color:#666;
		}
		#menu-security-win .i-searchbox td.i-label{
			width:80px;
		}
		.textbox.tb200{
			width:193px;
		}
	    #edit-sep{
			height:14px;
			width:100%;   
			position:relative;
		}
		#edit-sep-line{
			border-bottom:1px dashed #f9efef;
			height:6px;
			width:100%; 
			
		}
		#edit-sep-text{
			height:14px;line-height:14px;color:#f9efef;
			background:#fff;
			position:absolute;top:0;left:230px;
			cursor:pointer;
		}
		#edit-sep:hover #edit-sep-line,
		#edit-sep.active #edit-sep-line{
			border-bottom:1px solid #dad5d5;
		}
		#edit-sep:hover #edit-sep-text,
		#edit-sep.active #edit-sep-text{
			color:blue;
		}
		#auth-groups{
			width:200px;
		}
		.authgroup{
			border:1px solid #95B8E7;
			border-radius:3px;
			cursor:pointer;
			width:auto;
			margin-bottom:5px;
			margin-right:5px;
			float:left;
		}
	    .authgroup>span{padding:1px 3px;}
		.authgroup>span.Delete{border-right:0;padding:1px 5px}
		.authgroup>span.Delete:hover{color:red;}
		.icon-security{
			display:inline-block;
			width:30px;
			background:url(../images/uiimages/lock-new.png) center center no-repeat;
			cursor:pointer;
		}
		.checkbox-label{
			display:inline-block;
			width:100%;
			text-align:center;
		}
		#group-menu-tree-tools{
			background-color:#fff;
			border-bottom: 1px solid #ccc;
			line-height:30px;
			padding:1px 0 2px 10px;
			margin-bottom:10px;
			height:35px; /*自动填充ie下35.82*/
		}
		.dialog-button{
			padding:10px;	
		}
		
		

		.kw-section-list > li {
		    float: left;
		    margin: 2px 5px;
		}
		.kw-section-list > li:first-child{
		    margin-left:0;
		}
		.kw-section-list:after{
			clear:both;
			content: "";
            display: block;
            width: 0;
            height: 0;	
		}
		.icon-paper{
			background-position-y: -1px;
		}

		.panel-title.panel-with-icon {
		    padding-left: 31px;
		}
	</style>
</head>

