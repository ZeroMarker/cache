<!--药房药库-处方点评-查单
	csp:pha.prc.v2.comment.findno.csp
	js: pha/prc/v2/comment.findno.js
-->
<div id="diagFindNo">
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',border:false" class="pha-panel-body-content">
            <table id="gridFindNo"></table>
        </div>
    </div>
</div>
    
<div id="gridFindNoBar">
	<div class="pha-row">
        <div class="pha-col">
            <label for="conStartDate">#(..Get("开始日期"))#</label>
        </div>
        <div class="pha-col">
            <input id='conStartDate' data-pha="class:'hisui-datebox'" />
        </div>
        <div class="pha-col">
            <label for="conEndDate">#(..Get("截止日期"))#</label>
        </div>
        <div class="pha-col">
            <input id='conEndDate' data-pha="class:'hisui-datebox'" />
        </div>
        <div class="pha-col">
            <label for="conWay">#(..Get("点评方式"))#</label>
        </div>
        <div class="pha-col">
            <input id='conWay' data-pha />
        </div>
        <div class="pha-col">
            <a class="hisui-linkbutton" id="btnSearch">查询</a>
        </div>
    </div>
    <div class="pha-row">
        <div class="pha-col">
            <label for="conResult">#(..Get("点评结果"))#</label>
        </div>
        <div class="pha-col">
            <input id='conResult' data-pha />
        </div>
        <div class="pha-col">
            <label for="conPharmacist">#(..Get("点评药师"))#</label>
        </div>
        <div class="pha-col">
            <input id='conPharmacist' data-pha />
        </div>
        <div class="pha-col">
            <label for="comState">#(..Get("点评状态"))#</label>
        </div>
        <div class="pha-col">
            <input id='comState' data-pha />
        </div>
    </div>
	<div class="pha-line"></div>
	<a class="hisui-linkbutton" plain='true' iconCls="icon-cancel" id="btnDelete">删除</a>
    <a class="hisui-linkbutton" plain='true' iconCls="icon-submit" id="btnSubmit">提交</a>  
    <a class="hisui-linkbutton" plain='true' iconCls="icon-ok" id="btnSelect">确定</a> 
</div>

