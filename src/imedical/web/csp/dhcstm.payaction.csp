<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

 s Action=$Get(%request.Data("actiontype",1))
 s Start=$Get(%request.Data("start",1))
 s Limit=$Get(%request.Data("limit",1))
 s Sort=$Get(%request.Data("sort",1))
 s Dir=$Get(%request.Data("dir",1))
 s StrParam=$Get(%request.Data("strParam",1))
 s UserId=$Get(%request.Data("userId",1))
 s LocId=$Get(%request.Data("locId",1))
 s PayId=$Get(%request.Data("parref",1))
 
 //获取参数
 i Action = "GetParam" d
 .s SSACode=$Get(%request.Data("SSACode",1))
 .s SSAPCode=$Get(%request.Data("SSAPCode",1))
 .s PFType=$Get(%request.Data("PFType",1))
 .s result = ##class(web.DHCSTM.StkSysAppParame).GetPropValue(SSACode, SSAPCode , PFType) 
 .w result
 
 //完成付款单
 i Action = "SetComp" d
 .s payid=$Get(%request.Data("payid",1))
 .s result = ##class(web.DHCSTM.DHCPay).SetCompleted(payid)
 .i result = 0  d
 ..w "{success:'true',info:'"_result_"'}"
 .e  d
 ..w "{success:'false',info:'"_result_"'}"
 
 //取消完成付款单
 i Action = "CnlComp" d
 .s payid=$Get(%request.Data("payid",1))
 .s result = ##class(web.DHCSTM.DHCPay).CnclCompleted(payid)
 .i result = 0 d
 ..w "{success:'true',info:'"_result_"'}"
 .e  d
 ..w "{success:'false',info:'"_result_"'}"
 
 //生成新付款单主表
 i Action = "save" d
 .s MainInfo=$Get(%request.Data("MainInfo",1))
 .s storeId=$Get(%request.Data("StoreId",1))
 .s result = ##class(web.DHCSTM.DHCPay).Save(MainInfo,storeId)
 .i result = "" d
    ..w "{success:'false',info:'"_result_"'}"
 .e  d
 ..w "{success:'true',info:'"_result_"'}"  

 //检索待付款供应商列表
 i Action = "queryVendor" d
 .s strParam=$Get(%request.Data("strParam",1))
 .s result = ##class(web.DHCSTM.DHCPay).jsVendorListToPay(Start,Limit,"","",strParam)
 .i result = "" d
 ..w "{results:0,rows:[]}"
 .e  d
 ..w result  

 //检索待付款记录列表
 i Action = "queryItmToPay" d
 .s strParam=$Get(%request.Data("strParam",1))
 .d ##class(web.DHCSTM.DHCPay).jsGdRItmToPay(Start,Limit,"","",strParam)

 //取存储号
 i Action="GetStoreId" d
 .s storeId=##class(web.DHCSTM.DHCPay).GetStoreId()
 .i storeId'="" d
 ..w "{success:'true',info:'"_storeId_"'}"
 .e  d
 ..w "{success:'false',info:''}"

  //暂存业务(入/退)明细数据,为生成付款单准备数据
 i Action = "StorePayItm" d
 .s storeId=$G(%request.Data("storeId",1))
 .s detailData=$G(%request.Data("detailData",1))
 .s result = ##class(web.DHCSTM.DHCPay).StoreData(storeId,detailData)
 .i result >0 d
 ..w "{success:'true',info:'"_result_"'}"
 .e  d
 ..w "{success:'false',info:''}"
 
 i Action = "DelPayItm" d
 .s payi=$G(%request.Data("RowId",1))
 .s ret=##class(web.DHCSTM.DHCPayItm).Delete(payi)
 .i ret=0 d
 ..w "{success:'true',info:'"_ret_"'}"
 .e  d
 ..w "{success:'false',info:'"_ret_"'}"

 //查询付款单列表
 i Action = "query" d
 .d ##class(web.DHCSTM.DHCPay).jsDHCPay(Start,Limit,Sort,Dir,StrParam)

 //取明细
 i Action = "queryItem" d
 .d ##class(web.DHCSTM.DHCPayItm).jsDHCPayItm(Start,Limit,PayId)

 //采购确认信息
 i Action = "SetAck1" d
 .s PayId=$Get(%request.Data("PayId",1))
 .s user=%session.Get("LOGON.USERID")
 .s result = ##class(web.DHCSTM.DHCPayQuery).SetPayAck1(PayId,user)
 .i result = 0 d
 ..w "{success:'true',info:''}"
 .e  d
 ..w "{success:'false',info:'"_result_"'}"
 
 //会计确认信息
 i Action = "SetAck2" d
 .s PayId=$Get(%request.Data("PayId",1))
 .s user=%session.Get("LOGON.USERID")
 .s PayInfo=$Get(%request.Data("PayInfo",1))
 .s result = ##class(web.DHCSTM.DHCPayQuery).SetPayAck2(PayId,user,PayInfo)
 .i result = 0 d
 ..w "{success:'true',info:''}"
 .e  d
 ..w "{success:'false',info:'"_result_"'}"
 
 i Action = "payMainInfo" d
 .s pay=$g(%request.Data("pay",1))
 .i pay'="" d
 ..s result=##class(web.DHCSTM.DHCPay).jsSelect(pay) 
 ..i result = "" d
 ...w "{results:0,rows:[]}"
 ..e  d
 ...w result
 .e  d
 ..w "{success:'false',info:''}"

 i Action="GetPayMode" d
 .s result=##class(web.DHCSTM.DHCPay).jsPayMode()
 .i result = "" d
 ..w "{results:0,rows:[]}"
 .e  d
 ..w result 
 .

 i Action="UpdPayItm" d
 .s pay=$g(%request.Data("pay",1))
 .s payi=$g(%request.Data("payi",1))
 .s detailData=$g(%request.Data("detailData",1))
 .s result=##class(web.DHCSTM.DHCPayItm).Update(payi,pay,detailData)
 .i result = "" d
 ..w "{success:'false',info:''}"
 .e  d
 ..w "{success:'true',info:'"_result_"'}"

 i Action="UpdPay" d
 .s pay=$g(%request.Data("pay",1))
 .s loc=$g(%request.Data("loc",1))
 .s vendor=$g(%request.Data("vendor",1))
 .s user=%session.Data("LOGON.USERID")
 .s paymode=$g(%request.Data("paymode",1))
 .s pay=##class(web.DHCSTM.DHCPay).Update(pay,loc,vendor,user,paymode)
 .i pay>0 d
 ..w "{success:'true',info:'"_pay_"'}"
 .e  d
 ..w "{success:'false',info:'"_pay_"'}"
 
 i Action = "Select" d
 .S pay=$Get(%request.Data("pay",1))
 .s ret=##class(web.DHCSTM.DHCPay).Select(pay)	
 .w "{success:'true',info:'"_ret_"'}"
 
 //财务确认付款单
 i Action = "SetAck3" d
 .s PayId=$Get(%request.Data("PayId",1))
 .s user=%session.Get("LOGON.USERID")
 .s result = ##class(web.DHCSTM.DHCPayQuery).SetPayAck3(PayId,user)
 .i result = 0 d
 ..w "{success:'true',info:''}"
 .e  d
 ..w "{success:'false',info:'"_result_"'}"

</script>