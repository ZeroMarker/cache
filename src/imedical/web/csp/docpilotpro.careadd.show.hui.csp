<div class="hisui-layout" fit="true">
	<div data-options="region:'west',border:false,split:true,collapsible:false" style="width:420px;border:false;">
		<div class="hisui-layout" data-options="fit:true">   
	        <div data-options="region:'north',border:false,headerCls:'panel-header-gray'" style="height:135px;border:0px;padding:10px 0 0 10px;">
	        	<div class="hisui-panel" data-options="title:'科研项目列表',headerCls:'panel-header-gray',iconCls:'icon-add-note'" style="border-radius:0;">
		        	<table class="search-table">
		        		<tr>
		        			<td class="r-label"><label for="WarnSum">#(..Get("快速查找"))#</label></td>
		        			<td>
		        				<input class="textbox" id="FindPilotPro" style="width:128px;"/>
		        			</td>
		        			<td class="r-label" style="text-align:left;">
		        				<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:158px;">查询科研项目</a>
		        			</td>
		        		</tr>
		        		<tr>
		        			<td class="r-label">
		        				<label for="WarnSum">#(..Get("余额提醒值"))#</label>
		        			</td>
		        			<td>
		        				<input class="textbox hisui-numberbox" disabled id="WarnSum" style="width:135px;"/>
		        			</td>
		        			<td class="r-label" style="text-align:left;">
		        				<a id="UpdateWarnSum" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'" style="display:none;width:158px;">修改余额提醒值</a>
		        			</td>
		        		</tr>
		        	</table>
	        	</div>
	        </div>   
	        <div data-options="region:'center',border:false,headerCls:'panel-header-gray'" style="padding:0 0 10px 10px;">
	        	<div class="hisui-panel no-rightline" fit="true" data-options="bodyCls:'panel-body-gray'"  style="height:100%;border-top:0;border-radius:0 0 4px 4px;">
	        		<table id="PilotProListTab"></table>
	        	</div>
	        </div>   
	    </div>
	</div>
	<div data-options="region:'center',collapsible:false,border:false">
		<div class="hisui-layout" data-options="fit:true">  
			<div data-options="region:'north',border:false,headerCls:'panel-header-gray'" style="height:95px;padding:10px 10px 0 5px;">
		    	<div class="hisui-panel" data-options="title:'项目参与者列表',headerCls:'panel-header-gray',iconCls:'icon-add-note'" style="border-bottom-style:dashed;border-radius:0;">
			    	<table class="search-table">
			    		<tr>
			    			<td class="r-label"><label for="UserName">#(..Get("用户"))#</label></td>
			    			<td>
			    				<input class="hisui-combobox textbox" disabled id="UserName"/>
			    			</td>
			    			<td class="r-label"><label for="ContactTel">#(..Get("联系电话"))#</label></td>
			    			<td>
			    				<input class="textbox" disabled id="ContactTel"/>
			    			</td>
			    			<td class="r-label">
			    				<input id="ContactFlag" class='hisui-checkbox' disabled type="checkbox" label="是否联系人">
			    			</td>
			    		</td>
			    	</table>
		    	</div>  
		    </div>   
		    <div data-options="region:'center',border:false,headerCls:'panel-header-gray'" style="padding:0 10px 10px 5px;">
		    	<div class="hisui-panel" fit="true" data-options="bodyCls:'panel-body-gray'" style="height:100%;border-top:0;border-radius:0 0 4px 4px;">
		    		<table id="PilotProCareListTab"></table>
		    	</div>
		    </div>  
		</div>
	</div>
</div>