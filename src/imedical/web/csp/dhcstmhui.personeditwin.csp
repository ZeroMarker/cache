<!--物资弹窗-->
<div id="AddPersonWin" class="hisui-dialog" data-options="closed:'true',modal:true" title="人员信息编辑" style="padding-right:10px">
	<table id="AddPersonConditions" class="Condition">
		<tr>
			<input id="PersonRowId" name="PersonRowId" type="hidden">
			<td><label>姓名</label><input id="PersonName" name="PersonName" class="textbox"></td>
			<td><label>身份证</label><input id="PersonCard" name="PersonCard" class="textbox"></td>
		</tr>
		<tr>
			<td><label>手机</label><input id="PersonCarrTel" name="PersonCarrTel" class="textbox"></td>
			<td><label>电话</label><input id="PersonTel" name="PersonTel" class="textbox"></td>
		</tr>
		<tr>
			<td><label>邮箱</label><input id="PersonEmail" name="PersonEmail" class="textbox"></td>
			<td><label>传真</label><input id="PersonFax" name="PersonFax" class="textbox"></td>
		</tr>
		<tr>
			<td><label>授权开始日期</label><input id="PersonStartDate" name="PersonStartDate" class="hisui-dateboxq textbox"></td>
			<td><label>截止日期</label><input id="PersonEndDate" name="PersonEndDate" class="hisui-dateboxq textbox"></td>
		</tr>
		<tr>
			<td><label>是否展示</label><input id="PersonShowFlag" name="PersonShowFlag" class="hisui-combobox textbox"></td>
		</tr>
		<tr>
			<td colspan="2" align="center"><a href="#" id="PersonSaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a></td>
		</tr>
	</table>
</div>

<script type="text/javascript" src="../scripts/dhcstmhisui/CommonPerson/AddPersonWin.js"></script>