<body>
<TRAK:APPLET>
<SCRIPT language="Cache" RUNAT="SERVER">
	;
	;udhcaccregcard.csp
	;建立账户CSP
	n myComName
	
	s myComName=""
	
	s myVer=##class(web.DHCOPConfig).GetVersion()
	if (+myVer=0){
		;北京积水潭的
		;
		s myComName="UDHCAccRegCard.ICFNewCardNo"
	}elseif (+myVer=1){
		;合肥发卡建立帐户程序
		s myComName="UDHCAccRegCardFixSec"
	}elseif (+myVer=3){
		;为没有卡建立账户的支付模式
		;交验码仍然写?写登记号
		s myComName="UDHCAccRegCard.ICFNewCardNo"
		s myComName="UDHCAccRegCard.FNoCard"
	}elseif (+myVer=4){
		;宁波明州医院
		s myComName=""
	}else{
		s myComName="UDHCAccRegCardFixSec"
	}
	
	q:(myComName="")
	
	s mobj=##Class(websys.Component).OpenName(myComName)
	
	i mobj d
	.s mobj.HideMenus=1
	.s mobj.HideHeadings=1
	.;s mobj.Style="LEFT:0; WIDTH:100%; POSITION:relative;"
	.d mobj.Show()
	.d mobj.%Close()
	
	s mystr=""
	
	;w myComName
	
</Script>

</body>

</HTML>
