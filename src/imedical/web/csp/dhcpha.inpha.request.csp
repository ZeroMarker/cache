<!DOCTYPE html>
<!--住院退药申请  
	csp: dhcpha.inpha.request.csp
	js:  scripts/pharmacy/inpha/dhcpha.inpha.request.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>
    <Server language="CACHE" runat="Server">
        s admId = $G(%request.Data("EpisodeID",1))
    </Server>
    <SCRIPT Language="Javascript">
        // 接收参数
        var LoadAdmId = "#(admId)#" || "";
    </script>
    <style>

    </style>
</HEAD>

<body>
    <div class="hisui-layout" fit="true" border="false">
        <div data-options="region:'center',split:true,border:false,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">
            <div class="hisui-layout" fit="true" border="false">
                <div data-options="region:'center',split:true,headerCls:'panel-header-gray',title:'退药申请-病人已发药'" iconCls='icon-panel-common-query'>
                    <table id="gridDisped"></table>
                </div>
            </div>
        </div>
    </div>
    <div id="gridDispedBar">
        <table class="dhcpha-north-tbl" cellpadding=0 cellspacing=0>
            <tr>
                <td>
                    <label for="cmbPhaLoc">发药科室</label>
                    <input id="cmbPhaLoc" class="hisui-combobox" />
                    <a class="hisui-linkbutton" id="btnDefaultLoc">设为默认科室</a>
                </td>
                <td>
                    <label for="txtPatNo">登记号</label>
                    <input id="txtPatNo" class="hisui-validatebox" readonly/>
                </td>
                <td>
                    <label for="txtPatName">姓名</label>
                    <input id="txtPatName" class="hisui-validatebox" readonly/>
                </td>
                <td>
                    <a class="hisui-linkbutton" iconCls='icon-w-find' id="btnFind">查询</a>
                </td>
                <td>
                    <a class="hisui-linkbutton" iconCls='icon-w-save' id="btnSave">生成退药申请</a>
                </td>
                <td>
                    <a class="hisui-linkbutton" iconCls='icon-w-find' id="btnDirReq">申请单查询</a>
                </td>
                <td>
                    <!--a class="hisui-linkbutton" iconCls='icon-w-clean'  id="btnClear">清屏</a-->
                </td>
            </tr>
        </table>
    </div>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.store.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.tools.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/inpha/dhcpha.inpha.request.js"></script>
</body>

</HTML>