<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="height:92px;">
		<div class="hisui-panel" style="border:0;border-bottom:1px dashed #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="PPFItmMastDR">医嘱</label>
					</td>
					<td colspan="3">
						<input class="textbox" id="PPFItmMastDR" style="width:328px;"/>
					</td>
					<td class="r-label">
						<label for="PPFFreeNum">免费次数</label>
					</td>
					<td>
						<input class="textbox" id="PPFFreeNum" style="width:120px;"/>
					</td>
					<td class="r-label">
						<label for="PPFLimitEntryAfterNoFreeNum">免费次数用尽后限制录入</label>
					</td>
					<td>
						<input id="PPFLimitEntryAfterNoFreeNum" class='hisui-checkbox' type="checkbox">
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="PPFSttDate">开始日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="PPFSttDate" data-options="formatter:myformatter,parser:myparser" style="width:126px;"/>
					</td>
					<td class="r-label">
						<label for="PPFSttTime">开始时间</label>
					</td>
					<td>
						<input class="hisui-timespinner textbox" id="PPFSttTime" data-options="showSeconds:true" style="width:126px;"/>
					</td>
					<td class="r-label">
						<label for="PPFEndDate">结束日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="PPFEndDate" data-options="formatter:myformatter,parser:myparser" style="width:126px;"/>
					</td>
					<td class="r-label">
						<label for="PPFEndTime">结束时间</label>
					</td>
					<td>
						<input class="hisui-timespinner textbox" id="PPFEndTime" data-options="showSeconds:true" style="width:126px;"/>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false">
		<table id="FreeOrdSetTab"></table>
	</div>
</div>
