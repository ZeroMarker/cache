<!-- 主页面-->
<div id="NYZDReport" class="hisui-layout" data-options="fit:true" style="width:100%;height:100%;">
	<div data-options="region:'center',border:false,collapsible:false">
		<div class="page-body" id="divbody" style="width:1290px;">
			<div class="report-header">
				<span >糖尿病发病报告卡</span>
			</div>
			<div class="report-content">
				<div class="hisui-panel" id="BaseInfo" data-options="title:'基本信息',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="BaseInfoTab"  class="report-tab">
						<tr class="report-tr">
							<td class="report-td">卡片编号</td>
							<td><input class="textbox" id="txtCRKPBH" disabled /></td>
							<td class="report-td">登记号</td>
							<td><input class="textbox" id="txtPatientNo" disabled /></td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>姓名</td>				
							<td><input class="textbox" id="txtPatName" disabled /></td>
							<td class="report-td"><font color='red'>*</font>性别</td>					
							<td><input class="textbox" id="txtPatSex" disabled /></td>
							<td class="report-td"><font color='red'>*</font>年龄</td>					
							<td>
								<input class="textbox" id="txtPatAge" style="width:60px;" disabled />
								<input class="textbox" id="cboPatAgeDW" style="width:83px;" disabled />
							</td>
							<td class="report-td"><font color='red'>*</font>出生日期</td>				
							<td><input class="hisui-datebox textbox" id="txtBirthDay"/></td>
					   </tr>
					   <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>民族</td>				
							<td><input class="textbox" id="cboCRMZ" /></td>
							<td class="report-td"><font color='red'>*</font>身份证号</td>				
							<td><input class="textbox" id="txtPatCardNo"/></td>
							<td class="report-td"><font color='red'>*</font>联系电话</td>					
							<td><input class="textbox" id="txtJTDH"/></td>
							<td class="report-td"><font color='red'>*</font>文化</td>					
							<td><input class="textbox" id="cboCRWH"/></td>
						</tr>
					   
					   <tr class="report-tr">					
							<td class="report-td"><font color='red'>*</font>职业</td>					
							<td colspan="3"><input class="textbox" id="cboCRZY" style="width:443px;"/></td>
							<td class="report-td" ><font color='red'>*</font>具体工种</td>					
							<td><input class="textbox" id="cboCRGZ"/></td>
							<td class="report-td">工作单位</td>				
							<td colspan="3"><input class="textbox" id="txtGZDW"/></td>						
					   </tr>
					   
					   <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>目前居住地址省</td>				
							<td><input class="textbox" id="cboProvince1"/></td>
							<td class="report-td"><font color='red'>*</font>市</td>				
							<td><input class="textbox" id="cboCity1"/></td>
							<td class="report-td"><font color='red'>*</font>县</td>					
							<td><input class="textbox" id="cboCounty1" /></td>
							<td class="report-td"><font color='red'>*</font>乡/镇</td>					
							<td><input class="textbox" id="cboVillage1" /></td>
							
					    </tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>村(小区)</td>					
							<td><input class="textbox" id="txtCUN1" /></td>
							<td class="report-td"><font color='red'>*</font>居住详细地址</td>				
							<td colspan="5"><input class="textbox" id="txtAdress1" style="width:820px;"></td>
						</tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>常住户口地址省</td>				
							<td><input class="textbox" id="cboProvince2"/></td>
							<td class="report-td"><font color='red'>*</font>市</td>				
							<td><input class="textbox" id="cboCity2"/></td>
							<td class="report-td"><font color='red'>*</font>县</td>					
							<td><input class="textbox" id="cboCounty2" /></td>
							<td class="report-td"><font color='red'>*</font>乡/镇</td>					
							<td><input class="textbox" id="cboVillage2" /></td>
							
					    </tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>村(小区)</td>					
							<td><input class="textbox" id="txtCUN2" /></td>
							<td class="report-td"><font color='red'>*</font>户口详细地址</td>				
							<td colspan="5"><input class="textbox" id="txtAdress2" style="width:820px;"></td>
					   </tr>
					</table>
				</div>
				
				<div class="hisui-panel" id="DetailInfo" data-options="title:'糖尿病发病明细信息',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="DetailInfoTab"  class="report-tab">
					<tr class="report-tr">
							<td class="report-td">家族史</td>
							<td class="report-td" colspan="2" style="text-align:left">父母兄弟姐妹共
							<input class="textbox" id="txtCRRS" style="width:50px"/>&nbsp;&nbsp;人，其中有糖尿病史&nbsp;&nbsp; </td>
							<td colspan="2" style="text-align:left">
								<div id="cbgCRJZS">
								</div>
							</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td" ><font color='red'>*</font>糖尿病类型</td>					
							<td><input class="textbox" id="cboCRZDLX" style="width:220px"/></td>
							<td class="report-td" ><font color='red'>*</font>诊断</td>					
							<td><input class="textbox" id="cboCRZD" style="width:220px"/></td>
							<td class="report-td">ICD编码</td>				
							<td><input class="textbox" id="txtCRZDICD" style="width:220px" disabled /></td>
						</tr>
						<tr class="report-tr">
							<td class="report-td">体重</td>				
							<td><input class="textbox" id="txtCRTZ" style="width:170px"/>&nbsp;&nbsp;公斤</td>
							<td class="report-td">身高</td>					
							<td><input class="textbox" id="txtCRSG" style="width:170px"/>&nbsp;&nbsp;厘米</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>并发症</td>					
							<td colspan="7">
								<div id="cbgCRBFZ">
								</div>
							</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td">危害因素</td>					
							<td colspan="7">
								<div id="cbgCRWHYS">
								</div>
							</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>临床表现</td>					
							<td colspan="7">
								<div id="cbgCRLCBX">
								</div>
							</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td" >其他临床表现</td>				
							<td colspan="1"><input class="textbox" id="txtCRQTLCBX" style="width:213px"></td>
						</tr>

						<tr class="report-tr">
							<td class="report-td" rowspan="3" style="text-align:center">主要检查情况</td>		
							<td class="report-td" style="text-align:right">E1 空腹血糖值&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK1" style="width:70px" /></td>				
							<td style="text-align:left">(mol/L)</td>
							<td class="report-td" style="text-align:right">E2 随机血糖值&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK2" style="width:70px"  /></td>				
							<td style="text-align:left">(mol/L)</td>
							<td class="report-td"style="text-align:right">E3 OGTT&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK3" style="width:70px"  /></td>				
							<td style="text-align:left">(mol/L)</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"style="text-align:right">E4 总胆固醇&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK4" style="width:70px" /></td>				
							<td style="text-align:left">(mg/dl)</td>
							<td class="report-td"style="text-align:right">E5 HDL-C&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK5" style="width:70px" /></td>				
							<td style="text-align:left">(mg/dl)</td>
							<td class="report-td"style="text-align:right">E6 LDL-C&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK6" style="width:70px" /></td>				
							<td style="text-align:left">(mg/dl)</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"style="text-align:right">E7 甘油三酯&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK7" style="width:70px" /></td>				
							<td style="text-align:left">(mg/dl)</td>
							<td class="report-td"style="text-align:right">E8 尿微量蛋白&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK8" style="width:70px" /></td>				
							<td style="text-align:left">(mg/24h)</td>
							<td class="report-td"style="text-align:right">E9 糖化血红蛋白&nbsp;&nbsp;<input class="textbox" id="txtCRZYJCQK9" style="width:70px" /></td>				
							<td style="text-align:left">(%)</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>诊断日期</td>				
							<td><input class="hisui-datebox textbox" id="txtCRZDDate" style="width:220px"/></td>
							<td class="report-td"><font color='red'>*</font>最高诊断单位</td>				
							<td><input class="textbox" id="cboCRZGZDDW" style="width:220px"/></td>
							<td class="report-td"><font color='red'>*</font>报卡单位</td>				
							<td><input class="textbox" id="txtCRReportOrgan" style="width:220px" disabled /></td>
							<td class="report-td">报卡科室</td>				
							<td><input class="textbox" id="cboCRReportLoc" style="width:220px" /></td>
						</tr>
						<tr class="report-tr">
							
							<td class="report-td"><font color='red'>*</font>报卡医生</td>				
							<td><input class="textbox" id="txtCRReportUser" style="width:213px" disabled /></td>
							<td class="report-td"><font color='red'>*</font>报卡日期</td>				
							<td><input class="hisui-datebox textbox" id="txtCRReportDate"style="width:220px" disabled /></td>
							<td class="report-td">死亡日期</td>				
							<td><input class="hisui-datebox textbox" id="txtCRSWRQ" style="width:227px"/></td>		
							<td class="report-td">死亡原因</td>				
							<td><input class="textbox" id="cboCRSWYY" style="width:220px"/></td>
						</tr>
						<tr class="report-tr">
							<td class="report-td">死亡诊断</td>				
							<td><input class="textbox" id="cboCRSWZD" style="width:220px"/></td>		
							<td class="report-td">死亡ICD10</td>				
							<td><input class="textbox" id="txtCRSYICD" style="width:213px" disabled /></td>
							<td class="report-td">死亡具体原因</td>				
							<td colspan="3"><input class="textbox" id="txtCRJTSWYY" style="width:540px"/></td>
						</tr>
					</table>
				</div>
			</div>
		</div>
		<div class="page-footer">	
			<a id="btnSave" class="hisui-linkbutton">保存</a>
			<a id="btnCheck" class="hisui-linkbutton">审核</a>
			<a id="btnCanCheck" class="hisui-linkbutton">取消审核</a>
			<a id="btnDelete" class="hisui-linkbutton">作废</a>
			<a id="btnExport" class="hisui-linkbutton">导出</a>
			<a id="btnPrint" class="hisui-linkbutton">打印</a>	
			<a id="btnCancle" class="hisui-linkbutton">关闭</a>		
		</div>
	</div>
</div>