	<TITLE>MessageTab</TITLE>
 		<script language="cache" runat="server">
	 	s tabId=$G(%request.Data("tabId",1))
	 	s userId=$G(%request.Data("userId",1))
	 	s groupId=$G(%request.Data("groupId",1))
