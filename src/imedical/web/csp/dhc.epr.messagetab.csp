		s:(userId = "") userId = %session.Get("LOGON.USERID")
		s:(groupId = "") groupId = %session.Get("LOGON.GROUPID")
		s:(ctlocId = "") ctlocId = %session.Get("LOGON.CTLOCID")


	</script>
	<script type="text/javascript">
		var tabId = '#(tabId)#';
		var userId = '#(userId)#';
		var groupId = '#(groupId)#';
		var ctlocId = '#(ctlocId)#';
		var InstanceId = '#(InstanceId)#';
		var emrDocId = '#(emrDocId)#';
	</script>
	
