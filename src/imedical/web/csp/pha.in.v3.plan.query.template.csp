<script id="template-plan-query" type="text/html">
    <div class="hisui-layout" fit="true" id="lyMainView-q">
        <div data-options="region:'center',border:false" class="pha-body"  id = "mainDiv-q">
            <div class="hisui-layout" fit="true" border="false" id="layout-plan-query">
                <div data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',region:'north',height:600,split:true,title:'采购计划',collapsible:true,showCollapsedTitle:true" id="layout-panel-title-q">
                    <table id="gridMain-q"></table>
                </div>
                <div data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',region:'center',title:'单据明细'">
                    <table id="gridItm-q"></table>
                </div>
            </div>
        </div>
    </div> 
    <div id="gridMain-qBar">
        <table class="pha-con-table" id="qCondition-q">
            <tr>
                <td class="r-label">
                    <label for="startDate-q">#(..Get("开始日期"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="startDate-q"  data-pha="class:'hisui-datebox',clear:true,save:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="loc-q">#(..Get("采购科室"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="loc-q" data-pha="class:'hisui-combobox',clear:true,save:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="inci-q">#(..Get("药品"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="inci-q" data-pha="class:'hisui-combogrid',clear:true,save:true,query:true">
                </td>
                <td class="r-label">
                    <label for="no-q">#(..Get("单号"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="no-q" data-pha="class:'hisui-validatebox',clear:true,save:true,query:true">
                </td>
                
                <td class="r-label">
                    <a class="hisui-linkbutton" id="btnFind-q">查询</a>
                </td>
                <td class="r-label pha-plan-query-hide">
                    <a class="hisui-linkbutton" id="btnRetOk-q">选取</a>
                </td>
                <td class="r-label pha-plan-query-hide">
                    <a class="hisui-linkbutton" id="btnRetCopy-q">复制</a>
                </td>
                <td class="r-label pha-plan-create-hide">
                    <a class="hisui-linkbutton" id="btnPrint-q">打印</a>
                </td>
            </tr>
            <tr>
                <td class="r-label">
                    <label for="endDate-q">#(..Get("结束日期"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="endDate-q" data-pha="class:'hisui-datebox',clear:true,save:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="vendor-q">#(..Get("经营企业"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="vendor-q" data-pha="class:'hisui-combobox',clear:true,save:true,query:true">
                </td>
                <td class="r-label">
                    <label for="status-q">#(..Get("状态"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="status-q" data-pha="class:'hisui-combobox',clear:true,save:true,query:true,required:false">
                </td>
                <td class="r-label">
                    <label for="poStatus-q">#(..Get("订单状态"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="poStatus-q" data-pha="class:'hisui-combobox',clear:true,save:true,query:true">
                </td>
                <!--td class="r-label">
                    <label for="filterField-q">#(..Get("其他"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="filterField-q" data-pha="class:'hisui-validatebox',clear:true,save:true,query:true">
                </td-->
                
                <td class="r-label">
                    <a class="hisui-linkbutton" id="btnClean-q">清屏</a>
                </td>
                <td class="r-label pha-plan-query-hide" style="display: none;">
                    <a class="hisui-linkbutton" id="btnRetClose-q">关闭</a>
                </td>
                <td class="r-label pha-plan-query-hide">
                    <a class="hisui-linkbutton red" id="btnDelete-q">删除</a>
                </td>
            </tr>
        </table>
    </div>   
</script>
