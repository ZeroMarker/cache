<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>
<style type="text/css">
i{display:block;width:30px;}
</style>
</HEAD>
<SCRIPT language="Cache" RUNAT="SERVER">
	s BussID=$Get(%request.Data("BussID",1))
	s BussType=$Get(%request.Data("BussType",1))
</Script>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="eqtitle:'edittitle',title:'明细单信息'" style="padding:10px;margin-bottom:10px;">
			<div class="eq-table">
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cISLEquipName" for="ISLEquipName">设备名称</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLEquipName" data-options="'itype':'0','property':''"><i class="icon-blue-edit"></i></div>
					<div class="eq-table-td eq-table-label"><label id="cISLModelDR_MDesc" for="ISLModelDR_MDesc">规格型号</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLModelDR_MDesc" data-options="'itype':'9','property':'PLAT.L.Model'"><i class="icon-blue-edit"></i></div>
					<div class="eq-table-td eq-table-label"><label id="cISLUnitDR_UOMDesc" for="ISLUnitDR_UOMDesc">单位</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLUnitDR_UOMDesc" data-options="'itype':'9','property':'PLAT.L.UOM'"><i class="icon-blue-edit"></i></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cISLQuantityNum" for="ISLQuantityNum">数量</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLQuantityNum"></div>
					<div class="eq-table-td eq-table-label"><label id="cISLOriginalFee" for="ISLOriginalFee">原值</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLOriginalFee" data-options="'itype':'3','property':''"></div>
					<div class="eq-table-td eq-table-label"><label id="cISLManuFactoryDR_MFName" for="ISLManuFactoryDR_MFName">生产厂商</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLManuFactoryDR_MFName" data-options="'itype':'9','property':'PLAT.L.ManuFacturer'"><i class="icon-blue-edit"></i></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cISLStatCatDR_SCDesc" for="ISLStatCatDR_SCDesc">设备类型</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLStatCatDR_SCDesc" data-options="'itype':'9','property':'PLAT.L.StatCat'"><i class="icon-blue-edit"></i></div>
					<div class="eq-table-td eq-table-label"><label id="cISLEquipCatDR_ECDesc" for="ISLEquipCatDR_ECDesc">设备分类</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLEquipCatDR_ECDesc" data-options="'itype':'9','property':'EM.L.EquipCat'"><i class="icon-blue-edit"></i></div>
					<div class="eq-table-td eq-table-label"><label id="cISLLimitYearsNum" for="ISLLimitYearsNum">使用年限</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLLimitYearsNum" data-options="'itype':'3','property':''"><i class="icon-blue-edit"></i></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cISLInvoiceNos" for="ISLInvoiceNos">发票号</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLInvoiceNos" data-options="'itype':'0','property':''"><i class="icon-blue-edit"></i></div>
					<div class="eq-table-td eq-table-label"><label id="cISLHold5_EDesc" for="ISLHold5_EDesc">经费来源</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLHold5_EDesc" data-options="'itype':'9','property':'PLAT.L.Expenditures'"><i class="icon-blue-edit"></i></div>
					<div class="eq-table-td eq-table-label"><label id="cISLRemark" for="ISLRemark">备注</label></div>
					<div class="eq-table-td eq-table-input" style="display:flex;"><input class="hisui-validatebox" disabled="true" id="ISLRemark" data-options="'itype':'0','property':''"><i class="icon-blue-edit"></i></div>
				</div>
			</div>
		</div>
	</div>
	<INPUT type="hidden" id="BussID" value="#(BussID)#">
	<INPUT type="hidden" id="BussType" value="#(BussType)#">
	<INPUT type="hidden" id="MainFlag" value="N">
	<INPUT type="hidden" id="ISEquipTypeDR">
	<script type='text/javascript' src='../scripts/dhceq/plat/businessmodifylist.js'></script>
	<script type='text/javascript' src='../scripts/dhceq/plat/businessmodifycommon.js'></script>
</body>
</html>
