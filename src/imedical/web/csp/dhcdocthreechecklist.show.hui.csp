<div class="hisui-layout" fit="true">
	<div data-options="region:'center',border:false,border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:10px;">
		<div class="hisui-panel" fit="true" data-options="bodyCls:'panel-body-gray'" style="height:100%;border-radius:4px;">
			<table id="tabthreechecklist"></table>
		</div>
	</div>
	<div id="add-dialog" class="hisui-dialog" title="#(..Get("新增"))#" style="padding:0 10px 0 10px;width:290px;height:200px;" data-options="iconCls:'icon-w-add',shadow:false,modal:true,closed:true">
		<div class="hisui-panel" 
		data-options="border:false,headerCls:'panel-header-card-gray',fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false">
				<table class="search-table">
				<tr>
					<td class="r-label">
						<label class="left_label" for="DocTypeCode">#(..Get("类型"))#</label>
					</td>
					<td>
						<input class="hisui-validatebox textbox" id="DocTypeCode"/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label class="left_label" for="Doclist">#(..Get("医生"))#</label>
					</td>
					<td>
						<input class="hisui-validatebox textbox" id="Doclist"/>
					</td>
				</tr>
				<tr>
					<td colspan="2" style="text-align: center;" >
						<input id="DocDefined" class='hisui-checkbox' type="checkbox" data-options="checked:false" label="#(..Get("护士分床时默认增加"))#">
					</td>
				</tr>
				<tr>
				<td colspan="2" style="text-align: center;padding:0 0 0 35px"><a href="javascript:void(0)"id="BSave" data-options="iconCls:''"  class="hisui-linkbutton">保存</a></td>
				</tr>
				</table>
		</div>
	</div>
</div>