<!--发票录入-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<SERVER>
 d ##class(web.DHCSTMHUI.Util.RQServerConfig).GetApplet()  //润乾打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="title:'发票组合单',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div class="BorderLine" data-options="region:'north',border:false">
					<table id="MainConditions" class="Condition">
						<tr>
							<td><label>起始日期</label><input id="StartDate" name="StartDate" class="hisui-datebox textbox"></td>
							<td><label>供应商</label><select id="Vendor" name="Vendor" class="hisui-combobox" style="width:180px;"></select></td>
							<td><label>组合单号</label><input id="InvAssemNo" name="InvAssemNo" class="textbox" style="width:160px;"></td>
							<td><label>发票号</label><input id="InvNo" name="InvNo" class="textbox" style="width:160px;"></td>
							<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
						</tr>
						<tr>
							<td><label>截止日期</label><input id="EndDate" name="EndDate" class="hisui-datebox textbox"></td>
							<td><label>类组</label><input id="FScg" name="FScg" class="hisui-stkscgcombotree" style="width:180px;"></td>
							<td><label>组合进价</label><input id="InvRpAmt" name="InvRpAmt" class="textbox" style="width:160px;"></td>
							<td><label>发票状态</label><select id="InputFlag" name="InputFlag" class='hisui-combobox' style="width:167px;">
								<option value="" selected>全部</option>
								<option value="Y">已录入</option>
								<option value="N">未录入</option>
							</select></td>
							<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
						</tr>
						<input id="Complate" name="Complate" value="Y" type="hidden">
					</table>
				</div>
				<div data-options="region:'center',border:false,collapsible:false">
					<table id="InvMainGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div data-options="region:'south',border:false,collapsible:false" style="padding:0 10px 10px 10px;height:240px;">
		<div class="hisui-panel" title="发票组合单明细" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
			<table id="InvDetailGrid"></table>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InvManage/InvCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InvManage/InvPrint.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InvManage/InputInvMsg.js"></script>
</body>
</html>