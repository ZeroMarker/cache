<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

	s listData=$Get(%request.Data("data",1))
	s RowId=$Get(%request.Data("rowid",1))
	s StkGrpId=$Get(%request.Data("StkCatGroupId",1))
	s Type=$Get(%request.Data("Type",1))

//查询库存组
i Action = "selectAll" d
	.d ##class(web.DHCSTM.INCSTKCAT).SelectAll(Type)
	
//保存库存组
i Action = "save" d
	.s result = ##class(web.DHCSTM.INCSTKCAT).Save(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

//删除库存组
i Action = "delete" d
	.s result = ##class(web.DHCSTM.INCSTKCAT).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

//根据库存组查询库存小类
i Action = "selectStkCatBySCG" d
	.d ##class(web.DHCSTM.INCSTKCAT).SelectRelationCat(StkGrpId)

//保存库存组与库存小类的关系记录
i Action = "addRelation" d
	.s result = ##class(web.DHCSTM.INCSTKCAT).AddRelation(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

//删除库存组与库存小类的关系记录
i Action = "deleteRelation" d
	.s result = ##class(web.DHCSTM.INCSTKCAT).DeleteRelation(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

//查询库存组小类的CommBox
i Action = "INCSCStkGrp" d
	.d ##class(web.DHCSTM.INCSTKCAT).SelectNoRelationCat(Type)
</script>