<!--检出菌设置病原体-->
<div id="BactEdit1" class="hisui-dialog" data-options="closed:true"  style="height:500px;width:550px;border:none;overflow:auto;padding:0;">
	<div  class="hisui-layout" data-options='fit:true'>
		<div id="northP" data-options="region:'north'" style="height:45px;padding-left:5px;border:0px;"> 
			<div style="float:left;width:500px;padding:6px;display:inline;" >
				<input class="hisui-combobox textbox" id="Bact1" style="width:190px" placeholder="添加病原体" />
				<input class="hisui-combobox textbox" id="BactType1" style="width:100px" placeholder="类型" />
				<a href="#" id="BactSave1" class="hisui-linkbutton hover-dark" data-options="iconCls:'icon-w-save'"style="margin-left:8px;">保存</a>
				<a href="#" id="BactDel1" class="hisui-linkbutton hover-dark" data-options="iconCls:'icon-w-cancel'"style="margin-left:8px;">删除</a>
			</div>
		</div>
		<div id="centerP" data-options="region:'center'" style="padding:2px 10px 10px 10px;border:0;fit:true;"> 
			<table id='BactList1'></table>
		</div>
	</div>
</div>
