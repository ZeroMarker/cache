<html>
<head>
<!-- Put your page Title here -->
<title>节假日</title>
<csp:content charset="UTF-8">
<HISUI/>
<dhcclinic:head></dhcclinic:head>

<link rel="stylesheet" type="text/css" href="../service/dhcanop/css/fullcalendar.min.css" />
<script type="text/javascript" src="../service/dhcanop/js/moment.min.js"></script>
<script type="text/javascript" src="../service/dhcanop/js/fullcalendar.min.js"></script>
<script type="text/javascript" src="../service/dhcanop/js/fullcalendar-zh-cn.js"></script>
</head>
<body>
	<div id="calendar" style="margin:50px;"></div>
	<div id="dataDialog" class="hisui-dialog" data-options="closed:true,width:800,height:360,modal:true,title:'节假日设置'">
		<form id="dataForm" method="post">
			<table class="form-table">
				<tr>
					<td><label>日期</label></td>
					<td><input type="text" id="DayDate" name="DayDate" class="hisui-datebox" /></td>
					<td><label>类型</label></td>
					<td>
						<select id="DayType" name="DayType" class="hisui-combobox">
                        	<option value=""></option>
                        	<option value="YD">元旦</option>
                        	<option value="CJ">春节</option>
                        	<option value="QMJ">清明节</option>
                        	<option value="LDJ">劳动节</option>
                        	<option value="DWJ">端午节</option>
                        	<option value="ZQJ">中秋节</option>
                        	<option value="GQJ">国庆节</option>
                        	<option value="GZR">工作日</option>
                    	</select>
					</td>
				</tr>
			<table>
			<input type="hidden" id="RowId" name="RowId" />
		</form>
	</div>
	<script type="text/javascript" src="../service/dhcanop/js/CIS.AN.Framework.js"></script>
	<script type="text/javascript" src="../service/dhcanop/js/CIS.AN.DataForm.js"></script>
	<script type="text/javascript" src="../service/dhcanop/js/CF.AN.Holiday.js"></script>
</body>
</html>
