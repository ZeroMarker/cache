<!-- 主页面-->
<div id="XNXGReport" class="hisui-layout" data-options="fit:true" style="width:100%;height:100%;">
	<div data-options="region:'center',border:false,collapsible:false">
		<div class="page-body" id="divbody" style="width:1285px;">
			<div class="report-header">
				<span>#(..Get("心脑血管事件报告卡"))#</span>
			</div>
			<div class="report-content">
				<div class="hisui-panel" id="BaseInfo" data-options="title:'#(..Get("基本信息"))#',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="BaseInfoTab"  class="report-tab">
						<tr class="report-tr">
							<td class="report-td">#(..Get("卡片编号"))#</td>
							<td><input class="textbox" id="txtKPBH" disabled /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("报卡单位 省"))#</td>				
							<td><input class="textbox" id="cboRepProvince"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("市"))#</td>				
							<td><input class="textbox" id="cboRepCity"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("县"))#</td>					
							<td><input class="textbox" id="cboRepCounty" /></td>
						</tr>
						<tr class="report-tr">
							<td class="report-td">#(..Get("门/急诊号"))#</td>
							<td><input class="textbox" id="txtMZH" disabled /></td>
							<td class="report-td">#(..Get("住院号"))#</td>
							<td><input class="textbox" id="txtZYH" disabled /></td>
							<td class="report-td">#(..Get("登记号"))#</td>
							<td><input class="textbox" id="txtRegNo" disabled /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("报卡类型"))#</td>
							<td><input class="textbox" id="cboBGKLX"/></td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("姓名"))#</td>				
							<td><input class="textbox" id="txtPatName" disabled /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("性别"))#</td>					
							<td><input class="textbox" id="txtSex" disabled /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("年龄"))#</td>					
							<td>
								<input class="textbox" id="txtAge" style="width:60px;" disabled />
								<input class="textbox" id="cboPatAgeDW" style="width:84px;" disabled />
							</td>
							<td class="report-td"><font color='red'>*</font>#(..Get("出生日期"))#</td>				
							<td><input class="hisui-datebox textbox" id="txtBirthDay"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("民族"))#</td>				
							<td><input class="textbox" id="cboNation" /></td>
					   </tr>
					   <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("证件类型"))#</td>					
							<td><input class="textbox" id="cboCardType"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("证件号"))#</td>				
							<td><input class="textbox" id="txtPatCardNo"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("文化"))#</td>					
							<td><input class="textbox" id="cboEducation"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("婚姻情况"))#</td>					
							<td><input class="textbox" id="cboMarital"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("联系电话"))#</td>					
							<td><input class="textbox" id="txtLXDH"/></td>
						</tr>
					   
					   <tr class="report-tr">	
							<td class="report-td"><font color='red'>*</font>#(..Get("职业"))#</td>					
							<td colspan="3"><input class="textbox" id="cboOccupation" style="width:414px;"/></td>
							<td class="report-td" ><font color='red'>*</font>#(..Get("具体工种"))#</td>					
							<td colspan="3"><input class="textbox" id="cboCRGZ" style="width:415px;"/></td>
							<td class="report-td">#(..Get("工作单位"))#</td>				
							<td><input class="textbox" id="txtCompany"/></td>
							
					   </tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("户口地址 省"))#</td>				
							<td><input class="textbox" id="cboRegProvince"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("市"))#</td>				
							<td><input class="textbox" id="cboRegCity"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("县"))#</td>					
							<td><input class="textbox" id="cboRegCounty" /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("乡/镇"))#</td>					
							<td><input class="textbox" id="cboRegVillage" /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("村(小区)"))#</td>					
							<td><input class="textbox" id="txtRegRoad" /></td>
					    </tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("详细地址"))#</td>				
							<td colspan="9"><input class="textbox" id="txtRegAddress" style="width:1179px;"></td>
					   </tr>
					   <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("居住地址 省"))#</td>				
							<td><input class="textbox" id="cboCurrProvince"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("市"))#</td>				
							<td><input class="textbox" id="cboCurrCity"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("县"))#</td>					
							<td><input class="textbox" id="cboCurrCounty" /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("乡/镇"))#</td>					
							<td><input class="textbox" id="cboCurrVillage" /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("村(小区)"))#</td>					
							<td><input class="textbox" id="txtCurrRoad" /></td>
					    </tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("详细地址"))#</td>				
							<td colspan="9"><input class="textbox" id="txtCurrAddress" style="width:1179px;"></td>
					   </tr>
					  
					    <tr class="report-tr">
							<td class="report-td" colspan="2" ><font color='red'>*</font>#(..Get("在本辖区连续居住6个月以上"))#</td>				
							<td><div id="radIsLiveSixMonth"></div></td>
					   </tr>
					</table>
				</div>
				
				<div class="hisui-panel" id="DetailInfo" data-options="title:'#(..Get("心脑血管明细信息"))#',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="DetailInfoTab"  class="report-tab">
					
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("发病日期"))#</td>				
							<td><input class="hisui-datebox textbox" id="txtFBRQ"/></td>
							<td class="report-td">#(..Get("首次发病"))#</td>					
							<td><div id="radIsFirstAttck"></div></td>
							<td class="report-td" ><font color='red'>*</font>#(..Get("诊断"))#</td>					
							<td><input class="textbox" id="cboCRZD" /></td>
							<td class="report-td">#(..Get("ICD编码"))#</td>					
							<td><input class="textbox" id="txtICD" disabled /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td" >#(..Get("心绞痛"))#<br>#(..Get("治疗措施"))#</td>					
							<td colspan="3"><div id="radCureMethod"></div></td>
							<td class="report-td" >#(..Get("脑卒中类型"))#</td>					
							<td colspan="5"><div id="radApoplexyType"></div></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td" >#(..Get("心源性猝死"))#</td>					
							<td colspan="3"><div id="radSCD"></div></td>
							<td class="report-td" >#(..Get("推断"))#</td>					
							<td colspan="5"><div id="radInfer"></div></td>
					    </tr>
					    <tr class="report-tr">
							<td class="report-td" style="padding-bottom:20px;"><font color='red'>*</font>#(..Get("诊断依据"))#</td>
							<td colspan="7"><div id="chkDiagList"></div></td>						
					    </tr>
					    
					    <tr class="report-tr">
							<td class="report-td">#(..Get("生化标志物"))#</td>
							<td colspan="3"><div id="radBiochemicalMark"></div></td>
							<td class="report-td">#(..Get("补发"))#</td>
							<td colspan="3"><div id="radReissue"></div></td>						
					    </tr>
					    
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("确诊日期"))#</td>					
							<td><input class="hisui-datebox textbox" id="txtQZRQ" /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("确诊单位"))#</td>					
							<td><input class="textbox" id="cboQZDW"/></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("单位级别"))#</td>					
							<td><input class="textbox" id="cboLevel"/></td>
					    </tr>
					
						<tr class="report-tr">
							<!--<td class="report-td">#(..Get("死亡原因"))#</td>					
							<td><input class="textbox" id="cboDeathReason" /></td>-->
							<td class="report-td">#(..Get("转归"))#</td>					
							<td><input class="textbox" id="cboOutCome" /></td>
							<td class="report-td">#(..Get("死亡日期"))#</td>				
							<td><input class="hisui-datebox textbox" id="txtSWRQ"/></td>
							<td class="report-td">#(..Get("死亡诊断"))#</td>				
							<td><input class="textbox" id="cboCRSWZD" /></td>
							<td class="report-td">#(..Get("死亡ICD"))#</td>					
							<td><input class="textbox" id="txtSWICD" disabled /></td>
							<td class="report-td">#(..Get("死亡具体原因"))#</td>					
							<td><input class="textbox" id="txtSWJTYY" /></td>
					    </tr>
						
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>#(..Get("报告单位"))#</td>					
							<td><input class="textbox" id="txtRepDW"/></td>
							<td class="report-td">#(..Get("报卡科室"))#</td>				
							<td><input class="textbox" id="cboCRReportLoc" disabled /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("报卡医生"))#</td>					
							<td><input class="textbox" id="txtDoctor" disabled /></td>
							<td class="report-td"><font color='red'>*</font>#(..Get("报卡日期"))#</td>				
							<td><input class="hisui-datebox textbox" id="dtRepDate" disabled /></td>
							
					    </tr>
					</table>
				</div>
			</div>
		</div>
		<div class="page-footer">	
			<a id="btnSaveTemp" class="hisui-linkbutton">#(..Get("草稿"))#</a>
			<a id="btnSave" class="hisui-linkbutton">#(..Get("保存"))#</a>
			<a id="btnCheck" class="hisui-linkbutton">#(..Get("审核"))#</a>
			<a id="btnCanCheck" class="hisui-linkbutton">#(..Get("取消审核"))#</a>
			<a id="btnDelete" class="hisui-linkbutton">#(..Get("作废"))#</a>
			<a id="btnReturn" class="hisui-linkbutton">#(..Get("退回"))#</a>	
			<a id="btnExport" class="hisui-linkbutton">#(..Get("导出"))#</a>
			<a id="btnPrint" class="hisui-linkbutton">#(..Get("打印"))#</a>	
			<a id="btnCancle" class="hisui-linkbutton">#(..Get("关闭"))#</a>		
		</div>
	</div>
</div>