<!DOCTYPE Html>
<html>
<head>
<!-- Put your page Title here -->
<title>设备登记</title>
<csp:content charset="UTF-8">
<dhcclinic:easyui></dhcclinic:easyui>
<dhcclinic:head></dhcclinic:head>
</head>
<body>
	<div id="dataTools" style="padding-top:10px">
		<form id="queryForm" method="post">
			<label for="filterEquipType">设备类型</label>
			<select id="filterEquipType" name="filterEquipType" class="hisui-combobox"></select>
			<label for="filterRoom">手术间</label>
			<select id="filterRoom" name="filterRoom" class="hisui-combobox"></select>
			<a href="#" id="btnQuery" class="hisui-linkbutton" data-options="iconCls:'icon-search',plain:true" onclick="javascript:queryData();">查询</a>
			<a href="#" id="btnAdd" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true" onclick="javascript:addData();">新增</a>
			<a href="#" id="btnEdit" class="hisui-linkbutton" data-options="iconCls:'icon-edit',plain:true" onclick="javascript:editData();">修改</a>
			<a href="#" id="btnDel" class="hisui-linkbutton" data-options="iconCls:'icon-remove',plain:true" onclick="javascript:delData();">删除</a>
			<a href="#" id="btnPrintBarCode" class="hisui-linkbutton" data-options="iconCls:'icon-print',plain:true" onclick="javascript:printBarCode();">打印条码</a>
		</form>
	</div>
	<table id="dataBox">
	</table>
	<div id="dataDialog" class="hisui-dialog" data-options="closed:true,width:600,height:300">
		<form id="dataForm" method="post">
			<table class="form-table">
				<tr>
					<td><label>代码</label></td>
					<td><input type="text" id="EquipCode" name="EquipCode" class="hisui-validatebox textbox" data-options="required:true" /></td>
					<td><label>名称</label></td>
					<td><input type="text" id="EquipDesc" name="EquipDesc" class="hisui-validatebox textbox" data-options="required:true"/></td>
				</tr>
				<tr>
					<td><label>类型</label></td>
					<td><select id="EquipType" name="EquipType" class="hisui-combobox" data-options="required:true"></select></td>
					<td><label>型号</label></td>
					<td><select id="EquipModel" name="EquipModel" class="hisui-combobox" data-options="required:true"></select></td>
				</tr>
				<tr>
					<td><label>序列号</label></td>
					<td><input type="text" id="SerialNumber" name="SerialNumber" class="hisui-validatebox textbox"/></td>
					<td><label>位置</label></td>
					<td><select id="Location" name="Location" class="hisui-combobox"></select></td>
				</tr>
				<tr>
					<td><label>物理端口</label></td>
					<td><input type="text" id="PhysicalPort" name="PhysicalPort" class="hisui-validatebox textbox"/></td>
					<td><label>通信端口</label></td>
					<td><input type="text" id="ComPort" name="ComPort" class="hisui-validatebox textbox"></td>
				</tr>
				<tr>
					<td><label>采集代码</label></td>
					<td><input type="text" id="Program" name="Program" class="hisui-validatebox textbox"/></td>
					<td><label>IP地址</label></td>
					<td><input type="text" id="TcpipAddress" name="TcpipAddress" class="hisui-validatebox textbox"/></td>
				</tr>
			</table>
			<input type="hidden" id="RowId" name="RowId" />
			<input type="hidden" id="CollectionRowId" name="CollectionRowId" />
			<input type="hidden" id="Dept" name="Dept" />
		</form>
	</div>
	<script type="text/javascript" src="../service/dhcanop/js/datagrid-groupview.js"></script>
	<script type="text/javascript" src="../service/dhcanop/js/JsBarcode.all.js"></script>
	<script type="text/javascript" src="../service/dhcanop/js/CIS.AN.DataForm.js"></script>
	<script type="text/javascript" src="../service/dhcanop/js/LodopFuncs.js"></script>
	<script type="text/javascript" src="../service/dhcanop/js/CF.AN.DeptEquip.Collect.js"></script>
</body>
</html>