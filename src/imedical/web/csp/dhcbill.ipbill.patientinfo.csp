<!-- 
 * FileName: dhcbill.ipbill.patientinfo.csp
 * Anchor: ZhYW
 * Date: 2019-04-14
 * Description: 患者信息查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.patientinfo.css'/>
	<server>
		w "<script type=""text/javascript"">",!
		w "	var GV = {",!
		w "		Name: """_%request.Get("Name")_""",",!
		w "		PatientNo: """_%request.Get("PatientNo")_""",",!
		w "		IDNo: """_%request.Get("IDNo")_""",",!
		w "		BirthDate: """_%request.Get("BirthDate")_""",",!
		w "		HealthFundNo: """_%request.Get("HealthFundNo")_"""",!
		w " };",!
		w "</script>"
	</server>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true,border:false">
		<div data-options="region:'north',border:false,collapsible:false" style="height:91px;">
			<div class="hisui-panel" data-options="fit:true,bodyCls:'panel-header-gray'" style="border-bottom-style:none;border-radius:4px 4px 0 0;">
				<table class="search-table" style="width:90%">
					<tr>
						<td class="r-label td-first-col-58"><label>患者姓名</label></td>
						<td><input id="name" class="textbox"/></td>
						<td class="r-label"><label>性别</label></td>
						<td><input id="sex" class="hisui-combobox textbox"/></td>
						<td class="r-label"><label>出生日期</label></td>
						<td><input id="birthDate" class="hisui-datebox textbox"/></td>
						<td class="r-label"><a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a></td>
					</tr>
					<tr>
						<td class="r-label"><label>身份证号</label></td>
						<td><input id="IDNo" class="textbox"/></td>
						<td class="r-label"><label>医保手册号</label></td>
						<td><input id="healthFundNo" class="textbox"/></td>
					</tr>
				</table>
			</div>
		</div>
		<div data-options="region:'center',border:false">
			<div class="hisui-panel panel-header-gray" data-options="fit:true,headerCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
				<table class="hisui-datagrid" id="patInfoList"></table>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.patientinfo.js"></script>
</body>
</html>
