<div class="hisui-layout" fit="true">
	<div data-options="region:'center',border:false,collapsible:false" style="padding:10px;">
		<div class="hisui-panel" data-options="title:'生成长期领药医嘱',headerCls:'panel-header-gray',iconCls:'icon-edit'" style="border:1px solid #ccc;border-radius:0 0 4px 4px;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="PatientNo">登记号</label>
					</td>
					<td>
						<input class="textbox" id="PatientNo" disabled/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="wardDesc">病区</label>
					</td>
					<td>
						<input class="textbox" id="wardDesc" disabled/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="StartDate">开始日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="StartDate" data-options="disabled:true,formatter:myformatter,parser:myparser"/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="EndDate">结束日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="EndDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
				</tr>
				<tr class="CreateByWard-tr">
					<td class="r-label">
						<label for="CreateByWard">生成本病区所有记录</label>
					</td>
					<td>
						<input id="CreateByWard" class='hisui-checkbox' type="checkbox">
					</td>
				</tr>
				<tr>
					<td colspan="2" style="text-align:center;">
						<a id="Create" href="#" class="hisui-linkbutton" data-options="iconCls:''">医嘱生成</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
</div>
