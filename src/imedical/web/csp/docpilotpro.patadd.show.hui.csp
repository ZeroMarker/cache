<div class="hisui-layout" fit="true">
	<div data-options="region:'west',border:false,split:true,collapsible:false" style="width:450px;border:false;">
		<div class="hisui-layout" data-options="fit:true">   
	        <div data-options="region:'north',border:false,headerCls:'panel-header-gray'" style="height:95px;padding:10px 0 0 10px;">
	        	<div class="hisui-panel" data-options="title:'科研项目列表',headerCls:'panel-header-gray',iconCls:'icon-add-note'" style="border:1px solid #ccc;border-radius:0;">
		        	<table class="search-table">
		        		<tr>
		        			<td class="r-label"><label for="WarnSum">快速查找</label></td>
		        			<td>
		        				<input class="textbox" id="FindPilotPro" style=""/>
		        			</td>
		        			<td class="r-label">
		        				<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询科研项目</a>
		        			</td>
		        		</tr>
		        	</table>
	        	</div>
	        </div>   
	        <div data-options="region:'center',border:false,headerCls:'panel-header-gray'" style="padding:0 0 10px 10px;">
	        	<div class="hisui-panel no-rightline" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
	        		<table id="PilotProListTab"></table>
	        	</div>
	        </div>   
	    </div>
	</div>
	<div data-options="region:'center',border:false,title:'',collapsible:false">
		<div class="hisui-layout" data-options="fit:true">  
			<div data-options="region:'north',border:false,headerCls:'panel-header-gray'" style="height:135px;padding:10px 10px 0 5px;">
		    	<div class="hisui-panel" data-options="title:'受试者列表',headerCls:'panel-header-gray',iconCls:'icon-add-note'" style="border:1px solid #ccc;border-radius:0;">
			    	<table class="search-table">
			    		<tr>
		        			<td class="r-label"><label for="PatInfo">病人信息</label></td>
		        			<td colSpan=5>
		        				<input class="textbox" id="PatInfo" disabled style="width:98.7%;"/>
		        			</td>
		        		</tr>
			    		<tr>
			    			<td class="r-label"><label for="AgreementDate">签署知情同意书日期</label></td>
			    			<td>
			    				<input class="hisui-datebox textbox" id="AgreementDate" data-options="formatter:myformatter,parser:myparser"/>
			    			</td>
			    			<td class="r-label"><label for="PPPReMark">备注</label></td>
			    			<td >
			    				<input class="textbox" id="PPPReMark"/>
			    			</td>
			    			<td class="r-label">
			    				<label for="ScreenNo">筛选号</label>
			    			</td>
			    			<td>
			    				<input id="ScreenNo" class='textbox' type="ScreenNo">
			    			</td>
			    			<td class="r-label">
			    				<a id="BSave" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
			    			</td>
			    		</td>
			    	</table>
		    	</div>
		    </div>   
		    <div data-options="region:'center',border:false,headerCls:'panel-header-gray'" style="padding:0 10px 10px 5px;">
		    	<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
	        		<table id="PilotProPatListTab"></table>
	        	</div>
	        </div>   
		</div>
	</div>
</div>