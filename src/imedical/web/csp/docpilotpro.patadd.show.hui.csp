<div class="hisui-layout" fit="true">
	<div data-options="region:'west',border:false,split:false,collapsible:false" style="width:450px;border:false;padding-right:10px">
		<div class="hisui-panel" data-options="fit:true,title:'#(..Get("科研项目列表"))#',headerCls:'panel-header-gray',iconCls:'icon-apply-check'">
			<div class="hisui-layout" data-options="fit:true">   
				<div data-options="region:'north',iconCls:'icon-add-note',headerCls:'panel-header-gray',collapsible:false,border:false" style="height:auto;padding:0 10px;border-bottom:1px solid #e2e2e2;border-radius:0;">
					<table class="search-table">
						<tr>
							<td class="r-label"><label for="WarnSum">#(..Get("快速查找"))#</label></td>
							<td>
								<input class="textbox" id="FindPilotPro" style=""/>
							</td>
							<td class="r-label">
								<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询科研项目</a>
							</td>
						</tr>
					</table>
				</div>   
				<div data-options="region:'center',border:false,headerCls:'panel-header-gray'">
						<table id="PilotProListTab"></table>
				</div>   
			</div>
		</div>
	</div>
	<div data-options="region:'center',border:false,title:'',collapsible:false">
		<div class="hisui-panel" data-options="fit:true,title:'#(..Get("受试者列表"))#',headerCls:'panel-header-gray',iconCls:'icon-apply-check'">
			<div class="hisui-layout" data-options="fit:true">  
				<div data-options="region:'north',border:false,iconCls:'icon-add-note',headerCls:'panel-header-gray',collapsible:false" style="height:auto;padding:0 10px;border-bottom:1px solid #e2e2e2;border-radius:0;">
					<table class="search-table">
						<tr>
							<td class="r-label"><label for="PatInfo">#(..Get("病人信息"))#</label></td>
							<td colSpan=5>
								<input class="textbox" id="PatInfo" disabled style="width:98.7%;"/>
							</td>
							<td class="r-label">
								<a id="BSave" href="#" class="hisui-linkbutton">新增</a>
							</td>
						</tr>
						<tr>
							<td class="r-label"><label for="AgreementDate">#(..Get("签署知情同意书日期"))#</label></td>
							<td>
								<input class="hisui-datebox textbox" id="AgreementDate" data-options="formatter:myformatter,parser:myparser"/>
							</td>
							<td class="r-label"><label for="PPPReMark">#(..Get("备注"))#</label></td>
							<td >
								<input class="textbox" id="PPPReMark"/>
							</td>
							<td class="r-label">
								<label for="ScreenNo">#(..Get("筛选号"))#</label>
							</td>
							<td>
								<input id="ScreenNo" class='textbox' type="ScreenNo">
							</td>
							<td class="r-label">
								<a id="BEdit" href="#" class="hisui-linkbutton">修改</a>
							</td>
						</tr>
					</table>
				
				</div>   
				<div data-options="region:'center',border:false,headerCls:'panel-header-gray'">
					<table id="PilotProPatListTab"></table>
				</div>   
			</div>
		</div>
	</div>
</div>