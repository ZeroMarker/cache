<!--盘点单汇总-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" style="width:360px;" data-options="region:'west',border:false">
		<div class="hisui-panel" data-options="title:'',bodyCls:'panel-header-gray',headerCls:'panel-header-gray',fit:true,iconCls:'icon-paper'">
			<div class="hisui-layout" data-options="fit:true">
				<div class="GridPadding" data-options="region:'north',border:false" style="height:225px;">
					<table class="Condition" id="Conditions">
						<tr>
							<td>科室</td>
							<td><input id="FLocId" name="FLocId" class="hisui-combobox" style="width:180px;"></td>
						</tr>
						<tr>
							<td>起始日期</td>
							<td><input id="FStartDate" name="FStartDate" class="hisui-datebox textbox" style="width:180px;"></td>
						</tr>
						<tr>
							<td>截止日期</td>
							<td><input id="FEndDate" name="FEndDate" class="hisui-datebox textbox" style="width:180px;"></td>
						</tr>
						<tr>
							<td>
							实盘完成
							</td>
							<td style="float:left">
								<input id="FStkTkComp" name="FStkTkComp" class='hisui-checkbox' type="checkbox" label="" value="Y">
							</td>
						</tr>
						<tr>
							<td colspan="2">
								<a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
								<a href="#" id="CompleteBT" class="hisui-linkbutton">确认完成</a>
								<a href="#" id="CancelCompleteBT" class="hisui-linkbutton">取消完成</a>
							</td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false">
					<table id="MasterGrid" data-options="displayMsg:''"></table>
				</div>
			</div>
		</div>
	</div>
	<div class="hisui-layout" data-options="region:'center'" id="tabs" style="border:0px;padding-left:0px">
		<div data-options="region:'north',border:false"  class="GridPadding" style="height:220px;padding-left: 0px">
			<div title="物资汇总"  class="hisui-panel" data-options="iconCls:'icon-paper',bodyCls:'panel-header-gray',fit:true,headerCls:'panel-header-gray'">
				<table id="DetailGrid"></table>
			</div>
		</div>
		<div data-options="region:'west',border:false" class="GridPadding" style="width:350px;padding-top:0; padding-left:0;">
			<div class="hisui-panel" data-options="title:'批次汇总',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">
				<table id="InstItmGrid" data-options="displayMsg:''"></table>
			</div>
		</div>
		<div class="hisui-panel" data-options="region:'center',border:false" class="GridPadding" style="padding-left:0;padding-top:0;">
			<div class="hisui-panel" data-options="title:'批次明细',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">
				<table id="InstDetailGrid" data-options="displayMsg:''"></table>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InStkTk/InStkTkCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InStkTk/INStkTkItmWdComp.js"></script>
</body>
</html>