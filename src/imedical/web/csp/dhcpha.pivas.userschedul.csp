<!DOCTYPE html>
<!--人员排班日历
	csp:dhcpha.pivas.userschedul.csp
	js: scripts/pharmacy/pivas/userschedul.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>
    </TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>
    <style>
        .fc-other-month {
            background-image: linear-gradient(135deg,rgba(0,0,0,.03) 25%,transparent 25%,transparent 50%,rgba(0,0,0,.03) 50%,rgba(0,0,0,.03) 75%,transparent 75%,transparent);
            background-color: #FAFCFD;
            background-size: 16px 16px;
        }


        .fc-sun, .fc-sat {
            background-color: #f4f4f4;
            background-size: 16px 16px;
        }
		.fc-event-inner{
			padding-left:10px
		}
		.batch-title{
			color:#007ACF;
			font-size:15px;
			font-weight:bold;
			padding-left:10px
		}
		.batch-title-sign{
			height:14px;
			margin-top: 3px;
			border-left:5px solid #007ACF;
			float:left;
			margin-right: 10px;
			}

    </style>
</HEAD>

<body>
    <div class="hisui-layout"  fit="true" border="false">
        <div data-options="region:'center'" class="dhcpha-hisui-container">
		    <div class="hisui-layout" fit="true">
		        <div data-options="region:'west',width:200,split:true,title:'',headerCls:'panel-header-gray',iconCls:'icon-molecule'" class="dhcpha-hisui-border" style="border-radius: 4px;">
		            <div id="gridUser">
		            </div>
		        </div>
		     	<div data-options="region:'center',title:'',split:true,headerCls:'panel-header-gray',iconCls:'icon-molecule',border:false" >
		            <div class="hisui-layout" fit="true">
				        <div data-options="region:'center',split:true,width:800,title:'',headerCls:'panel-header-gray',iconCls:'icon-molecule'" class="dhcpha-hisui-border" style="border-radius: 4px;padding:10px">
				            <div id="calUserSche">
				            </div>
				        </div>	
				        <!--div  data-options="region:'center',title:'存假信息',headerCls:'panel-header-gray',iconCls:'icon-molecule'"-->
				        <div style="display:none">
				            <div>
								<div class="batch-title"> 
									<div class="batch-title-sign"></div>
										年假
								</div>
								<div style="padding-left:25px">
									10天
								</div>
				            </div>
				            <div>
								<div class="batch-title"> 
									<div class="batch-title-sign"></div>
										存假
								</div>
								<div style="padding-left:25px">
									15天
								</div>
				            </div>
				        </div>
		            </div>	

		        </div>   
		    </div>
	    </div>
    </div>
    <div id="gridUserBar">
    	<table style="width:100%">
    		<tr>
                <td>
                    <input id="txtAlias" class="hisui-validatebox" placeholder="检索条件..."/>
                </td>
		    </tr>
		</table>	   
    </div>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/plugins/fullcalendar-2.0.2/dist/fullcalendar.css"></link>
	<script type="text/javascript" src="../scripts/pharmacy/plugins/fullcalendar-2.0.2/dist/moment.min.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/plugins/fullcalendar-2.0.2/dist/fullcalendar.min.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/userschedul.js"></script>
</body>

</HTML>