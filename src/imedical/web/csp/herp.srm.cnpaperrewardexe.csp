<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	 i ##Class(websys.SessionEvents).SessionExpired() q 1
	 q 1
</csp:method>
<csp:content charset="utf-8">
<script language="cache" runat="server">

	s action = $Get(%request.Data("action",1))
	s sortField = $Get(%request.Data("sort",1))
	s sortDir = $Get(%request.Data("dir",1))
	s start = $Get(%request.Data("start",1))
	s limit = $Get(%request.Data("limit",1))
	s searchField = $Get(%request.Data("searchField",1))
	s searchValue = $Get(%request.Data("searchValue",1))
	s parent=$Get(%request.Data("parent",1))
	s str=$Get(%request.Data("query",1))
	
	
	s rowid  =  $Get(%request.Data("rowid",1))
	s userdr =  $Get(%request.Data("userdr",1)) // µÇÂ½ÓÃ»§ID
	s data = $Get(%request.Data("data",1))
	s rewardamount = $Get(%request.Data("rewardamount",1))
	
	i action = "list" d
	.w ##class(herp.srm.udata.uPaperRewardApply).List(data, sortField, sortDir, start, limit)	
	i action = "caltypename" d
	.w ##class(herp.srm.udata.uPaperRewardApply).Calusertype(start, limit, str)
	
	i action = "deptList" d
	.w ##class(herp.srm.udata.uPaperRewardApply).GetDept(start, limit, str)
				
	i action = "userList" d
	.w ##class(herp.srm.udata.uPaperRewardApply).GetAuthor(start, limit, str)
	
	i action = "submit" d
	.s result = ##class(herp.srm.udata.uPaperRewardApply).Submit(rewardamount,userdr, rowid)
	.i result = 0 d
	..w "{success:'true',refresh:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

</script>