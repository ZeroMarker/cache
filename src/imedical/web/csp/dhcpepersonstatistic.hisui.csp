
<!-- 
 * FileName: dhcpepersonstatistic.hisui.csp
 * Anchor: xy
 * Date: 2019-06-26
 * Description: HISUI 体检综合查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<ADDINS require="CmdShell"/>
	<link rel="stylesheet" type="text/css" href="../scripts/DHCPECommon.hisui.css"/>
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 20px;
}


</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:175px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'体检综合查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border-radius:0;border-bottom:1px solid #C0C0C0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="BeginDate">开始日期</label></td>
					<td>
						<input id="BeginDate" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1"><label for="Name">姓名</label></td>
					<td>
						<input class="textbox" id="Name"/>
					</td>
					
					
					<td class="r-label r-label-1"><label for="GADMDesc">团体名称</label></td>
					<td>
						<input class="hisui-combogrid"  id="GADMDesc"/>
					</td>
					
					<td class="r-label r-label-1"><label for="ChargeStatus">收费状态</label></td>
					<td>
						<input class="hisui-combobox"  id="ChargeStatus"/>
					</td>
					
				
					
					<td class="r-label r-label-1"></td>
					<td>
						<input class='hisui-checkbox' type="checkbox"  id="REGISTERED"/><label for="REGISTERED">登记</label>
					</td>
								
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BExport" href="#" class="hisui-linkbutton" style="width:130px;" data-options="iconCls:'icon-w-export'">导出数据</a>
					</td>		
				</tr>
				<tr>
					<td class="r-label"><label for="EndDate">结束日期</label></td>
					<td>
						<input id="EndDate" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1"><label for="Sex">性别</label></td>
					<td>
						<input class="hisui-combobox"  id="Sex" style="width:155px;"/>
					</td>
					
					<td class="r-label r-label-1"><label for="VIPLevel">VIP等级</label></td>
					<td>
						<input class="hisui-combobox"  id="VIPLevel"/>
					</td>
					
					<td class="r-label r-label-1"><label for="ReCheck">复查</label></td>
					<td>
						<input class="hisui-combobox"  id="ReCheck"/>
					</td>
					
					<td class="r-label r-label-1"></td>
					<td>
						<input class='hisui-checkbox' type="checkbox"  id="ARRIVED"/><label for="ARRIVED">到达</label>
					</td>
					
		
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>	
					
					
				</tr>
				<tr>
					<td class="r-label"><label for="RegNo">登记号</label></td>
					<td>
						<input class="textbox" id="RegNo">
					</td>
					
					<td class="r-label r-label-1"><label for="Depart">部门</label></td>
					<td>
						<input class="textbox" id="Depart"/>
					</td>
					
					<td class="r-label r-label-1"><label for="IFSTSF">视同收费</label></td>
					<td>
						<input class="hisui-combobox"  id="IFSTSF"/>
					</td>
				</tr>
				
				
				
					
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="PersonStatisticGrid"></table>
			</div>
        </div>
	      
</div>


    
<script type="text/javascript" src="../scripts/DHCPEPersonStatistic.hisui.js"></script>
<script type="text/javascript" src="../scripts/DHCPEIAdmItemStatusAdms.PatItemPrint.js"></script>
<script type="text/javascript" src="../scripts/DHCPEPrintDJDCommon.js"></script>
<script type="text/javascript" src="../scripts/DHCPEPrintReportCommon.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
</body>		
</html>
			
		
 




