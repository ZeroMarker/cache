<!-- 
 * FileName: dhcbill.ipbill.strikedeplist.csp
 * Anchor: ZhYW
 * Date: 2019-12-05
 * Description: 已冲退预交金查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.strikedeplist.css'/>
	<script type="text/javascript" src="../scripts/DHCCPMRQCommon.js"></script>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',collapsible:false,title:'已冲退预交金查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:87px;">
			<table class="search-table" style="width:90%">
				<tr>
					<td class="r-label td-first-col-58"><label>开始日期</label></td>
					<td><input id="stDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>结束日期</label></td>
					<td><input id="endDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>收费员</label></td>
					<td><input id="guser" class="textbox"/></td>
					<td><input id="isHandin" class='hisui-checkbox' type="checkbox" label="按结算日期"/></td>
					<td class="r-label">
						<a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a>
						<a href="javascript:;" class="hisui-linkbutton" id="btn-export" data-options="iconCls:'icon-w-export'" style="margin-left:20px;">导出</a>
					</td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-top-style:dashed;border-radius:0 0 4px 4px;">
			<table class="hisui-datagrid" id="strikeDepList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.strikedeplist.js"></script>
</body>
</html>
