<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false" style="height:95px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'卡管理查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="StDate">开始日期</label></td>
					<td>
						<input class="hisui-datebox textbox" id="StDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label">
						<label for="EndDate">结束日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="EndDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label">
						<label for="CardNo">卡号</label>
					</td>
					<td>
						<input class="textbox" id="CardNo"/>
					</td>
					<td class="r-label">
						<label for="CardStatus">卡状态</label>
					</td>
					<td>
						<input class="textbox" id="CardStatus"/>
					</td>
					<td class="r-label">
						<label for="UserCodeA">办卡人</label>
					</td>
					<td>
						<input class="textbox" id="UserCodeA"/>
					</td>
					<td class="r-label">
						<a id="Bfind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					<td class="r-label">
						<a id="BExport" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">导出</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="CardQryTab"></table>
		</div>
	</div>
</div>
