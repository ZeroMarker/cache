<!--批次调价-全部批次 弹窗-->
<div id="IncItmBatWindowAll" class="hisui-dialog" title="科室库存项批次信息" data-options="minimizable:false,closed:true">
	<div class="hisui-layout" data-options="fit:true">
		<div class="GridPadding" data-options="region:'north',border:false" style="height:300px;">
			<div class="hisui-panel" title="物资信息" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
				<table id="IncItmBatMasterGrid"></table>
			</div>
		</div>
		<div data-options="region:'center',border:false" style="padding:0 10px;">
			<div class="hisui-panel" title="批次信息" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
				<table id="IncItmBatDetailGrid"></table>
			</div>
		</div>
	</div>
</div>

<!--批量填写进价售价弹窗-->
<div id="SaveRpSp" title="填充价格" class="hisui-dialog" data-options="closed:'true',modal:true" style="width:280px">
	<table class="Condition" align="center" style="padding-right:10px">
		<tr>
			<td>进价</td>
			<td><input id="Rp" name="Rp" class="hisui-numberbox textbox"></td>
		</tr>
		<tr>
			<td>售价</td>
			<td><input id="Sp" name="Sp" class="hisui-numberbox textbox"></td>
		</tr>
		<tr>
			<td>调价原因</td>
			<td><input id="AdjReason" name="AdjReason" class="hisui-combobox"></td>
		</tr>
		<tr>
			<td>跳过已填</td>
			<td align="left">
				<input class="hisui-radio" type="radio" name="Fill" value="1" data-options="checked:true">
			</td>
		</tr>
		<tr>
			<td>覆盖已填</td>
			<td align="left">
				<input class="hisui-radio" type="radio" name="Fill" value="2">
			</td>
		</tr>
		<tr>
			<td colspan="2" style="text-align:center;">
				<a href="#" id="RpSpSureBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
			</td>
		</tr>
	</table>
</div>

<script type="text/javascript" src="../scripts/dhcstmhisui/Common/IncItmBatWindowAll.js"></script>