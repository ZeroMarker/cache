  <!-- 品种通用名-->
  <script id="scrDHCPHChemical" type="text/html">
    <div id="lyDHCPHChemical" class="hisui-layout" fit="true">
        <div data-options="region:'north',border:false" style="overflow: hidden;">
            <div class="datagrid-toolbar" style="border-bottom:0">
                <a class="hisui-linkbutton" plain='true' iconCls="icon-clear-screen" id="btnClearDHCPHChemical">#(..Get("清屏"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-update" id="btnSaveDHCPHChemical">#(..Get("更新"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-add" id="btnAddDHCPHChemical">#(..Get("新增"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-max-no" id="btnMaxDHCPHChemical">#(..Get("最大码"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-cancel" id="btnDelDHCPHChemical">#(..Get("删除"))#</a>
            </div>

            <div class="pha-line"></div>
        </div>
        <div data-options="region:'center',border:false" id="dataDHCPHChemical">
            <div class="pha-row" style="display:none;">
                <div class="pha-col">
                    <div class="pha-drug-chem-label">
                        <label for="chemCatGrp">#(..Get("类组"))#</label>
                    </div>
                </div>
                <div class="pha-col">
                    <div class="pha-drug-chem-text">
                        <input id="chemCatGrp" class="hisui-combobox" data-pha='class:"hisui-combobox",requied:false,clear:true,save:2' style="width:536px">
                    </div>
                </div>
            </div>
            <div class="pha-row">
                <div class="pha-col">
                    <div class="pha-drug-chem-label">
                        <label for="chemCode"><span style="color:red">*</span>#(..Get("代码"))#</label>
                    </div>
                </div>
                <div class="pha-col">
                    <div class="pha-drug-chem-text">
                        <input id="chemCode" class="hisui-validatebox"
                            data-pha='class:"hisui-validatebox",requied:true,clear:true,save:0'>
                    </div>
                </div>
            </div>
            <div class="pha-row">
                <div class="pha-col">
                    <div class="pha-drug-chem-label">
                        <label for="chemDesc"><span style="color:red">*</span>#(..Get("名称"))#</label>
                    </div>
                </div>
                <div class="pha-col">
                    <div class="pha-drug-chem-text">
                        <input id="chemDesc" class="hisui-validatebox"
                            data-pha='class:"hisui-validatebox",requied:true,clear:true,save:1'>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>