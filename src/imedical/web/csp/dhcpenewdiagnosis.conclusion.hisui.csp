
<!-- 
 * FileName: dhcpenewdiagnosis.conclusion.hisui.csp
 * Anchor: jinlei
 * Date: 2020-02-19
 * Description: HISUI 总检初审/总检信息/检查结论
-->

<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
<SCRIPT language="Cache" RUNAT="SERVER"> 
 s GSID=$g(%request.Data("GSID",1))
 s UserID=$g(%request.Data("USERID",1))
 s IADM=$P(^DHCPEGS(GSID,1),"^",1)
 i IADM'="" d
 .s VIPLevel=+##class(web.DHCPE.PreCommon).GetVIPLevel("IADM",IADM)
 e  d
 .s VIPLevel=1
 s GSEXID=$O(^User.DHCPEGeneralSummarizeExI("GeneralSummarizeID",GSID,0))
</SCRIPT> 
<script type="text/javascript">
var GSID="#(GSID)#"
var UserID="#(UserID)#"
var VIPLevel="#(VIPLevel)#"
</script>
 <style type="text/css">
 .search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	/*padding: 0 10px;*/
}
.r-label{
	padding-left: 10px;
	white-space: nowrap;
}
.r-label-1{
	padding-left: 30px;
	/*width:500px;*/
	text-align:center;
}
.newclsInvalid{
	border-color: red;
	color: red;
}
.panel-header{
	border-bottom: 0;
}
.excombobox{
	width:460px;
}
textarea.textbox{
	width:460px;
	height:50px;
	
}

</style>
</head>
  

<body id="ConclusionPanel" class="hisui-layout">

<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
				  	<td class="r-label"><label for="Conclusion">结论分类</label></td>
					<td>
						<input class="hisui-combobox excombobox" type="text" Id="Conclusion">
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="DiagnosticCriteria">诊断标准</label></td>
					<td>
						<input class="hisui-combobox excombobox" type="text" Id="DiagnosticCriteria">
					</td>					
				</tr>
				<tr>
					<td class="r-label"><label for="Suggestions">处理意见</label></td>
					<td>
						<textarea id="Suggestions" class="textbox"></textarea>
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="TestResult">检查结果</label></td>
					<td>
						<textarea id="TestResult" class="textbox" ></textarea>
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="Clinical">临床表现</label></td>
					<td>
						<textarea id="Clinical" class="textbox"></textarea>
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="OccupationalHistory">职业接触史</label></td>
					<td>
						<textarea id="OccupationalHistory" class="textbox" ></textarea>
					</td>
				</tr>
			
				<tr>
					<td class="r-label r-label-1" colspan=2>
						<a id="BSave" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-msg'">保存</a>
					</td>
				</tr>
				<input type="hidden" id="VIPLevel" value="#(VIPLevel)#">
			</table>
		</div>
	</div>
</div>

  <script type="text/javascript" src="../scripts/dhcpenewdiagnosis.conclusion.hisui.js"></script>
  <script type="text/javascript" src="../scripts/DHCPECommon.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPCommon.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPOEData.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPCommonManageCard.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.COMMHardManager.js"></script>
  <script type="text/javascript" src="../scripts/DHCCPM_DOMComm.js"></script>
  <script type="text/javascript" src="../scripts/websys.jquery.js"></script>
  <script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
</body>
</html>