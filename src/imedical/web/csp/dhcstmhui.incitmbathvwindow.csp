<!--批次弹窗-->
<div id="IncItmBathvWindow" class="hisui-dialog FindWin" title="库存批次" data-options="closed:'true',modal:true">
	<div class="hisui-layout" data-options="fit:true">
		<div class="hisui-layout" data-options="region:'north',border:false,split:true">
			<table id="HVWinConditions" class="Condition" onkeydown="if(event.keyCode==13){FQueryBT.click()}">
				<tr>
					<td><label>批号</label><input id="BatchNo" name="BatchNo" class="textbox"></td>
					<td><label>效期</label><input id="ExpDate" name="ExpDate" class="textbox hisui-datebox"></td>
					<td><a href="#" id="IncItmBathvSearchBT" class="hisui-linkbutton">查询</a></td>
					<td><a href="#" id="IncItmBathvSelBT" class="hisui-linkbutton">选取</a></td>
				</tr>
			</table>
		</div>
		<div data-options="region:'west',border:false" class="FindGridWest GridPadding">
			<div class="hisui-panel" title="物资信息" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
				<table id="IncItmBathvMasterGrid"></table>
			</div>
		</div>
		<div data-options="region:'center',border:false" style="padding-left:0px;" class="NonPadGrid GridPadding">
			<div class="hisui-panel" title="高值条码信息" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
				<table id="IncItmBathvDetailGrid"></table>
			</div>
		</div>
	</div>
</div>