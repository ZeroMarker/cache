<!-- 
 * FileName: dhcpecheckprogress.hisui.csp
 * Anchor: xy
 * Date: 2019-06-21
 * Description: HISUI 体检状态查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<!--link rel="stylesheet" type="text/css" href="../scripts/DHCPECommon.hisui.css"/-->
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 10px;
}


</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:212px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'体检状态查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border-radius:0;border-bottom:1px dashed #C0C0C0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="BeginDate">开始日期</label></td>
					<td>
						<input id="BeginDate" class="hisui-datebox textbox" style="width:140px;"/>
					</td>
					
					<td class="r-label r-label-1"><label for="RegNo">登记号</label></td>
					<td>
						<input class="textbox" id="RegNo">
					</td>
					
					<td class="r-label r-label-1"><label for="GADMDesc">团体</label></td>
					<td>
						<input class="hisui-combogrid" type="text" id="GADMDesc" style="width:155px;"/>
					</td>
					
					<td class="r-label r-label-1"><label for="Sex">性别</label></td>
					<td>
						<input class="hisui-combobox" type="text" id="Sex"/>
					</td>
					
					<td class="r-label r-label-1"><label for="CheckStatus">状态</label></td>
					<td>
						<input class="hisui-combobox" type="text" id="CheckStatus"/>
					</td>
					
					<td class="r-label r-label-1"><label for="DietFlag">早餐</label></td>
					<td>
						<input class="hisui-combobox" type="text" id="DietFlag"/>
					</td>
								
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
									
				</tr>
				<tr>
					<td class="r-label"><label for="EndDate">结束日期</label></td>
					<td>
						<input id="EndDate" class="hisui-datebox textbox" style="width:140px;" />
					</td>
					
					<td class="r-label r-label-1"><label for="Name">姓名</label></td>
					<td>
						<input class="textbox" id="Name">
					</td>
					
					<td class="r-label r-label-1"><label for="Depart">部门</label></td>
					<td>
						<input class="textbox" id="Depart"/>
					</td>
					
					<td class="r-label r-label-1"><label for="VIPLevel">VIP等级</label></td>
					<td>
						<input class="hisui-combobox" type="text" id="VIPLevel"/>
					</td>
					<td class="r-label r-label-1"><label for="ReCheck">复查</label></td>
					<td>
						<input class="hisui-combobox" type="text" id="ReCheck"/>
					</td>
					<td class="r-label r-label-1"></td><td></td>
		
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>	
					
				</tr>
				<tr>
					<td class="r-label"><label for="Content">短信内容</label></td>
					<td colspan="7">
						<textarea id="Content" class="textbox" style="width:552px; height:60px;"> </textarea>
					</td>

				</tr>
				
					
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="CheckProgressGrid"  toolbar="#allbar"></table>
			</div>
        </div>
	      
</div>

<!--数据面板工具栏-->
<div id="allbar" style='padding-top:4px'>
  
    <a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-add-item',plain:true,stopAllEventOnDisabled:true" id="BAddItem">加项</a>
    <a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true" id="BRec">收表</a> 
 	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-copy',plain:true,stopAllEventOnDisabled:true" id="BSendAudit">粘贴</a>
    <a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-finish-report',plain:true,stopAllEventOnDisabled:true" id="BReport">报告已完成</a> 
 	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-take-report',plain:true,stopAllEventOnDisabled:true" id="BFetchReport">取报告</a>
    <a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-send-msg',plain:true,stopAllEventOnDisabled:true" id="BSendMessage">发短信</a> 
	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-paper',plain:true,stopAllEventOnDisabled:true" id="BReportView">报告预览</a> 
</div> 
    
<script type="text/javascript" src="../scripts/DHCPECheckProgress.hisui.js"></script>
<script type="text/javascript" src="../scripts/DHCPEPrintReportCommon.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
</body>		
</html>
			
		
 




