<div style="margin:5px;">
	<table cellpadding="5" style="border-spacing: 5px;width:100%">
		<tr>
			<td align=right for ="patno">#(..Get("登记号"))#</td>
			<td>
				<input id="patno" disabled class="textbox">
			</td>
			<td align=right for ="patname">#(..Get("姓名"))#</td>
			<td>
				<input id="patname" disabled class="textbox">
			</td>
			<td rowspan="4" style="WIDTH: 120px;">
				<img id="imgPic" style="WIDTH: 100%; HEIGHT: 150px">
			</td>
		</tr>
		<tr>
			<td align=right for ="Age">#(..Get("年龄"))#</td>
			<td>
				<input class="textbox" disabled id="Age">
			</td>
			<td align=right for ="Sex">#(..Get("性别"))#</td>
			<td>
				<input class="textbox" disabled id="Sex">
			</td>
		</tr>
		<tr>
			<td align=right for ="National">#(..Get("民族"))#</td>
			<td>
				<input class="textbox" disabled id="National">
			</td>
			<td align=right for ="Address">#(..Get("地址"))#</td>
			<td>
				<input class="textbox" disabled id="Address">
			</td>
		</tr>
		<tr>
			<td align=right for ="lxman">#(..Get("联系人"))#</td>
			<td>
				<input class="textbox" disabled id="lxman">
			</td>
			<td align=right for ="homtel">#(..Get("联系电话"))#</td>
			<td>
				<input class="textbox" disabled id="homtel">
			</td>
		</tr>			
	</table>
</div>