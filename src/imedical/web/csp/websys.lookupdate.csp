document.focus();
function RegainFocus() {
	if (!isSelected) {
		try {
			var par_win=window.opener;
			par_win.websys_setfocus('#($g(%request.Data("ID",1)))#');
			if ('#($g(%request.Data("TBLI",1)))#'!='') par_win.websys_setfocus('#($g(%request.Data("TBLI",1)))#');
		} catch(e) {};
	}
}
document.body.onunload=RegainFocus;
</SCRIPT>
</BODY>
