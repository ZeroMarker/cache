<!DOCTYPE html>
<!--煎药配置-方案维护
	csp:pha.dec.v2.decscheme.csp
	js: pha/dec/v2/decscheme.js
-->
<html>
<!-- 验证session过期 -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
</csp:method>

<head>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>
    </TRAK:HEAD>
    <HISUI />
    <PHADECV2/>
</head>
<body>
	<div class="hisui-layout" fit="true">
        <div data-options="region:'center',border:false">
            <table id="gridDecScheme"></table>
        </div>
	</div>
	<div id="gridDecSchemeBar">
		<div class="pha-row">
			<div class="pha-col">方案名称</div>
			<div class="pha-col">
				<input id="qTxtScheName" data-pha='class:"hisui-validatebox textbox"' data-options="">
			</div>
			<div class="pha-col">方案状态</div>
			<div class="pha-col">
				<input id="qCmbScheState" data-pha='class:"hisui-combobox"'>
			</div>
			<div class="pha-col">处方剂型</div>
			<div class="pha-col">
				<input id="qCmbPresForm" data-pha='class:"hisui-combobox"'>
			</div>
			<!--	<div class="pha-col">处方功效</div>
			<div class="pha-col">
				<input id="qCmbPresEffect" data-pha='class:"hisui-combobox"'>
			</div> 	-->
			<div class="pha-col">是否二煎</div>
			<div class="pha-col">
				<input id="qCmbSecDec" data-pha='class:"hisui-combobox"'>
			</div>
			<div class="pha-col">是否制膏</div>
			<div class="pha-col">
				<input id="qCmbPaste" data-pha='class:"hisui-combobox"'>
			</div>
			<div class="pha-col">
                <a class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" id="btnFind" id="btnFind" onclick="javascript:queryGrid()">查询</a>
            </div>
            <div class="pha-col">
                <a class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="btnClear" id="btnFind" onclick="javascript:Clear()">清屏</a>
            </div>
        </div>
        <div class="pha-line"></div>
        <div>
        	<a class="hisui-linkbutton" plain='true' iconCls="icon-add" id="btnAdd" onclick="javascript:MainTain('A')">新增</a>
            <a class="hisui-linkbutton" plain='true' iconCls="icon-write-order" id="btnUpdate" onclick="javascript:MainTain('U')">修改</a>
        <!--	<a class="hisui-linkbutton" plain='true' iconCls="icon-clear" id="btnClear" onclick="javascript:Clear()">清屏</a>	-->
        </div>
    </div>
	<div id="gridDecSchemeWin" class="hisui-dialog" title="煎药方案维护" closed="true" style="width:795px;height:287px;" data-options="iconCls:'icon-w-save',modal: true,border:false">
	  	<div class="hisui-layout" fit="true">  
        	<div data-options="region:'center',border:false">	
        		<div class="hisui-layout" data-options="fit:true" style="height:100%;">
					<div class="pha-row">
						<div class="pha-col" >
							<label for="labelScheCode"><span style="color:red;margin-left:17px;">*</span>方案代码</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtScheCode" data-options="required:true"/>
						</div>
						<div class="pha-col" >
							<label for="labelScheCode"><span style="color:red;margin-left:18px;">*</span>方案名称</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtScheDesc" data-options="required:true"/>
						</div>
						<div class="pha-col" >
							<label for="labelActive"><span style="color:red;margin-left:25px;">*</span>方案状态</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="cmbActive" data-options="required:true"/>
						</div>
					</div>
					<div class="pha-row">
						<div class="pha-col" >
							<label for="labelPresForm"><span style="color:red;margin-left:17px;">*</span>处方剂型</label>
						</div>
						<div class="pha-col">
							<input class="hisui-combobox" id="cmbPresForm" data-options="required:true"/>
						</div>
						<!--
						<div class="pha-col" >
							<label for="labelPresEffect"><span style="color:red;margin-left:18px;">*</span>处方功效</label>
						</div>
						<div class="pha-col">
							<input class="hisui-combobox" id="cmbPresEffect" data-options="required:true"/>
						</div>
						-->
						<div class="pha-col" >
							<label for="labelPaste"><span style="margin-left:25px;"></span>是否制膏</label>
						</div>
						<div class="pha-col">
							<input class="hisui-combobox" id="cmbPaste" />
						</div>
						<div class="pha-col" >
							<label for="labelPressure"><span style="margin-left:35px;"></span>压强(帕)</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtPressure"/>
						</div>
					</div>
					<div class="pha-row">
						<div class="pha-col" >
							<label for="labelSoakInt">浸泡时长(分)</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtSoakInt"/>
						</div>
						<div class="pha-col" >
							<label for="labelFirDecInt"><span style="margin-left:1px;"></span>首煎时长(分)</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtFirDecInt"/>
						</div>
						<div class="pha-col" >
							<label for="labelFirWaterQua">首煎水量(ML)</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtFirWaterQua"/>
						</div>
					</div>
					<div class="pha-row">
						<div class="pha-col" >
							<label for="labelSecDec"><span style="margin-left:23px;"></span>是否二煎</label>
						</div>
						<div class="pha-col">
							<input class="hisui-combobox" id="cmbSecDec"/>
						</div>
						<div class="pha-col" >
							<label for="labelSecDecInt"><span style="margin-left:2px;"></span>二煎时长(分)</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtSecDecInt"/>
						</div>
						<div class="pha-col" >
							<label for="labelSecWaterQua">二煎水量(ML)</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtSecWaterQua"/>
						</div>
					</div>
					<div class="pha-row">
						<div class="pha-col" >
							<label for="labelTemper"><span style="margin-left:28px;"></span>温度(℃)</label>
						</div>
						<div class="pha-col">
							<input class="hisui-validatebox phadec-validatebox" id="txtTemper"/>
						</div>
					</div>
					<div class="pha-row">
						<div class="pha-col" >
							
						</div>
						<div class="pha-col">
							<a id="btnSave" href="#" class="hisui-linkbutton" style="margin-left:284px;" data-options="iconCls:'icon-w-save'" onclick="javascript:SaveData()">保存</a>
						</div>
						<div class="pha-col">
							<a id="btnClose" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-close'" onclick="javascript:CloseWin()">关闭</a>
						</div>
					</div>

        		</div>
			</div>
        </div>
    </div>
    <!-- 本页面js -->
    <script type="text/javascript" src="../scripts/pha/dec/v2/decscheme.js"></script>
</body>
</html>