<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
	<style>
	.imgbox{
		    position: relative;
		    width: 100px;
		    height: 100px;
		    margin-top: 20px;
		    background: #000000;
		    
	}
	.imgbox img{
		    position: absolute;
		    top: 0;
		    bottom: 0;
		    left: 0;
		    right: 0;
		    margin: auto;
		    max-width: 100%;
		    max-height: 100%;
		    outline: 1px solid #000;
	}
	.selectedpic { 
			border:4px solid #378ec4; 
			padding:0;
			position: relative;
			z-index:0
	}		
	.largeImg img{
			width:100%;
			height:auto;
	}
	</style>
	<link href="../scripts/cssd/Common/Viewer/viewer.css" type="text/css" rel="stylesheet"/>
</head>
<body class="hisui-layout" data-options="fit:true">
<div class="GridPadding" data-options="region:'center',border:false">
		<div class="hisui-panel" data-options="title:'回收单据',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true" style="height:300px">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false">
					<table class="Condition" id="MainCondition">
						<tr>
							<td>回收科室</td>
							<td>
								<input id="ReqLoc" name="ReqLoc" class="textbox">
								<input class="textbox" hidden id="fromLocDr" name="fromLocDr">
							</td>
							<td>
								<label>回收人</label><input class="textbox" id="toUser" name="toUser">
								<input class="textbox" hidden id="toUserDr" name="toUserDr">
							</td>
							<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:90px;">查询</a></td>
						</tr>
						<tr>
							<td>供应科室</td>
							<td><input id="SupLoc" name="toLocDr" class="textbox hisui-combobox"></td>
							<td>
								<input id="isOPRFlag" name="isOPRFlag" value="Y" hidden >
							</td>
							<td><a href="#" id="SaveBT" class="textbox hisui-linkbutton" data-options="iconCls:'icon-w-add'" style="width:90px;">新建</a></td>
						</tr>
						<tr hidden>
							<td>开始日期</td>
							<td><input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq" ></td>
							<td>结束日期</td>
							<td><input id="FEndDate" name="FEndDate" class="textbox hisui-dateboxq" ></td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false" style="border-top-width:1px;border-top-color:#ddd">
					<table id="MainList"></table>
				</div>
			</div>
		</div>
	</div>

	<div data-options="region:'south',border:false" class="GridPadding" style="padding-top:0;padding-bottom:15px;">
		<div class="hisui-panel" data-options="title:'单据明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true" >
		<div id="CodeTB">
			<table class="Condition">
				<tr>
					<td><label>条码</label><input class="hisui-numberbox textbox" id="BarCode" name="BarCode" style="width:148px"></td>
				</tr>
			</table>
		</div>	
			<table id="ItemList"></table>
		</div>
	</div>
	
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:480px;padding-left:0px">	
		<div class="hisui-panel" data-options="title:'器械明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<table id="ItemSList"></table>
		</div>
	</div>
	
	<div id="ViewPic" class="hisui-dialog" data-options="closed:'true',modal:true" style="width:950px;height:550px" title="图片浏览">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'center',border:false,headerCls:'panel-header-gray'">
			<ul id="PicsList" class="" style="padding:5px 30px 5px 30px;"></ul>	
		</div>
		<div data-options="region:'east',border:false,headerCls:'panel-header-gray'"   style="width:750px;padding-left:0px" Class="largeImg">
			<img id="showimg"></img>
		</div>
	</div>
</div>
<!--选取请求单Dialog-->
	<div id="FindWin" class="hisui-dialog FindWin" data-options="closed:'true',modal:true,iconCls:'icon-w-find'" title="查询条件" style="width:520px;height:250px">
		<div class="hisui-layout" data-options="fit:true">
			<div data-options="region:'center',border:false">
				<table id="SelBarCodeConditions" class="Condition" align="center">
					<tr>
					    <input id="FIsOPRFlag" name="FIsOPRFlag" class="textbox" value="Y" hidden>
						<td>申请科室</td>
						<td><input id="FReqLoc" name="FReqLoc" class="textbox hisui-combobox"></td>
						<td>供应科室</td>
						<td><input id="FSupLoc" name="FSupLoc" class="textbox hisui-combobox"></td>
					</tr>
					<tr>
						<td>开始日期</td>
						<td><input id="FStartDate" name="FStartDate" class="textbox hisui-datebox" ></td>
						<td>结束日期</td>
						<td><input id="FEndDate" name="FEndDate" class="textbox hisui-datebox" ></td>
					</tr>
					<tr>
						<td>单据号</td>
						<td><input id="FReqNo" name="FReqNo" class="hisui-numberbox textbox"  ></td>
						<td>单据状态</td>
						<td>
							<select name="FStatus" class="hisui-combobox" style="width:155px">
								<option value="" >全部</option>
								<option value="Y" >仅提交</option>
								<option value="N" >仅未提交</option>
							</select>
						</td>
					</tr>
					<tr>
						<td>楼号</td>
						<td><input id="FloorCode" name="FloorCode" class="textbox hisui-combobox"></td>
						<td>线路号</td>
						<td><input id="LineCode" name="LineCode" class="textbox hisui-combobox"></td>
					</tr>
					<tr align="center">
						<td colspan="4">
							<a href="#" id="FSuerBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-ok'">确定</a>
							<a href="#" id="FCancelBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-close'">取消</a>
						</td>
					</tr>
				</table>
			</div>
			
			
		</div>
	</div>
	
