<!--界面科室Combo授权配置-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="" data-options="region:'center',border:false">
		<div class="hisui-layout" data-options="fit:true">
			<div class="GridPadding" data-options="region:'west',border:false,collapsible:false" style="width:350px;">
				<div class="hisui-panel" title="安全组"  data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
					<div class="hisui-layout" data-options="fit:true">
						<div data-options="region:'north',border:false,collapsible:false">
							<table id="GroupCondition" class="Condition" onkeydown="if(event.keyCode==13){document.getElementById('SearchBT').focus();SearchBT.click()}">
								<tr>
			 						<td><label>名称</label><input type="text" id="GroupDescription" name="GroupDescription" class="hisui-textbox textbox" style="width:120px;"></td>
									<td><a href="#" id="SearchBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
									<!--td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td-->
								</tr>
							</table>
						</div>
						<div data-options="region:'center',border:false">
							<table id="GroupGrid"></table>
						</div>
					</div>
				</div>
			</div>	
			<div class="GridPadding" data-options="region:'center',border:false" style="padding-left:0;">
				<div class="hisui-panel" title="菜单授权维护" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
					<div class="hisui-layout" fit="true">
						<div data-options="region:'center',border:false">
							<table id="MenuGrid"></table>
						</div>
					</div>
					<div id="myWindow" data-options="modal:true,iconCls:'icon-w-paper'"></div>
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonHosp.js"></script>	
	<script type="text/javascript" src="../scripts/dhcstmhisui/LocControl/LocControl.js"></script>
</body>
</html>