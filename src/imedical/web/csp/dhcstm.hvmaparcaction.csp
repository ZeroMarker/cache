<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))
	
	//²éÑ¯
	i Action = "Query" d
	.d ##class(web.DHCSTM.DHCHvMapArc).Query(Start,Limit,Sort,Dir)

	//±£´æ
	i Action = "save" d
	.s ListDetail=$Get(%request.Data("ListDetail",1))
	.s result = ##class(web.DHCSTM.DHCHvMapArc).Save(ListDetail)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//É¾³ý
	i Action = "delete" d
	.s RowId=$Get(%request.Data("RowId",1))
	.s result = ##class(web.DHCSTM.DHCHvMapArc).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

    i Action="GetItem" d
    .s HvFlag=$Get(%request.Data("HvFlag",1))
    .s result=##class(web.DHCSTM.DHCHvMapArc).GetItem(HvFlag)
    .w "{success:'true',info:'"_result_"'}"
	
</script>