<!--dhcperoomrecorddetail.hisui.csp -->
<!--诊室调整等候名单-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
quit 1
</csp:method>
<HTML XMLNS=TRAK>
<HEAD>
	<HISUI/>
	<SCRIPT language="Cache" RUNAT="SERVER"> 
	s RoomID=$g(%request.Data("RoomID",1))
	s RoomDesc=$g(%request.Data("RoomDesc",1))
	
</SCRIPT> 
<script type="text/javascript">
	var RoomID="#(RoomID)#"
	var RoomDesc="#(RoomDesc)#"
	var TitleDesc=RoomDesc+"等候名单"
	</script>
</HEAD>

<body>

	<div class="hisui-layout"  border="true" id="RoomRecordDetail"> 
	
		<div class="hisui-panel" data-options="" style="'height:100%';border:1px #ccc;border-radius:4px;">
			<table class="hisui-datagrid" data-options="bodyCls:'panel-header-gray'" id="dhcperoomrecorddetail"></table>
		</div>
	</div>
	
	 
	<script type="text/javascript" src="../scripts/DHCPERoomRecordDetail.hisui.js"></script>
	<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
</body>		
</html>
			
			