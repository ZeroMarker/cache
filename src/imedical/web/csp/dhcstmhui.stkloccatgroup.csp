<!--科室及人员类组授权-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'west',border:false,collapsible:false" style="width:400px;">
		<div class="hisui-panel" title="未授权类组"  data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order',fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'center',border:false">
					<table id="StkCatGroupGrid" ></table>
				</div>
			</div>
		</div>
	</div>
	<div class="GridPadding" data-options="region:'center',border:false" class="GridPadding" style="padding-left:0;padding-right:0;">
		<div class="hisui-panel" title="科室" data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order',fit:true">
			<div class="hisui-layout" fit="true">
				<div data-options="region:'north',border:false,collapsible:false">
					<table id="LocTB" class="Condition">
						<tr>
							<td>科室名称</td>
							<td><input id="LocName" name="LocName" class="hisui-textbox textbox"></td>
							<td><a href="#" id="SearchBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
							<td><a href="#" id="LocSaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">保存</a></td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false">
					<table id="LocGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div class="GridPadding" data-options="region:'east',border:false,collapsible:false"  style="width:350px;">
		<div class="hisui-panel" title="授权类组"  data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order',fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'center',border:false">
					<table id="AuthorStkCatGroupGrid" ></table>
				</div>
			</div>
		</div>
	</div>
	<div class="GridPadding" data-options="region:'south',border:false" style="padding-top:0;">
		<div class="hisui-panel" title="人员授权" data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order',fit:true">
			<div class="hisui-layout" fit="true">
				<div data-options="region:'center',border:false">
					<table id="LocUserGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/StkLocCatGroup/StkLocCatGroup.js"></script>
</body>
</html>