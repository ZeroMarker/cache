<csp:method name=OnPreHTTP
            arguments=""
            returntype=%Boolean>
    i ##Class(ext.websys.SessionEvents).SessionExpired() q 1 
    q 1
</csp:method>
<html lang="zh-CN">
<head>
	<meta http-equiv="Content-Type"
		  content="text/html; charset=utf-8">
	<title>
		<EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE>
	</title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<script type="text/javascript"
			src="../scripts/nurse/hisui-0.1.0/dist/js/config/nurseTemplateConfig.js" charset=gbk></script>
	<STYLE type='text/css'>
		body {
            background-color: #fff;
            padding:5px!important;
        }
        .td_label {
        	text-align:right;
        	height: 40px;
        	padding:0 5px 0 10px;
        }
        .td_item {
        	height: 40px;
        }
        .datagrid-wrap.panel-body.panel-body-noheader{
	        border-color:#ccc;
	        border-top-right-radius:0;
	        border-top-left-radius:0;
	        border-right:0;
	        border-left:0;
	        border-bottom:0;
        }
	</STYLE>
</head>
<body>
	<div class="hisui-layout" 
		 style="width:600px;height:400px;" 
		 data-options="fit:true,border:false">
		<div data-options="region:'north',title:'',split:true,headerCls:'panel-header-gray',iconCls:'icon-paper',border:false" 
			 style="height:60px;padding:5px;">
			<table cellpadding="20">
				<tr>
					<td class="td_label">科室</td>
					<td class="td_item">
						<select id="comboDepartment" 
								class="hisui-combobox" 
								style="width:200px;">
						</select>
					</td>
					<td class="td_label">类型</td>
					<td class="td_item">
						<select id="comboFilter" 
								class="hisui-combobox" 
								style="width:100px;">
						</select>
					</td>
					<td class="td_label">模板名称</td>
					<td class="td_item">
						<input id="textTemplateName" href="#" class="hisui-searchbox"/>
					</td>
					<td class="td_label">
						<a id="btnSearch" class="hisui-linkbutton hover-dark">查询</a>
					</td>
					<td class="td_label">
						<a id="btnSync" title="点击可把旧版的模板配置数据导入到新版模板配置" class="hisui-linkbutton hover-dark hisui-tooltip" data-options="iconCls:'icon-w-import'">导入旧版配置</a>
					</td>
				</tr>
			</table>
		</div>
		<div id="hospitalDiv" data-options="region:'west',title:'模板列表',split:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" 
		     style="width:400px;padding:10px;">
			<div id="leftTemplateTree" 
			     data-options="fit:true,animate:true">
			</div>
		</div> 		
		<div data-options="region:'center',title:'',split:true,headerCls:'panel-header-gray'">
			<table id="allTemplatesGrid" 
				   class="hisui-datagrid"
				   data-options="fit:true">
			</table>
		</div>
		<div id="divAdd"
			 class="hisui-dialog" 
			 style="width:300px;height:250px;top:120px;left:350px;padding:10px"	
			 data-options="iconCls:'icon-w-save',resizable:true,modal:false,isTopZindex:true,closed:true">
			<table cellpadding="20">
				<tr>
					<td class="td_title">权限</td>
					<td class="td_item">
						<select id="comboGroup" 
								class="hisui-combobox" 
								style="width:150px;">
							<option value="A">全院</option>
							<option value="L">本科室</option>
						</select>
					</td>
				</tr>
				<tr>
					<td class="td_title">描述</td>
					<td class="td_item">
						<input id="inputDesc"
							   class="textbox"
							   style="width:220px;">
						</input>
					</td>
				</tr>
				<tr>
					<td class="td_title">序号</td>
					<td class="td_item">
						<input id="numberNo" class="hisui-numberbox textbox" style="width:80px;">
					</td>
				</tr>
			</table> 
		</div>
		<div id="directoryDiv" 
			 class="hisui-dialog" 
			 data-options="closed:true,model:false">
		</div>
		<div id="menu" class="hisui-menu" style="width:120px;">
		</div>
	</div>
</body>

</html>