<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!--dhcem.prehosvis.csp 急诊院前出诊记录表-->
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<script language="Cache" runat="server">
		d ##Class(web.DHCBillPrint).InvBillPrintCLSID()  //打印时使用
   </script>
	<!--------------打印相关---------->
	<script type='text/javascript' src='../scripts/DHCWeb.OPCommon.js'></script>
	<script type='text/javascript' src='../scripts/DHCPrtComm.js'></script>
	<!-------------------------------->
	<link href="../scripts/dhcnewpro/dhcem/css/prehosvis.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/prehosvis.js"></script>
	<script Language="Javascript">
		var PhvID="#(%request.Get("PhvID"))#" 
   </script>
</head>
<body>

<div class="hisui-layout" fit="true">
	<div data-options="region:'center',headerCls:'panel-header-gray',title:'急诊院前出诊记录'">
		<table border="1" cellspacing="0" cellpadding="1" class="report_table">
			<tr class="tr" style="margin:10px 0px;">
				<td align="left" colspan="6" style="background:rgb(238, 247, 254);font-weight:bold;">出诊信息：</td>
			</tr>
			
			<tr class="tr">
				<td align="right"  class="tb_td_bk tb_td_required">病人姓名：</td>
				<td width="180px">
					<input id="PatName" class="hisui-validatebox" style="width:170px;">
				</td>
				<td align="right" class="tb_td_bk tb_td_required">性<label style="margin-left:25px;"></label>别：</td>
				<td width="180px">
					<input id="PatSex" class="hisui-validatebox" style="width:170px;">
				</td>
				<td align="right" class="tb_td_bk tb_td_required">年<label style="margin-left:25px;"></label>龄：</td>
				<td width="180px">
					<input id="PatAge" class="hisui-validatebox" style="width:170px;">
				</td>
			</tr>
			
			<tr class="tr">
				<td align="right" class="tb_td_bk tb_td_required">出诊地点：</td>
				<td align="left" colspan="5">
					<input id="VisPlace" class="hisui-validatebox" style="width:760px;height:46px">
				</td>
			</tr>
			
			<tr class="tr">
				<td align="right" class="tb_td_bk tb_td_required">出诊事由：</td>
				<td align="left" colspan="5">
					<textarea id="VisRea" style="width:760px;height:120px;resize:none;"></textarea>
				</td>
			</tr>
			
			<tr class="tr">
				<td align="right"  class="tb_td_bk">接通知日期：</td>
				<td width="180px">
					<input id="VisDate" class="hisui-datebox" style="width:150px;">
				</td>
				<td align="right" class="tb_td_bk">接通知时间：</td>
				<td width="180px">
					<input id="VisTime"  style="width:150px;" class="hisui-validatebox" onblur="this.value=CheckEmPcsTime(this.id)" onfocus="this.value=SetEmPcsTime(this.id)">
				</td>
				<td align="right" class="tb_td_bk">接通知者：</td>
				<td width="180px">
					<input id="RecUser" class="hisui-validatebox" style="width:170px;">
				</td>
			</tr>
			<tr class="tr">
				<td align="right" class="tb_td_bk">呼救者姓名：</td>
				<td width="180px">
					<input id="CallerName" class="hisui-validatebox" style="width:170px;">
				</td>
				<td align="right" class="tb_td_bk">联系电话：</td>
				<td width="180px">
					<input id="CallerPhone" class="hisui-validatebox" style="width:170px;">
				</td>
				<td colspan="2"></td>
			</tr>
			
			<tr class="tr">
				<td align="right" class="tb_td_bk">信息来源：</td>
				<td align="left" colspan="5">
					<input class='hisui-radio' type="radio" label='120急救中心' name='VisSource' value='1'>
					<input class='hisui-radio' type="radio" label='其他' name='VisSource' value='2'>
				</td>
			</tr>
			
			<tr class="tr" style="display:none">
				<td align="right"  class="tb_td_bk">登记日期：</td>
				<td width="180px">
					<input id="CrateDate" class="hisui-datebox" style="width:150px;" disabled>
				</td>
				<td align="right" class="tb_td_bk">登记时间：</td>
				<td width="180px">
					<input id="CrateTime" style="width:150px;" class="hisui-validatebox" onblur="this.value=CheckEmPcsTime(this.id)" onfocus="this.value=SetEmPcsTime(this.id)" disabled>
				</td>
				<td align="right" class="tb_td_bk">登记人：</td>
				<td width="180px">
					<input id="CrateUser" class="hisui-validatebox" style="width:170px;" disabled>
				</td>
			</tr>
			
		</table>
		<!--底部按钮-->
		<div id="footer" style="opacity:0.9;border:1px solid #E5E5E5;background-color:#F9F9F9;width:100%;height:35px;position:fixed;left:0px;bottom:0px;z-index:2;text-align:center;">
			<a id="SaveBut" class="hisui-linkbutton" style='' data-options="iconCls:'icon-w-save'"onclick="SaveHosVis()" >保存</a> 
	    	<a id="CanelBut" class="hisui-linkbutton" style='' data-options="iconCls:'icon-w-cancel'"onclick="CancelHosVis()" >取消</a> 
	    </div>
	</div>
</div>
 <server>
  //s PhvID=%request.Get("PhvID")
  //w "<script Language=""JavaScript"">"_$c(13,10)
  //w "var PhvID="""_$g(PhvID)_""";"_$c(13,10)
  //w "</script>"
</server>
</body>
</html>
