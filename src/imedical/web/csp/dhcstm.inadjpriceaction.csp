<csp:content charset="UTF-8">
<SERVER>
s Action=$Get(%request.Data("actiontype",1))
s Start=$Get(%request.Data("start",1))
s Limit=$Get(%request.Data("limit",1))
s Sort=$g(%request.Data("sort",1))
s Dir=$g(%request.Data("dir",1))
s sHospID=$G(%session.Data("LOGON.HOSPID"))
i Action = "Save" d
	.S StkGrp=$Get(%request.Data("StkGrp",1))
	.S LocId=$Get(%request.Data("LocId",1))
	.S AspNo=$Get(%request.Data("AspNo",1))
	.S list=$Get(%request.Data("list",1))
	.s ret=##class(web.DHCSTM.INAdjSalePrice).Save(AspNo,StkGrp,LocId,list)
	.i $l(ret,"^")>1  d
	..w "{success:'false',info:'"_ret_"'}"
	.e  d
	..w "{success:'true',info:'"_ret_"'}"
	
i Action = "DeleteAspItm" d
	.S AspRowid=$Get(%request.Data("AspRowid",1))
	.s ret=##class(web.DHCSTM.INAdjSalePrice).Delete(AspRowid)
	.i ret'=0  d
	..w "{success:'false',info:'"_ret_"'}"
	.e  d
	..w "{success:'true',info:'"_ret_"'}"
	
i Action = "QueryAspDetail" d
	.S AspNo=$Get(%request.Data("AspNo",1))
	.S Status=$Get(%request.Data("Status",1))
	.s StkGrpId=$g(%request.Data("StkGrpId",1))
	.s IncId=$g(%request.Data("IncId",1))
	.d ##class(web.DHCSTM.INAdjSalePrice).Query(Start,Limit,AspNo,Status,StkGrpId,IncId)
	.
i Action = "QueryAspNo" d
	.S Params=$Get(%request.Data("Others",1))
	.S StartDate=$Get(%request.Data("StartDate",1))
	.S EndDate=$Get(%request.Data("EndDate",1))
	.d ##class(web.DHCSTM.INAdjSalePrice).QueryAdjSpNo(Start,Limit,StartDate,EndDate,Params,sHospID)

i Action = "QueryAspInfo" d
	.S StartDate=$Get(%request.Data("StartDate",1))
	.S EndDate=$Get(%request.Data("EndDate",1))
	.s Params=$g(%request.Data("Others",1))
	.d ##class(web.DHCSTM.INAdjSalePrice).QueryAspInfo(Start,Limit,StartDate,EndDate,Params,sHospID)

i Action = "AuditAsp" d
	.S AspIdStr=$Get(%request.Data("StrAspId",1))
	.S AuditUserId=$Get(%request.Data("AuditUser",1))
	.s ret=##class(web.DHCSTM.INAdjSalePrice).Audit(AspIdStr,AuditUserId)
	.i ret=0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"

i Action = "CancelAuditAsp" d
	.S AspIdStr=$Get(%request.Data("StrAspId",1))
	.S User=$Get(%request.Data("CancelUser",1))
	.s ret=##class(web.DHCSTM.INAdjSalePrice).CancelAudit(AspIdStr,User)
	.i ret=0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"
	
i Action = "GetItmInfo" d
	.S ItmId=$Get(%request.Data("InciId",1))
	.S Params=$Get(%request.Data("Params",1))
	.s result=##class(web.DHCSTM.INAdjSalePrice).GetItmInfo(ItmId,Params)
	.s result=##class(web.DHCSTM.Common.UtilCommon).hJsonChar(result)
	.w "{success:'true',info:'"_result_"'}"

i Action="GetMtSp"  d
	.s ItmId=$g(%request.Data("InciId",1))
	.s UomId=$g(%request.Data("UomId",1))
	.s Rp=$g(%request.Data("Rp",1))
	.s Sp=##class(web.DHCSTM.Common.PriceCommon).GetMtSp(ItmId,UomId,Rp)
	.w Sp
	
i Action="GetParamProp"  d
	.s GroupId=$g(%request.Data("GroupId",1))
	.s LocId=$g(%request.Data("LocId",1))
	.s UserId=$g(%request.Data("UserId",1))
	.s ret=##class(web.DHCSTM.INAdjSalePrice).GetParamProp(GroupId,LocId,UserId)
	.w "{success:'true',info:'"_ret_"'}"

i Action = "Execute" d
	.S AspIdStr=$Get(%request.Data("StrAspId",1))
	.S Params=$Get(%request.Data("Params",1))
	.s ret=##class(web.DHCSTM.INAdjSalePrice).SetExe(AspIdStr,Params)
	.i ret=0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"

i Action = "UpdatePrice" d
	.S ListData=$Get(%request.Data("ListData",1))
	.s ret=##class(web.DHCSTM.INAdjSalePrice).BatUpdatePrice(ListData)
	.i ret=0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"

i Action = "QueryAspAmount" d
	.S StartDate=$Get(%request.Data("StartDate",1))
	.s EndDate=$g(%request.Data("EndDate",1))
	.s Others=$g(%request.Data("Others",1))
	.s ret=##class(web.DHCSTM.AspAmount).jsQueryAspAmount(Start,Limit,Sort,Dir,StartDate,EndDate,Others)
	.i ret=""  d
	..w "{success:'true',info:''}"
	.e  d
	..w ret

</SERVER>
