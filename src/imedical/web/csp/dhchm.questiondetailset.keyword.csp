<!--
Description: 问卷展示界面  dhchm.questiondetailset.keyword.csp
Created by wangguoying
-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<html lang="en">  
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<script type="text/javascript" src="../scripts/dhchm.questiondetailset.keyword.js"></script>
	<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
	<script type="text/javascript" src="../scripts/websys.hisui.js"></script>
	<style>	
		.f24 {
   			 font-size: 24px;
		}
		.mt15{
			margin-top: 15px;
		}
		.mt25{
			margin-top: 25px;
		}
		.lh200{
			line-height: 200%;
		}
		.text_i2{
			text-indent: 2em;
		}
		.detail_remark
		{
			padding:10px 0px;
			margin-bottom: 10px;
		}
	</style>
	<SCRIPT language="Cache" RUNAT="SERVER">
		s EQID=%request.Get("EQID")
		s QuesID=""
		i EQID'=""
		{
			s EQObj=##class(User.DHCHMOQEQuestion).GetObj(EQID)
			s QuesID=EQObj.QEQCQuestionnaireDR.%Id()
		}
		s Job=##class(web.DHCPE.HM.ExamSurveyHandler).QuerySurveyDetail(QuesID,"","","",EQID)
	</SCRIPT>
	 <Script  type='text/javascript'>
		var Job=#(Job)#
		var QuesID=#(QuesID)#
	</Script>
</head>

<body  class="hisui-layout" fit="true" onload="ShowDetailPanel('');">
	<input type="hidden" id="EQID" value="#(EQID)#">
	<div id="NorthPanel" data-options="region:'north',headerCls:'panel-header-gray',border:false" style="padding:5px;height:36px;text-align:center;">
		<SCRIPT language="Cache" RUNAT="SERVER">
			d ##class(web.DHCPE.HM.ExamSurveyHandler).OutSubjectKeys(Job,QuesID)			
		</SCRIPT>
	</div>
	<div id='CenterPanel' data-options="region:'center',headerCls:'panel-header-gray',border:false">
		<div id='DetailPanelDiv' fit='true' style='height:100%;'>
			<iframe id='SurveyFrame' width='100%' height='100%'  frameborder='0'>
		</div>
	</div>		
	
</body>
</html>

