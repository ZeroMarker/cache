<!--HIS接口日志查询-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="" data-options="region:'center',border:false">
		<div class="hisui-layout" data-options="fit:true">
			<div class="BorderLine" data-options="region:'north',headerCls:'panel-header-gray'">
				<table id="MainConditions" class="Condition" onkeydown="if(event.keyCode==13){QueryBT.click()}">
				<tr>
					<td><label>开始日期</label><input id="StartDate" name="StartDate" class="textbox hisui-datebox"></td>
					<td><label>截止日期</label><input id="EndDate" name="EndDate" class="textbox hisui-datebox"></td>
					<td><label>参数内容</label><input id="ParamValue" name="ParamValue" class="textbox"></td>
					<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
				</tr>
				<tr>
					<td><label>起始时间</label><input id="StartTime" name="StartTime" class=" textbox hisui-timespinner" data-options="showSeconds:true"></td>
					<td><label>截止时间</label><input id="EndTime" name="EndTime" class="textbox hisui-timespinner" data-options="showSeconds:true"></td>
					<td><label>方法名</label><input id="MethodName" name="MethodName" class="textbox hisui-combobox"></td>
					<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
					
				</tr>
			</table>
			</div>
			<div data-options="region:'center',border:false">
				<table id="LogGrid"></table>
			</div>
		</div>	
	</div>
	<script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonHosp.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/HISInterfaceLog/HISInterfaceLog.js"></script>
</body>
</html>
