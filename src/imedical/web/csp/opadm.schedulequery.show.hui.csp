<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false" style="height:136px;padding:10px 10px 0 10px;">
		<div id="pagetitle" class="hisui-panel" data-options="title:'停替诊查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="StDate">开始日期</label></td>
					<td>
						<input class="hisui-datebox textbox" id="StDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label r-label-1">
						<label for="EndDate">结束日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="EndDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label r-label-1">
						<label for="Loc">科室</label>
					</td>
					<td>
						<input class="textbox" id="Loc"/>
					</td>
					<!--td class="r-label-1">
						<input id="submit" class='hisui-checkbox' type="checkbox" label="上报">
					</td-->
					<td class="r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="Doc">医生</label>
					</td>
					<td>
						<input class="textbox" id="Doc"/>
					</td>
					<td class="r-label r-label-1">
						<label for="Status">类型</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="Status"/>
					</td>
					<td class="r-label r-label-1">
						<label for="Reason">停替诊原因</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="Reason"/>
					</td>
					<td class="r-label-1">
						<a id="BPrint" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">打印</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="ScheduleQueryTab"></table>
		</div>
	</div>
</div>