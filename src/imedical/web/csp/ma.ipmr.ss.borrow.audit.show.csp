<!--主页面-->
<div class="hisui-layout" data-options="fit:true,border:false" >
	<div data-options="region:'center',border:false">
		<table id="gridLendRequest" data-options="toolbar:'#QueryToolBar',border:true"></table>
	</div>
</div>
<div id="QueryToolBar" style="display: none;" class="hisui-toolbar">
	<div data-options="" style="">
		<table cellspacing="0" cellpadding="0" style="padding-bottom: 7px;padding-top: 7px;">
			<tr>
				<td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-checkin" id="btnPass">审核通过</a></td>
				<td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-uncheckin" id="btnUnPass">审核不通过</a></td>
			</tr>
		</table>
	</div>
</div>

<!--申请卷列表-->
<div id="RequestVolDialog" class="hisui-window" data-options="closed:true" style="width:1200px;height:600px;padding:10px 10px 10px 10px;">
	<div  class="hisui-layout" data-options="fit:true"> 
		<div style="border:none;" data-options="region:'center'" >
			<table id="gridRequestVol" data-options="fit:true,singleSelect:true" style="text-align:right; width:100%">
			</table>
		</div>
	</div>
</div>
