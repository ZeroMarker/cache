<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="height:92px;">
		<div class="hisui-panel" style="border:0;border-bottom:1px dashed #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label id="_HospListLabel" style='color:red;'>医院</label>
					</td>
				
					<td colspan="3">
						<input id="_HospList" />
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label class="left_label" for="room">诊室</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="room"/>
					</td>
					<td class="r-label">
						<label class="left_label" for="code">计算机代码</label>
					</td>
					<td>
						<input class="textbox" id="code"/>
					</td>
					<td class="r-label">
						<label class="left_label" for="name">计算机名称</label>
					</td>
					<td>
						<input class="textbox" id="name"/>
					</td>
					<td class="r-label">
						<label class="left_label" for="IP">IP地址</label>
					</td>
					<td>
						<input class="textbox" id="IP"/>
					</td>
					<td class="r-label">
						<a id="Bfind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">按诊室查询</a>
					</td>
				</tr>
			 </table>
		 </div>
	</div>
	<div data-options="region:'center',border:false">
		<table id="RoomCompTab"></table>
	</div>
</div>
