<!--验收-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>

</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:10px">
		<div class="hisui-panel" data-options="title:'清洗信息验收',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="UomTB">
				<table class="Condition" id="cleantable">
					<input id="cleantypev" name="cleantypev" type="hidden">
					<input id="cleanCodev" name="cleanCodev" type="hidden">
					<input id="cleanStrov" name="cleanStrov" type="hidden">
					<input id="Iscmt" name="Iscmt" type="hidden">
					<tr>
						<td>开始日期
							<input id="FStartDate" name="FStartDate" class="hisui-dateboxq textbox" style="width:120px"></input>
						</td>
						<td align="left" style="padding-left:30px">结束日期
							<input id="FEndDate" name="FEndDate" class="hisui-dateboxq textbox" style="width:120px"></input>
						</td>
						<td style="padding-left:30px">
							<a href="#" id="checknpass" class="hisui-linkbutton">验收不合格</a>
						</td>
					</tr>
					<tr>
						<td style="text-align:left">清洗机号
							<input id="cleanmachine" name="cleanmachine" class="textbox" style="width:113px"></input>
						</td>
						<td style="padding-left:30px">
							<a href="#" id="query" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
							<a href="#" id="check" class="hisui-linkbutton">验收合格</a>
							
						</td>
					</tr>
				</table>
			</div>
			<table id="tabDrugList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:630px;padding-left:0px">
		<div class="hisui-panel" data-options="title:'清洗明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true" style="padding-bottom:10%">
			<table class="Condition" hidden>
				<tr>
					<td><label>不合格原因</label><input id="UnqualifiedReason" class="textbox hisui-combobox" style="width:140px"></td>
					<td style="padding-left:30px"><a href="#" id="CheckFailBT" class="hisui-linkbutton" style="width:100px;">验收不合格</a></td>
				</tr>
			</table>
			<table id="ItemList"></table>
		</div>
	</div>	
	<script type="text/javascript" src="../scripts/cssd/Clean/CleanCommon.js"></script>
	<script type="text/javascript" src="../scripts/cssd/Clean/CleanCheck.js"></script>
</body>
</html>
