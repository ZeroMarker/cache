<!-- dhcem.shiftsaddpat.csp  新增病人页面-->
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<link href="../scripts/dhcnewpro/dhcem/css/base.css" rel="stylesheet">
	<link href="../scripts/dhcnewpro/dhcem/css/addpat.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/bedsideshiftscom.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/addpat.js"></script>
</head>
<body class="background-warp">
	<div class="container">
		<div class="page-center">
			<div class="list-title">
				<div class="list-icon">
					<img src="../scripts/dhcnewpro/dhcem/images/infomation.png" border=0/>
				</div>
				<label>新增</label>
				<div class="list-tools">
				</div>
	        </div>
	    	<div class="list-order-item">
				<table border="1" cellspacing="0" cellpadding="1" class="report_table">
					<tr style="display:none">
						<td align="right" class="tb_td_bk">EpisodeID</td>
						<td width="200px"><span id="EpisodeID" class="td-span-m"/></td>
						<td align="right" class="tb_td_bk">床号ID</td>
						<td width="200px"><span id="BedID" class="td-span-m"/></td>
					</tr>
					<tr class="tr">
						<td align="right" class="tb_td_bk">登记号</td>
						<td width="200px">
							<input id="PatNo" class="hisui-validatebox" style="width:170px;">
						</td>
						<td align="right" class="tb_td_bk">姓名</td>
						<td width="200px">
							<span id="PatName" class="td-span-m"></span>
						</td>
					</tr>
					<tr class="tr">
						<td align="right" class="tb_td_bk">床号</td>
						<td width="200px">
							<span id="PatBed" class="td-span-m"></span>
						</td>
						<td align="right" class="tb_td_bk">主管医生</td>
						<td width="200px">
							<span id="PatDoc" class="td-span-m"></span>
						</td>
					</tr>
					<tr class="tr">
						<td align="right" class="tb_td_bk">诊断</td>
						<td width="180px" colspan="3"><span id="PatDiags" class="td-span-m"></span></td>
					</tr>
				</table>
	    	</div>
	    </div>
    	<div class="page-tools">
    		<div style="text-align:center;margin-top:5px;">
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-cancel'" onclick="closewin()" style="margin-left:15px">取消</a>
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-ok'" onclick="sure()" style="margin-left:30px">确认</a>
			</div>
    	</div>
	</div>
</body>
</html>
