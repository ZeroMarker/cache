<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!--ma.ipmr.fp.frontpage.icdsearch.csp-->
<!DOCTYPE html>
<html lang="en">
	<head>
		<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<HISUI/>
		<IPMR:LOGON/>
		<link type="text/css" rel="styleSheet"  href="../scripts/ipmr/css/base.css" />
		<script language="cache" runat="server">
			Set icdType=$Get(%request.Data("icdType",1),"")
			Set selectTypeID=$Get(%request.Data("selectTypeID",1),"")
			Set icdVerID=$Get(%request.Data("icdVerID",1),"")
	  	</script>
	</head>
	<body class="hisui-layout" data-options="fit:true,border:false" style="border-top:none;">
		<div class="hisui-layout" data-options="fit:true,border:false" >
			<div data-options="region:'center',border:false" style="padding:10px;">
				<div  class="hisui-panel" data-options="fit:true,border:true" >
					<table id="gridICD"  data-options="fit:true,singleSelect:true,pagination:true,toolbar:'#gridICDTBar',border:false"  width="100%" >
					</table>
				</div>
			</div>
		</div>
		<!--定义按钮-->
		<div id="gridICDTBar" >
			<table data-options="fit:true;">
				<tr>
					<td style="text-align: right;">
						<input id="searchICD"  href="#" data-options="prompt:'#(..Get("请输入别名"))#'" class="hisui-searchbox"/>
					</td>
				</tr>
			</table>
		</div>
		<script type="text/javascript"  src="../scripts/websys.js"></script>
		<script type="text/javascript" src="../scripts/ipmr/extend.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/common.js"></script>
		<script type="text/javascript"  src="../scripts/ipmr/fp/frontpage/common.js"></script>
		<script type="text/javascript" src="../scripts/ipmr/plugins/datagrid-dnd.js"></script>
		<script type="text/javascript" src="../scripts/ipmr/fp/frontpage/icdsearch.js"></script>
		<SCRIPT language = 'javascript' >
			var ServerObj={
				icdType:'#(icdType)#',
				selectTypeID : '#(selectTypeID)#',
				icdVerID : '#(icdVerID)#'
			};
		</SCRIPT>
	</body>
</html>

