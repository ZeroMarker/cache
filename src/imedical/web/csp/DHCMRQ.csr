<!-- HEAD TAG --------------------------------------------- -->

<csr:rule name="DHCMRQ" match="DHCMRQ:HEAD">
<csr:description>
The <b>DHCMRQ:HEAD</b> tag defines defines standard header info and initialisation for each page. For example javascript utilties, style sheet, theme and initialisation code
</csr:description>

<csr:action>
	<SCRIPT LANGUAGE=CACHE RUNAT=SERVER>
		do ##Class(DHCMRQ.Base.Config).Head()
	</SCRIPT>
</csr:action>
</csr:rule>

