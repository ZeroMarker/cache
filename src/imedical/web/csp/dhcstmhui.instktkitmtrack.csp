<!--盘点录入方式高值盘点-->
<!DOCTYPE html>
<server>
	s InstRowid=$g(%request.Data("Rowid",1))
	s InstLocId=$g(%request.Data("LocId",1))
	s InstwWin=$g(%request.Data("InstwWin",1))
</server>
<html lang="zh-CN">
<head>
	<script language=javascript>
	var gRowid="#(InstRowid)#";
	var gLocId="#(InstLocId)#";
	var gInstwWin="#(InstwWin)#";
	</script>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
<div data-options="region:'north',border:false">
		<table class="Condition" id="Conditions">
			<tr>
			<input id="inst" name="inst" type="hidden" >
				<td>类组</td>
				<td><input id="ScgStk" name="ScgStk" class="hisui-stkscgcombotree" style="width:180px;"></td>
				<td>管理组</td>
				<td><input id="LocManaGrp" name="LocManaGrp" class="hisui-combobox" style="width:180px;"></td>
				<td>单号</td>
				<td><input id="InstNo" name="InstNo" class="textbox" disabled="true" style="width:180px;"></td>
				<td></td>
				<td></td>
			</tr>
			<tr>
				<td>库存分类</td>
				<td><select id="StkCatBox" name="StkCatBox" class="hisui-combobox" style="width:180px;"></select></td>
				<td>货位</td>
				<td><input id="LocStkBin" name="LocStkBin" class="hisui-combobox" style="width:180px;"></td>
				<td></td>
				<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
				<td>高值条码录入</td>
				<td><input id="BarCode" class='textbox' /></td>
			</tr>
		</table>
	</div>
	<div class="GridPadding" style="padding-top:0;" data-options="region:'center',border:false,collapsible:false">
		<div class="hisui-panel" title="盘点明细信息" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
			<table id="DetailGrid"></table>
		</div>
	</div>
	<div class="GridPadding" style="width:500px;padding-top:0;padding-left:0;" data-options="region:'east',border:false,collapsible:false">
		<div class="hisui-panel" title="盘点批次信息" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
			<table id="HVBarCodeInfoGrid"></table>
		</div>
	</div>
	
	<script type="text/javascript" src="../scripts/dhcstmhisui/InStkTk/INStkTkItmTrack.js"></script>
</body>
</html>