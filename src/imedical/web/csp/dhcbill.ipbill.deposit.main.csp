<!-- 
 * FileName: dhcbill.ipbill.deposit.main.csp
 * Anchor: ZhYW
 * Date: 2019-07-03
 * Description: 押金管理
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<ADDINS/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.deposit.css'/>
	<script type='text/javascript' src='../scripts/DHCWeb.OPCommonManageCard.js'></script>
	<script type='text/javascript' src='../scripts/DHCBillPayService.js'></script>
	<script type='text/javascript' src='../scripts/DHCBillMisPosPay.js'></script>
	<script type='text/javascript' src='../scripts/dhcbill/dhcipbill/dhcbill.ipbill.config.js'></script>
	<script type='text/javascript' src='../scripts/dhcbill/dhcipbill/dhcbill.ipbill.common.js'></script>
	<server>
		set PatientListPage=""
		do ##class(web.UDHCCardCommLinkRegister).GetCardHardJSFunction()
		w "<input id='ReadAccExpEncrypt' name='ReadAccExpEncrypt' type='hidden' value='"_##class(%CSP.Page).Encrypt($lb("web.UDHCAccManageCLSIF.getaccinfofromcardno"))_"'>",$c(13,10)
		w "<input id='papmi' name='papmi' type='hidden'>",$c(13,10)
		w "<input id='episodeId' name='episodeId' type='hidden'>",$c(13,10)
	</server>
</head>

<body class="hisui-layout">
	<div data-options="region:'north',border:false" style="height:36px;overflow:hidden;">
		<csp:Include Page="dhcbill.in.patient.banner.csp">
	</div>
	<div data-options="region:'west',border:false" style="width:520px;padding:0 10px 10px 10px;">
		<div class="hisui-layout" id="head-menu" data-options="fit:true">
			<div data-options="region:'north',border:false" style="height:204px;">
				<div class="hisui-panel" data-options="title:'患者查询',fit:true,tools:'#search-panel-tools',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border-bottom-style:none;border-radius:0;">
					<table class="search-table" style="width:100%;">
						<tr>
							<td class="r-label"><label>卡号</label></td>
							<td colspan="4" class="box-td">
								<input id="cardNo" class="textbox" placeholder="刷卡/请输入卡号" style="margin-right:8px;"/><input id="cardType" class="textbox tb139"/><a href="javascript:;" class="hisui-linkbutton" id="btn-readCard" data-options="iconCls:'icon-w-card'" style="margin-left:8px;">读卡</a>
							</td>
						</tr>
						<tr>
							<td class="r-label"><label>登记号</label></td>
							<td><input id="patientNo" class="textbox"/></td>
							<td class="r-label"><label>病案号</label></td>
							<td><input id="medicareNo" class="textbox"/></td>
						</tr>
						<tr>
							<td class="r-label"><label>患者姓名</label></td>
							<td><input id="patName" class="textbox"/></td>
							<td class="r-label"><label>就诊类型</label></td>
							<td><input id="admType" class="textbox"/></td>
						</tr>
						<tr class="display-more-tr">
							<td class="r-label"><label>病区</label></td>
							<td><input id="ward" class="textbox"/></td>
							<td class="r-label"><label>床号</label></td>
							<td><input id="bed" class="textbox"/></td>
						</tr>
					</table>
					<div style="text-align:center;">
						<!--a href="javascript:;" class="hisui-linkbutton" id="btn-readInsuCard">读医保卡</a>
 						<div style="display:inline-block;margin:0 10px;"></div>
						<a href="javascript:;" class="hisui-linkbutton" id="btn-IDCard">读二代身份证</a>
						<div style="display:inline-block;margin:0 10px;"></div-->
 						<a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a>
 						<div style="display:inline-block;margin:0 5px;"></div>
						<a href="javascript:;" class="hisui-linkbutton" id="btn-clear" data-options="iconCls:'icon-w-clean'">清屏</a>
					</div>
				</div>
			</div>
			<div data-options="region:'center',border:false">
				<div class="hisui-panel panel-header-gray" data-options="fit:true,headerCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
					<table id="admList"></table>
				</div>
        	</div>
		</div>
		<span id="search-panel-tools">
			<div id="more-container" class="menu-container">
				<div class="spread-b-down"></div>
				<div class="arrows-b-text">更多</div>
			</div>
		</span>
	</div>
	<div data-options="region:'center',border:false" style="padding:0 10px 10px 0;">
		<div id="deposit-tabs" class="hisui-tabs tabs-gray" data-options="fit:true">
			<div title="交押金">
				<csp:Include Page="dhcbill.ipbill.deposit.pay.csp">
			</div>
			<div title="退押金">
				<csp:Include Page="dhcbill.ipbill.deposit.refund.csp">
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.deposit.main.js"></script>
</body>
</html>