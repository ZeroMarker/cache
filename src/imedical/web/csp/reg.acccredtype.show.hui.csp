<div class="hisui-layout" data-options="fit:true,border:false" style=""> 
	<div data-options="region:'north',split:false,title:'',border:false" style="height:91px;border-bottom:1px dashed #ccc;">
    	<div class="hisui-panel" data-options="fit:true,border:false" style="font-size:0;">
    		<table class="search-table">
				<tr>
					<td class="FindLabel">
						<label for="Code">证件名称</label>
					</td>
					<td>
						<input class="textbox" id="Code"/>
					</td>
					<td class="FindLabel">
						<label for="Desc">证件描述</label>
					</td>
					<td>
						<input class="textbox" id="Desc"/>
					</td>
					<td class="r-label">
						<label for="Default">默认</label>
					</td>
					<td style="margin-right:10px">
						<div id="Default" class="hisui-switchbox" style="margin-left:5px;margin-right:10px" data-options="onText:'Yes',offText:'No',size:'small',animated:true,onClass:'primary',offClass:'gray',onSwitchChange:function(event,value){console.log(event);console.log(value);}">
						</div>
					</td>
					<td class="r-label">
						<label for="CredNoRequired">证件号不允许为空</label>
					</td>
					<td style="margin-right:10px">
						<div id="CredNoRequired" class="hisui-switchbox" style="margin-left:5px;margin-right:10px" data-options="onText:'Yes',offText:'No',size:'small',animated:true,onClass:'primary',offClass:'gray',onSwitchChange:function(event,value){console.log(event);console.log(value);}">
						</div>
					</td>
					<td class="FindLabel">
						<a href="#" id="Add" class="hisui-linkbutton" data-options="iconCls:'icon-w-add'">新增</a>
					</td>
					<td class="FindLabel">
						<a href="#" id="acccredSort" class="hisui-linkbutton" data-options="iconCls:'icon-w-list'">排序</a>
					</td>
					<!--td class="FindLabel">
						<a href="#" id="translate" class="hisui-linkbutton" data-options="iconCls:'icon-w-update'">翻译</a>
					</td-->
				</tr>
				<tr>
					<td class="FindLabel">
						<label for="DateFrom">开始日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="DateFrom"/>
					</td>
					<td class="FindLabel">
						<label for="DateTo">结束日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="DateTo"/>
					</td>
					<td class="r-label">
						<label for="Active">激活</label>
					</td>
					<td>
						<div id="Active" class="hisui-switchbox" style="margin-left:5px" data-options="onText:'Yes',offText:'No',size:'small',animated:true,onClass:'primary',offClass:'gray',onSwitchChange:function(event,value){console.log(event);console.log(value);}">
						</div>
					</td>
					<td></td><td></td>
					<td class="FindLabel">
						<a href="#" id="Update" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'">修改</a>
					</td>
					<td class="FindLabel">
						<a href="#" id="acccredSortPlan" class="hisui-linkbutton" data-options="iconCls:'icon-w-update'">排序方案选择</a>
					</td>
				</tr>
				
			</table>
    	</div>
    </div>
    <div class="hisui-panel" data-options="region:'center',split:true,border:false" > 
    	<table id="DataList"></table>
    </div>
    
</div>
<div id="acccredtype-dialog" class="hisui-dialog" title="证件类型排序方案设置" style="padding:10px;width:330px;height:180px;" data-options="iconCls:'icon-w-save',shadow:false,modal:true,closed:true">
	<div class="hisui-panel" style="border-color:#ccc;" data-options="border:true,headerCls:'panel-header-card-gray',fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false">
		<table class="search-table">
			<tr>
				<td style="padding:10px;">
					<label for="acccredtypeSort">证件类型</label>
				</td>
				<td style=""><input class="hisui-textbox textbox" id="acccredtypeSort"></td>
			</tr>
			<tr>
				<td colspan="2" align="center"><a href="#" class="hisui-linkbutton" id="SaveConfig">保存</a></td>
			</tr>
		</table>
	</div>
</div>
