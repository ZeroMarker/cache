<!--主页面-->
<div class="hisui-layout" data-options="fit:true,border:false" >
	<div data-options="region:'center',border:false">
		<table id="gridConfig" class="hisui-datagrid" data-options="fit:true,border:true" toolbar="#custtb"></table>
	</div>
	<div id="custtb" style="">
		<table class="search-table">
			<tr>
			    <td class="r-label"><label>#(..Get("术语集关键字"))#</label></td>
				<td><input type="text" id="search" class="textbox"></td>
				<td class="r-label"><a class="hisui-linkbutton" style="width:90px" data-options="iconCls:'icon-w-find'" id="c-search">查询</a></td>
			</tr>
		</table>
		<div class="tool-bar-line" style="border-bottom-color:#cccccc; border-bottom-width: 1px; border-bottom-style: dashed;">	</div>
		<table cellspacing="0" cellpadding="0" style="padding-bottom: 7px;padding-top: 7px;">
			<tr>
				<td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-add" id="c-add">新增</a></td>
				<td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-edit" id="c-edit">修改</a></td>
				<td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-cancel" id="c-delete">删除</a></td>
			</tr>
		</table>
	</div>
</div>


<!--字典类型编辑框-->
<div id="ConfigDialog" style="width:405px;height:auto;padding:10px 10px 0px 10px;display:none;">
	<input class="textbox" id="txtId" type="hidden"></input>

	<table class="form-table">
		<tr>
			<td class="form-r-label">
				<label for="cboBGFPType">#(..Get("首页类型"))#</label>
			</td>
			<td>
				<input class="hisui-combobox textbox" id="cboBGFPType" style="width:300px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtBGCode">#(..Get("代码"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtBGCode" style="width:293px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtBGDesc">#(..Get("名称"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtBGDesc" style="width:293px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="cboBGDataType">#(..Get("数据类型"))#</label>
			</td>
			<td>
				<input class="hisui-combobox textbox" id="cboBGDataType" style="width:300px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtBGIsActive">#(..Get("是否有效"))#</label>
			</td>
			<td>
				<input class="hisui-combobox textbox" id="txtBGIsActive" style="width:300px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtBGResume">#(..Get("备注"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtBGResume" style="width:293px;"></input>
			</td>
		</tr>
	</table>
</div>
