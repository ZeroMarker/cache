<!-- 主页面-->
<div class="hisui-layout" data-options="fit:true" style="border:none">
	<div data-options="region:'center'" fit="true" style="text-align:center;border:none">
		<div  class="hisui-panel" fit="true"  style="border:none">
			<table id="gridLabAntibiotic" data-options="toolbar:'#custtb'" border="0"></table>
			<div id="custtb" style="padding:3px" >
				<a class="hisui-linkbutton" id="btnAdd"  data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true"> 新增</a>
				<a class="hisui-linkbutton" id="btnEdit" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"> 修改 </a>
				<a class="hisui-linkbutton" id="btnDelete"  data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true" > 删除 </a>
				<div style="float: right;">
					<input id="search" data-options="prompt:'搜索'" class="hisui-searchbox"/>
				</div>
			</div>
		</div>
	</div>
</div>

<div id="winEdit" style="border:none;overflow:hidden;padding-right:10px;display:none">
	<table class="search-table">
		<tr>
			<td class="r-label">
				<label for="txtAntCode"><font color="red">*</font>代码</label>
			</td>
			<td>
				<input class="textbox" id="txtAntCode" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="txtAntDesc"><font color="red">*</font>名称</label>
			</td>
			<td>
				<input class="textbox" id="txtAntDesc" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label" style="width:80px">
				<label for="cboAntCat"><font color="red">*</font>抗生素分类</label>
			</td>
			<td>
				<input class="hisui-combobox textbox" id="cboAntCat" style="width:267px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="txtWCode">whone码</label>
			</td>
			<td>
				<input class="textbox" id="txtWCode" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="chkActive">是否有效</label>
			</td>
			<td>
				<input class='hisui-checkbox' type="checkbox" id="chkActive" checked/>
			</td>
		</tr>
	</table>
</div>
