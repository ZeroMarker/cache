<csp:content charset="UTF-8">
<SERVER>
s Action=$Get(%request.Data("actiontype",1))

i Action = "GetBatInfo" d
	.S Inci=$Get(%request.Data("Inci",1))
	.d ##class(web.DHCSTM.ItmBatLock).GetBatInfo(Inci)
	
i Action = "Save" d
	.S listData=$Get(%request.Data("listData",1))
	.s ret=##class(web.DHCSTM.ItmBatLock).Save(listData)
	.i ret = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"
</SERVER>