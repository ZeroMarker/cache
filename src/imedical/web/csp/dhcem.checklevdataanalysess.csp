	<div class="hisui-layout" style="" data-options="fit:true">
		<div data-options="region:'north',title:'',split:true,headerCls:'panel-header-gray'" style="height:125px;overflow:hidden;"><!--hxy 2020-02-21 115->125-->
			<div style="margin:10px 10px 10px 10px;">
				<span style="margin-right:5px">开始日期</span><!--hxy-->
				<input id="stDate" style="width:160px" class="hisui-datebox textbox"></input>
				
				<span style="margin-right:5px;margin-left:15px">结束日期</span>
				<input id="endDate" style="width:160px" class="hisui-datebox textbox"></input>
				<td><a href="#" id="search" class="hisui-linkbutton cancel-Btn" iconCls="icon-search-white" style="margin-left:10px">查询</a></td>
			</div>
			
			<div style="height:75px">
				<table id="typeTable" class="typeTable" style="width:100%" cellspacing="0" >
					<tr>
						<td>
							<div class="tdDiv" data-type="ZSDO" style="background:url(../scripts/dhcnewpro/images/nosel.png)">
