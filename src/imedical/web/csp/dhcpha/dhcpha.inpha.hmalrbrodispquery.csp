<!--（草药揭药）揭药查询 csp:dhcpha/dhcpha.inpha.hmalrbrodispquery.csp--> 
<!--（草药揭药）揭药查询 js:pharmacy/inpha/dhcpha.inpha.hmalrbrodispquery.js--> 
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
q 1
</csp:method>
<html lang="zh-CN">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge"/>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<Server language="CACHE" runat="Server">
   	s Application=%request.Application
   </Server>
	<TRAK:HEAD/>
	<DHCPHARMACY:COMMONLIB/>	
	<DHCAPP:PHARBOOTSTRAP/>
	<!--dhcpha js-->
	<script type="text/javascript" src="#(Application)#scripts/pharmacy/inpha/dhcpha.inpha.hmalrbrodispquery.js"></script> 
</head>
<body >
	<div id="dhcpha-query-condition">
		<div class="container-fluid dhcpha-condition-container">
			<div class="row row_style">
				<form class="form-inline"> 
					<!-- 
					<div class="input-group">
						<span for="date-daterange" class="input-group-addon dhcpha-input-group-addon-in"><strong>揭药日期</strong></span>
						<input id="date-daterange" class="form-control input_style" type="text" style="border-right:none;width:14em" placeholder="请选择日期">
						<span  class="input-group-addon span_style" style="cursor:pointer;background-color:#fff;">
							<i class="fa fa-calendar"></i>
						</span>                 
					</div>
					-->
					<div class="input-group">
						<span for="date-start" class="input-group-addon dhcpha-input-group-addon-in"><strong>揭药日期</strong></span>
	                    <input id="date-start" class="form-control input_style" type="text" style="border-right:none;width:6.5em" placeholder="请选择日期">
	                    <span  class="input-group-addon span_style" style="cursor:pointer;background-color:#fff;">
	                          <i class="fa fa-calendar"></i>
	                    </span>  
	               </div>
	               <span><strong>至</strong></span>
	               <div class="input-group">
	                    <input id="date-end" class="form-control input_style" type="text" style="border-right:none;width:6.5em" placeholder="请选择日期">
	                    <span  class="input-group-addon span_style" style="cursor:pointer;background-color:#fff;">
	                          <i class="fa fa-calendar"></i>
	                    </span>                       
	               </div>
					<div class="input-group" style="padding-left:20px">		                   
						<select class="form-control in_select" id="sel-phaward"></select>                   
					</div> 
					<div class="input-group" style="padding-left:20px">
						<input id="txt-regno" class="form-control input_style" type="text" style="width:13em;height:25px;font-size:14px" placeholder="登记号..."> 
					</div>                
					<div class="btn-group dhcpha-btn-right">  
						<button class="btn btn-primary dhcpha-btn-common dhcpha-btn-normal dhcpha-btn-split" type="button" id="btn-find" onclick="javascript:QueryGridBroPre()">查询</button>
						<button class="btn btn-primary dhcpha-btn-common dhcpha-btn-normal dhcpha-btn-split" type="button" id="btn-clear" onclick="javascript:ClearConditions()">清屏</button>
					</div>
				</form>
			</div>
		</div>
	</div> 
	<div class="row dhcpha-row-split" ></div>
	<div class="container-fluid ">
		<div class="row">
			<div>
				<div class="panel div_content">
					<div class="panel-heading"><span class="pal_style"><strong>揭药处方查询</strong></span></div>
					<table id="grid-brodisppres" class="table table-bordered table_style"></table>
					<div id="jqGridPager"></div>  
				</div>
				<div class="panel div_content">
					<div class="panel-heading"><span class="pal_style"><strong>揭药详细信息</strong></span></div>
					<table id="grid-brodispdetail" class="table table-bordered table_style"></table> 
				</div>
			</div>
		</div>
	</div>
</body>
</html>