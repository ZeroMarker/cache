<!DOCTYPE html>
<!--医嘱发药状态查询
	csp:csp/dhcpha/dhcpha.inpha.queryorddispstatus.csp
	js: scripts/pharmacy/inpha/dhcpha.inpha.queryoedispstatus.js
-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(ext.websys.SessionEvents).SessionExpired() q 1 
    q 1
</csp:method>
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>
</HEAD>
</head>

<body>
	<div class="hisui-layout" fit="true">
        <div data-options="region:'center'" border=false  class="dhcpha-hisui-container"> 
            <div class="hisui-layout" fit="true">
                <div data-options="region:'north',title:'住院医嘱发药状态查询 <b>【三种必选条件 ： <医嘱ID+执行日期> ，<登记号+药品+执行日期> ，<执行记录ID> 】</b>',split:true,headerCls:'panel-header-gray',collapsible:false,iconCls:'icon-panel-common-query'" class="dhcpha-north-r1">
                    <table class="dhcpha-north-tbl">
                        <tr>
                            <td>
                                <label for="txtPatno">登记号</label>
                                <input id="txtPatno" class="hisui-validatebox" style="width:100px" />
                            </td>
                       		<td>
                                <label for="cmgIncItm">药品</label>
                                <input id="cmgIncItm" type="text" />
                            </td>
                            <td>
                                <label for="txtOeori">医嘱ID</label>
                                <input id="txtOeori" class="hisui-validatebox" style="width:100px;"/>
                            </td>
                            <td>
                                <label for="dateStart">执行日期</label>
                                <input id="dateStart" class="hisui-datebox dhcpha-date" type="text" style="width:125px;"/>
                            </td>
                       		<td>
                                <label for="txtOrdExe">执行记录ID</label>
                                <input id="txtOrdExe" class="hisui-validatebox" style="width:125px;" />
                            </td>
                            <td>
                                <a id="btnFind" class="hisui-linkbutton" data-options="plain:false,iconCls:'icon-w-find'">查询</a>
                            </td>
                            <td>
                                <a id="btnClear" class="hisui-linkbutton" data-options="plain:false,iconCls:'icon-w-clean'">清屏</a>
                            </td>
                        </tr>
                    </table>
                </div>
                <div data-options="region:'center'" border="false" >
            		<div class="hisui-layout" fit="true" border="false">
                		<div data-options="region:'center',title:'医嘱明细<b>【有单元格为红色时不能发药】</b>',split:true,headerCls:'panel-header-gray',iconCls:'icon-panel-common'">
                    		<table id="gridOrdItem"></table>
                		</div>
           			</div>
        		</div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.plugins.js" ></script>	
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/inpha/dhcpha.inpha.queryoedispstatus.js"></script>
</body>
</html>
