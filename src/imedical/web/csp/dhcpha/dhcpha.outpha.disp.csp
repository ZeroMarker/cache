	<script type="text/javascript" src="#(Application)#scripts/pharmacy/outpha/dhcpha.outpha.disp.js" ></script>
	
	
 		
	<server>
		s ReadAccExpEncrypt=##class(websys.Page).Encrypt($lb("web.UDHCAccManageCLSIF.getaccinfofromcardno"))
		s GetCardTypeEncrypt=##class(websys.Page).Encrypt($lb("DHCDoc.OPDoc.PatientList.GessCardType"))
		s ReadCardTypeEncrypt=##class(websys.Page).Encrypt($lb("web.UDHCOPOtherLB.ReadCardTypeDefineListBroker"))
	</server>
</head>
<STYLE>
.prescinfo{
	position: absolute;
	top: 1px;
	right:1px;
}
</STYLE>
<body style="overflow:hidden;">
	<div id="dhcpha-query-condition">
		<div style="width:0px;height:0px">
			<input id='ReadAccExpEncrypt' type='hidden' value='#(ReadAccExpEncrypt)#'></input>
			<input id='GetCardTypeEncrypt' type='hidden' value='#(GetCardTypeEncrypt)#'></input>
			<input id='ReadCardTypeEncrypt' type='hidden' value='#(ReadCardTypeEncrypt)#'></input>
			<server>
			 	d ##class(web.UDHCCardCommLinkRegister).GetCardHardJSFunction()
			</server>
		</div>
		<div class="container-fluid dhcpha-condition-container">
		    <div class="row">
		    	<div class="col-lg-6 col-md-6 col-sm-6">
		    	    <button class="btn btn-default btn_button btn_important" id="btn-fy"><img src="#(Application)#scripts/pharmacy/images/disp-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("发药"))#</strong></button>
				    <button class="btn btn-default btn_button btn_refuse" id="btn-reffy"><img src="#(Application)#scripts/pharmacy/images/refdisp-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("拒绝发药"))#</strong></button>
				   	<button class="btn btn-default btn_button btn_common" id="btn-cancelreffy"><img src="#(Application)#scripts/pharmacy/images/return-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("撤消拒绝"))#</strong></button>
				    <button class="btn btn-default btn_button btn_common" id="btn-allfy"><img src="#(Application)#scripts/pharmacy/images/dispall-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("全发"))#</strong></button>
				    <button class="btn btn-default btn_button btn_common dhcpha-col-border-left" id="btn-print"><img src="#(Application)#scripts/pharmacy/images/reprint-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("打印处方"))#</strong></button>
					<button class="btn btn-default btn_button btn_common" id="btn-printlabel"><img src="#(Application)#scripts/pharmacy/images/printlabel-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("重打标签"))#</strong></button>
				    <button class="btn btn-default btn_button btn_common" id="btn-clear"><img src="#(Application)#scripts/pharmacy/images/clear-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("清屏"))#</strong></button>
		    		<button class="btn btn-default btn_button btn_common dhcpha-col-border-left" id="btn-redir-return"><img src="#(Application)#scripts/pharmacy/images/return-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("退药"))#</strong></button>
		    		<button class="btn btn-default btn_button btn_common dhcpha-col-border-left" id="btn-cardpay"><img src="#(Application)#scripts/pharmacy/images/cardpay-default.png" class="dhcpha-btn-image"><br><strong>#(..Get("卡消费"))#</strong></button>
		    	</div>
		    	<div class="col-lg-6 col-md-6 col-sm-6">
					<div class="dhcpha-window-right">
						<span style="margin-left:15px;">#(..Get("当前药房"))#：<lable id="currentctloc"></lable></span>
						<span style="margin-left:15px;">#(..Get("当前窗口"))#：<lable id="currentwin"></lable></span>
						<span style="margin-left:15px;">#(..Get("配药人"))#：<lable id="currentpyuser"></lable></span>
						<div class="btn-group" style="margin-left:20px;">  
						<span class="fa fa-exchange dhcpha-btn-icon-common dhcpha-btn-icon-normal"></span>
						<button class="btn btn-primary  dhcpha-icon-btn-common dhcpha-icon-btn-normal" type="button" id="btn-change">#(..Get("切换窗口"))#</button>
						</div>
					</div>
		    	</div>
		    </div>
		 </div>
		 <div class="row dhcpha-row-split-all" ></div>
		 <div class="container-fluid dhcpha-condition-container">
		 	<div class="row row_style">
		 		<form class="form-inline">
	               
			       <div class="input-group">
	                    <span for="date-start" class="input-group-addon dhcpha-input-group-addon-in"><strong>#(..Get("收费日期"))#</strong></span>
	                    <input id="date-start" class="form-control input_style" type="text" style="border-right:none;width:6em" placeholder='#(..Get("请选择日期"))#'>
	                    <span  class="input-group-addon span_style" style="cursor:pointer;background-color:#fff;">
	                          <i class="fa fa-calendar"></i>
	                    </span>  
	               </div>
	               <span><strong>#(..Get("至"))#</strong></span>
	               <div class="input-group">
	                    <input id="date-end" class="form-control input_style" type="text" style="border-right:none;width:6em" placeholder='#(..Get("请选择日期"))#'>
	                    <span  class="input-group-addon span_style" style="cursor:pointer;background-color:#fff;">
	                          <i class="fa fa-calendar"></i>
	                    </span>                       
	               </div>
					<!--div class="form-group dhcpha-col-split">
				    </div-->				
	               <div class="input-group">	                    
	                	<select class="form-control in_select" id="sel-basicloc">				              
			            </select>                   
	                </div>
	                <div class="input-group">	                    
	                	<select class="form-control in_select" id="sel-special">				              
			            </select>                   
	                </div>
					<!--div class="form-group dhcpha-col-split">
				    </div-->
	               
					<input class="form-control input_style" type="text" id="txt-patno" style="width:8em" placeholder='#(..Get("请输入登记号"))#'>
					<input class="form-control input_style" type="text" id="txt-cardno" style="width:8em" placeholder='#(..Get("刷卡/请输入卡号"))#'>
	                <input class="form-control input_style" type="hidden" id="txt-cardtype">
		           	<div class="btn-group ">  
	                    <span class="fa fa-credit-card dhcpha-btn-icon-common dhcpha-btn-icon-normal"></span>
			            <button class="btn btn-primary  dhcpha-icon-btn-common dhcpha-icon-btn-normal" type="button" id="btn-readcard">#(..Get("读卡"))#</button>
	                </div>
	                <div class="input-group" >                                     
						<input type="checkbox" id="chk-disp" />
						<lable for="chk-disp"><strong>#(..Get("已发药"))#</strong></lable>	
					</div>
					<div class="input-group" >                                     
						<input type="checkbox" id="chk-recover" />
						<lable for="chk-recover"><strong>#(..Get("应急恢复"))#</strong></lable>	
					</div>
					<input class="form-control input_style" type="text" id="txt-recoverBarCode" style="width:8em" placeholder='#(..Get("请输入扫描码"))#'>

		            <div class="btn-group dhcpha-btn-right">  
	                    <span class="fa fa-search dhcpha-btn-icon-common dhcpha-btn-icon-normal"></span>
			            <button class="btn btn-primary  dhcpha-icon-btn-common dhcpha-icon-btn-normal" type="button" id="btn-find">#(..Get("查询"))#</button>
	                </div> 
	  
				</form>
		 	</div>			
		 </div> 
				
		 <div class="row dhcpha-row-split" ></div>
	 </div>
  	 <div class="container-fluid ">
	 	<div class="row">    
		 	<div class="col-lg-9 col-md-9 col-sm-9" style="padding-right:0px;padding-left:0px" id="col-left">
				<div class="panel div_content">
				   <div class="panel-heading"><span class="pal_style"><strong>#(..Get("发药列表"))#</strong></span></div>
				        <!--div class="panel-body"-->
				        <table id="grid-disp" class="table table-bordered table_style">
				        </table>
				        <div id="jqGridPager"></div>
				        <!--/div-->  
				</div>
				<div class="panel div_content">
				   <div class="panel-heading"><span class="pal_style"><strong>#(..Get("药品详细列表"))#</strong></span><i id="a-changegrid" class="fa fa-exchange dhcpha-btn-fa" style="position: absolute;right: 30px;cursor: pointer;">#(..Get("切换样式"))#</i></div>
					  <div class="js-pha-orders-preview">
					  	<table id="grid-dispdetail" class="table table-bordered table_style">
					  		</table>
					  </div>
					  <div class="js-pha-orders-preview" id="pha-orders-preview" style="display:none;overflow:auto">
					  
					  </div>
				</div>
			</div>
			 <div class="col-lg-3 col-md-3 col-sm-3 dhcpha-col dhcpha-col-border-left" id="col-right">
				  <div class="panel div_content">
					  <div class="panel-heading"><span class="pal_style"><strong>#(..Get("待发药列表"))#</strong></span><a id="a-refresh" class="dhcpha-btn-fa" style="position: absolute;right: 30px;cursor: pointer;"><i class="fa fa-refresh"></i></a></div>
						  <table id="grid-waitfy" class="table table-bordered table_style">
						  </table>
		          </div>
		     </div>
		</div>
	</div>
			 
<!-- system modal start -->
<csp:Include Page="dhcpha.outpha.disp.window.csp">
<csp:Include Page="dhcpha.pha.permission.csp">
<csp:Include Page="dhcpha.outpha.presc.csp">
  <!-- system modal end -->
<csp:Include Page="dhcpha.pha.style.csp">
<csp:Include Page="#(Application)#csp/pha.op.v4.orders.csp">
