<div id="FormWindow" class="easyui-layout" style="width:900px;height:600px;display:none;">
	<div data-options="region:'center',border:false,split:true" style="">
		<div class="easyui-layout" data-options="fit:true,border:false">  
            <div data-options="region:'center',split:true,border:false">
            	<div class="easyui-panel" title="诊断基本信息"  data-options="fit:true"> 
            		<table id="DiagForm1">
					     <tr>
				    		<td class="td_label"><label>当前诊断</label></td>
				    		<td>
				    		   <input class="easyui-validatebox" type="text" id="Form_DiagDesc"  disabled="disabled" style="color:red;background:transparent;border:0px;width:100%;"/>
				    		</td>
				    		<td>
				    			<a id="ChangeDiagDisplayDesc" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-save'">切换诊断</a>
				    		</td>
				    	</tr>
				    	<tr>
				    		<td class="td_label"><label>诊断备注</label></td>
				    		<td colspan="2"><input class="easyui-validatebox" type="text" id="Form_DiagNotes" style="width:99.8%;"></td>
				    	</tr>
				    	<tr>
				    		<td class="td_label"><label>诊断状态</label></td>
				    		<td colspan="2"><label><input name="DiagStatus" type="radio" value="QZ" />确诊</label><label><input name="DiagStatus" type="radio" value="DZ" />待诊</label><label><input name="DiagStatus" type="radio" value="YZ" />疑诊</label></td>
				    	</tr>
				    	<tr>
				    		<td class="td_label"><label>诊断类型</label></td>
				    		<td colspan="2">
				    			<input id="Form_DiagType" class="easyui-combobox" name="Form_DiagType">
				    			<input id="Form_MainDiaType" type="checkbox" class="easyui-validatebox" style="vertical-align:middle;" />主诊断
				    		</td>
				    	</tr>
				    	<csp:if condition='NeedStolicMast="1"'>
				    		<tr>
					    		<td class="td_label"><label>血&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp压</label></td>
				    			<td colspan="2">
		              	 			<input class="easyui-validatebox" type="text" id="Form_BPDiastolic" style="width:70px;"/>
		              	 			/<input class="easyui-validatebox" type="text" id="Form_BPSystolic" style="width:70px;"/>mmHg
		          	 			</td>
				    	    </tr>
				    	</csp:if>
			    	</table>
            	</div>
            </div>   
            <div data-options="region:'south',split:true,border:false" style="height:400px;">
            	<div class="easyui-panel" title="修改历史"  data-options="fit:true"> 
				     <table id="tabDiagPropertyChangeLog"></table> 
				</div>
            </div>   
        </div>
	</div>   
	<div data-options="region:'east',split:true" style="width:500px;"> 
	    <div class="easyui-layout" data-options="fit:true,border:false">  
		    <div data-options="region:'north',split:true,border:false" style="height:510px;"> 
		    	<div id="" class="easyui-panel" title="诊断属性" style="" data-options="border:false,fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false"> 
      				<div id="" style="width:450px;">
	      				<table id="DiagForm" class="stripe" style="">
					    	<tr id="formTemplate" style="display:none;">
					    		<td nowrap style="white-space:nowrap;word-break:nowrap" class="td_label"><label for="email">分型1</label></td>
					    	</tr>
					    </table>
					    <input id="SelDiagRowId" type="hidden"></input>
					    <input id="SelTKBRowId" type="hidden"></input>
				    </div> 
     			</div> 
				
		    </div> 
		    <div data-options="region:'center',split:true,border:false">
		    	<div id="formbtn" style="">
				    <a id="FormSure" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-save'">确定</a>
				    <a id="FormClose" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-remove'">关闭</a>
				    <a id="FormLookUserDKB" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-find-custom'">诊断知识库</a>
			    </div>
		    </div> 
	    </div>
    </div>
</div>
<div id="ChangeDiagDescWindow" class="" style="display:none;">
	<div class="easyui-layout" data-options="fit:true,border:false">
		<div data-options="region:'center',split:true" > 
			<input id="Form_DiagDisplayNameList" class="easyui-combobox" name="Form_DiagDisplayNameList">
			<a id="BtnFormSure" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-save'">确定</a>
	        <a id="BtnFormClose" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-remove'">关闭</a>
		</div>
	</div>
</div>