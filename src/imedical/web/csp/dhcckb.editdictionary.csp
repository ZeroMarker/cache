<html >
<head>
<title>临床知识库字典添加属性维护</title>
<HISUI/>
<DHCCKB/>
	<link rel="stylesheet" href="../scripts/dhcnewpro/dhcckb/css/common.css"><!-- 禁止复制 -->
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/component.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/editdictionary.js"></script>
	<style type="text/css">
		.datagrid-row-selected {
	  		background: #ADFAFC;
		}
		.panel-header, .panel-body {
		    border-color: #ccc;
		}
   </style>
</head>

<body >	
	<div class="hisui-layout"  fit="true">
		<!--查询条件区-->
		<div data-options="region:'west',title:''"  style="width:500px;padding:10px 0px 10px 10px;" border=0>					
			<div class="hisui-panel" data-options="title:'字典列表',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0;border-radius:0px;">	
				<div id="griddiv" style="height:100%;">
					<div id="diclist" toolbar="#dictb"></div>
				</div>
				<div id="treediv" style="display:none;">
					<div id="queryForm" style="margin:10 0 10 10;">
						<table cellspacing="0" cellpadding="0" >
							<tr>
								<td>字典<input id="treeType" href="#" class="hisui-combobox" prompt='请输入代码/名称/拼音码'/></td>								
								<td style="padding-left:5px;"><input id="treequery" href="#" class="hisui-searchbox" style="width:155px;" prompt='请输入代码/名称/拼音码'/> </td>		
								<td ><a style="margin-left:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="treereset">重置</a> </td>					
							</tr>
						</table>
					</div>
					<div  style="margin-top:10px;border-bottom-color:#cccccc;border-bottom-width:1px; border-bottom-style:dashed;">	</div>
					<div>
						<table cellspacing="0" cellpadding="0" >
							<tr>
				  				<td><a id="updTreeDic" href="#" class="hisui-linkbutton" plain="true" iconCls="icon-add">修改归属字典</a></td>
						    </tr>
						</table>
					</div>
					<ul id="dictree"></ul>
				</div>
	    	</div>						
		</div>
		
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray'" style="padding:10px 10px 0px 10px;">
			<div class="hisui-panel" data-options="title:'属性维护<font color=red font-weight=bold font-size=12pt >【双击进行编辑】</font>',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0;border-radius:0px;">	
				<div id="subtoolbar" style="padding:0px 0px 0px 11px; display:none">							
					<!-- <input id="subQueryCode" href="#" class="hisui-searchbox" prompt='请输入代码/名称/拼音码'/>	 -->
					<td ><a style="margin-left:10px; display:none" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="subreset">重置</a> </td>
					<table cellspacing="0" cellpadding="0" >
						<tr>
					      <td><a class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true" id="subinsert">新增</a></td>
					      <td><a class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true" id="subsave">保存</a> </td>
					      <td><a class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true" id="subdel">删除</a></td>	
					    </tr>
					</table>
				</div>
				<div id="linkattrlist" border="false"></div>
	    	</div>		
		</div> 
	</div>	
<div id="dictb">	
	<div id="queryForm" style="margin:10 0 10 10;">
		<table cellspacing="0" cellpadding="0" >
			<tr>
				<td>字典<input style="width:250px;margin-left:10px" id="dicType" href="#" class="hisui-combobox" prompt='请输入代码/名称/拼音码'/></td>	
				<td ><a style="margin-left:40px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" onclick = 'QueryDicList()'>查询</a> </td>	
			</tr>
			<tr>
				<td style="padding-left:28px"><input id="queryCode" href="#" class="hisui-searchbox" style="width:250px;" prompt='请输入代码/名称/拼音码'/> </td>
				<td ><a style="margin-left:40px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="reset">重置</a> </td>					

			</tr>
		</table>
	</div>
	<div  style="margin-top:10px;border-bottom-color:#cccccc;border-bottom-width:1px; border-bottom-style:dashed;">	</div>
	<div>
		<table cellspacing="0" cellpadding="0" >
			<tr>
  				<td><a id="updDic" href="#" class="hisui-linkbutton" plain="true" iconCls="icon-add">修改归属字典</a></td>
		    </tr>
		</table>
	</div>
</div>
</body>
	
<!-- 属性值编辑弹窗 -->	
<div id="myWin" style="display:none;">  

	<!--textArea方式-->
	<div id="mytextarea" class="div-common" style="display:none;">
		<div data-options="region:'north',title:'',border:false,collapsible:false" style='height:200px;'>
			<div class="hisui-layout" fit="true" border="false">
				<div id="linklist"></div>
			</div>
		</div>
		<div data-options="region:'center',title:'',border:false,collapsible:false">
			<input id="linkID" type="text" style="display:none"></input>
			<textarea id="myarea" type="text" border="1" class="hisui-validatebox" placeholder="请编辑要保存的内容..." style="width:94%;height:80px;resize:none;overflow:hidden;margin:10px;" data-options="required:true"></textarea>
		</div>
	</div>
	
	<!-- datagrid和tree方式(使用treegrid)-->
	<div id="mytree" class="div-common" style="height:290px;padding:10px;display:none;">   
		<div class="hisui-panel" data-options="title:'属性列表',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0;border-radius:0px;">	
			<div id="mygrid" toolbar="#treegridbar" data-options="fit:true" border="false"></div>
	    </div>	
	</div>
	
	<!-- datagrid -->
	<div id="mydatagrid" class="div-common" style="height:290px;padding:10px;display:none;">   
		<div class="hisui-panel" data-options="title:'属性列表',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0;border-radius:0px;">	
			<div id="addgrid" toolbar="#gridbar" data-options="fit:true" border="false"></div>
	    </div>	
	</div>
	
	<!-- CheckBox形式-->
	<div id="mycheckbox" class="div-common" style="width:360px;height:400px;padding:10px;display:none;"> 
		
	</div>
	
	<!--treegrid工具栏-->
    <div id="treegridbar" style="display:none;">
		<table fit="true">
		    <tr>
		        <td class="tdlabel">检索</td>
		        <td><input id="myChecktreeDesc" href="#" class="hisui-searchbox" prompt='请输入代码/名称/拼音码'/></td>
		    </tr>                    
		 </table>                     	
    </div> 
    
	<!--grid工具栏-->
    <div id="gridbar" style="display:none;">
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true" id="add_btn">新增</a>
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true" id="del_btn">删除</a>    	
    </div> 
</div>
<!-- 修改字典归属窗口 -->
<div id="UpdDicWin" style="padding:0px 1px;display:none;">   
	<div style="padding:8px;">
		<div  style="margin:12px 10px;">
			<label style="margin:0px 10px 0px 50px;">字典</label><input id="dicDesc" class="hisui-combobox" />
		</div>
    </div>
    <div style="padding:2px;">
    	<a style="margin-left:67px;" href="#" class="hisui-linkbutton" iconCls="icon-w-save" onclick="saveDiction()">保存</a>
		<a style="margin-left:42px;" href="#" class="hisui-linkbutton" iconCls="icon-w-cancel" onclick="closeDicWin()">取消</a>
    </div>
</div>
</html>
