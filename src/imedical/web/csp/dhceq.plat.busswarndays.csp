<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>
<SCRIPT language="Cache" RUNAT="SERVER">
	s SourceType=$Get(%request.Data("SourceType",1))
	s SubType=$Get(%request.Data("SubType",1))
	s SourceID=$Get(%request.Data("SourceID",1))
	s WarnDay=$Get(%request.Data("WarnDay",1))
	s ReadOnly=$Get(%request.Data("ReadOnly",1))
	s pName=$Get(%request.Data("Name",1))
</Script>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="eqtitle:'maintitle',fit:true,title:'预警设置'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:dashed 0px #ccc;padding:5px 10px;">
					<div class="eq-table">
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label for="Name">名称</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Name" style="width:148px;" disabled="true"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-input"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-input"></div>
						</div>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cBWDSourceType_Desc" for="BWDSourceType_Desc">预警类型</label></div>
							<!--div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'EM.L.Config.FromType'" id="CType_Desc"></div-->
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BWDSourceType_Desc" disabled="true"></div>
							<div class="eq-table-td eq-table-label"><label id="cBWDSubType_Desc" for="BWDSubType_Desc">业务类型</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BWDSubType_Desc" disabled="true"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-input"></div>
						</div>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cBWDWarnDay" for="BWDWarnDay">预警日期</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="BWDWarnDay"></div>
							<div class="eq-table-td eq-table-label"><label for="BWDEndDay">结束预警日期</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="BWDEndDay" disabled="true"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-input"></div>
						</div>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cBWDWarnDaysNum" for="BWDWarnDaysNum">触发预警天数</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BWDWarnDaysNum" style="width:148px;"></div>
							<div class="eq-table-td eq-table-label"><label for="BWDOverDueDaysNum">终止警告天数</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BWDOverDueDaysNum" style="width:148px;"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-input"></div>
						</div>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label for="BWDRemark">备注</label></div>
							<div class="eq-table-td"><textarea id="BWDRemark" style="width:151px"></textarea></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-input"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-input"></div>
						</div>
					</div>
				</div>
				<div data-options="region:'south',border:false" style="border-bottom:dashed 0px #ccc;height:50px;">
			        <div style="margin:0px auto;fit:true;padding-top:10px;vertical-align:middle;text-align:center;">
		            	<a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">保存</a>
		                <a id="BDelete" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">删除</a>
			        </div>
			    </div>
			</div>
		</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="BWDSourceType" value=#(SourceType)#>
			<INPUT type="hidden" id="BWDSubType" value=#(SubType)#>
			<INPUT type="hidden" id="BWDBussID" value=#(SourceID)#>
			<INPUT type="hidden" id="pWarnDay" value=#(WarnDay)#>
			<INPUT type="hidden" id="ReadOnly" value=#(ReadOnly)#>
			<INPUT type="hidden" id="pName" value=#(pName)#>
			
			<INPUT type="hidden" id="BWDRowID">
			<INPUT type="hidden" id="BWDHold1">
			<INPUT type="hidden" id="BWDHold2">
			<INPUT type="hidden" id="BWDHold3">
			<INPUT type="hidden" id="BWDHold4">
			<INPUT type="hidden" id="BWDHold5">
		</form>
	</div>
	<script type="text/javascript" src="../scripts/dhceq/plat/busswarndays.js"></script>
</body>
</html>
