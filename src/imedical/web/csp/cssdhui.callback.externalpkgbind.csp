<!--回收-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<SERVER>
	d ##class(web.DHCXMLPConfig).LODOPInit()  //Lodop打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:0px">
		<div class="hisui-panel" data-options="title:'手术排班数据',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="CondTB"> 
				<table class="Condition" id="MainCondition">
					<tr>
						<td>手术日期</td>
						<td><input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq" style="width:120px"></td>
						<td>申请科室</td>
						<td><input id="ReqLoc" name="fromLocDr" class="textbox hisui-combobox" style="width:140px"></td>
						<td><a href="#" id="ImportOP" class="hisui-linkbutton"  style="width:150px">导入排班数据</a></td>
						<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:90px">查询</a></td>
					</tr>
				</table>		
			</div>
				<table id="MainList"></table>
		</div>
	</div>
		<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:500px;padding-left:10px">	
			<div class="hisui-panel" data-options="title:'外来器械明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="IpuntTB" > 
				<div>
					<table class="Condition" id="DetailCondition">
						<tr>
						<td>包标牌编码</td>
						<td><input id="MainDr" name="MainDr" class="textbox" style="width:120px" hidden><input class="hisui-numberbox textbox" id="BarCode" name="BarCode"  style="width:130px" ></td>
						<td><a href="#" id="QueryBTExt" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:90px">查询</a></td>
						</tr>
					</table>
				</div>
			</div>
			<table id="ItemList"></table>
		</div>
		</div>
<script type="text/javascript" src="../scripts/cssd/CallBack/ForeignPkgBind.js"></script>
</body>
</html>