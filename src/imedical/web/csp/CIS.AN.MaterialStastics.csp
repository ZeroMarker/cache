<!DOCTYPE HTML>
<HTML XMLNS=TRAK>
<head>
<meta charset="UTF-8">
<csp:content charset="UTF-8">
<!--<dhcclinic:easyui></dhcclinic:easyui>-->
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI></HISUI>
<dhcclinic:head></dhcclinic:head>

<script type="text/javascript" src="../service/dhcanop/js/json2.js"></script>
<script type="text/javascript" src="../service/dhcanop/js/LodopFuncs.js"></script>
<script type="text/javascript" src="../service/dhcanop/js/CIS.AN.MaterialStastics.js"></script>
<link rel="stylesheet" type="text/css" href="../service/dhcanop/css/operationlist.css" />
<style type="text/css">
*{
	margin:0;
	padding:0;
}
</style>
</head>

<body class="hisui-layout">
	<div id="dataTools">
    	<form id="conditionForm">
    		<div class="form-row-group">
     			<div class="form-rows">
     				<div class="form-row" style="margin:9px 10px 9px 8px;">
     					<div class="form-title-right4">开始日期</div>
		    			<span class="form-item">
		    				<input type="text" id="DateFrom" name="DateFrom" class="hisui-datebox textbox"/>
		    			</span>
     					<div class="form-title-right4">结束日期</div>
		    			<span class="form-item">
		    				<input type="text" id="DateTo" name="DateTo" class="hisui-datebox textbox"/>
		    			</span>
		    			<span style="margin-left:10px;"><input class='hisui-checkbox' type="checkbox" id="IsShowDetail" label="显示明细">
	     				</span>
		    			<span class="form-btn">
		    				<a id="btnQuery" class="hisui-linkbutton" href="#" data-options="iconCls:'icon-w-find'" style="margin-left:20px">查询</a>
		    			</span>
		    			<span class="form-btn">
    						<a id="btnPrint" class="hisui-linkbutton" href="#" data-options="iconCls:'icon-w-print'">打印</a>
		    			</span>
    				</div>
    			</div>
    		</div>
    	</form>
    </div>
    <div data-options="region:'center',title:'',headerCls:'panel-header-gray',border:false ">
    	<table id="MaterialData"></table>
	</div>
</body>
</html>
