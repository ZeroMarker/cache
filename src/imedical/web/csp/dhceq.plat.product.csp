			<div class="hisui-layout" data-options="fit:true,border:false">
				<!--MODIFY BY LMM 2020-03-30 LMM0068-->
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #ccc;padding:5px 0 10px 0;">
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label"><label for="PDesc">设备名称</label></div>   <!--MODIFY BY MWZ 2020-04-09 MWZ0033-->
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="PDesc"></div>
						<div class="eq-table-td eq-table-label"><label for="PAssetItemDR_AIDesc">设备项</label></div> 	<!--MODIFY BY MWZ 2020-4-09 MWZ0033-->
