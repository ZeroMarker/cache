<!DOCTYPE html>
<!--药房药库-处方点评-抗菌药专项点评
	csp:pha.prc.v2.create.antibacter.csp
	js: pha/prc/v2/create.antibacter.js
-->
<html>
<!-- 验证session过期 -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
</csp:method>

<head>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>
    </TRAK:HEAD>
    <HISUI />
    <PHALIBV1 />
</head>

<body>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',border:false" class="pha-body">
            <div class="hisui-layout" fit="true">
				<div data-options="region:'north',height:95,border:false,split:true">
                    <div class="hisui-panel" title="文件导入生成点评单"
                        data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true,bodyCls:''" >
                        <div class="pha-row">							
							<div class="pha-col" >
								<label for="conFile" >文件选择</label>
							</div>
							<div class="pha-col">
								<input id='conFileBox' data-pha />
							</div>
							<a  id='btnFileBox' class="hisui-linkbutton">导入</a>
							<div class="pha-col">
								<a class="hisui-linkbutton" data-options="iconCls:'icon-w-download'" id="btnDownLoad">导入模板下载</a> 
							</div>
							<div class="pha-col">
				                 点评单号
				            </div>
				            <div class="pha-col">
				                <input id="importConComNo" class='hisui-validatebox' disabled="true" data-pha="class:'hisui-validatebox'">
				            </div>
				        </div>
                   </div>
                </div>
				
				<div data-options="region:'center',border:false">
                    <div class="hisui-panel" title="手工录入生成点评单" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true,bodyCls:''">
						<div class="hisui-layout" fit="true">
							<div data-options="region:'center',border:false" style="padding:10px;padding-bottom:0px;">
								<div class="hisui-panel" title="统计处方"  data-options="headerCls:'panel-header-card-gray',fit:true" style="padding-top:10px;overflow:hidden">
									<div class="pha-row">
										<div class="pha-col" >
											<label for="conStartDate">开始日期</label>
										</div>
										<div class="pha-col">
											<input id='conStartDate' data-pha />
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conDocCent">医生比例</label>
										</div>
										<div class="pha-col">
											<input id="conDocCent" class='hisui-validatebox' style="width:153px;" data-pha="class:'hisui-validatebox'">
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="conPresCent">处方张数</label>
										</div>
										<div class="pha-col">
											<input id="conPresCent" class='hisui-validatebox' style="width:153px;" data-pha="class:'hisui-validatebox'" >
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<label for="chkAllFlag">不足则全抽取</label>
										</div>
										<div class="pha-col" style="margin-left:-8px;">
											<input id="chkAllFlag" type="checkbox" class="hisui-checkbox">
										</div>
										<div class="pha-col" style="margin-left:154px;">
											 抗菌药物级别
										</div>
										<div class="pha-col">
											<input id="conMultiAntDrugLevel" class='hisui-validatebox' data-pha="class:'hisui-validatebox'">
										</div>
									</div>
									<div class="pha-row">
										<div class="pha-col">
											<label for="conEndDate">截止日期</label>
										</div>
										<div class="pha-col">
											<input id='conEndDate' data-pha />
										</div>
									</div>									
									<div class="pha-row" style="margin-top:20px;">
										<div class="pha-col">
											<a class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:160px;margin-left:348px;" id="btnQuery">统计处方总数</a> 
										</div>
										<div class="pha-col" style="margin-left:42px;">
											 处方总数
										</div>
										<div class="pha-col">
											<input id="conPresNum" class='hisui-validatebox' disabled="true" style="width:155px;" data-pha="class:'hisui-validatebox'">
										</div>
										<div class="pha-col" style="margin-left:42px;">
											<a class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="btnClean">清屏</a> 
										</div>
									</div>	
								</div>	
							</div>
						
							<div data-options="region:'south',height:250,border:false,split:false" style="padding:0px 10px;">
								<div class="hisui-panel" title="生成点评单" data-options="headerCls:'panel-header-card-gray',fit:true" style="padding-top:10px;overflow:hidden">	
									<div style="height:205px">	
										<div class="pha-row">
											<div class="pha-col">
												<a class="hisui-linkbutton" data-options="iconCls:'icon-w-save'" style="margin-left:348px;width:159px;" id="btnSave">抽取处方样本</a> 
											</div>
											<div class="pha-col" style="margin-left:42px;">
												 点评单号
											</div>
											<div class="pha-col">
												<input id="conComNo" class='hisui-validatebox' disabled="true" style="width:155px;" data-pha="class:'hisui-validatebox'">
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>     
                    </div>
                </div>
            </div>
        </div>
    </div>
	<!-- 导入excel文件读取插件 -->
    <script type="text/javascript" src="../scripts/pha/plugins/xlsx/xlsx.full.min.js"></script>
    <!-- 本页面js -->
    <script type="text/javascript" src="../scripts/pha/prc/com/v2/js/store.js"></script>
    <script type="text/javascript" src="../scripts/pha/prc/v2/create.antibacter.js"></script>
</body>

</html>