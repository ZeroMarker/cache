<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	
<server>
s AuditID=%request.Get("AuditID")
s SplitType=%request.Get("SplitType")
s ADMType=%request.Get("ADMType")
s GIADM=%request.Get("GIADM")
</server>
<script type="text/javascript">
var AuditID="#(AuditID)#"
var SplitType="#(SplitType)#"
var ADMType="#(ADMType)#"
var GIADM="#(GIADM)#"
if(ADMType=="I"){var ItemNameTitle="项目名称";}
if(ADMType=="G"){var ItemNameTitle="状态";}
//alert(AuditID+"^"+SplitType+"^"+ADMType+"^"+GIADM)
</script> 	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 20px;
}

</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div id="layoutcenter" data-options="region:'center',border:false,collapsible:false" style="padding:10px;">
		<div class="hisui-panel" fit="true" data-options="title:'拆分明细',headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-radius:0 0 4px 4px;">
		 	<div class="hisui-layout" fit="true">
		 		<div data-options="region:'north',border:false,collapsible:false" style="height:93px;border-bottom:1px solid #C0C0C0;">
		 			<table class="search-table">
		 				<tr>
		 					<td class="r-label"><label for="ARCIMDesc">医嘱</label></td>
          					<td>
            					<input class="hisui-combogrid" type="text" Id="ARCIMDesc">
         					</td>
         					<td class="r-label"><label for="SplitType">拆分类型</label></td>
          					<td>
            					<input class="hisui-combobox" type="text" Id="SplitType">
         					</td>
		 					
							<td class="r-label">
								<input class='hisui-checkbox' type="checkbox" Id="SelectR"  disabled/><label for="SelectR">全选登记</label>
         			 		</td>
         			 		<td class="r-label">
								<a id="BFind" href="#"  class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
							</td>
						</tr>
		 				<tr>
		 					<td class="r-label"><label for="SetDesc">套餐</label></td>
          					<td>
            					<input class="hisui-combogrid" type="text" Id="SetDesc">
         					</td>
         					<td class="r-label"><label for="LName">姓名</label></td>
							<td>
								<input style="width:140px" class="textbox" id="Name">
							</td>
							<td class="r-label">
								<input class='hisui-checkbox' type="checkbox" Id="SelectA" disabled/><label for="SelectA">全选到达</label>
         			 		</td>
         			 		<td><input type="hidden" id="AuditID"/></td>
         			 		<td><input type="hidden" id="ToAuditID"/></td>
						</tr>
 					</table>
		 		</div>
		 		 <div data-options="region:'center',split:true,headerCls:'panel-header-gray'" border="false">
					<table data-options="fit:true" id="FeeListLeftGrid" border="false"></table>
				</div>
		 		
			</div> 
		</div>
	</div>
	<div id="layouteast" data-options="region:'east',border:false" style="width:730px;border:0;">
		<div class="hisui-layout" fit="true">
			<div data-options="region:'center',border:false" style="padding:10px 10px 10px 0;border:0;">

				<div class="hisui-panel" fit="true" data-options="title:'操作',headerCls:'panel-header-gray',iconCls:'icon-edit'" style="height:100%;border:1px solid #ccc;border-radius:0 0 4px 4px;">
					<div class="hisui-layout" fit="true">
		 				<div data-options="region:'center',border:false,collapsible:false">
		 					<table class="search-table">
		 						<tr></tr>
		 						<tr></tr>
		 						<tr></tr>
		 					   <tr>
		 					   		<td class="r-label">
										<a id="MoveToR" href="#"  class="hisui-linkbutton" data-options="iconCls:'icon-w-arrow-right'">移动选择项</a>
									</td>
		 					   </tr>
		 					   <tr></tr>
		 						<tr></tr>
		 						<tr></tr>
		 					   <tr>
		 					   		<td class="r-label">
										<a id="MoveAllToR" href="#"  class="hisui-linkbutton" data-options="iconCls:'icon-w-arrow-right'">移动所有项</a>
									</td>
		 					   </tr>
		 					   <tr></tr>
		 						<tr></tr>
		 						<tr></tr>
		 					   <tr>
		 					   		<td class="r-label">
										<a id="MoveToL" href="#"  class="hisui-linkbutton" data-options="iconCls:'icon-w-arrow-left'">移动选择项</a>
									</td>
		 					   </tr>
		 					   <tr></tr>
		 						<tr></tr>
		 						<tr></tr>
		 					   <tr>
		 					   		<td class="r-label">
										<a id="MoveAllToL" href="#"  class="hisui-linkbutton" data-options="iconCls:'icon-w-arrow-left'">移动所有项</a>
									</td>
		 					   </tr>
		 					   
                   			</table>
		 				</div>
					</div> 
				</div>
			</div>
			<div data-options="region:'east',border:false" style="width:550px;padding:10px 10px 10px 0;border:0;">

				<div class="hisui-panel" fit="true" data-options="title:'拆分明细',headerCls:'panel-header-gray',iconCls:'icon-paper'" style="height:100%;border:1px solid #ccc;border-radius:0 0 4px 4px;">
					<div class="hisui-layout" fit="true">
		 				<div data-options="region:'center',split:true,headerCls:'panel-header-gray'" border="false">
							<table data-options="fit:true" id="FeeListRightGrid" border="false"></table>
						</div>
					</div> 
				</div>
			</div>
	  </div>
	</div> 		
</div>
<script type="text/javascript" src="../scripts/DHCPESplitAudit.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>

</body>		
</html>