    <SCRIPT LANGUAGE=CACHE RUNAT=SERVER>
    ;导入润乾直接打印所需包
	s PrintFlag=##class(web.DHCEQCommon).GetSysInfo("990062") 
	s PreviewRptFlag=##class(web.DHCEQCommon).GetSysInfo("990075") //add by WL 2019-11-11 WL0010 增加润乾打印预览标志
