<csp:method name=OnPreHTTP
            arguments=""
            returntype=%Boolean>
    i ##Class(ext.websys.SessionEvents).SessionExpired() q 1 
    q 1
</csp:method>
<html lang="zh-CN">
<head>
	<meta http-equiv="Content-Type"
		  content="text/html; charset=utf-8">
	<title>
		<EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE>
	</title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI />
	<script type="text/javascript"
			src="../scripts/nurse/hisui-0.1.0/dist/js/config/nurseAuthorityConfig.js" charset=gbk></script>
	<STYLE type='text/css'>
		body {
            background-color: #fff;
            padding:5px!important;
        }
        .td_label {
        	text-align:right;
        	height: 40px;
        	padding:0 5px 0 40px;
        }
        .td_item {
        	height: 40px;
        }
        .datagrid-wrap.panel-body.panel-body-noheader{
	        border-color:#ccc;
	        border-top-right-radius:0;
	        border-top-left-radius:0;
	        border-right:0;
	        border-left:0;
	        border-bottom:0;
        }
        
	</STYLE>
</head>
<body>
	<div class="hisui-layout" data-options="fit:true,border:false">
		<div data-options="region:'north',border:false" style="height:0px;border-bottom:1px solid dotted;">
		</div>
		<div data-options="region:'center',split:true,headerCls:'panel-header-gray',iconCls:'icon-paper',border:false">
			<div class="hisui-panel" 
				 title="病历书写权限配置规则（例：Demo-系统配置-菜单管理-表达式：&AuthorityFlag=1）"
				 data-options="fit:true,headerCls:'panel-header-gray'"  
				 style="border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
				<table id="authorityGrid" 
					   class="hisui-datagrid"
					   data-options="fit:true,border:false">
				</table>
			</div>
		</div>
	</div>
	<div id="authorityDialog" style="padding:10px;">
		<table>	
			<tr style="height:40px;">
				<td class="r-label">参数名称</td>
				<td>
					<input id="name" style="width:200px;" class="textbox">
				</td>
			</tr>
			<tr style="height:40px;">
				<td class="r-label">参数值</td>
				<td>
					<input id="value" style="width:200px;" class="textbox">
				</td>
			</tr>
			<tr style="height:40px;">
				<td class="r-label">权限说明</td>
				<td>
					<input id="authorityDesc" style="width:300px;" class="textbox">
				</td>
			</tr>
		</table>
	</div>
</body>

</html>