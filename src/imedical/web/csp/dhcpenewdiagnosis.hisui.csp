<body id="DiagnosisPanel" class="hisui-layout">
  <div class="hisui-layout" id="PersonPanel" data-options="region:'west',onCollapse:function(){setFramSize(1);},onExpand:function(){setFramSize(0);},title:'患者列表'" style="width:290px;padding:4px;">
  	 <div data-options="region:'north',title:''" style="height:305px;border: 1px solid #ccc;">
  	  <form class="form">
      <table style="width:100%;border-collapse:separate; border-spacing:10px 4px;">
      <tr>
      <td align="right" style="width:100px;">
         <label for="RegNo">登记号</label></td>
      <td align="left">
        <div><input class="hisui-validatebox" type="text" Id="RegNo" data-options="prompt:'登记号'" style="width:151px;" /></div> </td>
      
      </tr>

       <tr>
      <td align="right" style="width:100px;">
         <label for="Name">姓名</label></td>
      <td align="left">
        <div><input class="hisui-validatebox" type="text" Id="Name" data-options="prompt:'姓名'" style="width:151px;" /></div> </td>
      
      </tr>
       <tr>
      <td align="right" style="width:100px;">
         <label for="Group">团体名称</label></td>
      <td align="left">
        <div><input class="hisui-combobox" type="text" Id="Group" data-options="prompt:'团体名称'" style="width:156px;" /></div> </td>
      
      </tr>

      <tr>
       <td align="right" style="width:100px;">
         <label for="VIP">VIP等级</label></td>
      <td align="left">
     	<div><input class="hisui-combobox" type="text" Id="VIP" data-options="prompt:'VIP等级'" style="width:156px;" /></div> </td>
      </tr>
      
       <tr>
      <td align="right" style="width:100px;">
         <label for="BDate">开始日期</label></td>
      <td align="left">
        <div><input class="hisui-datebox" type="text" Id="BDate" style="width:158px;" data-options="prompt:'开始日期'" /></div></div> </td>
      
      </tr>
      <tr>
      <td align="right" style="width:100px;">
         <label for="EDate">结束日期</label></td>
      <td align="left">
        <div><input class="hisui-datebox" type="text" Id="EDate" style="width:158px;" data-options="prompt:'结束日期'" /></div></div> </td>
      
      </tr>
      
      <tr>
       <td colspan="2" align="center">
       <div>
              
             <input class='hisui-radio' type="radio" label='未初审' name='GenStatus' value='NoGS' id="NoGS">
             <input class='hisui-radio' type="radio" label='已初审' name='GenStatus' value='HadGS'>
             <input class='hisui-radio' type="radio" label='已复审' name='GenStatus' value='HadMain'>
       </div>
              
        </td>
      </tr>
      
       <tr>
       <td colspan="2" align="center">
       <div><a id="Query" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></div></td>
       </tr>
  	  </table>
  	  </form>
  	 </div>	
  	   <div data-options="region:'center',title:''" style="border: 1px solid #ccc;">
  	 	<table id='CanDiagnosisList' headerCls='panel-header-gray' class='hisui-datagrid' border='false' autoSizeColumn='false'></table>
  	 </div>
  </div>
 <div class="hisui-layout" fit="true" data-options="region:'center', title:'',headerCls:'panel-header-gray',border:false">
    <div class="hisui-panel" data-options="region:'north',title:'',headerCls:'panel-header-gray',collapsible:false" style="height:40px;">
		<div id="sex"></div>
		<div class="patientInfo">
		<span class="patName" id="patName"></span>
		<span class="infoLabel">性别:</span><span id="sexName"></span>
		<span class="sline">/</span>
		<span class="infoLabel">年龄:</span><span id="Age"></span>
		<span class="sline">/</span>
		<span class="infoLabel" id="PatNoName">登记号:</span><span id="PatNo"></span>
		<span class="sline">/</span>
		<span class="infoLabel">体检日期:</span><span id="PEDate"></span>
		<span class="sline">/</span>
		<span class="infoLabel">VIP等级:</span><span id="VIPLevel"></span>
		<span class="sline">/</span>
		<span class="infoLabel">团体名称:</span><span id="GName"></span>
		<span class="sline">/</span>
		<span class="infoLabel">高危备注:</span><font color=red><span id="Remark"></span></font>
