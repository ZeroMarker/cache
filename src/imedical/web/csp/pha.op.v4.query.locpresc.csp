<!DOCTYPE html>
<!--门诊药房hisui--处方集中打印/查询
    csp:csp/pha.op.v4.query.locpresc.csp
    js: scripts/pha/op/v4/query.locpresc.js
-->
<!-- 验证session过期 -->
<csp:method name = OnPreHTTP arguments = "" returntype = %Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
    
</csp:method>

<HEAD>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id = title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <Server language="CACHE" runat="Server">
        s Application=%request.Application
   </Server>
    <HISUI css="websys.menugroup.css"></HISUI>
    <ADDINS require="RWCard,WebsysTool" />
    <DOCCARD medStepCode="Other" ></DOCCARD>
    <PHALIBV1 />
    <PHAPRINTCOM />
    <PHAOPV4 />
    <script type="text/javascript" src='../scripts/DHCWeb.OPCommonManageCard.js'></script>
    
    <!-- 样式扩展 -->
    <style>
    </style>

</HEAD>
<body>
    
    <div class = "hisui-layout" fit = "true">
        <div data-options = "region:'center',border:false" class = "pha-body">
            <div class = "hisui-layout" fit = "true" id="layout-op―condition">
                <div id ="qcondPanel" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',region:'north',height:200,border:true, split:true, title:'处方集中打印',collapsible: false" >
                    <table id="gridPrescList"></table>
                </div>
                <div data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',region:'center',title:'医嘱详细列表'">
                    <table id="gridPrescDetail"></table>
                </div>
            </div>      
        </div>
        
        
    </div> 
    <div id = "gridPrescListBar" >
        <table id = "qCondition" class = "pha-con-table">
            <tr>
                <td class = "r-label">
                    <label for = "stDate">#(..Get("医嘱开始日期"))#</label>
                </td>
                <td>
                    <input id = 'stDate' class = "hisui-datebox" data-pha = 'class:"hisui-datebox",clear:true,query:true,required:true'>
                    <input id = 'stTime' class = "hisui-timespinner" data-pha = 'class:"hisui-timespinner",clear:true,query:true' data-options="showSeconds:true" >
                </td>
                <td class = "r-label">
                    <label for = "docLoc">#(..Get("开单科室"))#</label>
                </td>
                <td>
                    <input id = 'docLoc' class = "hisui-combobox"  data-pha = 'class:"hisui-combobox",clear:true,query:true'>
                </td>
                <td>
                    <input id = 'patNo' class = "hisui-validatebox" data-pha = 'class:"hisui-validatebox",clear:true,query:true' placeholder = "登记号">
                </td>
                <td>
                    <input id="fyFlag" class='hisui-checkbox' type="checkbox"  label="已发药" data-pha = 'class:"hisui-checkbox",clear:true,query:true' >
                </td>
                <td>
                    <a class = "hisui-linkbutton" style = "width:100%" data-options = "iconCls:'icon-w-find'" id = 'btnFind'>查询</a>
                </td>
                <td>
                    <a class = "hisui-linkbutton" style = "width:100%" data-options = "iconCls:'icon-w-clean'" id = 'btnClean'>清屏</a>
                </td>
            </tr>
            <tr>
                <td class = "r-label">
                    <label for = "stDate">#(..Get("医嘱结束日期"))#</label>
                </td>
                <td>
                    <input id = 'endDate' class = "hisui-datebox"  data-pha = 'class:"hisui-datebox",clear:true,query:true,required:true'>
                    <input id = 'endTime' class = "hisui-timespinner" data-pha = 'class:"hisui-timespinner",clear:true,query:true' data-options="showSeconds:true" >
                </td>
                <td class = "r-label">
                    <label for = "prescType">#(..Get("处方类型"))#</label>
                </td>
                <td>
                    <input id = 'prescType' class = "hisui-combobox"  data-pha = 'class:"hisui-combobox",clear:true,query:true' placeholder = "处方类型">
                </td>
                <td>
                    <input id = 'cardNo' class = "hisui-validatebox " data-pha = 'class:"hisui-validatebox",clear:true,query:true' placeholder = "卡号">
                </td>
                <td>
                    <a class = "hisui-linkbutton" style = "width:100%" data-options = "iconCls:'icon-w-card'" id = 'btnReadCard'>读卡</a>
                </td>
                <td>
                    <a class = "hisui-linkbutton" style = "width:100%" data-options = "iconCls:'icon-w-print'" id = 'btnRePrint'>打印</a>
                </td>
                <td>
                    <a class = "hisui-linkbutton" style = "width:100%" data-options = "iconCls:'icon-w-export'" id = 'btnExport'>导出</a>
                </td>
                
            </tr>
        </table>

    </div>
    <script type="text/javascript" src="../scripts/pha/op/v4/query.locpresc.js"></script>
</body>
</html>
