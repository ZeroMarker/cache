 <csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
 // dhc.bdp.ct.hospost.csp
 // 名称: HOS 业务岗位
 // 修改人:基础数据平台-钟荣枫
 // 修改日期:2022年11月10日
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
 <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
 <TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
 <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
 <HISUI/> 
 <link rel="stylesheet" type="text/css" href="../scripts/bdp/Framework/css/styleUI.css" />
 <script type="text/javascript" src="../scripts/bdp/Framework/scripts/FunLibUI.js"></script>
 <script type="text/javascript" src="../scripts/bdp/AppHISUI/HOS/CT_BDP_CT.HOS_Post.js"></script>
</HEAD>
<style>
	.tbclass{
	    text-align: right;
   }
   .dialog-button {
    padding: 0px 10px 20px 10px;
    text-align: center;
    border: none;
    background-color: #fff;
}
form table td, form table th {
    margin: 0;
    padding: 0px;		
    word-break: keep-all;	
    word-wrap:break-word;
    white-space: nowrap;
}
</style>
<BODY> 
    <div class="hisui-layout" data-options="fit:true" border="false" > 
            <div data-options="region:'north' ,split:false,collapsible:false,headerCls:'panel-header-gray' " style="border-bottom:1px dashed #C0C0C0;overflow: hidden;" title="" border="false">  
                 <table cellspacing=10 >
                    <tr>
                    	<td class="tbclass">代码</td>
			    		<td><input id="TextCode"  class="hisui-validatebox" style="width:150px"></td>
			    		<td class="tbclass">名称</td>
			    		<td><input id="TextDesc"  class="hisui-validatebox" style="width:150px"></td>
			    		
                           <td ><a href="#" class="hisui-linkbutton" iconCls="icon-w-find" id="btnSearch" >查询</a></td>
                           <td ><a href="#" class="hisui-linkbutton" iconCls="icon-w-reset" id="btnRefresh" >重置</a></td>
                    </tr>     
                 </table > 
             </div>  
            <div data-options="region:'center'" border="false"> 
                <table data-options="fit:true" id="mygrid" border="false" toolbar="#mybar"></table> 
             </div>  
     </div> 
      
    <div id="myWin" style="width:auto;height:auto;resizable:true;display:none;"> 
            <div title="基本信息" style="padding:10px;">
                <form id="form-save" method="post" data-options="fit:true,border:false,plain:true"> 
                    <table cellspacing=10>
                    	<tr style="display:none">
                           <td class="tbclass">ID</td>
                           <td><input id="ID" name="ID"  class="hisui-validatebox"  style="width:200px" data-options=""></td>
                        </tr>
                         <tr>
                           <td class="tbclass"><font color=red>*</font>代码</td>
                           <td><input id="POSTCode" name="POSTCode"  class="hisui-validatebox"  style="width:200px" data-options="required:true"></td>
                           <td class="tbclass"><font color=red>*</font>名称</td>
                           <td><input id="POSTDesc" name="POSTDesc"  class="hisui-validatebox" style="width:200px"  data-options="required:true"></td>
                        </tr>
                         <tr>
                           <td class="tbclass">业务单元</td>
                           <td><input id="POSTBUCode" name="POSTBUCode"  class="hisui-combobox" style="width:207px"  data-options=""></td>
                           <td class="tbclass"><font color=red>*</font>岗位字典</td>
                           <td><input id="POSTPOSTCCode" name="POSTPOSTCCode"  class="hisui-combobox" style="width:207px"  data-options="required:true"></td>
                         </tr>
                         
                         <tr>
                         	<td class="tbclass">是否有效</td>
							<td>
								<input id="POSTActivity" name="POSTActivity" class='hisui-checkbox' type="checkbox"    data-options="" value="Y">
							</td>
              				<td class="tbclass">排序号</td>
                           <td><input id="POSTSeqNo" name="POSTSeqNo"  class="hisui-validatebox" style="width:200px" validType="NotNegativeInteger" data-options=""></td>
                         </tr>
                         <tr>
                           <td class="tbclass"><font color=red>*</font>开始日期</td>
                           <td><input id="POSTStartDate" name="POSTStartDate"  class='hisui-datebox' style="width:207px" data-options="required:true"></td>
                           <td class="tbclass">结束日期</td>
                           <td><input id="POSTEndDate" name="POSTEndDate"  class='hisui-datebox' style="width:207px"></td>
                         </tr>
                         
                         <tr>
                           
                           <td class="tbclass">拼音码</td>
                           <td><input id="POSTPYCode" name="POSTPYCode"  class="hisui-validatebox" style="width:200px"  data-options=""></td>
                           <td class="tbclass">五笔码</td>
                           <td><input id="POSTWBCode" name="POSTWBCode"  class="hisui-validatebox" style="width:200px"  data-options=""></td>
                         </tr>
                         <tr>
                           <td class="tbclass"><font color=red>*</font>角色</td>
                           <td><input id="POROROLECode" name="POROROLECode"  class="hisui-combobox" style="width:207px"  data-options="required:true"></td>
                         
                           <td class="tbclass">备注</td>
                           <td><input id="POSTMark" name="POSTMark"  class="hisui-validatebox" style="width:200px"  data-options=""></td>
                         </tr>
                    </table >
                </form>
             </div> 
    </div>
   
    <!--工具栏-->
    <div id="mybar">
        <a  href="#"  class="hisui-linkbutton"   data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true" id="btnAdd">新增</a> 
        <a  href="#"  class="hisui-linkbutton"  data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"id="btnUpdate">修改</a> 
        <a  href="#"  class="hisui-linkbutton"  data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true"  id="btnDel">删除</a>
    </div>

</BODY>
 </html>
 
