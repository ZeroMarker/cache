<!-- 
 * FileName: dhcbill.ipbill.depinvlist.csp
 * Anchor: ZhYW
 * Date: 2019-11-28
 * Description: 预交金发票明细查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.depinvlist.css'/>
	<script type="text/javascript" src="../scripts/DHCCPMRQCommon.js"></script>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',collapsible:false,title:'预交金发票明细查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:167px;">
			<table class="search-table" style="width:90%">
				<tr>
					<td class="r-label td-first-col-58"><label>开始日期</label></td>
					<td><input id="stDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>结束日期</label></td>
					<td><input id="endDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>收费员</label></td>
					<td><input id="guser" class="textbox"/></td>
					<td class="r-label"><label>病案号</label></td>
					<td><input id="medicareNo" class="textbox"/></td>
					<td><a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a></td>
				</tr>
				<tr>
					<td class="r-label"><label>支付方式</label></td>
					<td><input id="paymode" class="hisui-combobox textbox"/></td>
					<td class="r-label"><label>票据状态</label></td>
					<td><input id="rcptStatus" class="hisui-combobox textbox"/></td>
					<td class="r-label"><label>票据类型</label></td>
					<td><input id="rcptType" class="hisui-combobox textbox"/></td>
					<td class="r-label"><label>票据号</label></td>
					<td><input id="receiptNo" class="textbox"/></td>
					<td><a href="javascript:;" class="hisui-linkbutton" id="btn-export" data-options="iconCls:'icon-w-export'">导出</a></td>
				</tr>
				<tr>
					<td class="r-label"><label>患者类型</label></td>
					<td><input id="patType" class="hisui-combobox textbox"/></td>
					<td class="r-label"><label>费别</label></td>
					<td><input id="admReason" class="hisui-combobox textbox"/></td>
					<td class="r-label"><label>押金类型</label></td>
					<td><input id="depositType" class="hisui-combobox textbox"/></td>
					<td></td>
					<td></td>
					<td><a href="javascript:;" class="hisui-linkbutton" id="btn-clear" data-options="iconCls:'icon-w-clean'">清屏</a></td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-top-style:dashed;border-radius:0 0 4px 4px;">
			<table class="hisui-datagrid" id="depInvList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.depinvlist.js"></script>
</body>
</html>
