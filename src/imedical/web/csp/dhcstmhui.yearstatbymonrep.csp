<!--月报年度统计-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
<div class="GridPadding" style="width:340px;" data-options="region:'west',border:false" >
		<div class="hisui-panel" data-options="title:'年度统计',headerCls:'panel-header-gray',fit:true,iconCls:'icon-paper'">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false" style="overflow: hidden;">   <!--overflow-y: scroll;-->
		<table id="YearConditions" class="Condition">
			<tr>
				<td>科室</td>
				<td><input id="PhaLoc" name="PhaLoc" class="hisui-combobox" style="width:160px;"></td>
			</tr>
			<tr>
				<td>年份</td>
				<td><input id="Year" name="Year" class="textbox" style="width:153px;"></td>
			</tr>
			<tr>
				<td>起始月</td>
							<td><input id="StMonth" name="StMonth" class="textbox" style="width:153px;"></td>
			</tr>
			<tr>
				<td>截止月</td>
							<td><input id="EdMonth" name="EdMonth" class="textbox" style="width:153px;"></td>
			</tr>
		</table>
				</div>
				<div class="PartLine" data-options="region:'center',border:false" style="height:350px;padding-top: 5px">
		<table>
			<tr>
				<td>
				<input class='hisui-radio' type="radio" label='类组汇总(库存转移)' name='ReportType' value='ScgStat' data-options='checked:true'>
				</td>
			</tr>
			<tr>
				<td>
				<input class='hisui-radio' type="radio" label='类组库存分类(库存转移)' name='ReportType' value='StkCatStat'>
				</td>
			</tr>
			<tr>
				<td>
				<input class='hisui-radio' type="radio" label='入库退货明细(按品种)' name='ReportType' value='InciRecRetStat'>
				</td>
			</tr>
			<tr>
				<td>
				<input class='hisui-radio' type="radio" label='出库汇总(按科室)' name='ReportType' value='OutLocStat'>
				</td>
			</tr>
			<tr>
				<td>
				<input class='hisui-radio' type="radio" label='类组库存分类(分类)' name='ReportType' value='ScgCatStatCat'>
				</td>
			</tr>
			<tr>
				<td>
				<input class='hisui-radio' type="radio" label='出库汇总(按科室分类)' name='ReportType' value='OutLocStatCat'>
				</td>
			</tr>
		</table>
	</div>
				<div data-options="region:'south',border:false" style="overflow: hidden;padding: 10px">
					<table id="YearBar">
						<tr>
							<td>
								<span style="padding: 19px"></span>
								<a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
								<span style="padding: 10px;"></span>
								<a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
							</td>
						</tr>
					</table>
				</div>
			</div>
		</div>
</div>
<div class="hisui-tabs tabs-gray GridPadding" data-options="region:'center'" id="Yeartabs" style="border:0px;padding-left:0px">
	<div title="报表" data-options="iconCls:'icon-add-note'" class="ReportImage">
	</div>  
</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/StkMonRep/YearStatByMonRep.js"></script>
</body>
</html>

