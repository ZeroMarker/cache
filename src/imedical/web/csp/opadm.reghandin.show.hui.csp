<div style="font-size:22px;text-align:center;">挂&nbsp&nbsp&nbsp&nbsp号&nbsp&nbsp&nbsp&nbsp员&nbsp&nbsp&nbsp&nbsp日&nbsp&nbsp&nbsp&nbsp结&nbsp&nbsp&nbsp&nbsp账</div>
<div style="height:100%;width:100%;margin:0 auto;">
	<div class="hisui-panel" fit="true" style="border:0;border-radius:0;">
		<table class="search-table">
			<tr>
				<td class="r-label">
					<label>开始日期</label>
				</td>
				<td>
					<input id="StartDate" disabled class="textbox"/>
				</td>
				<td class="r-label" style="width:89px;">
					<label>开始时间</label>
				</td>
				<td>
					<input id="StartTime" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>结束日期</label>
				</td>
				<td>
					<input id="EndDate" disabled class="textbox"/>
				</td>
				<td class="r-label">
					<label>结束时间</label>
				</td>
				<td>
					<input id="EndTime" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>结算总金额</label>
				</td>
				<td>
					<input id="AmtSum" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>现金金额</label>
				</td>
				<td>
					<input id="CashAmt" disabled class="textbox"/>
				</td>
				<td class="r-label">
					<label>其它金额</label>
				</td>
				<td>
					<input id="OtherAmt" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>收款金额</label>
				</td>
				<td>
					<input id="Pamt" disabled class="textbox"/>
				</td>
				<td class="r-label">
					<label>退款金额</label>
				</td>
				<td>
					<input id="Ramt" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>收据张数</label>
				</td>
				<td>
					<input id="ReceiptsNum" disabled class="textbox"/>
				</td>
				<td class="r-label">
					<label>退款收据张数</label>
				</td>
				<td>
					<input id="RefundNum" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>收据号段</label>
				</td>
				<td colspan="3">
					<textarea rows="3" id="ReceiptsField" disabled style="width:460px;"></textarea>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>退款收据</label>
				</td>
				<td colspan="3">
					<textarea rows="4" id="RefundNo" disabled style="width:460px;"></textarea>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>挂号总数</label>
				</td>
				<td>
					<input id="AdmTot" disabled class="textbox"/>
				</td>
				<td class="r-label">
					<label>退号数</label>
				</td>
				<td>
					<input id="RefundAdmTot" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>挂号总金额</label>
				</td>
				<td>
					<input id="AdmAmtSum" disabled class="textbox"/>
				</td>
			</tr>
			<tr>
				<td colspan="4" style="text-align:center;">
					<csp:if condition='RepID=""'>
						<a id="BFind" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
						<div style="display:inline-block;margin:0 20px;"></div>
						<a id="BFoot" class="hisui-linkbutton" data-options="iconCls:'icon-w-inv'">结算</a>
						<div style="display:inline-block;margin:0 20px;"></div>
					</csp:if>
					<a id="BPrint" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">打印</a>
				</td>
			</tr>
		</table>
	</div>
</div>
