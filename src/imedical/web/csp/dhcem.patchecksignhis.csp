<html>
<head>
<meta content="text/html;charset=utf-8"/>
<title>	分诊体征历史查询 </title>
<script Language="Javascript">
	var PCLRowID = "#(%request.Get("EmPCLvID"))#";	
</script>
<HISUI/>


</head>

<body class="hisui-layout">
<div data-options="region:'center',border:false" style="padding: 5px 10px 10px 10px;" >
	<table id="signHis"></table>
</div>
</body>
<script type="text/javascript">
var columns = [[
     {
        field: 'Time',
        align: 'center',
        title: '时间',
        hidden:true,
        width:50
    },{
        field: 'Temp',
        align: 'center',
        title: '温度(℃)',
        width:50
    },{
        field: 'Heart',
        align: 'center',
        title: '心率(次/分)',
        width:50
    },{
        field: 'Pulse',
        align: 'center',
        title: '脉搏(次/分)	',
        width:50
    },{
        field: 'DBP',
        align: 'center',
        title: '舒张压(mmHg)',
        width:50
    },{
        field: 'SBP',
        align: 'center',
        title: '收缩压(mmHg)',
        width:50
    },{
        field: 'SOP2',
        align: 'center',
        title: '血氧饱和浓度(%)',
        width:50
    },{
        field: 'SCSR',
        align: 'center',
        title: '呼吸频率(次/分)',
        width:50
    },{
        field: 'EmPcsGLU',
        align: 'center',
        title: '血糖(mmol/l)',
        width:50
    }
    ]]
    
    
        
  
	$HUI.datagrid('#signHis',{
		url: 'dhcapp.broker.csp?ClassName=web.DHCEMRegister&MethodName=GetSignHis&EmPCLvID='+PCLRowID,
		fit:true,
		rownumbers:true,
		columns:columns,
		fitColumns:true,
		pageSize:60,  
		pageList:[60], 
		loadMsg: '正在加载信息...',
		rownumbers : false,
		pagination:false,
		singleSelect:true,
		selectOnCheck: false,
		checkOnSelect: false,
		toolbar:'#toolbar',
		iconCls:'icon-paper',
		headerCls:'panel-header-gray', 
		onDblClickRow:function(index,row){
			
		},
		onLoadSuccess:function(data){

		}
    })
</script>
</html>
