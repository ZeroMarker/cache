<div class="hisui-layout" fit="true">
	<csp:if condition='FromTabFlag="Y"'>
		<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0px;">
			<div class="hisui-panel" data-options="fit:true,border:false" style="height:100%;">
				<table id="OrdListTab"></table>
			</div>
		</div>
	<csp:else>
		<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-radius:4px;">
				<table id="OrdListTab"></table>
			</div>
		</div>
	</csp:if>
</div>
