<!--盘点损益汇总-->
<!DOCTYPE html>
<html lang="zh-CN">
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<SERVER>
	d ##class(web.DHCSTMHUI.Util.RQServerConfig).GetApplet()
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<style type="text/css">
		#tab-tools{
			left:300px;
			line-height: 33px;
		}
</style>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" style="width:340px" data-options="region:'west',border:false">
		<div class="hisui-panel" data-options="title:'盘点单',headerCls:'panel-header-gray',fit:true,iconCls:'icon-paper'">
			<div class="hisui-layout" data-options="fit:true,region:'center'">
				<div data-options="region:'north',border:false">
					<table class="Condition" id="Conditions" align="center">
						<tr>
							<td colspan="2">
								<label class="required">*</label><label>起始日期</label><input id="FStartDate" name="FStartDate" class="textbox hisui-datebox ">
							</td>
							
						</tr>
						<tr>
							<td colspan="2">
								<label class="required">*</label><label>截止日期</label><input id="FEndDate" name="FEndDate" class="textbox hisui-datebox">
							</td>
						</tr>
						<tr>
							<td colspan="2">
								<label class="required">*</label><label>盘点科室</label><input id="FLocId" name="FLocId" class="textbox hisui-combobox">
							</td>
						</tr>
						<tr>
							<tr>
							<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
							<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
						</tr>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false">
					<table id="MainGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div id="tab-tools">
		<input class="hisui-radio" type="radio" label="全部" name="LossFlag" value="0" data-options="checked:true">
		<input class="hisui-radio" type="radio" label="仅盘盈" name="LossFlag" value="1">
		<input class="hisui-radio" type="radio" label="仅盘亏" name="LossFlag" value="2">
		<input class="hisui-radio" type="radio" label="仅无损益" name="LossFlag" value="3">
		<input class="hisui-radio" type="radio" label="仅有损益" name="LossFlag" value="4">
	</div>
	<div id="DetailTabs" data-options="region:'center',tools:'#tab-tools'" class="hisui-tabs tabs-gray NonPadGrid GridPadding" style="padding-left:0px;border:0px;">
		<div title="损益明细" class="ReportImage">
			<iframe id="DetailIFrame" src="" frameborder="0" scrolling="yes" height="99%" width="99.8%" noresize="noresize"></iframe>
		</div>
		<div id="InciDetailTab" title="品种汇总" class="ReportImage">
			<iframe id="IncIFrame" src="" frameborder="0" scrolling="yes" height="99%" width="99.8%" noresize="noresize"></iframe>
		</div>
		<div title="品种汇总(高值条码)" class="ReportImage">
			<iframe id="IncWithBarcodeIFrame" src="" frameborder="0" scrolling="yes" height="99%" width="99.8%" noresize="noresize"></iframe>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/encode.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InStkTk/INStkTkStat.js"></script>
</body>
</html>
