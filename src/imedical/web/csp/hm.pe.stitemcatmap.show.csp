<!-- 主页面 -->
<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',border:false" style="height:50px;">
        <table class='search-table'>
            <tr>
                <td class="r-label"><label>知识库版本</label></td>
				<td>
					<select class="hisui-combobox textbox" type="text" id="cboHospTags" style="width:200px;" ></select>
				</td>
                <td class="r-label" style="padding-left:20px">
                    <a id="btnFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
                </td>
            </tr>
        </table>
    </div>
    
	<!-- 右页面 -->
	<div data-options="region:'east',border:false" style="width:500px;padding:10px;padding-top:0px;padding-left:0px;">
		<div class="hisui-panel" fit="true" title="标准站点项目分类" data-options="border:true,iconCls:'icon-resort',fit:true,headerCls:'panel-header-gray'">
	        <input type="hidden" id="gridBTExamItemCat_ID"/>
			<table id="gridBTExamItemCat" data-options="border:false,toolbar:'#gridBTExamItemCat_custtb'"></table>
			<div id="gridBTExamItemCat_custtb" style="padding:3px;" >
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-compare',plain:true,stopAllEventOnDisabled:true" id="btnMapping">项目分类对照</a>
				<div style="float: right;">
					<input id="gridBTExamItemCat_search" data-options="prompt:'搜索'" class="hisui-searchbox"/>
				</div>
			</div>
		</div>
	</div><!-- 右页面 End-->
	
	<!-- 中心页面 -->
	<div data-options="region:'center',border:false" style="padding:10px;padding-top:0px;">
		<div class="hisui-panel" fit="true" title="医院站点项目分类" data-options="border:true,iconCls:'icon-resort',fit:true,headerCls:'panel-header-gray'">
	        <input type="hidden" id="gridPESTItemCat_ID"/>
			<table id="gridPESTItemCat" data-options="border:false,toolbar:'#gridPESTItemCat_custtb'"></table>
			<div id="gridPESTItemCat_custtb" style="float: right;" >
				<input id="gridPESTItemCat_search" data-options="prompt:'搜索'" class="hisui-searchbox"/>
			</div>
		</div>
    </div><!-- 中心页面 End-->
    
</div><!-- 主页面 End-->