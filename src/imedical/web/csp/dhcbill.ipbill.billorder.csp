<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.billorder.css'/>
	<script type='text/javascript' src='../scripts/dhcbill/dhcipbill/dhcbill.ipbill.common.js'></script>
	<server>
		set PatientListPage=""
	</server>
</head>

<body class="hisui-layout">
	<div data-options="region:'north',border:false" style="height:36px;overflow:hidden;">
		<csp:Include Page="dhcbill.in.patient.banner.csp">
	</div>
	<div data-options="region:'center',border:false" style="padding: 0 10px 10px 10px;">
		<div class="hisui-layout" data-options="fit:true">
			<div data-options="region:'north',bodyCls:'panel-header-gray'" style="height:92px;border-radius:4px 4px 0 0;">
				<table class="search-table">
					<tr>
						<td class="r-label"><label>开始日期</label></td>
						<td><input id="stDate" class="hisui-datebox textbox"></td>
						<td class="r-label r-label-1"><label>结束日期</label></td>
						<td><input id="endDate" class="hisui-datebox textbox"/></td>
						<td class="r-label r-label-1"><label>开单科室</label></td>
						<td><input id="userDept" class="hisui-combobox textbox"/></td>
						<td class="r-label r-label-1"><a href="javascript:;" class="hisui-linkbutton query-panel-btn" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a></td>
					</tr>
					<tr>
						<td class="r-label"><label>审核标志</label></td>
						<td><input id="confirmFlag" class="hisui-combobox textbox"/></td>
						<td class="r-label r-label-1"><label>审核原因</label></td>
						<td colspan="3"><input id="confirmReason" class="textbox tb-colspan"/></td>
						<td class="r-label r-label-1"><a href="javascript:;" class="hisui-linkbutton query-panel-btn" id="btn-confirm">审核</a></td>
						<td class="r-label"><a href="javascript:;" class="hisui-linkbutton query-panel-btn" id="btn-cancel">撤销审核</a></td>
					</tr>
				</table>
			</div>
			<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
				<table id="pboList"></table>
			</div>
		</div>
	</div>
	<div id="execDlg" style="padding:10px;display:none;">
		<table class="hisui-datagrid" id="execList"></table>
		<div id="exec-tb" style="padding:10px;">
			<span style="margin-right:7px;">开始日期</span>
			<input id="execStDate" class="hisui-datebox textbox">
			<span style="margin-left:30px;margin-right:7px;">结束日期</span>
			<input id="execEndDate" class="hisui-datebox textbox">
			<a href="javascript:;" class="hisui-linkbutton" id="btn-execFind" data-options="iconCls:'icon-w-find'" style="margin-left:30px;">查询</a>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.billorder.js"></script>
</body>
</html>
