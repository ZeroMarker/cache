<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>

<script language="cache" runat="server">

    s Action=$Get(%request.Data("actiontype",1))
    s Start=$Get(%request.Data("start",1))
    s Limit=$Get(%request.Data("limit",1))
    s Sort=$Get(%request.Data("sort",1))
    s Dir=$Get(%request.Data("dir",1))
    s GroupId=$G(%session.Data("LOGON.GROUPID"))
    s LocId=$G(%session.Data("LOGON.CTLOCID"))
    s UserId=$G(%session.Data("LOGON.USERID"))
    
     i Action="queryIng" d
     .s Params=$Get(%request.Data("Params",1))
     .d ##class(web.DHCSTM.DHCReimPay).jsDHCING(Start,Limit,Sort,Dir,Params)
     i Action = "addReimPay" d
     .s MainInfo=$Get(%request.Data("MainInfo",1))
     .s ListDetail=$Get(%request.Data("ListDetail",1))
     .s result = ##class(web.DHCSTM.DHCReimPay).SaveReimPay(MainInfo,ListDetail)
     .i +result>0 d
     ..w "{success:'true',info:'"_result_"'}"
     .e  d
     ..w "{success:'false',info:'"_result_"'}"
   	 i Action="queryReimPay" d
     .s Params=$Get(%request.Data("Params",1))
     .d ##class(web.DHCSTM.DHCReimPayInv).jsReimPay(Start,Limit,Sort,Dir,Params)
     i Action = "Delete" d
	.s RowId=$Get(%request.Data("RPIRowId",1))
	.s result = ##class(web.DHCSTM.DHCReimPay).Delete(RowId)
	.i result=0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
    i Action = "Save" d
	.s data=$Get(%request.Data("data",1))
	.s result = ##class(web.DHCSTM.DHCReimPayInv).Update(data)
	.i result>0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
  
</script>