<!--定价信息-->
<!DOCTYPE html>
<html lang="zh-CN">
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div id="DetailTabs" class="hisui-tabs tabs-gray GridPadding" data-options="region:'center',tabPosition:'left',headerWidth:100,headerCls:'panel-header-gray'" style="border:0px">
		<div title="定价类型" data-options="fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'center',border:false" class="GridPadding" style="padding-right:0px;padding-top: 0px;padding-bottom: 0px;" >
					<div class="hisui-panel" data-options="fit:true" style="border-right:0px;border-bottom: 0px;border-top: 0px;border-color: #ccc">
						<table id="MarkTypeGrid"></table>
					</div>
				</div>
			</div>
		</div>
		<div title="小数规则">
			<div class="hisui-layout" data-options="fit:true" >
				<div data-options="region:'west',border:false" class="GridPadding" style="width:450px;" >
					<div class="hisui-panel" data-options="fit:true,title:'小数规则',headerCls:'panel-header-gray'">
						<table id="StkDecimalGrid"></table>
					</div>
				</div>
				<div data-options="region:'center',border:false" class="GridPadding" style="padding-left:0px;" >
					<div class="hisui-panel" data-options="fit:true,title:'规则明细',headerCls:'panel-header-gray'">
						<table id="StkDecimalItmGrid"></table>
					</div>
				</div>
			</div>
		</div>
		<div title="定价规则">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false" class="GridPadding" style="height:300px;">
					<div class="hisui-panel" data-options="fit:true,title:'定价规则',headerCls:'panel-header-gray'">
						<table id="MarkRuleGrid"></table>
					</div>
				</div>
				<div data-options="region:'center',border:false" class="GridPadding" style="padding-top: 0px;" >
					<div class="hisui-panel" data-options="fit:true,title:'阶梯加成',headerCls:'panel-header-gray'">
						<table id="MarkRuleItmGrid"></table>
					</div>
				</div>
				<div id="myWindow" data-options="modal:true"></div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonHosp.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/MarkInfo/MarkInfo.js"></script>
	
</body>
</html>