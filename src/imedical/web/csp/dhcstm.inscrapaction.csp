<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

	s StrParam=$Get(%request.Data("strParam",1))
	s Inscrap=$Get(%request.Data("inscrap",1))
	s listData=$Get(%request.Data("data",1))
	s UserId=$Get(%request.Data("userId",1))
	s LocId=$Get(%request.Data("locId",1))
	
	//查询报损单
	i Action = "query" d
	.w ##class(web.DHCSTM.DHCINScrap).jsDHCINSpM(Start,Limit,Sort,Dir,StrParam)

	//查询报损单明细
	i Action = "queryItem" d
    .w ##class(web.DHCSTM.DHCINScrapItm).jsDHCINSpD(Start,Limit,Inscrap)
	
	//删除报损单
	i Action = "delete" d
	.s result = ##class(web.DHCSTM.DHCINScrap).Delete(Inscrap)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//删除报损单明细
	i Action = "deldetail" d
	.s RowId=$Get(%request.Data("RowId",1))
	.s result = ##class(web.DHCSTM.DHCINScrapItm).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//审核报损单
	i Action = "audit" d
	.s result = ##class(web.DHCSTM.DHCINScrap).Audit(Inscrap,UserId,LocId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	//审核报损单
	i Action = "canaudit" d
	.s result = ##class(web.DHCSTM.DHCINScrap).UnAudit(Inscrap,UserId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	//保存报损单记录
	i Action = "save" d
	.S MainInfo=$Get(%request.Data("MainInfo",1))
	.S ListDetail=$Get(%request.Data("ListDetail",1))
	.s result = ##class(web.DHCSTM.DHCINScrap).Save(Inscrap,MainInfo,ListDetail)
	.i result >0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
	//保存报损单主表记录
	i Action = "saveM" d
	.s result = ##class(web.DHCSTM.DHCINScrap).Update(Inscrap,MainInfo)
	.i result >0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//保存报损单明细记录
	i Action = "saveDetail" d
	.s result = ##class(web.DHCSTM.DHCINScrapItm).Update(Inscrap,listData)
	.i result >0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	//获得报损单主记录信息
	i Action = "Select" d
	.S InscpId=$Get(%request.Data("InscpId",1))
	.s result=##class(web.DHCSTM.DHCINScrap).jsSelect(InscpId)	
	.i result'= "" d
	..w result
	.e  d
	..w "{results:'',rows:''}"
	.
	
	//完成报损单
	i Action = "finish" d
	.S InscpId=$Get(%request.Data("InscpId",1))
	.s result = ##class(web.DHCSTM.DHCINScrap).SetComplete(InscpId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//取消完成报损单
	i Action = "noFinish" d
	.S InscpId=$Get(%request.Data("InscpId",1))
	.s result = ##class(web.DHCSTM.DHCINScrap).CancelComplete(InscpId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
</script>