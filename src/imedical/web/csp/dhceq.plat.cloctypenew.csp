<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<SCRIPT language="Cache" RUNAT="SERVER">
 //s ReadOnly=%request.Data("ReadOnly",1)
 s QXType=0
</Script>
<HISUI/>
<script type="text/javascript" src="../scripts/dhceq/plat/cloctypenew.js"></script>
</head>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px">
		<div class="hisui-panel"  title="设备科室类型" data-options="fit:true,eqtitle:'maintitle'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:dashed 1px #ccc;height:120px;">
				<div class="eq-table-tr">
					<!--modify by lmm 2019-09-05-->
					<div class="eq-table-td eq-table-label"><label id="cLoc" for="Loc">科室</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Loc','paras':[{'name':'CTLocAllFlag','type':'2','value':'Y'},{'name':'LocDesc','type':'1','value':'Loc'}]" id="Loc"  ></div>	<!-- // modify by yh 2019-10-28-->
					<div class="eq-table-td eq-table-label"><label  for="Hospital">院区</label></div>
					<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox"  id="Hospital" ></div>
					<div class="eq-table-td eq-table-label"><label id="cManageUser" for="ManageUser">管理员</label></div>
					<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox" data-options="'component':'PLAT.L.EQUser'" id="ManageUser" ></div>
					<div class="eq-table-td eq-table-label"><label for="AllLocFlag">全部科室</label></div>
					<div class="eq-table-td eq-table-input"><input data-options="checked:true" class='hisui-checkbox' type="checkbox" id="AllLocFlag" ></div>	
					<div class="eq-table-td eq-table-label"><a id="BFind" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">查询</a></div>		<!--MZY0025	1318615	2020-05-13-->
					</div>
				   <div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cGroupIDOne" for="GroupIDOne">科室库房</label></div>
					<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox" data-options="'component':'PLAT.L.LocGroupType','paras':[{'name':'Type','type':'2','value':'1'}]," id="GroupIDOne" ></div>
					<div class="eq-table-td eq-table-label"><label id="cGroupIDTwo" for="GroupIDTwo">科室职能</label></div>
					<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox" data-options="'component':'PLAT.L.LocGroupType','paras':[{'name':'Type','type':'2','value':'2'}]," id="GroupIDTwo" ></div>
					<div class="eq-table-td eq-table-label"><label id="cGroupIDThree" for="GroupIDThree">库房属性</label></div>
					<div class="eq-table-td eq-table-input"><input  class="hisui-combobox textbox"  id="GroupIDThree"  ></div>
					<div class="eq-table-td eq-table-label"><label  for="UnCheckFlag">未匹配标志</label></div>
					<div class="eq-table-td eq-table-input"><input  class='hisui-checkbox' type="checkbox" id="UnCheckFlag" ></div>
					<div class="eq-table-td eq-table-label"><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">修改</a></div>		<!--MZY0025	1318615	2020-05-13-->
					</div>
					<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label  for="Location">位置</label></div>
					<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox"  id="Location"  ></div>
					<div class="eq-table-td eq-table-label"><label for="Remark">备注</label></div>
					<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox" id="Remark"  ></div>
					<div class="eq-table-td eq-table-label"><label  for="Tel">联系电话</label></div>
					<div class="eq-table-td eq-table-input"><input  class="hisui-validatebox" id="Tel" ></div>
	                </div>
					
				</div>
				<div data-options="region:'center',border:false" style="padding:0px;position:relative">
					<table id="tDHCEQClocTypeNew" data-options="border:false,fit:true"></table>
				</div>
				</div>
			</div>
		</div>
	</div>
	<div id="hiddenEncrypt" style="width:0;height:0;">
		<form name="DHCEQ">	
			<INPUT TYPE="hidden"  id="RowID">
			<INPUT TYPE="hidden" name="LocDR" id="LocDR">
			<INPUT TYPE="hidden" name="TypeIDOne" id="TypeIDOne">
			<INPUT TYPE="hidden" name="TypeIDTwo" id="TypeIDTwo">
			<INPUT TYPE="hidden" name="TypeIDThree" id="TypeIDThree">
			<INPUT TYPE="hidden" name="ManageUserDR" id="ManageUserDR">
			<INPUT TYPE="hidden" name="GroupIDOneDR" id="GroupIDOneDR">
			<INPUT TYPE="hidden" name="GroupIDTwoDR" id="GroupIDTwoDR">
			<INPUT TYPE="hidden" name="QXType" id="QXType" value=#(QXType)#>
		</form>
	</div>
</body>

</html>
