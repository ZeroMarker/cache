<div class="hisui-panel" fit="true" data-options="title:'#(..Get("医嘱模板查看"))#',headerCls:'panel-header-gray',iconCls:'icon-apply-check'">
	<div class="hisui-layout" fit="true">
		<div data-options="region:'north',border:false,collapsible:false" style="height:auto;padding:0 10px;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="objtype">#(..Get("类型"))#</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="objtype"/>
					</td>
					<td class="r-label">
						<label for="objvalue">#(..Get("值"))#</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="objvalue"/>
					</td>
					<td class="r-label">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">#(..Get("查询"))#</a>
					</td>
					
				</tr>
			</table>
		</div>
		<div data-options="region:'center',title:'',bodyCls:'panel-body-gray'" style="border-width:1px 0 0 0 ;border-radius:0;">
			<table id="PrefTabList"></table>
		</div>
	</div>
</div>