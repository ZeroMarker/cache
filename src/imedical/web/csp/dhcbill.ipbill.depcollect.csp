<!-- 
 * FileName: dhcbill.ipbill.depcollect.csp
 * Author: ZhYW
 * Date: 2019-12-06
 * Description: 预交金汇总
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<server>
		set DefDate=##class(websys.Conversions).DateLogicalToHtml(+$h)
	</server>
	<script type="text/javascript">
		//常量
		var CV = {
			DefDate: "#(DefDate)#"
		};

		//全局变量
		var GV = {};
	</script>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',collapsible:false,title:'预交金汇总',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:87px;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label>#(..Get("开始日期"))#</label></td>
					<td><input id="stDate" class="hisui-datebox textbox"/></td>
					<td class="r-label r-label-20"><label>#(..Get("结束日期"))#</label></td>
					<td><input id="endDate" class="hisui-datebox textbox"/></td>
					<td class="r-label r-label-20"><label>#(..Get("支付方式"))#</label></td>
					<td><input id="paymode" class="textbox"/></td>
					<td class="r-label r-label-20"><label>#(..Get("收费员"))#</label></td>
					<td><input id="guser" class="textbox"/></td>
					<td class="r-label r-label-20"><input id="isRefRcpt" class='hisui-checkbox' type="checkbox" label="#(..Get("查退款票"))#"/></td>
					<td class="r-label"><a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a></td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
			<table id="depList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="#(##class(BILL.Util.Library).#JSPath)#dhcipbill/dhcbill.ipbill.depcollect.js"></script>
</body>
</html>