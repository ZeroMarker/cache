<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))
	s strPar=$Get(%request.Data("strPar",1))

	i Action="SelectDsReq" d
	.s dsrq=$Get(%request.Data("dsrq",1))
	.s result=##class(web.DHCSTM.DHCINDispReq).Select(dsrq)
	.i result="" d
	..w "{success:'false',info:''}"
	.e  d
	..w "{success:'true',info:'"_result_"'}"	

	///检索请领单主信息
	i Action="DispReqList" d
	.s StartDate=$p(strPar,"^",1)
	.s EndDate=$p(strPar,"^",2)
	.s Loc=$p(strPar,"^",3)
	.s LocUser=$p(strPar,"^",4)
	.s comp=$p(strPar,"^",5)
	.s status=$p(strPar,"^",6)
	.s userGrp=$p(strPar,"^",7)
	.s dispStatus=$p(strPar,"^",8)
	.d ##class(web.DHCSTM.DHCINDispReq).jsINDispReq(Start,Limit,Sort,Dir,Loc,StartDate,EndDate,LocUser,comp,status,userGrp,dispStatus)

	///保存请领
	i Action="SaveDispReq" d
	.s dsrq=$Get(%request.Data("dsrq",1)) 
	.s mData=$Get(%request.Data("mData",1)) 
	.s dData=$Get(%request.Data("dData",1))
	.s result=##class(web.DHCSTM.DHCINDispReq).Save(dsrq,mData,dData)
	.i result>0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	///删除请领
	i Action="DelDispReq" d
	.s DSRQ=$Get(%request.Data("DSRQ",1))   // rowid of DHC_INDispReq
	.s result=##class(web.DHCSTM.DHCINDispReq).Delete(DSRQ)
	.i result = "0" d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	///检索请领单明细信息
	i Action="SelDispReqItm" d
	.s dsrq=$Get(%request.Data("dsrq",1))
	.d ##class(web.DHCSTM.DHCINDispReqItm).jsINDispReqItm(Start,Limit,Sort,Dir,dsrq)

	///删除请领明细
	i Action="DelDispReqItm" d
	.s DSRQI=$Get(%request.Data("DSRQI",1))   // rowid of DHC_INDispReq
	.s result=##class(web.DHCSTM.DHCINDispReqItm).Delete(DSRQI)
	.i result = "0" d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
	i Action="SetComp" d
	.s dsrq=$Get(%request.Data("dsrq",1))
	.s result=##class(web.DHCSTM.DHCINDispReq).SetComp(dsrq)
	.i result = "0" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	i Action="CancelComp" d
	.s dsrq=$Get(%request.Data("dsrq",1))
	.s result=##class(web.DHCSTM.DHCINDispReq).CancelComp(dsrq)
	.i result = "0" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	i Action="CancelDispReq" d
	.s dsrq=$Get(%request.Data("dsrq",1))
	.s result=##class(web.DHCSTM.DHCINDispReq).CancelDispReq(dsrq)
	.i result = "0" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//拒绝请领单
	i Action="RejectInDispReq" d
	.s dsrq=$G(%request.Data("dsrq",1))
	.s result=##class(web.DHCSTM.DHCINDispReq).RejectInDispReq(dsrq)
	.i result=0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//拒绝(取消)请领单明细
	i Action="HandleItm" d
	.s dsrqi=$G(%request.Data("dsrqi",1))
	.s moveStatus=$G(%request.Data("moveStatus",1))
	.s result=##class(web.DHCSTM.DHCINDispReqItm).HandleItm(dsrqi,moveStatus)
	.i result=0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	i Action="GetItmInfo" d
	.s IncId=$G(%request.Data("IncId",1))
	.s LocId=$G(%request.Data("LocId",1))
	.s result=##class(web.DHCSTM.DHCINDispReq).GetItmInfo(IncId,LocId)
	.s result=##class(web.DHCSTM.Common.UtilCommon).hJsonChar(result)
	.w "{success:'true',info:'"_result_"'}"

</Script>