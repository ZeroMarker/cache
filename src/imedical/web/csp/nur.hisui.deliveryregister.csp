<csp:method name=OnPreHTTP
            arguments=""
            returntype=%Boolean>
    i ##Class(ext.websys.SessionEvents).SessionExpired() q 1 
    q 1
</csp:method>
<script language="cache"
        runat="SERVER">
        s EpisodeID = $G(%request.Data("EpisodeID", 1))
        </script>
<HTML lang="zh-CN">

<head>
    <meta http-equiv="Content-Type"
          content="text/html; ">
    <title>
        <EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE>
    </title>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>
    <script type="text/javascript"
            src="../scripts/DHC.WMR.PinYinHead.js"></script>
    <script type="text/javascript"
            src="../scripts/nurse/hisui/utils.js"></script>
    <script type="text/javascript"
            src="../scripts/nurse/hisui/deliverRegister.js"></script>
    <STYLE type='text/css'>
        body {
            background-color: #fff;
            padding:0;
            margin:0;
        }
        .register {
            position: absolute;
            top:0;
            left:0;
            bottom:0;
            right:0;

        }
        .register__body{
	        position: absolute;
            top:0;
            left:0;
            bottom:0;
            right:0;
            margin:10px;
          
        }
		.textbox{
 			width:120px;
 			height:28px;
		}
		.r-label{
			width:100px;
		}
		tr{
			height:35px;
		}
		
    </STYLE>
</head>

<body>
    <div class="hisui-layout register">
        <div data-options="region:'center'" style="border:none">
           <div class="register__body">
	           <div class="hisui-panel" title="登记" style="padding:10px;" 
				data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order'">
					<table>
						<tr>
							<td class="r-label" style="width:110px;">
	                            患者信息
	                        </td>
							<td style="padding:0 5px 0 0;">
								<input class="textbox" disabled id="PatInfo" style="width:174px;"></input>
							</td>
						</tr>
						<tr>
							<td class="r-label">
	                            当前状态
	                        </td>
							<td style="padding:0 5px 0 0;">
								<input class="textbox" disabled  id="CurrentStauts" style="width:174px;"></input>
							</td>
						</tr>
						<tr>
							<td class="r-label">
	                            产房
	                        </td>
							<td style="padding:0 5px 0 0;">
                            <select id="DeliveryRoom"
                                    class="hisui-combobox"
                                    style="width:181px;">
                            </select>
							</td>
						</tr>
						<tr>
							<td class="r-label" colspan="2">
								<a href="#"
								   id="CancelRegisteBtn"
								   class="hisui-linkbutton hover-dark"
								   
								   style="position:relative;top:5px;width:90px;left:-25px;">
								   取消登记
								</a>
								<a href="#"
								   id="RegisteBtn"
								   class="hisui-linkbutton hover-dark"
								   
								   style="position:relative;top:5px;width:90px;left:-56px">
								   登记
								</a>
								<a href="#"
								   id="FinishBtn"
								   class="hisui-linkbutton hover-dark"
								   style="position:relative;top:5px;width:90px;left:-15px; ">
								   分娩完成
								</a>
	                        </td>
						</tr>
					</table>	
				</div>
           </div>
        </div>
    </div>
    <script language="javascript">
    var EpisodeID = "#(EpisodeID)#";
    </script>
</body>

</html>