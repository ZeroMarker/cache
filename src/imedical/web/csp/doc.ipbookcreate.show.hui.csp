<div style="width:1600px;height:170px;margin-left:5px;">
	<div class="panel-header panel-header-card" style="width:80px;">
		<div class="panel-title">#(..Get("患者信息"))#</div>
	</div>
	<div class="panel-body">
				<table class="search-table">
					<tr>
						<td class="r-label" style="width:56px;">
							<label for="PatNo">#(..Get("登记号"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatNo"/>
						</td>
						<td class="r-label">
							<label for="PatName">#(..Get("姓名"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatName"/>
						</td>
						<td class="r-label">
							<label for="PatSex">#(..Get("性别"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatSex"/>
						</td>
						<td class="r-label">
							<label for="PatAge">#(..Get("年龄"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatAge"/>
						</td>
						<td class="r-label">
							<label for="PatCompany">#(..Get("工作单位"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatCompany"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="PatMRNo">#(..Get("病案号"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatMRNo"/>
						</td>
						<td class="r-label">
							<label for="PatType">#(..Get("患者类型"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatType"/>
						</td>
						<td class="r-label">
							<label for="PatPhone">#(..Get("联系电话"))#</label>
						</td>
						<td>
							<input class="textbox" id="PatPhone"/>
						</td>
						<td class="r-label">
							<label for="PatID">#(..Get("身份证号"))#</label>
						</td>
						<td>
							<input class="textbox" disabled id="PatID"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="PatFName">#(..Get("联系人"))#</label>
						</td>
						<td>
							<input class="textbox" id="PatFName"/>
						</td>
						<td class="r-label">
							<label for="PatFPhone">#(..Get("联系人电话"))#</label>
						</td>
						<td>
							<input class="textbox" id="PatFPhone"/>
						</td>
						<td class="r-label">
							<label for="PatFRelation">#(..Get("联系人关系"))#</label>
						</td>
						<td>
							<input class="textbox" id="PatFRelation"/>
						</td>
						<td class="r-label">
							<label for="PatAddress">#(..Get("家庭住址"))#</label>
						</td>
						<td colspan="3">
							<input class="textbox" id="PatAddress" style="width:100%;height:30px;line-height:30px;box-sizing:border-box;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;"/>
						</td>
					</tr>
				</table>
	</div>
</div>
<div style="height:5px;"></div>
<div style="width:1600px;height:130px;margin-left:5px;">
	<div class="panel-header panel-header-card" style="width:80px;">
		<div class="panel-title">#(..Get("诊断信息"))#</div>
	</div>
	<div class="panel-body" style="padding:5px 0 0 0;">
				<div style="margin:10px 0 0 11px;">
					<label class="clsRequired" for="MRDiaList">#(..Get("临床诊断"))#</label>
					<div id="MRDiaList" name="MRDiaList" style="width:100%;display:inline;margin-left:2px;">
					</div>
			    </div>
				<table class="search-table" style="border-spacing:0 10px;">
					<tr>
						<td class="r-label">
							<label for="AdmInitState">#(..Get("入院病情"))#</label>
						</td>
						<td>
							<input class="textbox" id="AdmInitState"/>
						</td>
						<td class="r-label" style="width:70px;">
							<label for="DiaStatus">#(..Get("诊断状态"))#</label>
						</td>
						<td>
							<input class="textbox" id="DiaStatus"/>
						</td>
						<td class="r-label" style="width:70px;">
							<label for="AdmDiadesc">#(..Get("临床诊断"))#</label>
						</td>
						<td>
							<input class="textbox" id="AdmDiadesc"/>
						</td>
						<td class="r-label">
							<input id="DiaType" class='hisui-checkbox' type="checkbox" label="中医诊断">
						</td>
					</tr>
				</table>
	</div>
</div>
<div style="height:5px;"></div>
<div style="width:1600px;height:210px;margin-left:5px;">
	<div class="panel-header panel-header-card" style="width:80px;">
		<div class="panel-title">#(..Get("住院证信息"))#</div>
	</div>
	<div class="panel-body" style='padding:10px 0 0 0;'>
				<table class="search-table">
					<tr>
						<TD class="r-label" colspan="4" style="text-align:left;">
							<div id='CLocWard' style='display:inline'><input class='hisui-radio' type="radio" label='本科室病区' name='WardAre' value='LocWard' id='LocWard'></div>
  							<div id='CLinkWard' style='display:inline'><input class='hisui-radio' type="radio" label='其他允许病区' name='WardAre' value='LinkWard' id='LinkWard'></div>
           					<div id='CAllWard' style='display:inline'><input class='hisui-radio' type="radio" label='无收治限制' name='WardAre' value='AllWard' id='AllWard' ></div>
						</TD>
						<td class="r-label">
							<label class="clsRequired" for="InCurStatuKW">#(..Get("当前操作"))#</label>
						</td>
						<td colspan="3">
							<div id="InCurStatuKW"></div>
						</td>
						
					</tr>
				
					<tr>
						<td class="r-label">
							<label class="clsRequired" for="InCtloc">#(..Get("住院科室"))#</label>
						</td>
						<td>
							<input class="textbox" id="InCtloc"/>
						</td>
						<td class="r-label" style="width:70px;">
							<label class="clsRequired" for="InWard">#(..Get("意向病区"))#</label>
						</td>
						<td>
							<input class="textbox" id="InWard"/>
						</td>
						<td class="r-label" style="width:70px;">
							<label for="CTLocMedUnit">#(..Get("医疗单元"))#</label>
						</td>
						<td>
							<input class="textbox" id="CTLocMedUnit"/>
						</td>
						<td class="r-label">
							<label for="InDoctor">#(..Get("主治医师"))#</label>
						</td>
						<td>
							<input class="textbox" id="InDoctor"/>
						</td>
						<td class="r-label">
							<label class="clsRequired" for="InSdate">#(..Get("预约日期"))#</label>
						</td>
						<td>
							<input class="hisui-dateboxq textbox" id="InSdate" data-options="formatter:myformatter,parser:myparser"/>
						</td>
					</tr>
					<tr>
						<!--td class="r-label">
							<label class="clsRequired" for="InCurStatu">当前状态</label>
						</td>
						<td>
							<input class="textbox" id="InCurStatu"/>
						</td-->
						<td class="r-label">
							<label class="clsRequired" for="InReason">#(..Get("操作原因"))#</label>
						</td>
						<td>
							<input class="textbox" id="InReason"/>
						</td>
						<td class="r-label">
							<label for="InSorce">#(..Get("入院途径"))#</label>
						</td>
						<td>
							<input class="textbox" id="InSorce"/>
						</td>
						<td class="r-label">
							<label for="PatientLevel">#(..Get("患者等级"))#</label>
						</td>
						<td>
							<input class="textbox" id="PatientLevel"/>
						</td>
						<td class="r-label">
							<label for="IPDeposit">#(..Get("住院押金"))#</label>
						</td>
						<td>
							<input class="textbox" id="IPDeposit"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="TreatedPrinciple">#(..Get("收治原则"))#</label>
						</td>
						<td>
							<input class="textbox" id="TreatedPrinciple"/>
						</td>
						<td class="r-label">
							<label for="InBedType">#(..Get("床位类型"))#</label>
						</td>
						<td>
							<input class="textbox" id="InBedType"/>
						</td>
						<td class="r-label">
							<label for="InResumeText">#(..Get("备注"))#</label>
						</td>
						<td>
							<input class="textbox" id="InResumeText" style=""/>
						</td>
						<td class="r-label">
							<label for="IsDayFlag">#(..Get("日间标志"))#</label>
						</td>
						<td>
							<input id="IsDayFlag" class='hisui-checkbox' type="checkbox" label="">
						</td>
						<td class="r-label">
							<label for="IsOutTriage">#(..Get("外院转来"))#</label>
						</td>
						<td>
							<input id="IsOutTriage" class='hisui-checkbox' type="checkbox" label="">
						</td>
						
					</tr>
				</table>
	</div>
</div>
<div style="margin-top:10px;margin-bottom:10px;text-align:center;">
				<csp:if condition='IPBKFlag="Booking"'>
					<a href="#" id="CreatNew" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">患者切换</a>
					<div style="display:inline-block;margin:0 20px;"></div>
				</csp:if>
				<csp:if condition='DoctorType="DOCTOR"'>
					<a href="#" id="Save" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
					<div style="display:inline-block;margin:0 20px;"></div>
					<a href="#" id="SaPrint" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">保存并打印</a>
					<div style="display:inline-block;margin:0 20px;"></div>
					<a href="#" id="Print" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">重打</a>
					<div style="display:inline-block;margin:0 20px;"></div>
					<a href="#" id="OrderLink" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'">院前医嘱录入</a>
					<div style="display:inline-block;margin:0 20px;"></div>
					<a href="#" id="OpertionLink" class="hisui-linkbutton" data-options="iconCls:'icon-w-list'">日间手术申请</a>
				</csp:if>
</div>


	<csp:if condition='IPBKFlag="Booking"'>
		<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 5px;border:0;">
				<div id="tt" fit="true" class="hisui-tabs tabs-gray" data-options="tabPosition:'left',headerWidth:100" style="width:auto;height:100%;">   
					<div title="住院证" style="">   
							<div style="height:51px;border-bottom:1px solid #ccc;">
								<table class="search-table" style="padding-left:10px;">
									<tr>
										<td class="r-label">
											<label for="FindBookDateF">#(..Get("预约日期"))#</label>
										</td>
										<td>
											<input class="hisui-dateboxq textbox" id="FindBookDateF" data-options="formatter:myformatter,parser:myparser"/>
										</td>
										<td class="r-label">
											<label for="FindBookDateN">-</label>
										</td>
										<td>
											<input class="hisui-dateboxq textbox" id="FindBookDateN" data-options="formatter:myformatter,parser:myparser"/>
										</td>
										<td class="r-label">
											<label for="i-sort">#(..Get("排序"))#</label>
										</td>
										<td>
											<input class="textbox" id="i-sort"/>
										</td>
									    <td class="r-label">
									    	<a href="#" id="BookListFind" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
									    </td>
									</tr>
								</table>
							</div>
							<div style="height:400px;">
								<table id="BookListTab"></table>
							</div>
						
					</div>   
					<div title="就诊记录" style="overflow:auto;">  
							<div style="height:51px;border-bottom:1px solid #ccc;">
								<table class="search-table" style="padding-left:10px;">
									<tr>
										<td class="r-label">
											<label for="AdmDateF">#(..Get("就诊日期"))#</label>
										</td>
										<td>
											<input class="hisui-dateboxq textbox" id="AdmDateF" data-options="formatter:myformatter,parser:myparser"/>
										</td>
										<td class="r-label">
											<label for="AdmDateN">-</label>
										</td>
										<td>
											<input class="hisui-dateboxq textbox" id="AdmDateN" data-options="formatter:myformatter,parser:myparser"/>
										</td>
									    <td class="r-label">
									    	<a href="#" id="AdmListFind" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
									    </td>
									</tr>
								</table>
							</div>
							<div style="height:400px;">
								<table id="AdmListTab"></table>
							</div> 
					</div>   
				</div>
		</div>
	</csp:if>

