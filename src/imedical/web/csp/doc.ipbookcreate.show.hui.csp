<div class="hisui-panel" data-options="border:false" style="border:none;">   
	<div style="height:auto;margin-bottom:10px;">
		<div class="hisui-panel " title=#(..Get("患者信息"))#  data-options="headerCls:'panel-header-gray',iconCls:'icon-patient-info'">
		<!--div class="panel-header panel-header-gray">
			<div class="panel-title">#(..Get("患者信息"))#</div>
		</！div>
		<div class="panel-body"-->
			<table class="search-table">
				<tr>
					<td class="r-label" style="width:56px;">
						<label for="PatNo">#(..Get("登记号"))#</label>
					</td>
					<td>
