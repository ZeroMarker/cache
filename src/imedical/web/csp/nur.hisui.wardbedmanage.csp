<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    ;s %session.Data("LOGON.LANGID")="20" ;简体中文 
    ;s %session.Data("LOGON.USERID")="4636" 
    ;s %session.Data("LOGON.WARDID")="1" ;s sc=%session.Login("_system","SYS",1)
    ;q 1
    i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
 	q 1
</csp:method>
<HTML lang="zh-CN">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>
        <EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE>
    </title>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>
    <link href="../scripts/nurse/hisui/css/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <script type="text/javascript" src="../scripts/nurse/hisui/utils.js"></script>
    <script type="text/javascript" src="../scripts/nurse/hisui/wardbedmanage.js"></script>

    <STYLE type='text/css'>
    .rowBtnLocation{	    
	    margin-top: 8px;
	    margin-left: 10px;
	 }
	.toolBtnLocation{	    
	    margin-top: 5px;
	 }
	.notCareSexBtn {
    	color: blue;
    	font-size: 1em;
	}
    .maleBtn{
	    color:#32BACA;
	}
	.femaleBtn{
	    color:#D987B8;
	}
	.lockBtn{
	    color:red;
	}
	.unlockBtn{
	    color:green;
	}
	.releaseBtn
	{
	    color:#66B032;
	}
	.cancelReleaseBtn
	{
	    color:red;
	}
    </STYLE>
</head>

<body>
    <div id="cc" class="hisui-layout" fit=true>   
    	<div  fit=true data-options="region:'center',title:'',split:true,headerCls:'panel-header-gray'" style="padding:0 0 5px 0;border-color:#cccccc">
	    	 <table fit=true id="wardBedGrid" class="hisui-datagrid" title="床位列表">   
	    		<thead>   
	    			<tr>	    			
	    				<th data-options="field:'ck',checkbox:true,width:10" ></th>   
	    				<th data-options="field:'BedCode',width:40">床号</th>   
	    				<th data-options="field:'BedStatus',width:70">床位状态</th> 
	    				<th data-options="field:'BedSexDesc',width:70">床位性别</th> 
	    				<th data-options="field:'EmptyDate',width:100">预空日期</th> 
	    				<th data-options="field:'BedBill',width:100">床位费</th> 
	    				<th data-options="field:'CurrPatName',width:100">当前患者</th> 
	    				<th data-options="field:'CurrRegNO',width:100">当前登记号</th> 
	    				<th data-options="field:'CurrPatInHosDate',width:150">入院时间</th>	    				 
	    				<th data-options="field:'AppPat',width:110">预约患者</th> 
	    				<th data-options="field:'AppRegNo',width:110">预约登记号</th> 
	    				<th data-options="field:'IPBookNo',width:120">住院证号</th> 
	    				<th data-options="field:'AppDate',width:100">预约日期</th>
	    				<th data-options="field:'Operate',align:'left',width:100"><span style="margin:0 20px">操&nbsp;作</span></th>
	    			</tr>   
	    		</thead> 
	    	</table>
	    	<div id="releaseDlg" class="hisui-dialog" title="确认释放?" style="width:400px;height:200px;padding:10px;" data-options="iconCls:'icon-save',modal:true,closed:true">   
		    	<div align="center">
		    		<div style="margin:10px">
		    			<span style="color:red">释放日期</span>
		    			<input id="releaseDateBox" class="hisui-datebox textbox" style="width:140px;"></input>			    		
                    </div>
                    <div>
                    	<a href="#" id="confirmReleaseBtn" class="hisui-linkbutton hover-dark"  data-options="iconCls:'icon-ok'">确认</a>
                    	<a href="#" id="closeDlgBtn" class="hisui-linkbutton hover-dark"  data-options="iconCls:'icon-w-close'">关闭</a>
					</div>
				</div>
			  </div>
    	   </div>
    </div>
</body>
</html>
