<!--dhcpesendaudit.hisui.csp-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	/*padding: 0 10px;*/
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 60px;
}
.newclsInvalid{
	border-color: red;
	color: red;
}
.panel-header{
	border-bottom: 0;
}


</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:135px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'粘贴',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
				  <td class="r-label"><label for="DoRegNo">登记号</label></td>
					<td>
						<input class="textbox" id="DoRegNo">
					</td>
					<td class="r-label r-label-1" ><label for="StartDate">开始日期</label></td>
					<td>
						<input id="StartDate" class="hisui-datebox textbox">
					</td>
					<td class="r-label r-label-1"><label for="PGADM_DR_Name">团体</label></td>
          			<td> 
              			<input class="hisui-combobox" type="text" Id="PGADM_DR_Name">
         			 </td>
					<td class="l-label r-label-1">
						<input class='hisui-checkbox' id="AutoSend" type="checkbox" data-options="checked:true" value="1" label="自动粘贴">
					</td>
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
				</tr>
				<tr>
				 	<td class="r-label"> <label for="Sex_DR_Name">性别</label></td>
				 	<td>
           				<input class="hisui-combobox" type="text" Id="Sex_DR_Name" style="width:155px;">
          		 	</td>
         			 <td class="r-label r-label-1"><label for="EndDate">结束日期</label></td>
					<td>
						<input id="EndDate" class="hisui-datebox textbox" >
					</td>
					
					<td class="r-label r-label-1"><label for="VIPLevel">类别</label></td>
          			<td>
            			<input class="hisui-combobox" type="text" Id="VIPLevel">
         			 </td>
         			 <td class="l-label r-label-1">
						<input class='hisui-checkbox' id="NoSend" type="checkbox" label="未粘贴">
					</td>
					<td class="r-label r-label-1">
						<a id="Bclear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
				</tr>
					
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="SendAuditQueryTab"></table>
				</div>
        	</div>
</div>
<div id="ContrastWin" data-options="iconCls:'icon-w-paper'" style="padding:10px;">
      <table id="QryContrastWin" data-options="bodyCls:'panel-header-gray',fit:true,singleSelect:true"></table>
  </div>

<script type="text/javascript" src="../scripts/DHCPESendAudit.hisui.js"></script>
<script type="text/javascript" src="../scripts/DHCPEPrintReportCommon.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
  
  
</body>		
</html>
			
		
