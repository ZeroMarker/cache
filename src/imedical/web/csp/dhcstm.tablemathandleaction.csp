<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">
	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

i Action = "query" d
	.s strPar=$Get(%request.Data("strPar",1))
	.d ##class(web.DHCSTM.TableMatHandle).Query(Start,Limit,Sort,Dir,strPar)
i Action = "QuerySum" d
	.s strPar=$Get(%request.Data("strPar",1))
	.d ##class(web.DHCSTM.TableMatHandle).QuerySum(Start,Limit,Sort,Dir,strPar)
	
i Action = "Create" d
	.s MainInfo=$Get(%request.Data("MainInfo",1))
	.s ListDetail=$Get(%request.Data("ListDetail",1))
	.s user=$Get(%request.Data("user",1))
	.s result = ##class(web.DHCSTM.TableMatHandle).Create(MainInfo,ListDetail,user)
	.i +result<0 d
	..w "{success:'false',info:'"_result_"'}"
	.e  d
	..w "{success:'true',info:'"_result_"'}"

i Action = "CancelNorMatRecStr" d
	.s IntrStr=$Get(%request.Data("IntrStr",1))
	.s user=$Get(%request.Data("user",1))
	.s result = ##class(web.DHCSTM.TableMatHandle).CancelNorMatRecStr(IntrStr,user)
	.w "{success:'true',info:'"_result_"'}"
</script>