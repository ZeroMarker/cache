<div class="hisui-layout" data-options="fit:true" style="border:none;">
		<div data-options="region:'west'"  style="width:976px;border:none;">
			<div  class="hisui-panel ma-panel" fit="true"  style="border:none;border-right:0px;padding:10px 5px 10px 10px;">
				<table id="gridMROBSItemMap" data-options="toolbar:'#custtb'"></table>
				<div id="custtb" style="padding:3px;display:none" >
					<div style="padding: 7px 0 7px 7px;"> 
						<input class="hisui-searchbox" id="searchbox" style="width:240px;" data-options="prompt:'搜索'" />			 
					</div>
					<span class="line" style="display: block;border-bottom: 1px dashed rgb(204, 204, 204);margin: 3px 0px;clear: both; padding-bottom: 1px; "></span>
					<a class="hisui-linkbutton" id="btnAll"  data-options="iconCls:'icon-all-select',plain:true,stopAllEventOnDisabled:true"> 全部 </a>
					<a class="hisui-linkbutton" id="btnPend" data-options="iconCls:'icon-compare-no',plain:true,stopAllEventOnDisabled:true"> 未对照 </a>
					<a class="hisui-linkbutton" id="btnFin"  data-options="iconCls:'icon-compare-yes',plain:true,stopAllEventOnDisabled:true" > 已对照 </a>
					<a class="hisui-linkbutton" id="btnSyn" style="height: 24px; border-left: 1px solid LightGrey; border-right: 0px solid LightGrey;margin: 1px 1px; " data-options="iconCls:'icon-radio',plain:true,stopAllEventOnDisabled:true" > 自动匹配 </a>
					<a class="hisui-linkbutton" id="btnEdit" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"> 修改 </a>
					<a class="hisui-linkbutton" id="btnAdd"  data-options="iconCls:'icon-compare',plain:true,stopAllEventOnDisabled:true"> 对照 </a>
					<a class="hisui-linkbutton" id="btnDelete"  data-options="iconCls:'icon-undo',plain:true,stopAllEventOnDisabled:true" > 撤销 </a>

				</div>
			</div>	
		</div>
		
		<div data-options="region:'center'"  style="padding:0;border:none;">
			<div class="hisui-panel ma-panel" fit="true" style="border:none;padding:10px 10px 10px 5px;">
				<table id="gridMROBSItem" data-options="toolbar:'#custtb2'"></table>
				<div id="custtb2" style="padding: 0.5px;;display:none" >
					<!--<div style="float:right;padding-right:10px;">
						<input class="hisui-searchbox" id="searchbox_two" style="width:240px;" data-options="prompt:'检索'" />			 
					</div>-->
				</div>	
			</div>
		</div>
</div>
<div id="winEdit" style="border:none;overflow:hidden;padding-right:10px;margin-bottom: -10px; display:none">
	<table class="search-table">
		<tr>
			<td class="r-label">
				<label for="txtMapNote">标准备注</label>
			</td>
			<td>
				<input class="textbox" id="txtMapNote" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label" colspan="1">
				<label for="chkIsActive" >是否有效</label>
			</td>
			<td>
				<input class='hisui-checkbox' type="checkbox" id="chkIsActive" checked/>
			</td>
		</tr>
	</table>
</div>