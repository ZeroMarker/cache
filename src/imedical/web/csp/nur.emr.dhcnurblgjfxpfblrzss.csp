{
$.messager.confirm($g("提示"), dataNew.PromptMsg, function (r) {
if (r) {
ChangeData(dataNew,currentvalue,'TextElement_150');
}
});
}
else{
ChangeData(dataNew,currentvalue,'TextElement_150');
}
});

}
