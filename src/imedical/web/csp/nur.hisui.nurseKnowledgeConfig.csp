<csp:method name=OnPreHTTP
            arguments=""
            returntype=%Boolean>
    i ##Class(ext.websys.SessionEvents).SessionExpired() q 1 
    q 1
</csp:method>
<html lang="zh-CN">
<head>
	<meta http-equiv="Content-Type"
		  content="text/html; charset=utf-8">
	<title>
		<EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE>
	</title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI />
	<script type="text/javascript"
			src="../scripts/nurse/hisui-0.1.0/dist/js/config/nurseKnowledgeConfig.js" charset=gbk></script>
	<STYLE type='text/css'>
		body {
            background-color: #fff;
            padding:5px!important;
        }
        .td_label {
        	text-align:right;
        	height: 40px;
        	padding:0 5px 0 40px;
        }
        .td_item {
        	height: 40px;
        }
        .datagrid-wrap.panel-body.panel-body-noheader{
	        border-color:#ccc;
	        border-top-right-radius:0;
	        border-top-left-radius:0;
	        border-right:0;
	        border-left:0;
	        border-bottom:0;
        }
	</STYLE>
</head>

<body>
	<div class="hisui-layout"
	     data-options="fit:true">
	    <div data-options="region:'north',title:'',split:true,headerCls:'panel-header-gray',iconCls:'icon-paper',border:false" 
			 style="height:60px;padding:5px;">
			<table cellpadding="20">
				<tr>
					<td>院区</td>
					<td class="td_item">
						<select id="comboHospital" 
								class="hisui-combobox" 
								style="width:220px;"
								data-options="disabled:true">
						</select>
					</td>
					<td class="td_label">病区</td>
					<td class="td_item">
						<select id="comboDepartment" 
								class="hisui-combobox" 
								style="width:220px;"
								data-options="disabled:true">
						</select>
					</td>
					<td class="td_label">
						<a id="btnSearch" class="hisui-linkbutton hover-dark">查询</a>
					</td>
				</tr>
			</table>
		</div>
	    <div data-options="region:'west',split:true"
	         style="width:250px;border:none">
	        <div id="knowledgePanel"
	        	 class="hisui-panel"
	             data-options="title:'知识库列表',fit:true,headerCls:'panel-header-gray'">
				<ul id="knowledgeCategoryTree" class="hisui-tree">
	            </ul>
	            <div id="menuTree" class="hisui-menu" style="width:120px;">
				</div>		            
	        </div>
	    </div>
	    <div data-options="title:'元素列表',region:'center',split:true,headerCls:'panel-header-gray'">
	        <div class="hisui-layout" data-options="fit:true,border:false">
	        	<div data-options="region:'west',border:false" style="width:60px;">
	        		<a id="btnAddT" style="width:60px;border-bottom:1px solid white" class="hisui-linkbutton">文本</a>
		        	<a id="btnAddO" style="width:60px;border-bottom:1px solid white" class="hisui-linkbutton">O</a>
		        	<a id="btnAddM" style="width:60px;border-bottom:1px solid white" class="hisui-linkbutton">M</a>
		        	<a id="btnAddD" style="width:60px;border-bottom:1px solid white" class="hisui-linkbutton">D</a>
	        	</div>
	        	<div data-options="region:'center',border:false" style="border-left:1px solid dotted;">
	            	<ul id="elementTree">
	            	</ul>
	            	<div id="menuElement" class="hisui-menu" style="width:120px;">
					</div>	
	        	</div>
	        </div>
	        
	    </div>
	    <div data-options="region:'east',split:true"
	         style="width:1000px;border:none">
	        <div class="hisui-layout" data-options="fit:true">
			    <div data-options="region:'north',title:'',split:true,headerCls:'panel-header-gray',iconCls:'icon-paper',border:false" 
			 		 style="height:350px;"> 
			        <div id="contentPanel"
			        	 class="hisui-panel"
			             data-options="title:'内容',fit:true,headerCls:'panel-header-gray'"
			             style="padding:4px;">
			        </div>
			    </div>
			    <div data-options="region:'center',split:true" style="border:none">
			    	<div>
						<a id="btnSave" title="保存" style="float:right" class="hisui-linkbutton">保存</a>
					</div>
			    </div>
			    <div data-options="region:'south',split:true" style="border:none;height:300px;">
				    	<div id="editPanel"
				    		 class="hisui-panel"
				             data-options="title:'编辑',fit:true,headerCls:'panel-header-gray'"
				             style="overflow-y:auto;padding:10px;">
				             <table id="tableEdit" cellpadding="20">
								<tr id="trFirst" style="display:none;">
									<td style="width:40px;">名称</td>
									<td>
										<input id="txtName" 
											   class="textbox" 
											   style="width:300px;">
									</td>
									<td></td>
								</tr>
								<tr>
									<td>内容</td>
									<td colspan="2" class="td_item">
										<div id="divArea"><textarea id="txtItem" style="width:800px;height:120px;"></textarea></div>
										<div id="divDatetime" style="display:none">
											<input id="date" class="hisui-datebox textbox datebox-f combo-f" value="2020-02-17"></input>
											<input id="time" class="hisui-timespinner" data-options="showSeconds:true" style="border-radius: 2px;">
											<br>
											<input class="hisui-checkbox" type="checkbox" label="日期" checked="checked" id="cbDate">
											<input class="hisui-checkbox" type="checkbox" label="时间" checked="checked" id="cbTime">
										</div>
									</td>
								</tr>
								<tr id="trLast" style="display:none;">
									<td>默认</td>
									<td>
										<input id="txtDefaultVal" 
											   class="textbox" 
											   style="width:300px;">
									</td>
									<td></td>
								</tr>
							</table>
				        </div>
			    	</div>
			    </div>
			</div>
	    </div>
	</div>
	<div id="dialogTreeConfig">
		<div style="padding:10px">	
			<table style="border-collapse:separate; border-spacing:0px 10px;">	
				<tr>
					<td class="td-label">病区</td>
					<td>
						<select id="comboWard" 
								class="hisui-combobox" 
								style="width:250px;">
						</select>
					</td>
				</tr>
				<tr>
					<td class="td-label">关键字</td>
					<td>
						<input id="wardKey" style="width:250px;" class="textbox">
					</td>
				</tr>
				<tr>
					<td class="td-label">描述</td>
					<td>
						<input id="desc" style="width:250px;" class="textbox">
					</td>
				</tr>
				<tr>
					<td class="td-label">授权</td>
					<td>
						<select id="comboAuthLoc" 
								class="hisui-combobox" 
								style="width:250px;">
						</select>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div id="dialogAddCate">
		<div style="padding:10px">	
			<table style="border-collapse:separate; border-spacing:0px 10px;">	
				<tr>
					<td class="r-label">名称</td>
					<td>
						<input id="cateName" style="width:250px;" class="textbox">
					</td>
				</tr>
				<tr>
					<td class="r-label">描述</td>
					<td>
						<input id="cateDesc" style="width:250px;" class="textbox">
					</td>
				</tr>
			</table>
		</div>
	</div>
</body>

</html>