
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>

<SCRIPT language="Cache" RUNAT="SERVER">
	s TitleColor=##class(web.DHCEQCommon).GetSysInfo("902001")   //add by WL 2019-11-20 WL0013 begin 润乾列标题背景色
	s RowColor=##class(web.DHCEQCommon).GetSysInfo("902002")	// 润乾行背景色
	s AmtRowColor=##class(web.DHCEQCommon).GetSysInfo("902003") //add by WL 2019-11-20 WL0013 end 润乾合计行背景色
</SCRIPT>
<!DOCTYPE html>
<HTML>
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />

<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<DHCEQ:Report></DHCEQ:Report>
<HISUI/>
</HEAD>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" title="科室领用配件汇总查询" data-options="eqtitle:'maintitle',fit:true">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="height:50px">
					<div class="eq-table" style=#(ReportStyle)#  d='tQueryPanel'>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><lable for="MonthStr">月份</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="MonthStr" data-options='validParams:"YM"' value=#(CurMonth)#></div>
							<div class="eq-table-td eq-table-label"><lable id="cEquipType" for="EquipType">配件类组</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox"  data-options="'component':'PLAT.L.AccessoryType'" id="EquipType"></div>
							<div class="eq-table-td eq-table-label"><lable id="cFromLoc" for="FromLoc">供给部门</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox"  data-options="'component':'PLAT.L.Loc'" id="FromLoc"></div>
							<div class="eq-table-td eq-table-label"><lable id="cToLoc" for="ToLoc">接收部门</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox"  data-options="'component':'PLAT.L.Loc'" id="ToLoc"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-label"><a id="BFind" href="#" class="hisui-linkbutton">查询</a></div>
							<div class="eq-table-td eq-table-label"><a id="BPrint" href="#" class="hisui-linkbutton">打印</a></div>
						</div>
					</div>
				</div>
				<!--润乾报表显示位置-->
				<div data-options="region:'center',border:false" style="overflow:hidden;">
					<iframe id="ReportFile" name="ReportFile" src=#(ReportFileSrc)# width=100% height=100% scrolling="no"></iframe>
					<iframe id="ReportFilePrint" name="ReportFilePrint" src=#(ReportFilePrintSrc)# width=100% height=0%></iframe>		
				</div>
			</div>
		</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="width:0;height:0;">
		<form name="DHCEQ">
			<INPUT TYPE="hidden" name="PrintFlag" id="PrintFlag" value=#(PrintFlag)#>
			<INPUT TYPE="hidden" name="ReportFileSrc" id="ReportFileSrc" value=#(ReportFileSrc)#>
			<INPUT TYPE="hidden" name="ReportFilePrintSrc" id="ReportFilePrintSrc" value=#(ReportFilePrintSrc)#>		    
			<INPUT TYPE="hidden" name="ReportFileName" id="ReportFileName" value=#(ReportFileName)#>
			<INPUT TYPE="hidden" name="EquipTypeDR" id="EquipTypeDR">
			<INPUT TYPE="hidden" name="FromLocDR" id="FromLocDR">
			<INPUT TYPE="hidden" name="ToLocDR" id="ToLocDR">
			<INPUT TYPE="hidden" name="TitleColor" id="TitleColor" value="#(TitleColor)#"><!--add by wl 2019-12-09 WL0013 begin -->
			<INPUT TYPE="hidden" name="RowColor" id="RowColor" value="#(RowColor)#">
			<INPUT TYPE="hidden" name="AmtRowColor" id="AmtRowColor" value="#(AmtRowColor)#"> <!--add by wl 2019-12-09 WL0013 end -->
		</form>
	</div>
	
</body>
<script type='text/javascript'>
MonthBox('MonthStr');
function setSelectValue(vElementID,rowData)
{
	if(vElementID=="FromLoc")
	{
		setElement("FromLocDR",rowData.TRowID);
		setElement("FromLoc",rowData.TName);
	}
	if(vElementID=="ToLoc")
	{
		setElement("ToLocDR",rowData.TRowID);
		setElement("ToLoc",rowData.TName);
	}
	if(vElementID=="EquipType")
	{
		setElement("EquipTypeDR",rowData.TRowID);
		setElement("EquipType",rowData.TName);
	}
	
}
</script>

</HTML>
