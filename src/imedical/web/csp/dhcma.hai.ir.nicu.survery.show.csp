<!-- ICU患者日志-->
<div class="hisui-layout" data-options="fit:true">
    <!--查询页面-->
    <div id="west" data-options="region:'west',border:false,collapsible:false" style="width:300px;padding:10px;" class="ma-panel">
        <table id="gridLocList" data-options="toolbar:'#custtb'"></table>
        <div id="custtb" style="height: 90px; padding-left: 10px;">
            <table class="search-table">
                <tr>
                    <td>
                        <input class="hisui-combobox textbox" id="cboHospital" style="width:258px" />
                    </td>
                </tr>
                <tr>
                    <td>
                       	<input class="hisui-combobox textbox" id="cboYear" style="width:127px" />
						<input class="hisui-combobox textbox" id="cboMonth" style="width:127px" />
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <div data-options="region:'center',border:false,collapsible:false" style="padding: 10px 10px 10px 0px; overflow: hidden; " class="ma-panel">
     	<!--NICU患者日志-体重(默认隐藏)-->
        <div id="LayerNICULogsW" style="height: 100%; width: 100%;display: none;">
            <table id="gridNICULogsW" data-options="toolbar:'#custsubtbW'"></table>
            <div id="custsubtbW" style="padding:5px">
                <a class="hisui-linkbutton" id="btnNICUExportW" data-options="iconCls:'icon-export',plain:true,stopAllEventOnDisabled:true"> 导出 </a>
            </div>
        </div>
        <!--NICU患者日志-Apgar(默认隐藏)-->
        <div id="LayerNICULogsA" style="height: 100%; width: 100%;display: none;">
            <table id="gridNICULogsA" data-options="toolbar:'#custsubtbA'"></table>
            <div id="custsubtbA" style="padding:5px">
                <a class="hisui-linkbutton" id="btnNICUExportA" data-options="iconCls:'icon-export',plain:true,stopAllEventOnDisabled:true"> 导出 </a>
            </div>
        </div>
    </div>
</div>
