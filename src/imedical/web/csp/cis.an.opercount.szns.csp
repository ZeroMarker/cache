                                    <input type="checkbox" class="hisui-checkbox" id="UseTourniquetSite2" data-formitem="UseTourniquetSite" label="下肢">
                                   (<input type="checkbox" class="hisui-checkbox" id="LowerLimb1" data-formitem="LowerLimb" label="左">
                                    <input type="checkbox" class="hisui-checkbox" id="LowerLimb2" data-formitem="LowerLimb" label="右">)
                                    <input type="hidden" class="operdata" id="UseTourniquetSite" data-title="止血带部位" data-multiple="N">
                                    <input type="hidden" class="operdata" id="UpperLimb" data-title="止血带部位-大腿" data-multiple="N">
                                    <input type="hidden" class="operdata" id="LowerLimb" data-title="止血带部位-小腿" data-multiple="N">
                                </div>
                                <div class="form-title-normal">止血带压力</div>
								<div class="form-item-normal">
									<input type="text" id="TourniquetPressure" data-title="止血带压力" class="textbox operdata" style="width:73px;" > mmHg(KPa)
								</div>
							</div>
                        </div>
                        <div>
							<div class="form-row">
                                <div class="form-title-right8">止血带使用时间</div>
