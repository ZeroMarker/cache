                                    <input type="checkbox" class="hisui-checkbox" id="PassageWay2" data-formitem="PassageWay" label="绿色通道">
									<input type="checkbox" class="hisui-checkbox" id="PassageWay3" data-formitem="PassageWay" label="放射手术">
									<input type="hidden" class="operdata" id="PassageWay" data-multiple="N" data-title="通道" data-required="Y">
                                </div>
							</div>
                        </div>
                        <div>
							<div class="form-row">
                                <div class="form-title-right8">灭菌包监测</div>
                                <div class="form-item-normal">
                                    <input type="checkbox" class="hisui-checkbox operdata" data-title="灭菌包监测" data-required="Y" id="SterilityPackMonitor" label="合格">
                                </div>
                                <div class="form-title-normal">皮肤消毒</div>
                                <div class="form-item-normal">
                                    <input type="checkbox" class="hisui-checkbox" id="SkinDesinfection1" data-formitem="SkinDesinfection" label="聚维酮碘消毒液">
                                    <input type="checkbox" class="hisui-checkbox" id="SkinDesinfection2" data-formitem="SkinDesinfection" label="安尔碘皮肤消毒液">
                                    <input type="checkbox" class="hisui-checkbox" id="SkinDesinfection3" data-formitem="SkinDesinfection" label="其他"> 
                                    <input type="text" id="SkinDesinfectionOther" class="textbox operdata" value="/" style="width:463px;" data-required="Y" data-title="皮肤消毒-其他">
									<input type="hidden" class="operdata" data-title="皮肤消毒" id="SkinDesinfection" data-required="Y" data-multiple="N">
                                </div>
							</div>
                        </div>
                        <div>
							<div class="form-row">
                                <div class="form-title-right8">手术体位</div>
