                                    <input type="checkbox" class="hisui-checkbox" id="NegativePlateSite2" data-formitem="NegativePlateSite" label="小腿">
                                   (<input type="checkbox" class="hisui-checkbox" id="Crus1" data-formitem="Crus" label="左">
