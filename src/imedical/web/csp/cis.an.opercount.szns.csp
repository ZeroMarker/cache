                                <div class="form-item-normal">
                                    <input type="text" id="DocArriveTime" data-title="医生入室时间" class="hisui-validatebox opertime operdata" data-title="医生入室时间" data-required="Y" style="width:113px;">
                                </div>
                                <div class="form-title-right8">抗生素开始时间</div>
                                <div class="form-item-normal">
                                    <input type="text" id="AntibioticStartTime" data-title="抗生素开始时间" class="hisui-validatebox opertime operdata" data-title="抗生素开始时间" data-required="Y" style="width:113px;">
                                </div>
                                <div class="form-title-right8">抗生素结束时间</div>
                                <div class="form-item-normal">
                                    <input type="text" id="AntibioticEndTime" data-title="抗生素结束时间" class="hisui-validatebox opertime operdata" data-title="抗生素结束时间" data-required="Y" style="width:113px;">
                                </div>
                                <div class="form-title-right10">抗生素术中追加时间</div>
                                <div class="form-item-normal">
                                    <input type="text" id="AntibioticAddiTime" data-title="抗生素术中追加时间" class="hisui-validatebox operdata" data-title="抗生素术中追加时间" data-required="Y" style="width:113px;" value="/">
                                </div>
							</div>
                        </div>
                        <div>
							<div class="form-row">
                                <div class="form-title-right8">备注</div>
