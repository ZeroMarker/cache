                                    <textarea id="RecordNote" class="textbox operdata" style="width:926px;" data-title="备注" data-required="Y" value="/">/</textarea>
                                </div>
							</div>
                        </div>
                        <div>
							<div class="form-row">
                                <div class="form-title-right8">血压</div>
                                <div class="form-item-normal">
                                    <input type="text" id="N_BPS" data-title="收缩压" class="hisui-validatebox operdata" data-title="收缩压" data-required="Y" style="width: 63px;"> / 
                                    <input type="text" id="N_BPD" data-title="舒张压" class="hisui-validatebox operdata" data-title="舒张压" data-required="Y" style="width: 63px;"> mmHg
                                </div>
                                <div class="form-title-normal">脉搏</div>
                                <div class="form-item-normal">
                                    <input type="text" id="Pulse" data-title="脉搏" class="hisui-validatebox operdata" data-title="脉搏" data-required="Y" style="width: 63px;"> 次/分
                                </div>
                                <div class="form-title-normal">呼吸</div>
                                <div class="form-item-normal">
                                    <input type="text" id="RR" data-title="呼吸" class="hisui-validatebox operdata" data-title="呼吸" data-required="Y" style="width: 63px;"> 次/分
                                </div>
                                <div class="form-title-normal">特殊情况</div>
