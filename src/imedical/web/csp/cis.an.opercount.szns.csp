                                <div class="form-item-normal">
                                    <input type="checkbox" class="hisui-checkbox" id="UseTourniquet1" data-formitem="UseTourniquet" label="无">
                                    <input type="checkbox" class="hisui-checkbox" id="UseTourniquet2" data-formitem="UseTourniquet" label="有">
									<input type="hidden" class="operdata" id="UseTourniquet" data-multiple="N" data-title="使用充气式止血带" data-required="Y">
                                </div>
                                <div class="form-title-normal">部位</div>
                                <div class="form-item-normal">
                                    <input type="checkbox" class="hisui-checkbox" id="UseTourniquetSite1" data-formitem="UseTourniquetSite" label="上肢">
                                   (<input type="checkbox" class="hisui-checkbox" id="UpperLimb1" data-formitem="UpperLimb" label="左">
