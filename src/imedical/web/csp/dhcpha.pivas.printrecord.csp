<!DOCTYPE html>
<!--配液流程单据记录
	csp:dhcpha.pivas.printrecord.csp
	js:scripts/pharmacy/pivas/printrecord.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI/>
    <PHAPRINTCOM/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>
</HEAD>
<body style="overflow:hidden;">
	<div class="dhcpha-win-mask">
		<img src="../scripts/pharmacy/common/image/page-loading.gif"></img>
	</div>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',split:true,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">
    <div class="hisui-layout" fit="true" border="false">
        <div data-options="region:'north',title:'流程单据打印记录',split:true,headerCls:'panel-header-gray',iconCls:'icon-print',collapsible:false" class="dhcpha-north-r2">
            <table class="dhcpha-north-tbl">
                <tr>
                    <td>
                        <label for="datePrtStart">操作开始时间</label>
                        <input id="datePrtStart" class="hisui-datebox" data-options="width:120" />
                        <input id="timePrtStart" class="hisui-timespinner" showSeconds="true" data-options="width:100" />
                    </td>
                    <td>
                        <input id="cmbPivasLoc" />
                    </td>
                    <td>
                        <input id="cmbWard" />
                    </td>
                    <td>
                        <input id="txtPatNo" class="hisui-validatebox" placeholder="登记号..." />
                    </td>
                    <td>
                        <a class="hisui-linkbutton" plain="false" iconCls="icon-w-find" id="btnFind">查询</a>
                        <a class="hisui-linkbutton" plain="false" iconCls="icon-w-print" id="btnPrint">补打勾选标签</a>
                        <a class="hisui-linkbutton" plain="false" iconCls="icon-w-print" id="btnPrintArrange">补打排药单</a>
                    </td>
                </tr>
                <tr>
                    <td>
                        <label for="datePrtEnd">操作结束时间</label>
                        <input id="datePrtEnd" class="hisui-datebox" data-options="width:120" />
                        <input id="timePrtEnd" class="hisui-timespinner" showSeconds="true" data-options="width:100" />
                    </td>
                    <td>
                        <input id="cmbWorkType" type="text" />
                    </td>
                    <td>
                    	<input id="cmbPivaStat" type="text" />
                    </td>
                    <td>
                        <input id="txtPNo" class="hisui-validatebox" placeholder="打印序号(1-99)..." />
                    </td>
                    <td>
                        <a class="hisui-linkbutton" plain="false" iconCls="icon-w-clean" id="btnClear">清屏</a>
                        <a class="hisui-linkbutton" plain="false" iconCls="icon-w-print" id="btnPrintAll">补打全部标签</a>
                   		<a class="hisui-linkbutton" plain="false" iconCls="icon-w-print" id="btnPrintWardBat">补打交接单</a>
                    </td>
                </tr>
            </table>
        </div>
        <div data-options="region:'west',title:'单据记录',split:true,headerCls:'panel-header-gray',width:530,iconCls:'icon-panel-print-record'">
            <table id="gridPrint"></table>
        </div>
        <div data-options="region:'center',title:'标签明细',split:true,headerCls:'panel-header-gray',iconCls:'icon-panel-label'">
            <table id="gridOrdExe"></table>
        </div>
    </div>
        </div>
    </div>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
    <script type="text/javascript" src="../scripts/DHCSTXMLPrint.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/print.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/printrecord.js"></script>
    <script type="text/javascript" src="../scripts/DHCCPMRQCommon.js"></script>
</body>
</HTML>