		  XCONTEXT:"#(XCONTEXT)#",
		  CMFlag:"#(CMFlag)#",
		  SaveParam:"#(SaveParam)#",
		  IsHaveMenuAuthOrderOrgFav:"#(IsHaveMenuAuthOrderOrgFav)#",
		  LogCTLocDesc:"#(LogCTLocDesc)#",
		  LocPrefType:"#(LocPrefType)#",
		  XYDefaultContext:"#(XYDefaultContext)#",
		  CMDefaultContext:"#(CMDefaultContext)#",
		  paraPreftabType:"#(paraPreftabType)#"
		}
