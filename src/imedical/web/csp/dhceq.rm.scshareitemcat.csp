<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>

<body class="hisui-layout">
		<div data-options="region:'center',border:false" style="padding:10px">
		<div class="hisui-panel"  title="资源项目分类" data-options="fit:true,eqtitle:'maintitle'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #ccc;height:100px;">
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cSICShareType" for="SICShareType">资源类型</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-combobox textbox" id="SICShareType" ></div>
					<div class="eq-table-td eq-table-label"><label id="cSICCode" for="SICCode">代码</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="SICCode" ></div>
					<div class="eq-table-td eq-table-label"><label id="cSICDesc" for="SICDesc">描述</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="SICDesc" ></div>
					
					<div class="eq-table-td eq-table-label"><a id="BAdd" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-add',stopAllEventOnDisabled:true">新增</a></div>
					<div class="eq-table-td eq-table-label"><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">保存</a></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cSICHospital" for="SICHospital">院区</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Hospital'" id="SICHospital" ></div>
					<div class="eq-table-td eq-table-label"><label for="SICRemark">备注</label></div>
					<div class="eq-table-td"><textarea id="SICRemark" style="width:151px"></textarea></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"></div>
					
					<div class="eq-table-td eq-table-label"><a id="BDelete" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">删除</a></div>
					<div class="eq-table-td eq-table-label"><a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></div>
				</div>
				</div>
				<div data-options="region:'center',border:false">
					<table id="tDHCEQSCShareItemCat" class="hisui-datagrid" data-options="border:false,fit:true"> </table>
				</div>
    	</div>
	</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="SICRowID">
			<INPUT type="hidden" id="SICHospitalDR">
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/rm/scshareitemcat.js'></script>
</body>
</HTML>
