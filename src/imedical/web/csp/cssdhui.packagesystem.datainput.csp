<!--数据导入-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<html lang="zh-CN">
<head>
    <title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>
    <CSSDHISUI/>
</head>
<style>
.panel-tool{
    height:30px;
    margin-top:-15px;
    }
</style>

<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',border:false"></div>
		<div class="GridPadding" data-options="region:'center',border:false" style="height:30px;">
  	  		<div id="btnPanel" class="panel-tool" style="padding-right:10px;">
  	  			<a href="#" id="LoadBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-download'" style="width:120px;height:30px" onmouseover="this.style.backgroundColor='#378ec4'" onmouseout="this.style.backgroundColor='#40a2de'">下载模板</a>
  	  		</div>
        	<div class="hisui-panel" data-options="tools:'#btnPanel',title:'导入数据',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'"  style="width:500px;height:50px;">
            	<div id="UomTB">
            	<table id="Conditions" style="border-spacing:10px 5px;">
            		<tr>
                    	<td style="padding-top:5px">
                    		<input class='hisui-radio' type="radio" label='消毒包' name='Type' value='Package' data-options="checked:true">
							<input class='hisui-radio' type="radio" label='消毒包明细' name='Type' value='PackageItem'>
						</td>    
                    	<td></td>      
                    	<td></td>      
                    	<td>文件</td>
                    	<td>
                    		<input id="File" name="File">
						</td>
						<td>
							<a href="#" id="ReadBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-run'">读取</a>
						</td>
                    	<td>
                    		<a href="#" id="CheckBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-eye'">数据校验</a>
						</td>
						<!--<td>
							<a href="#" id="TestImportBT" style="display:none" class="hisui-linkbutton" data-options="iconCls:'icon-w-import'">测试导入</a>
						</td> -->
						<td>
							<a href="#" id="ImportBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-import'">导入数据</a>
						</td>
						<td>
							<a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
						</td>
					</tr>
            </table>
            </div>
            <table id="DataGrid"></table>
        </div>
    </div>
    <div class="GridPadding" data-options="region:'east',border:false" style="padding-top:10;padding-left:0;width:220px">
        <div class="hisui-panel" data-options="iconCls:'icon-paper',fit:true" style="border:1px solid #ccc;">
            <div id="Msg" class="hisui-panel" data-options="title:'导入结果',border:false,headerCls:'panel-header-gray'"></div>
        </div>
    </div>

<script type="text/javascript" src="../scripts/cssd/Common/XLSX/xlsx.full.min.js"></script>
<script type="text/javascript" src="../scripts/cssd/System/DataInput.js"></script>
</body>
</html>
