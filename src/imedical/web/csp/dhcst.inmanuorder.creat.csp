<!DOCTYPE html>
<!--制剂
	csp:dhcst.inmanuorder.creat.csp
	js: scripts/pharmacy/dhcst/inmanuorder/creat.js
-->

<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.common.css"></link>
</HEAD>

<body>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',split:true,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">
            <div class="hisui-layout" fit="true" border="false">
                <div data-options="region:'north',title:'制剂生产',split:true,headerCls:'panel-header-gray',collapsible:false,iconCls:'icon-paper'" class="dhcpha-north-r4">
                     <div class="pha-row">
		                <div class="pha-col">
		                    <label for="cmbLoc"><font color=red>*</font>制剂科室</label>
		                </div>
		                <div class="pha-col">
		                    <input id="cmbLoc" placeholder="制剂科室..."/>
		                </div>
                    	<div class="pha-col">
		                    <label for="cmbInRec"><font color=red>*</font>制剂名称</label>
		                </div>
		                <div class="pha-col">
		                    <input id="cmbInRec" placeholder="制剂名称..."/>
		                </div>
                    	<div class="pha-col">
		                    <label for="cmbManuUser"><span style="visibility: hidden">占</span><font color=red>*</font>制剂人</label>
		                </div>
		                <div class="pha-col">
		                    <input id="cmbManuUser" placeholder="制剂人..."/>
		                </div>
                    	<div class="pha-col">
		                    <label for="txtManuDate"><font color=red>*</font>制剂日期</label>
		                </div>
		                <div class="pha-col">
		                    <input id="txtManuDate" class="hisui-datebox hisui-validatebox" />
		                </div>
                    	<div class="pha-col">
		                    <label for="txtInManuNo">制剂单号</label>
		                </div>
		                <div class="pha-col">
		                    <input id="txtInManuNo"  class="hisui-validatebox pha_text" placeholder="制剂单号..." disabled="true"/>
		                </div>
                    	<div class="pha-col">
		                    <a class="hisui-linkbutton" iconCls='icon-w-save' plain="false" id="btnSaveOk">保存</a>
		                </div>
		                <div class="pha-col">
		                   <a class="hisui-linkbutton" iconCls='icon-w-find' plain="false" id="btnFind">单据查询</a>
		                </div>
		            </div>
		            <div class="pha-row">
		            	<div class="pha-col" >
		                    <label for="cmbUom"><span style="visibility: hidden">占占</span><font color=red>*</font>单位</label>
		                </div>
		                <div class="pha-col">
		                   <input id="cmbUom"  placeholder="单位..." />
		                </div>
		                
		                <div class="pha-col" >
		                    <label for="txtTheoryQty"><font color=red>*</font>理论数量</label>
		                </div>
		                <div class="pha-col">
		                   <input id="txtTheoryQty" class="hisui-validatebox pha_text" placeholder="理论数量..." />
		                </div>
		                
		                <div class="pha-col" >
		                    <label for="txtFactQty"><font color=red>*</font>实际数量</label>
		                </div>
		                <div class="pha-col">
		                   <input id="txtFactQty" class="hisui-validatebox pha_text" placeholder="实际数量..." />
		                </div>
		                
		                <div class="pha-col" >
		                    <label for="txtAddCost"><span style="visibility: hidden">占</span><font color=red>*</font>附加费</label>
		                </div>
		                <div class="pha-col">
		                   <input id="txtAddCost"  class="hisui-validatebox pha_text" placeholder="附加费..." />
		                </div>
		                
		                <div class="pha-col" >
		                    <label for="chkComp"><span style="visibility: hidden">占占</span>完成</label>
		                </div>
		                <div class="pha-col" style="padding-left:5px;">
		                   <input id="chkComp" type="checkbox" class="hisui-checkbox" disabled="true" />
		                </div>
		                <div class="pha-col" style="padding-left:145px;">
		                    <a class="hisui-linkbutton" iconCls='icon-w-submit' plain="false" id="btnAudit">审核</a>
		                </div>
		                <div class="pha-col">
		                   <a class="hisui-linkbutton" iconCls='icon-w-clean' plain="false" id="btnClear"><span style="visibility: hidden">占</span>清屏<span style="visibility: hidden">占</span></a>
		                </div>
		            </div>
		            <div class="pha-row">
		            	<div class="pha-col" >
		                    <label for="txtBatNo"><span style="visibility: hidden">占占</span><font color=red>*</font>批号</label>
		                </div>
		                <div class="pha-col">
		                   <input id="txtBatNo" class="hisui-validatebox pha_text" placeholder="批号..." />
		                </div>
		                <div class="pha-col" >
		                    <label for="txtExpDate"><span style="visibility: hidden">占</span><font color=red>*</font>有效期</label>
		                </div>
		                <div class="pha-col">
		                   <input id="txtExpDate" class="hisui-datebox hisui-validatebox"/>
		                </div>
		                
		                <div class="pha-col" >
		                    <label for="txtRp"><span style="visibility: hidden">占占</span><font color=red>*</font>进价</label>
		                </div>
		                <div class="pha-col">
		                  <input id="txtRp" class="hisui-validatebox pha_text" placeholder="进价..." />
		                </div>
		                <div class="pha-col" >
		                    <label for="txtSp"><span style="visibility: hidden">占占</span><font color=red>*</font>售价</label>
		                </div>
		                <div class="pha-col">
		                  <input id="txtSp" class="hisui-validatebox pha_text" placeholder="售价..." />
		                </div>
		                <div class="pha-col" >
		                    <label for="chkAudited"><span style="visibility: hidden">占占</span>审核</label>
		                </div>
		                <div class="pha-col" style="padding-left:5px;">
		                  	<input id="chkAudited" type="checkbox"   class="hisui-checkbox" disabled="true"/>
		                </div>
		                <div class="pha-col" style="padding-left:145px;">
		                  	<a class="hisui-linkbutton" iconCls='icon-w-cancel' plain="false" id="btnDel">删除</a>
		                </div>
		            </div>
		            <div class="pha-row">
		            	<div class="pha-col">
		                  	<label for="txtRemarks"><span style="visibility: hidden">占占*</span>注释</label>
		                </div>
		                <div class="pha-col">
		                  	<input id="txtRemarks" placeholder="备注..."  class="hisui-validatebox pha_text" style="width:384px" />
		                </div>
		                <div class="pha-col">
		                  	<label for="txtManuStartDate"><span style="visibility: hidden">*</span>开始日期</label>
		                </div>
		                <div class="pha-col">
		                  	<input id="txtManuStartDate" class="hisui-datebox hisui-validatebox"  />
		                </div>
		            </div>
		            

                    
                </div>
                <div data-options="region:'center',border:false,split:true,width:500" > 
	        		<div class="hisui-layout" fit="true">
		        		<div data-options="region:'west',border:false,split:true,width:500">
			                <div class="hisui-panel"  data-options="headerCls:'panel-header-gray',title:'制剂原料信息',iconCls:'icon-paper',fit:true,bodyCls:''">
			                    <div id="gridInManuOrd"  ></div>
			                </div>
			                
		                </div>
		                <div data-options="region:'center',border:false,split:true,width:500">
			                <div class="hisui-panel"  data-options="headerCls:'panel-header-gray',title:'制剂原料批次信息',split:true,iconCls:'icon-paper',fit:true,bodyCls:''">
			                    <div id="gridInManuOrdBat" ></div>
			                </div>
			                
		                </div>
		                
	                </div>
	            </div>
	            
                
            </div>
        </div>
    </div>
    <div id="FindWindowDiv" class="hisui-dialog" title=' 制剂单查询' style="width:1000px;height:500px;color:white" data-options="closable:true,modal:true,closed:true,collapsible:false,resizable:true,iconCls:'icon-w-list'">
		<div class="dhcpha-hisui-container" fit="true" border="false">													
			<div style="border:1px solid #cccccc;border-radius:4px;height:440px;width:980px;"  >
				<table id="gridInManu"></table>
			</div>
		</div>
	</div>   
	<div id="gridQueryBar" data-options="region:'center'">
		<div class="pha-row">
			<div class="pha-col">
				<label for="txtStartDate">开始日期</label>
			</div>
			<div class="pha-col">
				<input id="txtStartDate" class="hisui-datebox dhcpha-date" style="width:120px" type="text" />
			</div>
			<div class="pha-col">
				<label for="txtEndDate">结束日期</label>
			</div>
			<div class="pha-col">
				<input id="txtEndDate" class="hisui-datebox dhcpha-date" style="width:120px" type="text" />
			</div>
			<div class="pha-col">
				<label for="cmbStatus">状态</label>
			</div>
			<div class="pha-col">
				<input id="cmbStatus" style="width:120px" type="text" />
			</div>
			<div class="pha-col">
				<a class="hisui-linkbutton" iconCls='icon-w-find' plain="false" id="btnSearch">查询</a>
			</div>
			<div class="pha-col">
				<a class="hisui-linkbutton" iconCls='icon-w-upload' plain="false" id="btnSelect">选取</a>
			</div>
			<div class="pha-col">
				<a class="hisui-linkbutton" iconCls='icon-w-close' plain="false" id="btnClose">关闭</a>
			</div>
		</div>   
	</div>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.export.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/dhcst/common/common.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/dhcst/inmanuorder/creat.js"></script>
</body>


</HTML>