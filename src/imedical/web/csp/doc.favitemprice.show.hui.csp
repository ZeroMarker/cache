<div class="hisui-layout" fit="true">
	<div data-options="region:'north',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:62px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" fit="true" data-options="title:'',headerCls:'',iconCls:''" style="border:1px solid #ccc;border-bottom-style:dashed;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="DateFrom">开始日期</label></td>
					<td>
						<input id="DateFrom" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label"><label for="DateTo">结束日期</label></td>
					<td>
						<input id="DateTo" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label"><label for="Tariff">征收</label></td>
					<td>
						<input id="Tariff" class="hisui-combobox textbox">
					</td>
					<td class="r-label"><label for="Price">价格</label></td>
					<td>
						<input id="Price" class="textbox">
					</td>
					<td class="r-label"><label for="Hosp">医院</label></td>
					<td>
						<input id="Hosp" class="hisui-combobox textbox">
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" data-options="title:'',headerCls:'',iconCls:''" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="PriceTab"></table>
		</div>
	</div>
</div>