<div class="hisui-panel" data-options="fit:true,border:false" style="padding:10px;">
	<div class="hisui-panel" data-options="fit:true,border:false,bodyCls:'panel-body-gray'" >
		<div class="hisui-layout" fit="true">
			<div data-options="region:'north',collapsible:false,bodyCls:'panel-body-gray'" style="height:50px;overflow:hidden;border-radius:4px 4px 0 0;border-bottom-style:dashed;">
				<table class="search-table">
					<tr>
						<td class="r-label"><label for="DateFrom">#(..Get("开始日期"))#</label></td>
						<td>
							<input id="DateFrom" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
						</td>
						<td class="r-label"><label for="DateTo">#(..Get("结束日期"))#</label></td>
						<td>
							<input id="DateTo" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
						</td>
						<td class="r-label"><label for="Tariff">#(..Get("征收"))#</label></td>
						<td>
							<input id="Tariff" class="hisui-combobox textbox">
						</td>
						<td class="r-label"><label for="Price">#(..Get("价格"))#</label></td>
						<td>
							<input id="Price" class="textbox">
						</td>
						<td class="r-label"><label for="Hosp">#(..Get("医院"))#</label></td>
						<td>
							<input id="Hosp" class="hisui-combobox textbox">
						</td>
					</tr>
				</table>
			</div>
			<div data-options="region:'center',bodyCls:'panel-body-gray'" style="border-radius:0 0 4px 4px;">
				<table id="PriceTab"></table>
			</div>
		</div>
	</div>
</div>