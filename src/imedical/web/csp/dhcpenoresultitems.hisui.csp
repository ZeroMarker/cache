
<!-- 
 * FileName: dhcpenoresultitems.hisui.csp
 * Anchor: xy
 * Date: 2019-06-10
 * Description: HISUI 未回传结果项目查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<ADDINS require="CmdShell"/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 20px;
}


</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:95px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'未回传结果项目查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border-radius:0;">
			<table class="search-table">
				<tr>
					

					<td class="r-label" id="tDateFrom"><label for="DateFrom">收表开始日期</label></td>
					<td>
						<input id="DateFrom" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1" id="tDateTo"><label for="DateTo">收表结束日期</label></td>
					<td>
						<input id="DateTo" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1"><label for="RecLoc">接收科室</label></td>
					<td>
						<input class="hisui-combogrid" type="text" id="RecLoc"/>
					</td>
					
					 <td class="l-label r-label-1">
						<input class='hisui-checkbox' id="ArrivedFlag" type="checkbox" label="到达">
					</td>

					
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BExport" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">导出</a>
					</td>
				</tr>
				
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="NoResultItemGrid"></table>
			</div>
        </div>
	      
</div>

  
    
<script type="text/javascript" src="../scripts/DHCPENoResultItems.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
</body>		
</html>
			
		
 




