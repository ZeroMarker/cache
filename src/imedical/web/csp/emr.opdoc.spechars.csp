<html>
    <head>
        <title> 特殊字符 </title>
        <HISUI/>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
        <link rel="stylesheet" type="text/css" href="../scripts/emr/css/opdoc.spechars.css"/>
    </head>
    <body class="hisui-layout">
        <div id="spechars" class="hisui-tabs tabs-gray" data-options="headerWidth:80" style="height:365px;padding:10px 10px 0 10px;">
        </div>
        <div style="padding:3px 0 0 10px;">
            <input class="hisui-validatebox textbox" readonly="readonly" id="selectValue" style="float:left;width:280px;"></input>
            <div style="float:left;padding:0 0 0 5px;">
                <a href="#" id="sure" class="hisui-linkbutton">#(..Get("确定"))#</a>
                <a href="#" id="clear" class="hisui-linkbutton">#(..Get("清空"))#</a>
                <a href="#" id="close" class="hisui-linkbutton">#(..Get("关闭"))#</a>
            </div>
        </div>

        <script type="text/javascript" src="../scripts/emr/js/opdoc.spechars.js"></script>
    </body>
</html>
