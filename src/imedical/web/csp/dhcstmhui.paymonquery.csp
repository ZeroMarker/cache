<!--付款月报明细-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
 <SERVER>
 d ##class(web.DHCSTMHUI.Util.RQServerConfig).GetApplet()  //润乾打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'west',border:false" class="GridPadding" style="width:450px;">
			<div class="hisui-panel" data-options="title:'历史付款月报',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">
				<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false" style=""> 
				<table id="HistoryPayMonStatConditions" class="Condition">
					<tr>
						<td>科室</td>
						<td><input id="HistoryPayMonStatLoc" name="HistoryPayMonStatLoc" class="hisui-combobox" style="width:130px;"></td>
					</tr>
					<tr>
						<td>月报范围</td>
						<td align="left"><input id="StYear" name="StYear" class="textbox" style="width:123px;"></td>
						<td align="left">年</td>
						<td align="left"><input id="StMonth" name="StMonth" class="textbox" style="width:123px;"></td>
						<td align="left">月</td>
					</tr>
					<tr>
						<td>至</td>
						<td align="left"><input id="EdYear" name="EdYear" class="textbox" style="width:123px;"></td>
						<td align="left">年</td>
						<td align="left"><input id="EdMonth" name="EdMonth" class="textbox" style="width:123px;"></td>
						<td align="left">月</td>
					</tr>
					<tr>
						<td></td>
						<td align="right"><a href="#" id="SearchBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
						<!--<td></td>
						<td><a href="#" id="PrintBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">打印</a></td>
						-->
					</tr>
				</table>
				</div>
				<div data-options="region:'center',border:false" style="">
				<table id="HistoryPayMonStatGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div data-options="region:'center',border:false" class="GridPadding" style="padding-left:0;">
		<div class="hisui-layout" data-options="fit:true">
			<div id="PayMonTab" class="hisui-tabs tabs-gray" data-options="tools:'#tab-tools',fit:true">
				<div title="付款月报汇总(供应商)"  class="hisui-layout ReportImage" data-options="headerCls:'panel-header-gray',fit:true">
					<iframe id="frameReportPayMonDetail" scrolling="auto" frameborder="0" style="width:100%;height:100%;"></iframe>   
				</div>
				<div title="付款月报明细(供应商分类)"  class="hisui-layout ReportImage" data-options="headerCls:'panel-header-gray',fit:true">
					<iframe id="frameReportPayMonVenCatDetail" scrolling="auto" frameborder="0" style="width:100%;height:100%;"></iframe>   
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/DHCPayMon/DHCPayMonPrint.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/DHCPayMon/DHCPayMonQuery.js"></script>
</body>
</html>
