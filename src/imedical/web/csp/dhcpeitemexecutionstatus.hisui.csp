<!--
 * FileName: dhcpeitemexecutionstatus.hisui.csp
 * Anchor: zhongricheng
 * Date: 2019-11-28
 * Description: HISUI 已检未检弃检项目查询
-->

<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	quit 1
</csp:method>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<script language="JavaScript"> 
		self.moveTo(0,0) 
		self.resizeTo(screen.availWidth,screen.availHeight)
	</script>
	<SCRIPT language="Cache" RUNAT="SERVER">
 		s curDate=+$h
	</SCRIPT>
	<style>
	</style>
</head>
<body>
	<!-- 主页面 start -->
	<div class="hisui-panel" border="false" fit="true" style="padding:10px;">
		<div class="hisui-layout" border="false" data-options="collapsible:false,fit:true">
			<div class="hisui-layout" data-options="region:'north'" style="border-radius:4px; border:1px solid #ccc; height:129px;">
				<!-- 查询条件 north start -->
				<div border="false" data-options="region:'center',title:'体检已检未检弃检项目查询',headerCls:'panel-header-gray',iconCls:'icon-add-note',collapsible:false">
					<table style="border-spacing: 10px;">
						<tr>
							<td align="right"><label for="BeginDate">开始日期</label></td>
							<td style=""><input class="hisui-datebox textbox" id="BeginDate" name="BeginDate" style="width:120px;"/></td>
							
							<td align="right"><label for="Sex">性别</label></td>
							<td style=""><input class="hisui-combobox" id="Sex" name="Sex" style="width:130px;"/></td>
							
							<td align="right"><label for="Station">站点</label></td>
							<td style=""><input class="hisui-combobox" id="Station" name="Station" style="width:150px;"/></td>
							
							<td align="right"><label for="AuditStatus">总检状态</label></td>
							<td style=""><input class="hisui-combobox" id="AuditStatus" name="AuditStatus" style="width:110px;"/></td>
							
							<td align="right"><label for="ItemID">项目</label></td>
							<td style="" colspan=3><input class="hisui-combobox" id="ItemID" name="ItemID" style="width:296px;"/></td>
							
							<td style="text-align:center;"><a class="hisui-linkbutton hover-dark" id="BSearch" data-options="iconCls:'icon-w-find'" href="#" style="width: 100px;">查询</a></td>
						</tr>
						<tr>
							<td align="right"><label for="EndDate">结束日期</label></td>
							<td style=""><input class="hisui-datebox textbox" id="EndDate" name="EndDate" style="width:120px;"/></td>
							
							<td align="right"><label for="VIPLevel">VIP等级</label></td>
							<td style=""><input class="hisui-combobox" id="VIPLevel" name="VIPLevel" style="width:130px;"/></td>
							
							<td align="right"><label for="PERoom">诊室位置</label></td>
							<td style=""><input class="hisui-combobox" id="PERoom" name="PERoom" style="width:150px;"/></td>
							
							<td align="right"><label for="ChcekStatus">检查状态</label></td>
							<td style=""><input class="hisui-combobox" id="ChcekStatus" name="ChcekStatus" style="width:110px;"/></td>
							
							<td align="right"><label for="ItemStatus">项目状态</label></td>
							<td style=""><input class="hisui-combobox" id="ItemStatus" name="ItemStatus" style="width:110px;"/></td>
							
							<td align="right"><label for="ShowType">查询类型</label></td>
							<td style=""><input class="hisui-combobox" id="ShowType" name="ShowType" style="width:110px;"/></td>
							
							<td style="text-align:center;"><a class="hisui-linkbutton hover-dark" id="BClear" data-options="iconCls:'icon-w-clean'" href="#" style="width: 100px;">清屏</a></td>
						</tr>
					</table>			
					<!-- 查询条件 north end -->
				</div>
			</div>
			
			<!-- 结果分析 润乾iframe start -->
			<div border="false" data-options="region:'center'" style="overflow:hidden; padding:0px 2px 2px 0;">
				<div border="false" id="ReportFileDiv" style="width:100%; height:100%; border-radius:4px; border:1px solid #ccc;">
					<iframe id="ReportFile" name="ReportFile" src="dhccpmrunqianreport.csp?reportName=DHCPEItemExecutionStatus.raq" width="100%" height="100%" frameborder="0" border="0" hspace="0" vspace="0" marginwidth="0" marginheight="0" scrolling="auto" allowtransparency="yes"></iframe>
				</div>
			</div>
			<!-- 结果分析 润乾iframe end -->
		</div>
	</div>
	<script type="text/javascript" src="../scripts/DHCPEItemExecutionStatus.hisui.js"></script>
	<script type="text/javascript" src="../scripts/DHCCPMRQCommon.js"></script>
	<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
</body>
</html>