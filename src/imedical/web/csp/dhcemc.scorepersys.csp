<!-- dhcemc.scorepersys.csp  急诊个人评分系统 -->
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<link href="../scripts/dhcnewpro/dhcemc/css/base.css" rel="stylesheet">
	<link href="../scripts/dhcnewpro/dhcemc/css/scorepersys.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcemc/scorepersys.js"></script>
</head>
<body class="background-warp">
	<div class="container">
		<div class="bd-left">
			<div class="panel-north">
				<div class="list-title">
					<div class="list-icon">
						<img src="../scripts/dhcnewpro/dhcem/images/infomation.png" border=0/>
					</div>
					<label>急诊医生评分</label>
					<div class="list-tools">
					</div>
	            </div>
	        	<div class="list-order-item">
	        		<div style="padding:10px 0px 3px 10px;">
						<label style="margin:0px 5px;">开始日期</label><input id="StartDate" class="hisui-datebox textbox" style="width:155px;">
					</div>
					<div style="padding:10px 0px 3px 10px;">
						<label style="margin:0px 5px;">结束日期</label><input id="EndDate" class="hisui-datebox textbox" style="width:155px;">
					</div>
					<div style="padding:10px 0px 3px 60px;">
						<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" onclick="find_click()" style="margin-left:20px;">查询</a>
					</div>
				</div>
			</div>
			<div class="panel-line"></div>
			<div class="panel-center">
				<table id="bmDetList"></table>
			</div>
		</div>
		<div class="bd-center">
			<div class="bd-center-north">
				<div class="item-label">
					<div class="pf-patimg">
						<img id="PatPhoto" src="../scripts/dhcnewpro/images/girl.png">
					</div>
					<div class="pf-patbase">
						<ul class="pf-nav">
							<li class="pf-nav-item"><span class="pf-nav-title pf-patname"><span id="PatName" class="ui-span-m"></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span class="i-sep"></span></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span id="PatSex" class="ui-span-m"></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span class="i-sep"></span></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span id="PatAge" class="ui-span-m"></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span class="i-sep"></span></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span id="PatNo" class="ui-span-m"></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span class="i-sep"></span></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span id="PatBed" class="ui-span-m"></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span class="i-sep"></span></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span id="PatBill" class="ui-span-m"></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span class="i-sep"></span></span></span></li>
							<li class="pf-nav-item"><span class="pf-nav-title"><span id="PatDiagDesc" class="ui-span-m"></span></span></li>
						</ul>
					</div>
				</div>
				<div class="dashline-div"></div>
				<div>
					<ul class="pf-nav" id="form-name">
						<li class='pf-nav-item-li item-li item-li-select' id='APACHE' data-name='APACHE II评分表'><span>APACHE II评分表</span></li>
						<li class='pf-nav-item-li item-li' id='crams' data-name='crams'><span>CRAMS评分</span></li>
						<li class='pf-nav-item-li item-li' id='XZCSPF' data-name='XZCSPF'><span>修正创伤评分</span></li>
						<li class='pf-nav-item-li item-li' id='Glasgow昏迷评分' data-name='Glasgow昏迷评分'><span>Glasgow昏迷评分</span></li>
						<li class='pf-nav-item-li item-li' id='ISS' data-name='ISS'><span>创伤严重程度评分(ISS) </span></li>
					</ul>
				</div>
			</div>
			<div class="bd-center-form">
				<iframe id="FormMain" scrolling="yes" width=100% height=100%  frameborder="0" src="" ></iframe>
			</div>
		</div>
	</div>
</body>
</html>