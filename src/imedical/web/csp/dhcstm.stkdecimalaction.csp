<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

	s listData=$Get(%request.Data("data",1))
	s RowId=$Get(%request.Data("rowid",1))
	s StkDecimalId=$Get(%request.Data("StkDecimalId",1))

	//查询小数规则维护
	i Action = "selectAll" d
	.s result = ##class(web.DHCSTM.StkDecimal).SelectAll()
	.i result ="" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result

	//保存小数规则维护
	i Action = "save" d
	.s result = ##class(web.DHCSTM.StkDecimal).Save(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//删除小数规则维护
	i Action = "delete" d
	.s result = ##class(web.DHCSTM.StkDecimal).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//根据小数规则维护查询明细
	i Action = "selectChild" d
	.s result = ##class(web.DHCSTM.StkDecimalItm).SelectAll(StkDecimalId)
	.i result = "" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result

	//保存小数规则维护与明细
	i Action = "addRelation" d
	.s result = ##class(web.DHCSTM.StkDecimalItm).Save(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//删除明细
	i Action = "deleteChild" d
	.s result = ##class(web.DHCSTM.StkDecimalItm).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

</script>