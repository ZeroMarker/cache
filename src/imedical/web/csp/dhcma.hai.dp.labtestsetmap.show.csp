<div class="hisui-layout" data-options="fit:true" style="border:none;">
	<div data-options="region:'center',border:false,collapsible:false" class="ma-panel" style="padding:10px;">
		<div id="divTabs" class="hisui-tabs tabs-gray " data-options="tools:'#tab-tools',fit:true,">   
			<div id="divtab0" title="检验医嘱对照" data-options="selected:true" style="overflow:hidden;">   
				<table id="gridLabTestSetMap" data-options="toolbar:'#ToolBar',border:false"></table>
				<div id="ToolBar" style="padding:3px" >
					<div style="padding: 7px 0 7px 7px;">
                    <input class="hisui-searchbox" id="search" style="width:260px;" data-options="prompt:'搜索'" />            
					</div>
					<span class="line" style="display: block;border-bottom: 1px dashed rgb(204, 204, 204);margin: 3px 0px;clear: both; padding-bottom: 1px; "></span>		 
			
					<a class="hisui-linkbutton" id="btnAll"  data-options="iconCls:'icon-all-select',plain:true,stopAllEventOnDisabled:true"> 全部 </a>
					<a class="hisui-linkbutton" id="btnPend" data-options="iconCls:'icon-compare-no',plain:true,stopAllEventOnDisabled:true"> 未对照 </a>
					<a class="hisui-linkbutton" id="btnFin"  data-options="iconCls:'icon-compare-yes',plain:true,stopAllEventOnDisabled:true" > 已对照 </a>
					<a class="hisui-linkbutton" style="height: 24px; border-left: 1px solid LightGrey; border-right: 0px solid LightGrey;margin: 1px 1px; " id="btnSyn"  data-options="iconCls:'icon-radio',plain:true,stopAllEventOnDisabled:true" > 自动匹配 </a>
					<a class="hisui-linkbutton" id="btnEdit" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"> 修改 </a>
					<a class="hisui-linkbutton" id="btnAdd"  data-options="iconCls:'icon-compare',plain:true,stopAllEventOnDisabled:true"> 对照 </a>
					<a class="hisui-linkbutton" id="btnDelete"  data-options="iconCls:'	icon-undo',plain:true,stopAllEventOnDisabled:true" > 撤销 </a>
					
				</div>
			</div>   
			<div id="divtab1" title="检验项目对照" data-options="closable:false" style="overflow:hidden;">   
				<table id="gridLabTestCode" data-options="toolbar:'#ToolBar2',border:false"></table>
				<div id="ToolBar2" style="padding:3px" >
				<div style="padding: 7px 0 7px 7px;">
                    <input class="hisui-searchbox" id="searchTC" style="width:260px;" data-options="prompt:'搜索'" />             
					</div>
					
					<span class="line" style="display: block;border-bottom: 1px dashed rgb(204, 204, 204);margin: 3px 0px;clear: both; padding-bottom: 1px; "></span>
					<a class="hisui-linkbutton" id="btnAllTC"  data-options="iconCls:'icon-all-select',plain:true,stopAllEventOnDisabled:true"> 全部 </a>
					<a class="hisui-linkbutton" id="btnPendTC" data-options="iconCls:'icon-compare-no',plain:true,stopAllEventOnDisabled:true"> 未对照 </a>
					<a class="hisui-linkbutton" id="btnFinTC"  data-options="iconCls:'icon-compare-yes',plain:true,stopAllEventOnDisabled:true" > 已对照 </a>
					<a class="hisui-linkbutton" style="height: 24px; border-left: 1px solid LightGrey; border-right: 0px solid LightGrey;margin: 1px 1px; " id="btnAddTC"  data-options="iconCls:'icon-compare',plain:true,stopAllEventOnDisabled:true"> 对照 </a>
					<a class="hisui-linkbutton" id="btnDeleteTC"  data-options="iconCls:'	icon-undo',plain:true,stopAllEventOnDisabled:true" > 撤销 </a>
					
				</div>
			</div>   
		</div>	
	</div>		
	
	<div data-options="region:'east',border:false,collapsible:false"  class="ma-panel" style="width:500px;padding:10px 10px 10px 0px;">
		<table id="gridLabTestSet" data-options="toolbar:'#tbLabTCMapRst'"></table>
		<div id="tbLabTCMapRst" style="padding:3px;overflow:hidden" >						
			<div style="padding: 7px 0 7px 7px;"> 
				<input id="btnsearch_two" data-options="prompt:'搜索'" class="hisui-searchbox"/>
			</div>
			 
		</div>
	</div>
</div>

<div id="winEdit" style="border:none;overflow:hidden;padding-right:10px;margin-bottom: -10px;display:none">
	<table class="search-table">
		<tr>
			<td class="r-label">
				<label for="txtMapNote">标准备注</label>
			</td>
			<td>
				<input class="textbox" id="txtMapNote" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label" colspan="1">
				<label for="chkActive" >是否有效</label>
			</td>
			<td>
				<input class='hisui-checkbox' type="checkbox" id="chkActive" checked/>
			</td>
		</tr>
	</table>
</div>

