<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!--dhcmdt.consultpathis.csp 会诊申请审核列表-->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<script language="Cache" runat="server">
		d ##Class(web.DHCBillPrint).InvBillPrintCLSID()  //打印时使用
   </script>
	<!--------------打印相关---------->
	<script type='text/javascript' src='../scripts/DHCWeb.OPCommon.js'></script>
	<script type='text/javascript' src='../scripts/DHCPrtComm.js'></script>
	<!-------------------------------->
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcmdt/print.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcmdt/consultpathis.js"></script>
</head>
<body>
	<div class="hisui-layout" fit="true">
		<div data-options="region:'center',headerCls:'panel-header-gray',title:'',iconCls:'icon-paper'" style="padding:10px 10px;">
			<div class="hisui-layout" fit="true">
				<div data-options="region:'north',headerCls:'panel-header-gray',title:'历次会诊记录',collapsible:false,iconCls:'icon-paper'" style="height:90px;padding:2px;">
					<div style="margin:8px;">
						<label>开始日期</label>
						<input id="StartDate" class="hisui-datebox textbox" style="width:130px;">
						<label style="margin-left:10px;">结束日期</label>
			        	<input id="EndDate" class="hisui-datebox textbox" style="width:130px;">
						<label style="margin-left:10px;">申请科室</label>
						<input id="CstRLoc" class="textbox" style="width:140px;height:30px;line-height:30px;"/>
						<label style="margin-left:10px;">疑难病种</label>
				        <input id="mdtDisGrp" class="textbox" style="width:180px;height:30px;line-height:30px;"/>
						<label style="margin-left:10px;">第几次会诊</label>
				        <input id="TimesArr" class="textbox" style="width:100px;height:30px;line-height:30px;"/>
						<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" onclick="QryPatList()">查询</a>	
					</div>
				</div>
				<div data-options="region:'center',headerCls:'panel-header-gray',title:'',iconCls:'icon-paper'">
					<table id="bmDetList"></table>
				</div>
				<div data-options="region:'south',headerCls:'panel-header-gray',title:'',collapsible:false,iconCls:'icon-paper'" style="height:320px;padding:10px 0px 10px 0px;border-left:0;border-right:0;border-bottom:0;">
					<div class="hisui-layout" fit="true">
						<div data-options="region:'center',title:'会诊内容',headerCls:'panel-header-gray',iconCls:'icon-paper'">
							<div style="margin:5px;">
								<div style="margin:5px;"><label style="font-weight:bold;">病情摘要</label></div>
								<div id="ConsTrePro" style="border:1px solid #ccc;padding:8px 5px;min-height:80px;max-height:400px;overflow-x:hidden;text-indent:25px;"></div>
							</div>
							<div style="margin:5px;">
								<div style="margin:5px;"><label style="font-weight:bold;">会诊目的</label></div>
								<div id="ConsPurpose" style="border:1px solid #ccc;padding:8px 5px;min-height:80px;max-height:400px;overflow-x:hidden;text-indent:25px;"></div>
							</div>
				           <div style="margin:5px;">
								<div style="margin:5px;"><label style="font-weight:bold;">会诊结论</label></div>
								<div id="TreMeasure" style="border:1px solid #ccc;padding:8px 5px;min-height:80px;max-height:400px;overflow-x:hidden;text-indent:25px;"></div>
							</div>
				            
				            
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!--------------打印相关---------->
	<div id="hiddenEncrypt" style="width:0;height:0;">
		<form name="DHCIPBILL">
			<INPUT TYPE="hidden" name="InvPrintEncrypt" id="InvPrintEncrypt" value='<%=##Class(%CSP.Page).Encrypt($lb("web.DHCXMLIO.ReadXML"))%>'>
		</form>
	</div>
	<!-------------------------------->
</body>
</html>
