<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false" style="height:57px;border-bottom:1px solid #ccc;">
		<table class="search-table">
			<tr>
				<td class="r-label">
					<div id="diagCatkw"></div>
				</td>
				<td>
					<input class='hisui-radio' type="radio" label='全部' checked=true name='AdmType' value='ALL' >
		            <input class='hisui-radio' type="radio" label='门急诊' name='AdmType' value='O^E^H'>
		            <input class='hisui-radio' type="radio" label='住院' name='AdmType' value='I'>
				</td>
				<td style="padding-left:10px;">
					<div class="vertical-line"></div>
				</td>
				<td class="r-label">
					<label for="DepSource">#(..Get("记录检索条件"))#</label>
				</td>
				<td>
					<input class="textbox" id="searchType"/>
					<input class="textbox" id="searchDesc"/>
				</td>
				<td class="r-label">
					<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
				</td>
				<td>
					<a id="BCopy" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-copy'">复制到本次</a>
				</td>
			</tr>
		</table>
	</div>
	<div data-options="region:'center',title:'',border:false">
		<table id="PatAllDiagTab"></table>
	</div>
</div>
