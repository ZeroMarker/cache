<!DOCTYPE html>
<!--
    入库 - 审核
	csp:  csp/pha.in.v3.rec.audit.csp
	js:   scripts/pha/in/v3/rec/audit.js
-->
<html>
<!-- 验证session过期 -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q $$$OK
</csp:method>

<head>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI />
    <PHALIBV1 />
    <PHAPRINTCOM />
    <style>
    </style>
</head>

<body>
    <div class="hisui-layout" fit="true" id="lyMainView">
        <div data-options="region:'center'" class="pha-body" border="false">
            <div class="hisui-layout" fit="true" id="layout-rec-audit">
                <div data-options="region:'north',height:500,split:true, title:'入库单',collapsible:false,bodyCls:'panel-body-gray',headerCls:'panel-header-gray',iconCls:'icon-paper'" id="layout-rec-audit-panel">
                    <table id="gridMain"></table>
                </div>
                <div data-options="region:'center', title:'单据明细',bodyCls:'panel-body-gray',headerCls:'panel-header-gray',iconCls:'icon-paper-info'">
                    <table id="gridItm"></table>
                </div>
            </div>
        </div>
    </div>

    <!-- 表格工具栏 -->
    <div id="gridMainBar">
        <table id="qCondition" class="pha-con-table">
            <tr>
                <td class="r-label">
                    <label for="startDate">#(..Get("开始日期"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="startDate" data-pha="class:'hisui-datebox',clear:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="loc">#(..Get("入库科室"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="loc" data-pha="class:'hisui-combobox',clear:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="nextStatus">#(..Get("预审流程"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="nextStatus" data-pha="class:'hisui-combobox',clear:true,save:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="no">#(..Get("单号"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="no" data-pha="class:'hisui-validatebox',clear:true,query:true">
                </td>
                <td class="r-label">
                    <a class="hisui-linkbutton" id="btnFind">查询</a>
                </td>
                <td>

                </td>
            </tr>
            <tr>
                <td class="r-label">
                    <label for="endDate">#(..Get("结束日期"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="endDate" data-pha="class:'hisui-datebox',clear:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="vendor">#(..Get("经营企业"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="vendor" data-pha="class:'hisui-combobox',clear:true,query:true">
                </td>
                <td class="r-label">
                    <label for="nextStatusResult">#(..Get("流程状态"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="nextStatusResult" data-pha="class:'hisui-combobox',clear:true,query:true,required:true">
                </td>
                <td class="r-label">
                    <label for="filterField">#(..Get("其他"))#</label>
                </td>
                <td class="r-label">
                    <input type="text" id="filterField" data-pha="class:'hisui-validatebox',clear:true,query:true">
                </td>
                <td class="r-label">
                    <a class="hisui-linkbutton" id="btnClean">清屏</a>
                </td>
                <td class="r-label">
                    <a class="hisui-linkbutton" id="btnPrint">打印</a>
                </td>
            </tr>
        </table>
        <div class="pha-line"></div>
        <div>
            <!-- <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-set-col'" id="btnAuditRefuse">列设置</a>
            <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-export'" id="btnExport">导出</a> -->
        </div>
        <div class="pha-table-toolbar-button">
            <div style="float: left;">
                <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-ok'" id="btnAudit">审核通过</a>
                <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-ok'" id="btnAuditPrint">审核通过并打印</a>
                <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-no'" id="btnAuditRefuse">审核拒绝</a>
                <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-undo'" id="btnAuditCancel">取消审核</a>
            </div>
            <!-- <div style="float:right">
                <div id="keyTools" class="hisui-keywords" data-options="items:[{text:'审核后自动打印',id:'PrintAfterAudit'}]"></div>
            </div> -->
            <div style="clear: both;"></div>
        </div>
    </div>

    <!-- 加载本页面js -->
    <script type="text/javascript" src="../scripts/pha/in/v3/rec/com.js"></script>
    <script type="text/javascript" src="../scripts/pha/in/v3/rec/component.js"></script>
    <script type="text/javascript" src="../scripts/pha/in/v3/rec/print.js"></script>
    <script type="text/javascript" src="../scripts/pha/in/v3/rec/audit.js"></script>
</body>

</html>
