<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

	s StrParam=$Get(%request.Data("strParam",1))
	s dsr=$Get(%request.Data("dsr",1))
	s listData=$Get(%request.Data("data",1))
	s UserId=$Get(%request.Data("userId",1))
	
	//查询退回单
	i Action = "query" d
	.d ##class(web.DHCSTM.Common.JsonObj).getJsonByQuery("web.DHCSTM.DHCINDispRet","DHCINDispRetM",Start,Limit,Sort,Dir,StrParam)
	.;d ##class(web.DHCSTM.DHCINDispRet).jsDHCINDispRetM(Start,Limit,Sort,Dir,StrParam)

	//查询退回单明细
	i Action = "queryItem" d
	.d ##class(web.DHCSTM.Common.JsonObj).getJsonByQuery("web.DHCSTM.DHCINDispRetItm","DHCINDispRetD",Start,Limit,dsr)
	.;d ##class(web.DHCSTM.DHCINDispRetItm).jsDHCINDispRetD(Start,Limit,dsr)

	//删除退回单明细
	i Action = "Delete" d
	.s result = ##class(web.DHCSTM.DHCINDispRet).Delete(dsr)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//删除退回单明细
	i Action = "deleteItem" d
	.s RowId=$Get(%request.Data("rowid",1))
	.s result = ##class(web.DHCSTM.DHCINDispRetItm).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//保存退回单主表记录
	i Action = "save" d
	.s result = ##class(web.DHCSTM.DHCINDispRet).Update(dsr,listData)
	.i result >0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//保存退回单明细记录
	i Action = "saveDetail" d
	.s result = ##class(web.DHCSTM.DHCINDispRetItm).Save(dsr,listData)
	.i result >0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//审核退回单
	i Action = "audit" d
	.s result = ##class(web.DHCSTM.DHCINDispRet).Audit(dsr,UserId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

    //保存退回单
	i Action = "SaveDispRet" d
	.s mainData=$g(%request.Data("mainData",1))
	.s detailData=$g(%request.Data("detailData",1))
	.s result = ##class(web.DHCSTM.DHCINDispRet).SaveDispRet(dsr,mainData,detailData)
	.i result > 0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:''}"


	i Action = "setComplete" d
	.s result = ##class(web.DHCSTM.DHCINDispRet).SetComplete(dsr,"Y")
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	i Action = "cancelComplete" d
	.s result = ##class(web.DHCSTM.DHCINDispRet).SetComplete(dsr,"N")
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"


	i Action = "select" d
	.s result = ##class(web.DHCSTM.DHCINDispRet).jsSelect(dsr)
	.w result
	.
	
	//根据请领单制作退回单
	i Action="CreateDispByReq" d
	.s req=$G(%request.Data("dsrq",1))  //请领单rowid
	.s details=$G(%request.Data("detailData",1))
	.s result=##class(web.DHCSTM.DHCINDispRet).CreateDispByReq(req, UserId, details )
	.i result>0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
	i Action="selectBatch" d
	.s strPar=$g(%request.Data("strPar",1))
	.d ##class(web.DHCSTM.DHCINDispRetItm).GetDispItm(Start,Limit,strPar)
	
</script>