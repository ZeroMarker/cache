<!--门诊单独打印界面-->
<style>
	#panel-footer{
		margin-top:10px;
	}
	#printItemIframeTemplate{
		width:100%;
		height:91%;
		overflow:auto;
	}
	.r-label{
		padding-right:10px;
	}
	#StDate,#EndDate{
		width: 110px !important;
	}
	.search-div{
		margin-left:10px;
		display:inline-block;
	}
	.search-div a{
		margin-left:10px;
	}
	.footer-div{
		float:right;
	}
</style>
<div id="printItemIframeTemplate" class="hisui-panel" style="" data-options="fit:true,border:false,footer:'#panel-footer'">
	 <div id="EMRInfo" style="height:auto;">
	 </div>
	 <div id="DiagnosInfo" class="panel-title" style="height:auto;"></div>
	<div id="templtable-div">
	</div>
</div>
<div id="panel-footer">
	<div class="search-div">
		<input id="selAll" class='hisui-checkbox' type="checkbox" label="全选/全消">
		<label class="r-label">#(..Get("开始日期"))#</label><input id="StDate" class="hisui-datebox textbox"></input>
		<label class="r-label" style="margin-left:10px;">#(..Get("结束日期"))#</label><input id="EndDate" class="hisui-datebox textbox"></input>
		<a href="#" class="hisui-linkbutton" id="BFind" data-options="iconCls:'icon-w-find'">查询</a>
	</div>
	<div class="footer-div">
		<a href="#" class="hisui-linkbutton" id="BCancel" data-options="iconCls:'icon-w-back'">撤销医嘱</a>
		<a href="#" class="hisui-linkbutton" id="previewBtn" data-options="iconCls:'icon-w-eye'" style="margin-left:15px;">预览</a>
		<a href="#" class="hisui-linkbutton" id="printBtn" data-options="iconCls:'icon-w-print'" style="margin-left:15px;">打印</a>
	</div>
</div>
