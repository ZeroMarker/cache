<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'center',headerCls:'panel-header-gray',border:false">
		<div class="hisui-panel" data-options="fit:true,bodyCls:'panel-body-gray'">
			<div class="hisui-layout" data-options="fit:true">
				
				
				
				<div data-options="region:'north',headerCls:'panel-header-gray',bodyCls:'panel-body-gray',collapsible:false,border:false" style="height:93px;border-bottom:1px dashed #ccc;">
					<div class="hisui-layout" data-options="fit:true">
						<div data-options="region:'north',bodyCls:'panel-body-gray',collapsible:false,border:true" style="height:43px;border-top:0;border-left:0;border-right:0;">
							<div id="i-title" class="c-img">
								<img id="i-msg-pic" alt="" src="../scripts/dhcdoc/chemo/img/bed/unKnownAvatar.png" style="position: absolute; top: 6px; width: 30px; height: 30px; border-radius: 30px;">
								<span id="i-msg-name" style="margin:0 5px;margin-left:38px;"></span>
								<span style="color:#bbb;">/</span>
								<span id="i-msg-sex" style="margin:0 10px;"></span>
								<span style="color:#bbb;">/</span>
								<span id="i-msg-age" style="margin:0 10px;"></span>
								<span style="color:#bbb;">/</span>
							</div>
						</div>
						<div data-options="region:'center',bodyCls:'panel-body-gray',border:false" style="overflow:hidden;">
							<div class="c-row">
								<span class="c-span">#(..Get("化疗日期"))#</span>
								<input id="ChemoDate" class="hisui-datebox textbox" >
								<span class="c-span2"></span>
								<!--<span class="c-span">卡类型</span>
								<input id="CardType" class="textbox" >
								<span class="c-span2"></span>
								<span class="c-span">卡号</span>
								<input id="CardNo" class="textbox" >
								<span class="c-span2"></span>-->
								<span class="c-span">#(..Get("登记号"))#</span>
								<input id="PatNo" class="textbox" >
								<span class="c-span2"></span>
								<a id="Find" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
								<span class="c-span2"></span>
								<a id="i-clean" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清空</a>
								<!--
								<span class="c-span2"></span>
								<a id="ReadCard" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'">读卡</a>
								<span class="c-span2"></span>
								<a id="Scan" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'">扫码</a>-->
								
							</div>
						</div>
					</div>
				</div>
				
				<div id="main-center" data-options="region:'center',headerCls:'panel-header-gray',border:false" style="padding:10px;">
					<div class="left">
						<div class="AM">
							<div class="t-row">
								<div class='datagrid-toolbar' style='border-bottom:0;'>
									<table cellspacing='0' cellpadding='0'>
										<tr id="T-AM">
											<td>
												<div style="float:left;background:#017BCE;width:5px;height:20px;"></div>
												<div style="float:left;margin-left:10px;"><span class="c-title">#(..Get("上午床位"))#</span></div>
												
											</td>
											<td class="BtnTD">
												<a id='AM_Add_btn' class='hisui-linkbutton m-button' data-options="iconCls:'icon-add',plain:true">新增</a>
											</td>
										</tr>
									</table>
								</div>
							</div>
							<div id="AMCard">
								
							</div>
							
						</div>
						
						<div class="PM">
							<div class="t-row">
								<div class='datagrid-toolbar' style='border-bottom:0;'>
									<table cellspacing='0' cellpadding='0'>
										<tr id="T-PM">
											<td>
												<div style="float:left;background:#017BCE;width:5px;height:20px;"></div>
												<div style="float:left;margin-left:10px;"><span class="c-title">#(..Get("下午床位"))#</span></div>
												
											</td>
											<td  class="BtnTD" >
												<a id='PM_Add_btn' class='hisui-linkbutton m-button' data-options="iconCls:'icon-add',plain:true">新增</a>
											</td>
										</tr>
									</table>
								</div>
							</div>
							<div id="PMCard">
								
							</div>
							
						</div>
					</div>
					
					<div id="anchor" class="right">
						<ul id="anchorList" class="anchor-list" style="padding-left:30px;">
							<li style='height:13px'>
								<div class='anchor-circle'></div>
								<span class='anchor-txt'></span>
							</li>
							<li>
								<div class='circle'></div>
								<a data-taget="T-AM" id="nac-T-AM">#(..Get("上午床位"))#</a>
							</li>
							<li>
								<div class='circle'></div>
								<a data-taget="T-PM" id="nac-T-PM">#(..Get("下午床位"))#</a>
							</li>
							<li>
								<div class='circle'></div>
								<a data-taget="GoTOP" id="GoTOP">#(..Get("返回顶部"))#</a>
							</li>
							<li>
								<i class='anchor-circle'></i>
							</li>
			            </ul>
					</div>
				</div>	
				
				
			</div>
		</div>
	</div>
	
	
	
</div>