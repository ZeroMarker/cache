							<td class='r-label'><label >#(..Get("姓名"))#</label></td>
							<td><input id="Name" class="textbox" name="Name" style="width:140px;" /></td>
							<td><div id="COterdocAppoint"><input class='hisui-checkbox' type="checkbox" checked id="OterdocAppoint" label="包含他人预约记录"></div></td>
						</tr>
