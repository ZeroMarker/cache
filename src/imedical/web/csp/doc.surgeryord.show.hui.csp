<csp:if condition='EntryFrom="1"'>
	<div class="hisui-layout" fit="true">
		<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="height:95px;padding:10px 10px 0 10px;">
			<div class="hisui-panel" data-options="title:'停医嘱',headerCls:'panel-header-gray',iconCls:'icon-edit'" style="border:1px solid #ccc;border-bottom-style:dashed;border-radius:0;">
				<table class="search-table">
					<tr>
						<td class="r-label">
							<label for="orderDesc">医嘱</label>
						</td>
						<td>
							<input class="textbox" id="orderDesc"/>
						</td>
						<td class="r-label">
							<label for="doctorList">开医嘱人</label>
						</td>
						<td>
							<input class="hisui-combobox textbox" id="doctorList"/>
						</td>
						<td class="r-label">
							<label for="scopeDesc">范围</label>
						</td>
						<td>
							<input class="hisui-combobox textbox" id="scopeDesc"/>
						</td>
						<td class="r-label">
							<label for="locDesc">开出科室</label>
						</td>
						<td>
							<input class="hisui-combobox textbox" id="locDesc"/>
						</td>
					</tr>
				</table>
			</div>
		</div>
		<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
				<table id="SurgeryOrdTab"></table>
			</div>
		</div>
	</div>
<csp:else>
