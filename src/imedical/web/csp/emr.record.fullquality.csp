		</script>
		<script language="javascript">
			var PatientID = '#(PatientID)#';
			var EpisodeID = '#(EpisodeID)#';
			var CTLocatID = '#(CTLocatID)#';
			var userID = '#(userID)#';
			var RuleID = '#(RuleID)#';
			var episodeLocID = '#(episodeLocID)#';
			var action = '#(action)#';
