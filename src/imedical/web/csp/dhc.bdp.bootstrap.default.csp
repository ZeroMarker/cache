    <script type="text/javascript">

        function setContentStyle(){
	        var outerHeight=""
	        if(window.parent.document.getElementById("centerPanel")!=null&&window.parent.document.getElementById("centerPanel")!=undefined)
	        {	
				outerHeight=window.parent.document.getElementById("centerPanel").style.height
				outerHeight=outerHeight.replace("px","");
				outerHeight=outerHeight-30
				
				//outerHeight=outerHeight-10 ///2020-02-13解决下面出现10px遮住了的问题
				
	        }
	        else
	        {
		        var windowHight="";
		        windowHight = document.documentElement.clientHeight;
	        	if(windowHight==0){
	         		windowHight = document.body.clientHeight;
	       		 }
	       		var Sys = {};
				var ua = navigator.userAgent.toLowerCase();
				var s;
				(s = ua.match(/msie ([\d.]+)/)) ? Sys.ie = s[1] :
				(s = ua.match(/firefox\/([\d.]+)/)) ? Sys.firefox = s[1] :
				(s = ua.match(/chrome\/([\d.]+)/)) ? Sys.chrome = s[1] :
				(s = ua.match(/opera.([\d.]+)/)) ? Sys.opera = s[1] :
				(s = ua.match(/version\/([\d.]+).*safari/)) ? Sys.safari = s[1] : 0;
			

				if(Sys.chrome){
					outerHeight = windowHight - 68;
				}
				else{
				    outerHeight = windowHight - 36; 
				}
	        }

			return outerHeight
        }
        //屏幕下方出现空白，高度不自适应 所以删掉  20171109
