<!DOCTYPE html>
<!--小字典维护
	csp:dhcpha.pivas.stkcomdictionary.csp
	js: scripts/pharmacy/pivas/stkcomdictionary.js
-->
<HTML XMLNS=TRAK>
<HEAD>
<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
<TRAK:HEAD>
</TRAK:HEAD>
<!-- Put your page Title here -->
	<HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/dhcpha/css/dhcpha.hisui.css" ></script> 
    <script type="text/javascript" src="../scripts/dhcst/EasyUI/Plugins/dhcst.plugins.js"></script> 
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.plugins.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
	<script type="text/javascript" src="../scripts/pharmacy/pivas/stkcomdictionary.js"></script>
 </HEAD>
<body>
<div class="hisui-layout" fit="true">    
	<div data-options="region:'center',title:'字典维护',headerCls:'panel-header-gray',iconCls:'icon-lt-rt-55'" >
		<div id="gridStkComDictionary" data-options="fit:true" border="false"> 
	  	</div>    
    </div>  
</div>
</body>
<div id="gridStkComDictionaryBar">
	<span >
		<input id="cmbStkComDict" type="text"/>
		<a class="hisui-linkbutton" plain='true' iconCls="icon-add" id="btnAdd">增加</a>
		<a class="hisui-linkbutton" plain='true' iconCls="icon-edit" id="btnUpdate">修改</a>
		<a class="hisui-linkbutton" plain='true' iconCls="icon-remove" id="btnDelete">删除</a>
	</span>
</div>
<div id="gridStkComDictionaryWin" class="hisui-dialog" title="字典维护" closed="true" style="width:300px;height:165px;"
	data-options="iconCls:'icon-save',modal:true,
	buttons:[{
		text:'确定',
		handler:function(){
			SaveStkComDictionary();
		}
	},{
		text:'取消',
		handler:function(){
			$('#gridStkComDictionaryWin').window('close');
		}
	}]">
   <div class="hisui-layout" data-options="fit:true">	
    <div data-options="region:'center'">
    	<table id="query-condition-left" class="dhcpha-height-2">
    		<tr>
    			<td>
    				字典代码<input id="txtSCDICode" type="text" class="hisui-validatebox" style="width:200px"/>
    			</td>
    		<tr>
    		<tr>
    			<td>
    				字典名称<input id="txtSCDIDesc" type="text" class="hisui-validatebox" style="width:200px"/>
    			</td>
    		<tr>
    	</table>
    </div>
    </div>
</div>
</HTML>