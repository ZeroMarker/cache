<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))
	s StrParam=$Get(%request.Data("StrParam",1))
	
///检索需要绑定的高值医嘱信息
i Action = "QueryOeori" d
	.d ##class(web.DHCSTM.OeoriBindBarcode).QueryOeori(Start,Limit,Sort,Dir,StrParam)

///供应商高值医嘱包关联库存项(同仁模式)
i Action = "Save" d
	.s MainInfo=$Get(%request.Data("MainInfo",1))
	.s ListDetail=$Get(%request.Data("ListDetail",1))
	.s HospId=$G(%session.Data("LOGON.HOSPID"))
	.s result = ##class(web.DHCSTM.OeoriBindInci).Save(MainInfo,ListDetail,HospId)
	.i result>=0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

i Action = "Delete" d
	.s RowId=$Get(%request.Data("RowId",1))
	.s result = ##class(web.DHCSTM.OeoriBindInci).Delete(RowId)
	.i result=0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

i Action="QueryItem" d
	.d ##class(web.DHCSTM.OeoriBindInci).QueryItem(Start,Limit,Sort,Dir,StrParam)
	
i Action = "Pur" d
	.s params=$Get(%request.Data("params",1))
	.s result = ##class(web.DHCSTM.OeoriBindInci).Pur(params)
	.i result=0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
</script>