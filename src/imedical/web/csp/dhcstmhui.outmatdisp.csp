<!--物资门诊发放-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',border:false,headerCls:'panel-header-gray'" class="GridPadding" style="padding-top:0;padding-bottom:0;">
		<table id="MainConditions" class="Condition">
			<tr>
				<td>起始日期</td>
				<td><input id="StartDate" name="StartDate" class="hisui-dateboxq" style="width:180px;"></td>						
				<td>护士执行科室</td>
				<td><input id="ExeLoc" name="ExeLoc" class="hisui-combobox" style="width:163px;"></td>
				<td>卡类型</td>
				<td><input id="CardType" name="CardType" class="hisui-combobox" style="width:143px;"></td>
				<td><input id="CardNo" name="CardNo" class="textbox" style="width:163px;" placeholder="刷卡/请输入卡号"></td>
				<td align="left"><a href="#" id="ReadCardBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-card'">读卡</a></td>
				<td align="left"><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
			</tr>
				<td>截止日期</td>
				<td><input id="EndDate" name="EndDate" class="hisui-dateboxq" style="width:180px;"></select></td>
				<td>接收科室</td>
				<td><input id="RecLoc" name="RecLoc" class="hisui-combobox" style="width:163px;"></td>
				<td>发放状态</td>
				<td>
					<select id="Status" name="Status" class="hisui-combobox" style="width:143px;">
						<option value="N">未发放</option>
						<option value="Y">已发放</option>
						<option value="">全部</option>
					</select>
				</td>
				<td><input id="PatNo" name="PatNo" class="textbox" style="width:163px;" placeholder="请输入登记号"></td>
				<td align="left"><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
				<td align="left"><a href="#" id="DispBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-ok'">发放</a></td>	
			</tr>
		</table>
	</div>
		<div data-options="region:'center',border:false">
			<div class="hisui-layout" data-options="fit:true">
				<!--div data-options="region:'west',border:false" class="GridPadding" style="padding-top:0;width:500px;"> -->  
					<div class="hisui-panel" data-options="title:'材料列表',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">   
						<table id="OutMatMainGrid"></table>
					</div>
				<!--/div>
				<div data-options="region:'center',border:false" class="GridPadding" style="padding-left:0;padding-top:0;">   
					<div class="hisui-panel" data-options="title:'材料详细列表',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">   
							<table id="OutMatDetailGrid"></table>
					</div>   
				</div-->
			</div>	
		</div>
	
	<script type="text/javascript" src="../scripts/dhcstmhisui/ItmDisp/OutMatDisp.js"></script>
</body>
</html>


