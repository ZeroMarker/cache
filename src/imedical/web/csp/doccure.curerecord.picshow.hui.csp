<!DOCTYPE html>
<HTML XMLNS=TRAK>
<!-- csp:doccure.curerecord.picshow.hui.csp-->
<HEAD>
<TITLE><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></TITLE>
<!-- <TRAK:HEAD></TRAK:HEAD> -->
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<meta http-equiv="Content-Type" content="text/html;">
<EXTHEALTH:EXT321></EXTHEALTH:EXT321>
<HISUI></HISUI>
<script type="text/javascript" src="../scripts/dhcdoc/dhcdoccure_hui/imageZoom.js"></script>
<style type="text/css">
	*{
		/*清除默认外边距*/
		margin: 0px;
		/*清除默认内边距*/
		padding: 0px;
	}
	p{
		line-height: 20px;
		text-align: center; /*文本居中*/
	}
	.picname{
		line-height: 60px;	
		font-weight:bold;
	}
	.picbtn{
		line-height: 40px;	
	}
	.wrap{
		margin: 10px auto;
		height: 100%;
		width: 100%;
		/*x方向的位移 y方向的位移 羽化与半径 影子颜色*/
		box-shadow:0px 0px 3px #888888;
	}
	.wrap ul{
		height: 100%;
		width: 100%;
		list-style-type: none;
		display: flex; /*开启CSS3弹性盒子模型*/
		flex-wrap: wrap;/*规定灵活的项目在必要的时候拆行或拆列。*/
		justify-content: space-around;/*内部元素主轴方向(x方向)默认排列方式:环绕*/
		align-items: center;/*内部元素辅轴(y方向)方向默认排列方式:环绕*/
	}
	.wrap ul li{
		width: 300px;
		height: 200px;
		margin-top:10px;
		border: 2px solid #fff;
		box-shadow:-2px -1px 3px #888888;
	}

	.wrap ul li img{
		height: 100%;
		width: 100%;
	}

	.wrap ul li .title{	
		/*display: none;*/ /*用css3 scale(0)代替*/
		width: 300px;
		height: 200px;
		background-color:rgba(255,255,255,.5);
		/*应用于元素的2D或3D转换。这个属性允许你将元素旋转，缩放，移动，倾斜等。scale定义2D缩放转换,scale(0)隐藏*/
		transform: translateY(-203px) scale(0); 
		/*CSS3 过渡 在一定时间内完成属性值的变化*/
		transition: .5s; 
	}

	.wrap ul li:hover .title{/*伪类选择器*/
		/*display: block;*/ /*用css3 scale(1)代替*/
		/*scale(1)*/
		transform: translateY(-203px) scale(1);
	}
	
</style>
</head>
<body>
<server>
s DCRRowId=%request.Get("DCRRowId")
s paramType=%request.Get("paramType")
s LogUserID=%session.Get("LOGON.USERID")

k PicPathAry
s PicCount=0
s PicStr=##class(DHCDoc.DHCDocCure.Record).GetPicDataByDCRRowID(DCRRowId)
if PicStr'=""{
	for j=1:1:$l(PicStr,"^") {
		s OnePic=$p(PicStr,"^",j)
		s OnePicBase64Data=##class(DHCDoc.DHCDocCure.Record).GetPicBase64Data(DCRRowId,OnePic)
		if OnePicBase64Data="" continue
		s DCRPRowId=DCRRowId_"||"_OnePic
		s PicPathAry(DCRPRowId)=OnePicBase64Data
		s PicCount=PicCount+1
	}	
}
;只有一张图片为查看原图,多张图片都查看平铺图
i PicCount=1 s paramType="Original"
e  s paramType="Tile"
if paramType="Original" {
	;查看原图	
	s DCRPRowId=$o(PicPathAry(""))
	s PicName=$p(PicPathAry(DCRPRowId),$c(1),1)
	s PicUserID=$p(PicPathAry(DCRPRowId),$c(1),2)
	s OnePicPath=$p(PicPathAry(DCRPRowId),$c(1),3)
	;ul li列表构造表格-start
	s DelBtnStr="<a href='javascript:void(0)' id='dlepic@"_DCRPRowId_"' style='height:30px' class='hisui-linkbutton' onclick='DeletePic(this)'>删除图片</a>"
	s ShowDetailStr="<a href='javascript:void(0)' id='"_OnePicPath_"' src='"_OnePicPath_"' style='height:30px' class='hisui-linkbutton' onclick='showImage(this)'>点击查看原图</a>"
	w "<div class='wrap'>",!
	w "<ul>",!
	w "<li>",!
	w "<img title='点击查看原图' id="_DCRPRowId_" src='"_OnePicPath_"' alt='"_PicName_"' width='400' height='340' onclick='showImage(this)'>",! ; onmouseover='ZoomImage(this)'
	w "<div class='title'>",!	
	w "<p class='picname'>"_PicName_"</p>",!	
	w "<p class='picbtn'>"_ShowDetailStr_"</p>",!	
	if LogUserID=PicUserID{
		w "<p class='picbtn'>"_DelBtnStr_"</p>",!	
	}
	w "</div>",!	
	w "</li>",!	
	w "</ul>",!	
	w "</div>",!
	;ul li列表构造表格-end	
	;不用表格，改用ul列表，css3样式构造,若不支持,请将上述代码屏蔽,下述代码放开	
	/*w "<table border=1>",!
	w "<tr>",!
	s tabDesc1=PicName
	if LogUserID=PicUserID{
		s tabDesc1=tabDesc1_"<a href='javascript:void(0)' id='dlepic@"_DCRPRowId_"' class='hisui-linkbutton' onclick='DeletePic(this)'>删除图片</a>"
	}
	w "<td>",!
	w "<div id='"_PicName_"' style=''>"
	w "<img title='点击查看原图' id="_DCRPRowId_" src='"_OnePicPath_"' alt='"_PicName_"' width='400' height='340' onclick='showImage(this)' onmouseover='ZoomImage(this)' ></div>",!
	w "</td>",!
	w "<tr><td>"_tabDesc1_"</td></tr>",!
	w "</table>",!*/
}elseif paramType="Tile" {
	;查看平铺图
	;w "<table border=1>",!
	s tabDesc1="",tabDesc2=""
	s DCRPRowId=""
	s i=1
	f {
		s DCRPRowId=$o(PicPathAry(DCRPRowId)) Q:DCRPRowId=""
		;s PicPathAry(0,"Index",i)=PicPathAry(DCRPRowId)
		s PicName=$p(PicPathAry(DCRPRowId),$c(1),1)
		s PicUserID=$p(PicPathAry(DCRPRowId),$c(1),2)
		s OnePicPath=$p(PicPathAry(DCRPRowId),$c(1),3)
		;ul li列表构造表格-start
		s DelBtnStr="<a href='javascript:void(0)' id='dlepic@"_DCRPRowId_"' style='height:30px' class='hisui-linkbutton' onclick='DeletePic(this)'>删除图片</a>"
		s ShowDetailStr="<a href='javascript:void(0)' id='"_OnePicPath_"' src='"_OnePicPath_"' style='height:30px' class='hisui-linkbutton' onclick='showImage(this)'>点击查看原图</a>"
		if i=1{
			w "<div class='wrap'>",!
			w "<ul>",!
		}
		w "<li>",!
		w "<img title='点击查看原图' id="_DCRPRowId_" src='"_OnePicPath_"' alt='"_PicName_"' width='400' height='340' onclick='showImage(this)'>",! ; onmouseover='ZoomImage(this)'
		w "<div class='title'>",!	
		w "<p class='picname'>"_PicName_"</p>",!	
		w "<p class='picbtn'>"_ShowDetailStr_"</p>",!	
		if LogUserID=PicUserID{
			w "<p class='picbtn'>"_DelBtnStr_"</p>",!	
		}
		w "</div>",!	
		w "</li>",!	
		if i=PicCount{
			w "</ul>",!	
			w "</div>",!	
		}
		;ul li列表构造表格-end
		;不用表格，改用ul列表，css3样式构造,若不支持,请将上述代码屏蔽,下述代码放开,w table放开
		/*s DelBtnStr="<a href='javascript:void(0)' id='dlepic@"_DCRPRowId_"' class='hisui-linkbutton' onclick='DeletePic(this)'>删除图片</a>"
		i i#2=1 {
			w "<tr>",!
			s tabDesc1=PicName
			if LogUserID=PicUserID{
				s tabDesc1=tabDesc1_DelBtnStr
			}
		}
		w "<td>",!
		w "<div id='"_PicName_"' style=''>"
		w "<img title='点击查看原图' id="_DCRPRowId_" src='"_OnePicPath_"' alt='"_PicName_"' width='400' height='340' onclick='showImage(this)' onmouseover='ZoomImage(this)' ></div>",!
		w "</td>",!
		i (i#2=0)||(i=PicCount) {
			w "</tr>",!
			s tabDesc2=""
			if (i#2=0){
				s tabDesc2=PicName
				if LogUserID=PicUserID{
					s tabDesc2=tabDesc2_DelBtnStr
				}
			}
			w "<tr><td>"_tabDesc1_"</td><td>"_tabDesc2_"</td></tr>",!
		}*/
		s i=i+1
	}
	;w "</table>",!
}

</server>
</body>
<SCRIPT Language="Javascript">
function showImage(e) {
	//alert(e.id+","+e.src);
	if(typeof(e.src)=="undefined"){
		e.src=e.id;	
	}
	//e.src="data:image/png;base64,"
	//window.showModalDialog(e.src,"OriginalPicture","dialogHeight=600px;dialogWidth=1100px;dialogLeft=50px;dialogTop=50px;resizable=yes;resizable=yes;status=yes");
	//window.location.href=e.src;
	const img = new Image();
	img.src = e.src;
	const newWin = window.open("", "_blank","height=700,width=1100,toolbar=0,location=0,directories=0,status=0,menubar=0,resizable=1,top=50,left=50");
	newWin.document.write(img.outerHTML);
	newWin.document.title = "图片详情"
	newWin.document.close();
}
function ZoomImage(e) {
	new imageZoom(e.id,{
		mul:6,//指定放大的倍数
		viewerMul:2,
		viewerPos:{h:1,v:0},//显示到指定位置(默认值为：{h:10,v:0})，h:-10表示左边偏移10像素
	onShow:function() {
	},
	onHide:function() {
	},
	bigImg:e.src //指定放大图片路径
	});
}
function DeletePic(e){
	//alert(e.id)
	var DCRPRowID=e.id;
	if(DCRPRowID==""){
		$.messager.alert('提示',"删除失败,图片信息获取失败");
		return false;	
	}else{
		DCRPRowID=DCRPRowID.split("@")[1];
	}
	$.messager.confirm('提示',"是否确认删除图片?",function(r){
		if (r){
			var ret=$.cm({
				ClassName:"DHCDoc.DHCDocCure.Record",
				MethodName:"DelRecordPic",
				'DCRPRowID':DCRPRowID,
				'UserID':session['LOGON.USERID'],
				dataType:"text"
			},function(ret){
				if(ret==0){
					window.location.reload()
				}else if(ret==100){
					websys_showModal('options').CureRecordDataGridLoad();	
					websys_showModal("close");
				}else{
					$.messager.alert('提示',"删除失败,错误代码:"+ret);	
					return false;	
				}	
			})
		}
	})
	
}
</SCRIPT>
</html>
