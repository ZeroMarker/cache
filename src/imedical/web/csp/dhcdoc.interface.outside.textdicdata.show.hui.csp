<div class="hisui-layout" data-options="fit:true,border:false">
	<div data-options="region:'center',border:false,split:true,title:''" style="padding:10px"> 
		<div class="hisui-panel" data-options="fit:true,border:true" style="border-color:#ccc">
			<table id="table_textdicdata"></table>
		</div>
	</div>
	
	<div id="add-dialog" class="hisui-dialog" title="增加/修改" style="padding:10px;top:100px;width:400px;height:400px;" data-options="iconCls:'icon-w-save',shadow:false,modal:true,closed:true">
		<div class="hisui-panel" style="padding:10px;border-color:#ccc;"
		data-options="border:true,headerCls:'panel-header-card-gray',fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false">
			<form id="add-form" method="post" >
				<input name="EditRowid" id="EditRowid" type="hidden"  value="">
				<table class="Edittextdicdata">
					<tr>
						<td class="r-label">
							<label for="EditType">字典类型</label>
						</td>
						<td>
							<input name="EditType" id="EditType" type="text"  style="width:200px"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="EditCode">代码</label>
						</td>
						<td>
							<input name="EditCode" id="EditCode" type="text"  style="width:200px"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="EditDesc">描述</label>
						</td>
						<td>
							<input name="EditDesc" id="EditDesc" type="text"  style="width:200px"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="EditDateFrom">起始日期</label>
						</td>
						<td>
							<input name="EditDateFrom" id="EditDateFrom" type="text" class="hisui-datebox" style="width:200px"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="EditDateTo">截止日期</label>
						</td>
						<td>
							<input name="EditDateTo" id="EditDateTo" type="text" class="hisui-datebox" style="width:200px"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="EditStrA">His代码</label>
						</td>
						<td>
							<input name="EditStrA" id="EditStrA" type="text"  style="width:200px"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="EditStrB">外部到His对照</label>
						</td>
						<td>
							<input name="EditStrB" id="EditStrB" type="checkbox" class="hisui-checkbox"/>
						</td>
					</tr>
					<tr>
						<td class="r-label">
							<label for="EditActive">是否启用</label>
						</td>
						<td>
							<input name="EditActive" id="EditActive" type="checkbox" class="hisui-checkbox"/>
						</td>
					</tr>
					<tr>
						<td colspan="2" style="text-align: center;"><a href="javascript:void(0)"id="btnSave" data-options="iconCls:''"  class="hisui-linkbutton">确定</a></td>
					</tr>
				</table>
			</form>
		</div>
	</div>
</div>