		<!--block 6-->
		<div id="HomeStatusList"style="width:1164px;float:left;margin:20px 0 0 0;">
			<div class="dhcc-panel" style="float:left;width:1162px">
				<div class="dhcc-panel-header"> 
					<div class="dhcc-panel-title">不良事件流程显示</div> 
				</div>
			
				<SERVER>
						s LgGroupID=%session.Get("LOGON.GROUPID")
						s LgCtLocID=%session.Get("LOGON.CTLOCID")
						s LgUserID=%session.Get("LOGON.USERID")
						S StaDate="2018-01-01"    //开始日期
						S:StaDate'="" StaDate=$zdh(StaDate,3)
						S EndData=+$h
						S StrParam=StaDate_"^"_EndData_"^"_LgCtLocID_"^^"_LgGroupID_"^"_LgCtLocID_"^"_LgUserID_"^^^^^^^^^^"
						s RetArr=##class(web.DHCADVCOMMONPART).QueryRepHomeList(StrParam)
						S len=RetArr.%Size()
						w "<table cellspacing='0' cellpadding='0'><tr style='height:0' ><th width='10'></th>"
						w "<th width='80'>事件类型</th>"
						w "<th width='80'>患者姓名</th>"
						w "<th width='100'>登记号</th>"
						w "<th width='100'>病案号</th>"
						w "<th width='100'>发生日期</th>"
						w "<th width='100'>报告日期</th>"
						w "<th width='100' style='display:none;'>转归</th>"
						w "<th width='540'>报告流程</th>"
						w "</tr>"
						
						F i=0:1:len-1 D
						.w "<tr style='height:0;'><td></td>"
						.w "<td width='80'>"_RetArr.%Get(i).RepType_"</td>"
						.w "<td width='80'>"_RetArr.%Get(i).PatName_"</td>"
						.w "<td width='100'>"_RetArr.%Get(i).PatID_"</td>"
						.w "<td width='100'>"_RetArr.%Get(i).AdmNo_"</td>"
						.w "<td width='100'>"_RetArr.%Get(i).OccurDate_"</td>"
						.w "<td width='100'>"_RetArr.%Get(i).RepDate_"</td>"
						.w "<td width='100' style='display:none;'>暂无</td>"
						.w "<td width='540' style='text-align:left'>"
						.w "<div id='status-detail' style='font-size:1.2em;padding-top:3px'>"
						.w "<ul class='status-list' style='float:left'>"
						.w "<li class='first' id='lifontcolor'><div class='circle'></div><div class='txt'>保存</div></li>"
						.;w "<li class='' id='"_tbliid_"'><div class='"_tbcircle_"'></div><div class='txt'>提交</div></li>"
						.;w "<li class='' id='"_hshliid_"'><div class='"_hshcircle_"'></div><div class='txt'>护士长审核</div></li>"
						.;w "<li class='' id='"_khsliid_"'><div class='"_khscircle_"'></div><div class='txt'>科护士长审核</div></li>"
						.;w "<li class='' id='"_hlbliid_"'><div class='"_hlbcircle_"'></div><div class='txt'>护理部审核</div></li>"
						.;w "<li class='last' id='"_gdliid_"'><div class='"_gdcircle_"'></div><div class='txt'>归档</div></li>"
						.S EvtWorkflowID=$o(^DHCADREVTWF(0,"Event",+RetArr.%Get(i).RepTypeDr,""))
						.Q:EvtWorkflowID=""  
						.S Active=$p(^DHCADREVTWF(EvtWorkflowID),"^",4) //是否可用
						.Q:Active="N"
						.S CH="",h=0,flag=0
						.F  S CH=$o(^DHCADREVTWFI(EvtWorkflowID,"ADREWI",CH)) Q:CH=""  D
						..q:+CH=0
						..S Desc=$p(^DHCADREVTWFI(EvtWorkflowID,"ADREWI",CH),"^",2) //描述
						..S ID=EvtWorkflowID_"||"_CH
						..S:$o(^DHCADREVTWFI(EvtWorkflowID,"ADREWI",CH))="" flag=-1
						..S MedadrID="",Medflag=0,liid="",circle="nocircle"
						..F  S MedadrID=$o(^DHCMEDREPADT(0,"Pointer",RetArr.%Get(i).RepTypeDr,RetArr.%Get(i).RepID,MedadrID)) Q:MedadrID=""  D
						...S liid="",circle="nocircle"
						...q:+MedadrID=0
						...S MedRepStausDr=$p(^DHCMEDREPADT(MedadrID),"^",3)
						...S:(MedRepStausDr=ID) Medflag=1
						..S:Medflag=1 liid="lifontcolor",circle="circle"
						..S h=h+1
						..I (h>1)&&(flag=0)  d
						...w "<li class='' id='"_liid_"'><div class='"_circle_"'></div><div class='txt'>"_Desc_"</div></li>"
						..I (h>1)&&(flag=-1)  d
						...w "<li class='last' id='"_liid_"'><div class='"_circle_"'></div><div class='txt'>"_Desc_"</div></li>"
						.w "</ul>"
						.w "</div></td></tr>"
						w "</table>"

				   </SERVER>
				
				

