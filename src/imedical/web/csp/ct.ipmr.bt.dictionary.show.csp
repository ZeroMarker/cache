<!--主页面-->
<div class="hisui-layout" data-options="fit:true,border:false" >
	<div data-options="region:'west',border:false" style="width:500px;">
		<div  class="hisui-panel" fit="true" style="border:0px;">
			<table id="gridDicType" data-options="toolbar:'#DicTypeBar'"></table>
		</div>
	</div>
	<div data-options="region:'center',border:false">
		<div  class="hisui-panel ipmr-panel" fit="true" style="border:0px;padding:0px 0px 0px 10px;">
			<table id="gridDicItem" data-options="toolbar:'#custtb'"></table>
		</div>
	</div>
</div>
<div id="DicTypeBar" style="">
	<table cellspacing="0" cellpadding="0" style="width:100%;">
		<tr>
	      	<td class="" style="width:69px;"><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-add" id="t-add">新增</a></td>
			<td class="" style="width:69px;"><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-edit" id="t-edit">修改</a></td>
			<td class="" style="float:right;align:right;">
				<div style="float:right;"><input id="typeAlias" prompt="#(..Get("请输入描述/代码"))#" searcher="searchType" href="#" class="hisui-searchbox" style=""></input></div>
			</td>
		</tr>
			
	</table>
</div>
<!--字典类型编辑框-->
<div id="DicTypeDialog" style="width:415px;height:auto;padding:10px 10px 0px 10px;display:none;">
	<input class="textbox" id="txtTypeId" type="hidden" ></input>
    <table class="form-table">
		<tr>
			<td class="form-r-label">
				<label for="txtTypeCode">#(..Get("代码"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtTypeCode" style="width:300px;"></input>
			</td>
		</tr>	
		<tr>
			<td class="form-r-label">
				<label for="txtTypeDesc">#(..Get("名称"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtTypeDesc" style="width:300px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtTypeResume">#(..Get("备注"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtTypeResume" style="width:300px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="chkTypeActive">#(..Get("是否有效"))# </label>
			</td>
			<td>
				<input class="hisui-checkbox" type="checkbox" id="chkTypeActive"> </input>
			</td>
		</tr>
	</table>
</div>
<!--字典项编辑框-->
<div id="DicItemDialog" style="width:420px;height:auto;padding:10px 10px 0px 10px;display:none;">
	<input class="textbox" id="txtItemId" type="hidden" ></input>
    <table class="form-table">
		<tr>
			<td class="form-r-label">
				<label for="txtItemCode">#(..Get("代码"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtItemCode" style="width:300px;"></input>
			</td>
		</tr>	
		<tr>
			<td class="form-r-label">
				<label for="txtItemDesc">#(..Get("名称"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtItemDesc" style="width:300px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtItemOrderNo">#(..Get("排序码"))#</label>
			</td>
			<td>
				<input class="hisui-numberbox textbox" data-options="isKeyupChange:true" id="txtItemOrderNo" style="width:307px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtPYAlias">#(..Get("检索码"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtPYAlias" style="width:300px;"></input>
			</td>
		</tr>
		<tr style="display:none">
			<td class="form-r-label">
				<label for="cboItemHosp">#(..Get("医院"))#</label>
			</td>
			<td>
				<input class="hisui-combobox textbox" id="cboItemHosp" style="width:307px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="txtItemResume">#(..Get("备注"))#</label>
			</td>
			<td>
				<input class="hisui-textbox textbox" id="txtItemResume" style="width:300px;"></input>
			</td>
		</tr>
		<tr>
			<td class="form-r-label">
				<label for="chkItemActive">#(..Get("是否有效"))# </label>
			</td>
			<td>
				<input class="hisui-checkbox" type="checkbox" id="chkItemActive"> </input>
			</td>
		</tr>
	</table>
</div>

