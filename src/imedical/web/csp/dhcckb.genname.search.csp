<html >
<head>
<title>通用名信息查询</title>
<!-- dhcckb.genname.search.csp-->
<HISUI/>
<DHCCKB/>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/component.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/genname.search.js"></script>
	<style type="text/css">
		.datagrid-row-selected {
	  		background: #ADFAFC;
		}
		.panel-header, .panel-body {
		    border-color: #ccc;
		}
   </style>
</head>

<body >	
<div class="hisui-layout"  fit="true">
	<div data-options="region:'west',title:''"  style="width:600px; padding:10px 10px 10px 10px;" border=0>			
		<div class="hisui-panel" data-options="title:'通用名',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0;border-radius:0px;">	
			<div id="tempToolbar" style="padding:10px 0px 0px 10px">							
				<input id="queryCode" href="#" class="hisui-searchbox" style = "width:250px" prompt='请输入代码/名称/拼音码'/>	
				<td ><a style="margin-left:30px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" id="find" onclick="QueryDicList()">查询</a> </td>

				<td ><a style="margin-left:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="reset">重置</a> </td>
				<table cellspacing="0" cellpadding="0" >
					<tr>
				      <td><a class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true" id="insert">新增</a></td>
				      <td><a class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true" id="save">保存</a> </td>
				      <td><a class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true" id="delete">删除</a></td>	
				    </tr>
				</table>
			</div>
			<div id="templist" toolbar="#tempToolbar"  border="false"></div>
    	</div>				
	</div>
	<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray'" style="overflow:hidden;padding:0px 0px 0px 0px;">
		<div class="hisui-panel" data-options="title:'属性',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0;border-radius:0px;">	
			<div id="tempElement"  border="false"></div>
		</div>		
	</div> 
	<!--
	<div data-options="region:'east',title:''"  style="width:300px; padding:10px 0px 0px 10px;" border=0>					
	</div>
	-->
</div>	
</body>
</html>
