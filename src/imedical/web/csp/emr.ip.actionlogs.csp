<html>
<head>
	<HISUI/>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <link rel="stylesheet" type="text/css" href="../scripts/emr/css/ip.actionlogs.css"/>
	<script type="text/javascript" src="../scripts/emr/lib/JQuery/datagrid-detailview.js"></script>
	<script language="cache" runat="server">
		s episodeID = $Get(%request.Data("EpisodeID",1),"")
		s userID = %session.Get("LOGON.USERID")
		s userLocID = %session.Get("LOGON.CTLOCID")
   </script>
    <script language = "javascript">
		var episodeID = '#(episodeID)#';
		var CTLocID = '#(userLocID)#';
		var UserID = '#(userID)#';
   </script>
    <title>电子病历首页 </title>
</head>
<body class="hisui-layout" data-options="fit:true,border:false">
<div data-options="region:'north',border:false" style="height:75px;">
	<table>
		<tr>
			<td class="desc"><label>#(..Get("开始日期"))#</label></td>
			<td class="value"><input id="StDate" class="hisui-datebox" data-options="width:200"/></td>
			<td class="desc"><label>#(..Get("用户"))#</label></td>
			<td class="value"><input id="User" class="hisui-combobox"></td>
			<td class="desc"><label class="labdesc">#(..Get("名称"))#</label></td>
			<td class="value"><input id="Model" class="hisui-combobox"/></td>
		</tr>
		<tr>
			<td class="desc"><label>#(..Get("结束日期"))#</label></td>
			<td class="value"><input id="EndDate" name="EndDate"  class="hisui-datebox" data-options="width:200"/></td>
			<td class="desc"><label>#(..Get("类型"))#</label></td>
			<td class="value"><input id="Type" class="hisui-combobox" /></td>
			<td class="desc"><label>#(..Get("病历"))#</label></td>
			<td class="value"><input id="EMRName" class="hisui-combobox" /></td>
			<td colspan=2><a id="Find" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">#(..Get("查询"))#</a></td>
		</tr>
	</table>
		
</div>
	
<div data-options="region:'center',border:false" style="padding:5px;">
    <table id="actionLogsListData"></table>
</div>
    <script type="text/javascript" src="../scripts/emr/js/ip.actionloglist.js"></script>    
</body>
</html>
