<!-- 
 * FileName: dhcbill.opbill.billcashcalc.csp
 * Anchor: ZhYW
 * Date: 2019-06-13
 * Description: 门诊收费计算器
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.opbill.billcashcalc.css'/>
	<server>
		w "<input id='job' name='job' type='hidden'>",$c(13,10)
		w "<input id='roundErrAmt' name='roundErrAmt' type='hidden'>",$c(13,10)
	</server>
</head>
<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',border:false,collapsible:false" style="height:131px;">
			<div class="hisui-panel" data-options="fit:true,bodyCls:'panel-header-gray'" style="border-bottom-style:none;border-radius:4px 4px 0 0;">
				<table class="search-table" style="width:100%">
					<tr>
						<td class="r-label td-first-col"><label>当前票据号</label></td>
						<td><input id="currInvNo" class="textbox text-bold" disabled/></td>
						<td class="r-label"><label>票据张数</label></td>
						<td><input id="invNum" class="hisui-numberbox textbox text-bold"/></td>
					</tr>
					<tr>
						<td class="r-label"><label>结算费用</label></td>
						<td><input id="chargeSum" class="hisui-numberbox textbox text-bold" data-options="precision:2,disabled:true"/></td>
						<td class="r-label"><label>现金</label></td>
						<td><input id="cashSum" class="hisui-numberbox textbox text-bold" data-options="precision:2,disabled:true"/></td>
					</tr>
					<tr>
						<td class="r-label"><label>收取现金</label></td>
						<td><input id="getCashAmt" class="hisui-numberbox textbox text-bold" style="color: #FF0000;" data-options="precision:2"/></td>
						<td class="r-label"><label>应找现金</label></td>
						<td><input id="retCashAmt" class="hisui-numberbox textbox text-bold" style="color: #FF0000;" data-options="precision:2,disabled:true"/></td>
					</tr>
				</table>
			</div>
		</div>
		<div data-options="region:'center',border:false">
			<div class="hisui-panel panel-header-gray" data-options="fit:true,headerCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
				<table id="paymList"></table>
			</div>
	  	</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcopbill/dhcbill.opbill.billcashcalc.js"></script>
</body>
</html>