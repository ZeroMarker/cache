<!-- dhcem.nurshiftindex.csp  急诊交班本指标项 -->
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<link href="../scripts/dhcnewpro/dhcem/css/base.css" rel="stylesheet">
	<link href="../scripts/dhcnewpro/dhcem/css/nurshiftindex.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/bedsideshiftscom.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/nurshiftindex.js"></script>
</head>
<body>
	<div class="hisui-layout" fit="true">
		<div data-options="region:'center',title:''" border=false style='padding:5px 5px 0px 5px;'>
			<div class="hisui-panel" data-options="title:'急诊病区交班指标',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0;border-bottom:0px;border-radius:0px;">		
				<div class="row">
					<label class="item-val-1">日期：</label><span class="item-val-110"><span id="WrDate">2022-10-12</span></span>
					<label class="item-val-1">班次：</label><span class="item-val-140"><span id="Schedule">早班(06:00~12:00)</span></span>
				    <label class="item-val-1">病区：</label><span class="item-val-140"><span id="CrLoc">急诊留观区</span></span>
				    <label class="item-val-1">交班人：</label><span class="item-val-110"><span id="LgUser">李子柒</span></span>
	    		</div>
				<table id="itemList" border="1" cellspacing="0" cellpadding="1" class="report_table">
					<tr style="display:none">
						<td align="right" class="tb_td_bk">WLID</td>
						<td class="tb_td_val"><span id="WLID" class="td-span-m"/></td>
						<td align="right" class="tb_td_bk">EpisodeID</td>
						<td class="tb_td_val"><span id="EpisodeID" class="td-span-m"/></td>
						<td align="right" class="tb_td_bk"></td>
						<td class="tb_td_val"></td>
						<td align="right" class="tb_td_bk"></td>
						<td class="tb_td_val"></td>
					</tr>
				</table>
			</div>
		</div>
	</div>
</body>
</html>
