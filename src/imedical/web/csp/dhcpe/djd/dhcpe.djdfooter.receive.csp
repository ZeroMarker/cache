<!--
Foot  领取信息
dhcpe/djd/dhcpe.djdfooter.receive.csp
 -->
<table>
	<tr>
		<!-- tdata="pageNO"是第几页，tdata="pageCount"是共几页。 -->
		<td style="width:25%;" align="left"><span tdata="pageNO">第#页</span>/<span tdata="pageCount">共#页</span></td>
		<td style="width:50%;" align="center">#(FootData.FootStr)#</td>
		<td style="width:25%;" align="right">#(FootData.PrtDate)#</td>
	</tr>
</table>

<table>
	<tr style="height:8pt;">
		<td style="width:45pt;"></td>
		<td style="width:65pt;"></td>
		<td style="width:80pt;"></td>
		<td style="width:55pt;"></td>
		<td style="width:40pt;"></td>
		<td style="width:55pt;"></td>
		<td style="width:40pt;"></td>
		<td style="width:55pt;"></td>
		<td style="width:95pt;"></td>
	</tr>
	<tr style="height:5pt;">
		<td colspan=9 style="border-top: 1px solid #000000;"></td>
	</tr>
	<tr>
		<td valign="middle" algin="center" rowspan=4 style="padding:2pt;">
			<img src="data:image/png;base64,#(FootData.BarCode)#"/>
		</td>
		
		<td valign="bottom" class="text"><span>姓&ensp;&emsp;名：</span></td>
		<td valign="bottom" class="val">#(FootData.PatName)#</td>
		
		<td valign="bottom" class="text"><span>性&emsp;别：</span></td>
		<td valign="bottom" class="val"><span>#(FootData.PatSex)#</span></td>
		
		<td valign="bottom" class="text"><span>年&emsp;龄：</span></td>
		<td valign="bottom" class="val"><span>#(FootData.PatAge)#</span></td>
		
		<td valign="bottom" class="text"><span>体检号：</span></td>
		<td valign="bottom"><span style="font-size:24pt; font-family:C39HrP24DhTt">#(FootData.BarHPNo)#</span></td>
	</tr>
	<tr>
		<td valign="bottom" class="text"><div style="height:3pt;"></div><span>VIP等级：</span></td>
		<td valign="bottom" class="val"><span>#(FootData.VIPLevel)#</span></td>
		
		<td valign="bottom" class="text"><span>证件号：</span></td>
		<td valign="bottom" class="val" colspan=3><span>#(FootData.PatIDCard)#</span></td>
		
		<td valign="bottom" class="text"><span>电&emsp;话：</span></td>
		<td valign="bottom" class="val"><span>#(FootData.PatTel)#</span></td>
	</tr>
	<tr>
		<td valign="bottom" class="text"><div style="height:3pt;"></div><span>单&ensp;&emsp;位：</span></td>
		<td valign="bottom" class="val" colspan=5><span>#(FootData.Company)#</span></td>
		
		<td valign="bottom" class="text"><span>部&emsp;门：</span></td>
		<td valign="bottom" class="val"><span>#(FootData.Position)#</span></td>
	</tr>
	<tr>
		<td colspan=8>&ensp;#(FootData.Remark)#</td>
	</tr>
</table>