<!DOCTYPE html>
<html lang="en">
<head>
    <TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>
    <style>
    </style>
</head>

<body class="hisui-layout">
<div data-options="region:'west',border:false" style="width:360px;padding:10px;">
    <div class="hisui-panel" style="height:100%;" data-options="title:'应用程序',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
        <table id="AppGrid"></table>
    </div>
</div>

<div style="padding:10px 10px 10px 0;" data-options="region:'center',border:false">
    <div class="hisui-layout" data-options="fit:true">
        <div style="padding-bottom:10px;" data-options="region:'center',border:false">
            <div class="hisui-panel"
                 data-options="title:'参数',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
                <table id="AppParamGrid"></table>
            </div>
        </div>

        <div data-options="region:'south',border:false" style="height:340px;">
            <div class="hisui-panel"
                 data-options="title:'参数值',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
                <table id="AppParamValueGrid"></table>
            </div>
        </div>
    </div>
</div>
<div id="myWin"></div>
<script type="text/javascript" src="../scripts/dhcpeappsettings.js"></script>
</body>
</html>