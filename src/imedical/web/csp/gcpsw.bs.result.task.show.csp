<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',collapsible:true" title="任务日志" style="padding:10px;height:90px;border-bottom:1px solid #ccc;">
		<label class="c-span">项目</label>
		<input id="prj" type="text" class="textbox"  />
		<label class="c-span2"></label>
		<label class="c-span">开始日期</label>
		<input id="startDate" class="textbox hisui-datebox"  />
		<label class="c-span2"></label>
		<label class="c-span">结束日期</label>
		<input id="endDate" class="textbox hisui-datebox"  />
		<label class="c-span2"></label>
		<a id="Find" class="green hisui-linkbutton" >查询</a>
		<label class="c-span2"></label>
		<a id="Clear" class="red hisui-linkbutton" >重置</a>
	</div> 
	<div id="i-center" data-options="region:'center'" >
		<div id="i-list"></div>
	</div> 
</div>