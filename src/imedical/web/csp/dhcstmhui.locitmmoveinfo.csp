<!--库存动销查询-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<SERVER>
	d ##class(web.DHCSTMHUI.Util.RQServerConfig).GetApplet()
</SERVER>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="BorderLine" data-options="region:'north',headerCls:'panel-header-gray'">
		<table id="Conditions" class="Condition" style="text-align:right;">
			<input id="Inci" name="Inci" type="hidden">
			<tr>
				<td><label class="required">*</label>科室</td>
				<td><input id="PhaLoc" name="PhaLoc" class="hisui-combobox textbox"></td>
				<td><label class="required">*</label>起始日期</td>
				<td><input id="StartDate" name="StartDate" class="hisui-datebox textbox"></td>
				<td><label class="required">*</label>截止日期</td>
				<td><input id="EndDate" name="EndDate" class="hisui-datebox textbox"></td>
				<td>库存分类</td>
				<td><input id="StkCat" name="StkCat" class="hisui-combobox textbox"></td>
				<!--<td><a href="#" id="PrintBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">打印</a></td>-->
			</tr>
			<tr>
				<td>类组</td>
				<td><input id="StkGrpId" name="StkGrpId" class="hisui-stkscgcombotree textbox"></td>
				<td>起始时间</td>
				<td><input id="StartTime" name="StartTime" class="hisui-timespinner textbox"></td>
				<td>截止时间</td>
				<td><input id="EndTime" name="EndTime" class="hisui-timespinner textbox"></td>
				<td>物资名称</td>
				<td><input id="InciDesc" name="InciDesc" class="textbox"></td>
				<td colspan="2">
					<a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					<a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
				</td>
			</tr>
		</table>
	</div>
	<div data-options="border:false,headerCls:'panel-header-gray',region:'center'">  
		<table id="StockQtyGrid"></table>
	</div>

	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/encode.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/StockQuery/LocItmMoveInfo.js"></script>
</body>
</html>
