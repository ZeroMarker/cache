<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI/>
<DHCEQ:HISUI></DHCEQ:HISUI>
<script type="text/javascript" src="../scripts/dhceq/plat/capproverole.js"></script>
</head>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px">
		<div class="hisui-panel"  title="角色定义" data-options="fit:true,eqtitle:'maintitle'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #E2E2E2;padding-bottom:10px;">
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label-first"><label id="cCode" for="Code">代码</label></div>
						<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox" id="Code"  ></div>
						<div class="eq-table-td eq-table-label-new"><label id="cDesc" for="Desc">描述</label></div>
						<div class="eq-table-td eq-table-input"><input   class="hisui-validatebox" id="Desc" ></div>
						<div class="eq-table-td eq-table-label-new"><label id="cLocFlag" for="LocFlag">管理科室范围</label></div>
						<div class="eq-table-td eq-table-input"><input  class="hisui-combobox textbox" id="LocFlag"></div>
						<div class="eq-table-td eq-table-label"><a id="BFind" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">查询</a></div>
						<div class="eq-table-td eq-table-label"><a id="BAdd" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">新增</a></div>
					</div>
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label-first"><label for="Remark">备注</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Remark" ></div>
						<div class="eq-table-td eq-table-label-new"></div>
						<div class="eq-table-td eq-table-input"></div>
						<div class="eq-table-td eq-table-label-new"></div>
						<div class="eq-table-td eq-table-input"></div>
						<div class="eq-table-td eq-table-label"><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">保存</a></div>
						<div class="eq-table-td eq-table-label"><a id="BDelete" href="#" class="hisui-linkbutton " data-options="stopAllEventOnDisabled:true">删除</a></div>
					</div>
				</div>
				<div data-options="region:'center',border:false" style="padding:0px;position:relative">
					<table id="tDHCEQCApproveRole" data-options="border:false,fit:true"></table>
				
				</div>
			</div>
		</div>
	</div>
	<div id="hiddenEncrypt" style="width:0;height:0;">
		<form name="DHCEQ">	
			<INPUT TYPE="hidden" id="RowID">
		</form>
	</div>
</body>

</html>