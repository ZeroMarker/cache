<!DOCTYPE html>
<!--
	desc: 药房药库-移动设置-用户/科室/安全组关联菜单
	csp:  csp/pha.mob.v2.menucfg.csp
	js:   scripts/pha/mob/v2/menu.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI />
    <PHALIBV1 />
</HEAD>

<body>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',border:false" class="pha-body">
            <div class="hisui-layout" fit="true" border="false">
                <div data-options="region:'west',split:true,width:430,border:false">
                    <div class="hisui-panel"
                        data-options="title:'授权类别',headerCls:'panel-header-gray',iconCls:'icon-paper-group',fit:true">
                        <div class="hisui-layout" fit="true">
                            <div data-options="region:'north',border:false,split:false,height:46"
                                style="overflow: hidden;padding:5px">
                                <div id="kwAuthType"></div>
                                <div style="float:left">
                                    <div style="border-left: 1px solid #cccccc;height:20px;margin:8px;"></div>
                                </div>
                                <div id="kwAuthStat"></div>
                            </div>
                            <div data-options="region:'center',border:false">
                                <table id="gridAuth"></table>
                            </div>
                        </div>
                    </div>
                </div>
                <div data-options="region:'center',border:false">
                    <div class="hisui-layout" fit="true" border="false">
                        <div data-options="region:'west',split:true,width:300,border:false,border:false">
                            <div class="hisui-panel"
                                data-options="title:'授权菜单',headerCls:'panel-header-gray',iconCls:'icon-paper-info',fit:true">
                                <table id="gridMenu"></table>
                            </div>
                        </div>
                        <div data-options="region:'center',border:false">
                            <div class="hisui-panel"
                                data-options="title:'菜单授权 [拖动行移动顺序]',headerCls:'panel-header-gray',iconCls:'icon-paper-key',fit:true">
                                <table id="gridCfgItm"></table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="gridAuthBar">
        <div style="padding-left:9px;padding-bottom: 10px;">
            <input class="hisui-searchbox" style="width:395px" id="conAuthAlias">
        </div>
    </div>
    <div id="gridMenuBar">
        <div style="padding: 10px 9px;">
            <input class="hisui-searchbox" id="conMenuAlias" style="width:263px">
        </div>
    </div>
    <!-- 右侧工具栏 -->
    <!-- <div id="gridCfgItmBar">
        <a class="hisui-linkbutton" plain='true' iconCls="icon-add" id="btnAdd">新增</a>
        <a class="hisui-linkbutton" plain='true' iconCls="icon-save" id="btnSave">保存</a>
        <a class="hisui-linkbutton" plain='true' iconCls="icon-remove" id="btnDel">删除</a>
    </div> -->
    <!-- 本页面js -->
    <script type="text/javascript" src="../scripts/pha/mob/v2/menucfg.js"></script>
</body>

</HTML>