<!--
文件名	DHCPEGTeamRegQuery.csp
功能实现	团体组项目查询
调用条件	要给一个参数 ParRef 团体编码
-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<HTML XMLNS=TRAK>
<HEAD>
<TITLE><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></TITLE>
<TRAK:HEAD></TRAK:HEAD>
<SCRIPT SRC="../scripts/websys.js"> </SCRIPT>
</HEAD>

<SCRIPT language="Cache" RUNAT="SERVER">
 s FFF=$g(%request.Data("FFF",1))
 s PAADMRowid=$p(FFF,"^")
 s PatientID=$p(FFF,"^",3)
</Script>

<SCRIPT language="Cache" RUNAT="SERVER">
	s ParRefBox=%request.Get("ParRef")	//
	s sTypeBox=%request.Get("sType")		//查询类型?GT :按团体组查询	G?:按团体查询
	i (""=sTypeBox) s sTypeBox="GT"
	s URLparam="&ListTargeURL=DHCPEGIAdmList&ParamName=EpisodeID"
				_"&ParRef="_ParRefBox_"&sType='"_sTypeBox_"'"
	s OtherParamBox="&ListTargeURL=DHCPEIAdmOrderList&ParamName=EpisodeID"
</Script>

<frameset cols="20%,*" frameborder="NO" border="0" framespacing="0">
	<frameset rows="40%,*" frameborder="NO" border="0" framespacing="0">
		<frame name="DHCPEGTeamList" src="websys.default.csp?WEBSYS.TCOMPONENT=DHCPEGTeamList&FFF=&ListTargeURL=DHCPEGIAdmList&ParamName=ParRef#(URLparam)#&OtherParam=#(..EscapeURL(OtherParamBox))#" frameborder=0>
		<frame name="DHCPEGIAdmList" src="websys.default.csp?WEBSYS.TCOMPONENT=DHCPEGIAdmList&ListTargeURL=DHCPEIAdmOrderList&ParamName=EpisodeID" frameborder=0>
	</frameset>
	<frame name="DHCPEIAdmOrderList" src="websys.default.csp?WEBSYS.TCOMPONENT=DHCPEIAdmOrderList" frameborder=0>	
</frameset>
<noframes>
	<body>
	<p>Your browser does not appear to support 'frames' which are required by this application. Please download version 5 or later of <a href="http://www.microsoft.com/">Microsoft Internet Explorer</a>.</p>
	</body>
</noframes>
</HTML>
