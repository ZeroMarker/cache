<csp:content charset="UTF-8">
<SERVER>
s Action=$Get(%request.Data("actiontype",1))
s Start=$Get(%request.Data("start",1))
s Limit=$Get(%request.Data("limit",1))
s Sort=$Get(%request.Data("sort",1))
s Dir=$Get(%request.Data("dir",1))
s sHospID=$G(%session.Data("LOGON.HOSPID"))

i Action = "GetMatAuditInfo" d
	.s StartDate=$Get(%request.Data("StartDate",1))
	.s EndDate=$Get(%request.Data("EndDate",1))
	.s AuditFlag=$Get(%request.Data("AuditFlag",1))
	.s other=$Get(%request.Data("other",1))
	.s ^litmp("GetMatAuditInfo")=AuditFlag
	.d ##class(web.DHCSTM.MatAudit).jsGetMatAuditInfo(Start,Limit,Sort,Dir,StartDate,EndDate,AuditFlag,other)
	.
	
;审核通过
i Action = "AuditStr" d
	.s IdStr=$Get(%request.Data("IdStr",1))
	.s result=##class(web.DHCSTM.MatAudit).AuditStr(IdStr)
	.i +result'=0 d
	..w "{success:'false',info:'"_result_"'}"
	.e  d
	..w "{success:'true',info:'"_result_"'}"
;审核不通过
i Action = "AuditNoStr" d
	.s IdStr=$Get(%request.Data("IdStr",1))
	.s result=##class(web.DHCSTM.MatAudit).AuditNoStr(IdStr)
	.i +result'=0 d
	..w "{success:'false',info:'"_result_"'}"
	.e  d
	..w "{success:'true',info:'"_result_"'}"
</SERVER>