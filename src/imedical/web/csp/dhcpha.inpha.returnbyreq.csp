<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
q 1
</csp:method>

<HTML XMLNS=TRAK>
<HEAD>
<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
<TRAK:HEAD></TRAK:HEAD> 
<!-- Put your page Title here -->
<title>	住院申请单退药 </title>
    
    <link rel="stylesheet" type="text/css" href="../scripts/dhcpha/jQuery/themes/default/easyui.css">
	<link rel="stylesheet" type="text/css" href="../scripts/dhcpha/jQuery/themes/icon.css">
	<link rel="stylesheet" type="text/css" href="../scripts/dhcpha/css/jquery-ext.css" ></script> 
	<script type="text/javascript" src="../scripts/dhcpha/jQuery/jquery-1.8.3.js"></script>
	<script type="text/javascript" src="../scripts/dhcpha/jQuery/jquery.easyui.min.js"></script>
	<script type="text/javascript" src="../scripts/dhcpha/jQuery/easyui-lang-zh_CN.js" charset="utf-8"></script>
	<script type="text/javascript" src="../scripts/dhcpha/jqueryplugins/commonfun.js"></script>
	<script type="text/javascript" src="../scripts/dhcpha/jqueryplugins/component.js"></script>
	<script type="text/javascript" src="../scripts/dhcpha/DHCSTCommonJs/CommonFun.js"></script>
	<script type="text/javascript" src="../scripts/dhcpha/inpha/dhcpha.inpha.printreturn.js"></script>
	<script type="text/javascript" src="../scripts/dhcpha/inpha/dhcpha.inpha.returnbyreq.js"></script>
 </HEAD>
<body>
	<div id="cc" class="easyui-layout" fit="true" border="false">
		<div data-options="region:'west',title:'申请单退药',split:true" style="width:340px;">
			<div class="easyui-layout" data-options="fit:true" border="false">
				<div data-options="region:'north',split:true" border="false" style="height:225px;">
					<table style="padding:4px">
						<tr>
							<td style="width:80px;">退药科室:</td>
							<td>
								<input id="phaLoc" style="width:150px;HEIGHT: 22px;border:1px solid #95B8E7;"></input>
							</td>
						</tr>
						<tr>
							<td style="width:80px;">病区:</td>
							<td>
								<input id="wardLoc" name="cWard" style="width:150px;HEIGHT: 22px;border:1px solid #95B8E7;" value=""></input>
							</td>
							<td>
								<a style="margin-left:5px;">
									<input id="checkedDocFlag" class="ui-checkbox" name="checkedDocFlag" type="checkbox">特殊科室</input>
								</a>
							</td>
						</tr>
						<tr>
							<td style="width:80px;">起始日期:</td>
							<td>
								<input id="startDate" class="easyui-datebox" style="width:150px;border:1px solid #95B8E7;" data-options="valueField:'val',textField:'text'"></input>
							</td>
						</tr>

						<tr>
							<td style="width:80px;">截止日期:</td>
							<td>
								<input id="endDate" class="easyui-datebox" style="width:150px;border:1px solid #95B8E7;" data-options="valueField:'val',textField:'text'"></input>
							</td>
						</tr>

						<tr>
							<td style="width:80px;">登记号:</td>
							<td>
								<input id="patNo" name="patNo" style="width:150px;HEIGHT: 22px;border:1px solid #95B8E7;"></input>
							</td>
						</tr>

						<tr>
							<td style="width:80px;">姓名:</td>
							<td>
								<input id="patName" name="patName" style="width:150px;HEIGHT: 22px;border:1px solid #95B8E7;" value=""></input>
							</td>
						</tr>
					</table>
					<table cellpadding=4>
						<tr>
							<td><a class="easyui-linkbutton" iconCls='icon-search' plain="true" id="btnFind">查找</a>
							</td>
							<td><a class="easyui-linkbutton" iconCls='icon-tip' plain="true" id="btnFindSelect">查看选择申请单</a>
							</td>
							<td><a class="easyui-linkbutton" iconCls='icon-ok' plain="true" id="btnReturn">退药</a>
							</td>
						</tr>
					</table>

				</div>
				<div data-options="region:'center',title:'申请单列表'" border="false" >
						<div id="reqlist"></div>
				</div>
			</div>
		</div>
		<div data-options="region:'center'">
			<div id="cc1" class="easyui-layout" fit="true">
				<div data-options="region:'center',split:true,title:'药品明细列表'" style="border: 0px none;">
					<div id="reqdetail"></div>
				</div>
				<div data-options="region:'south',split:true,title:'单品汇总'" style="border: 0px none;height:285px;">
					<div id="reqtotal"></div>
				</div>
			</div>
		</div>
	</div>
</body>
</HTML>