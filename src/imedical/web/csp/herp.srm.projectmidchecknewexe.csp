	i action = "add" d
	.s result = ##class(herp.srm.udata.ProjectMidCheckNew).InsertRec(rowid,Detail, userdr)
	.i result = 0 d
	..w "{success:'true',refresh:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
