<!DOCTYPE html>
<!--药品药学成分维护
	csp:dhcpha.pivas.drgingredient.csp
	js: scripts/pharmacy/pivas/drgingredient.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>

    </TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css">
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/pivas/css/ui.css">
</HEAD>

<body style="background:white;">
    <div class="dhcpha-win-mask">
        <img src="../scripts/pharmacy/common/image/page-loading.gif"></img>
    </div>
    <div class="hisui-layout" fit="true" border="false">
        <div data-options="region:'center'" class="dhcpha-hisui-container">
            <div class="hisui-layout" fit="true" border="false">
                <div data-options="region:'west',title:'药品列表',split:true,width:535,headerCls:'panel-header-gray',iconCls:'icon-panel-drug',collapsible:false">
                    <table id="gridDrug"></table>
                </div>
                <div data-options="region:'center',title:'药品药学成分',headerCls:'panel-header-gray',iconCls:'icon-panel-drug-mol'">
                    <table id="gridDrugIngred"></table>
                </div>
            </div>
        </div>
    </div>
    <div id="gridDrugBar">
        <table class="dhcpha-north-tbl" cellspacing=0 cellpadding=0>
            <tr>
                <td style="display:none;">
                    <input id="cmbCatGrp" type="text" />
                </td>

                <td>
                    <input id="cmbStkCat" type="text" />
                </td>

                <td>
                    <input id="txtAlias" type="text" class="hisui-validatebox" placeholder="别名..." style="width:125px"/>
                </td>
                <td>
                    <a class="hisui-linkbutton" iconCls="icon-w-find" plain="false" id="btnFind">查询</a>
                </td>
            </tr>
        </table>
    </div>
    <div id="gridDrugIngredBar">
        <table cellspacing=0 cellpadding=0>
            <tr>
                <td>
                    <a class="hisui-linkbutton" plain='true' iconCls="icon-add" id="btnAdd">增加</a>
                </td>
                <td>
                    <a class="hisui-linkbutton" plain='true' iconCls="icon-save" id="btnSave">保存</a>
                </td>
                <td>
                    <a class="hisui-linkbutton" plain='true' iconCls="icon-remove" id="btnDelete">删除</a>
                </td>
                <td>
                    <input type="text" class="hisui-validatebox" style="visibility: hidden;" />
                </td>
            </tr>
        </table>
    </div>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
    <script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/drgingredient.js"></script>
</body>

</HTML>