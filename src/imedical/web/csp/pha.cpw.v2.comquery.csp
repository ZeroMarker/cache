<HTML>
	<!--综合查询
		csp:pha.cpw.v2.comquery.csp
		js: scripts/pha/cpw/v2/compquery.js
	-->
	<!-- 验证session过期 -->
	<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
	</csp:method>
	<HEAD>
                <TRAK:HEAD></TRAK:HEAD>
	<meta http-equiv="Content-Type" content="text/html; ">
	<script Language="Javascript">
		var LgGroupID="#(%session.Get("LOGON.GROUPID"))#";      //安全组ID
		var LgGroupDesc="#(%session.Get("LOGON.GROUPDESC"))#";  //安全组
		var LgUserID="#(%session.Get("LOGON.USERID"))#";        //用户ID
		var LgUserCode="#(%session.Get("LOGON.USERCODE"))#";    //用户工号
		var LgUserName="#(%session.Get("LOGON.USERNAME"))#";    //用户姓名
		var LgCtLocID="#(%session.Get("LOGON.CTLOCID"))#";      //科室ID
		var LgHospID="#(%session.Get("LOGON.HOSPID"))#";        //医院ID
	</script>
	<style type="text/css">
		.cpw-north-tbl {
		    padding-top: 5px;
		    padding-bottom: 5px;
		    border-spacing: 1px;
		}
		.cpw-north-tbl td {
		    text-align: right;
		    padding-left: 30px;
		    margin-left: 30px;
		}
		.cpw-north-tbl tr {
		    height: 40px;
		}
	</style>
	<EASYUI:CLINICAL/>
	<script type="text/javascript" src="../scripts/pha/cpw/v2/compquery.js"></script>
	</head> 
	<body class="easyui-layout">
		<div data-options="region:'center'" style="padding:10px;" border="false">
			<div class="easyui-panel" style="border-radius:4px;" fit="true">
		    	<table id="patCompList" border="false" data-options="toolbar:'#toolBar'">
		    	</table>
		    </div>
		</div>
	</body>
	<div id="toolBar" style="background-color:white" >
		<table class="cpw-north-tbl">
			<tr>
				<td>
					<label for="StDate">开始日期</label>
					<input id="StDate" style="width:120" type="text" class="easyui-datebox" required="required"></input>
				</td>
				<td>
					<label for="ward">病区</label>
					<input id="ward" style="width:150" class="easyui-combobox" data-options="valueField:'value',textField:'text'"/>
				</td>
				<td>
    				<label>登记号<input id="PatNo" type="text" style="width:150"/></label>
    			</td>
    			<td>
					<label><input id="Phw" type="checkbox" name="qItm" value="药学查房" checked="checked"/>药学查房</label>
				</td>
    			<td>
    				<label><input id="Phcp" type="checkbox" name="qItm" value="医学查房" checked="checked"/>医学查房</label>
    			</td>
    			<td>
    				<label><input id="Phc" type="checkbox" name="qItm" value="药学监护" checked="checked"/>药学监护</label>
				</td>
				<td>
    				<label><input id="DrgCal" type="checkbox" name="qItm" value="电子药历" checked="checked"/>电子药历</label>
				</td>
				<td>
					<a href="#" id="Find" style="width:90" class="easyui-linkbutton" iconCls="icon-search">查询</a>
				</td>
			</tr>
			<tr>
				<td>
					<label for="EdDate">截止日期</label>
					<input id="EdDate" style="width:120" type="text" class="easyui-datebox" required="required"></input>
				</td>
				<td>
					<label for="Pharmacist">药师</label>
					<input id="Pharmacist" style="width:150" class="easyui-combobox" data-options="valueField:'value',textField:'text'"/>
				</td>
				<td colspan=2>
					<label><input id="MedEdu" type="checkbox" name="qItm" value="用药教育" checked="checked"/>用药教育</label>
    			</td>
    			<td>
    				<label><input id="Pha" type="checkbox" name="qItm" value="用药建议" checked="checked"/>用药建议</label>
    			</td>
    			<td>
    				<label><input id="AdrRep" type="checkbox" name="qItm" value="不良报告" checked="checked"/>不良报告</label>
    			</td>
    			<td colspan=2>
					<a href="#" id="Clear" style="width:90" class="easyui-linkbutton" iconCls="icon-clear">清屏</a>
				</td>
			</tr>
		</table>
	</div>
	<div id="newItmWin"></div>
</html>
