<div id="item_panel" class="hisui-panel" title="治疗申请单" data-options="border:true,iconCls:'icon-apply-opr',headerCls:'panel-header-gray',fit:true">   
	<div id="apply_form_region" class="hisui-layout" data-options="fit:true,border:false">
		<div data-options="region:'north',title:'',border:false,collapsible:false,headerCls:'panel-header-gray'" style="padding:5px;height:65px">
			<div class="hisui-panel" data-options="border:false,fit:true,headerCls:'panel-header-card',closable:false, collapsible:false,minimizable:false,maximizable:false">   			
				<div id="warning_panel" style="display:none;margin:3px 0 2px 2px;border-radius:4px;border:1px solid #FF3D2C;background-color:#FFE3E3">
					<label id="warning" style="font-size:14px;color:red"></label>
				</div>
				<div id="patient-label" style="padding:10px">
					<ul>
						<li>
							<label id="patSeximg" style="padding:0px 10px 0px 0px" name="patSeximg"></label>
						</li>
						<li>
							<label id="patName"  name="patName" style="font-size:16px;">-</label>
							<label class="line">/</label>
							<label id="patSex"  name="patSex">-</label>
							<label class="line">/</label>
							<label id="patAge"  name="patAge">-</label>
							<label class="line">/</label>
							<font style="color:#666666">#(..Get("登记号:"))#</font><label id="patNo"  name="patNo"></label>
							<label class="line">/</label>							
							<label id="patType"  name="patType">-</label>
							<label class="line">/</label>
							<label id="charge"  name="charge">-</label>
							<label class="line">/</label>
							<label id="patBed"  name="patBed">-</label>
							<label class="line">/</label>							
							<!--<label id="InfoAdmDays"  name="InfoAdmDays" style="width:100px">-</label>
							<label class="line">/</label>	-->						
							<font style="color:#666666">#(..Get("诊断:"))#</font><label id="Diagnosis"  name="Diagnosis"></label>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray'" style="padding:0px 10px;">
			<div class="hisui-panel"  title="医嘱信息" data-options="headerCls:'panel-header-card-gray',fit:true,closable:false,collapsible:false,minimizable:false,maximizable:false"> 
				<div id="apply_form_panel">
					<table  class="form-table" style="width:100%">
						<tr> 
							<td class="r-label">
								<label for="OrderArcimDesc">#(..Get("治疗医嘱"))#</label>
							</td>
							<td class="i-label">
								<input id="OrderArcimDesc" type="text" class="hisui-validatebox textbox" name="OrderArcimDesc" style="width:143px;" disabled/>
								<input id="ItemRowid" name="ItemRowid" type="hidden"/>
								<input id="DDCISRowid" name="DDCISRowid" type="hidden"/>
								<input id="OrderPHPrescType" name="OrderPHPrescType" type="hidden"/>
								<input id="OrderType" name="OrderType" type="hidden"/>
								<input id="OrderConFac" name="OrderConFac" type="hidden"/>
							</td> 
							<td class="r-label">
								<label for="OrderPrior">#(..Get("医嘱类型"))#</label>
							</td>
							<td class="i-label">
								<input id="OrderPrior" type="text" class="hisui-combobox midtext" name="OrderPrior" style="width:130px"/>
							</td> 
							<td  class="r-label">
								<label for="OrderDoseQty">#(..Get("单次数量"))#</label>
							</td>
							<td  class="i-label">
								<input id="OrderDoseQty" class="hisui-numberbox" type="text" name="OrderDoseQty" style="width:50px"/>
								<input id="OrderDoseUOM" type="text" class="hisui-combobox" name="OrderDoseUOM" style="width:80px"/>
								<input id="OrderDoseStr" name="OrderDoseStr" type="hidden"/>
							</td> 
						</tr> 
						<tr> 
							<td  class="r-label">
								<label for="OrderFreq">#(..Get("频次"))#</label>
							</td>
							<td  class="i-label">
								<input id="OrderFreq" type="text" name="OrderFreq" style="width:150px"/> <!--maxtext-->
								<input id="OrderFreqFactor" name="OrderFreqFactor" type="hidden"/>
								<input id="OrderFreqInterval" name="OrderFreqInterval" type="hidden"/>
								<input id="OrderFreqRowid" name="OrderFreqRowid" type="hidden"/>
								<input id="OrderFreqDispTimeStr" name="OrderFreqDispTimeStr" type="hidden"/>
							</td> 
							<td  class="r-label">
								<label for="OrderDur">#(..Get("疗程"))#</label>
							</td>
							<td class="i-label">
								<input id="OrderDur" type="text" name="OrderDur" style="width:130px"/><!--midtext-->
								<input id="OrderDurRowid" name="OrderDurRowid" type="hidden"/>
								<input id="OrderDurFactor" name="OrderDurFactor" type="hidden"/>
							</td> 
							<td  class="r-label">
								<label for="OrderQty">#(..Get("数量"))#</label>
							</td>
							<td class="i-label">
								<input id="OrderQty" type="text" class="hisui-numberbox" name="OrderQty" style="width:50px"/>
								<input id="BillingUOM" type="text" name="BillingUOM" class="hisui-combobox"  style="width:80px"/>
							</td>	
						</tr> 
						<tr>
							<td  class="r-label">
								<label for="OrderReLoc">#(..Get("接收科室"))#</label>
							</td> 
							<td  class="i-label">
								<input id="OrderReLoc" type="text" class="hisui-combobox maxtext" name="OrderReLoc" style="width:150px"/>
							</td> 
							<td  class="r-label">
								<label for="OrderBillType">#(..Get("费别"))#</label>
							</td> 
							<td class="i-label">
								<input id="OrderBillType" type="text" class="hisui-combobox midtext" name="OrderBillType" style="width:130px"/>
							</td> 
							<td class="r-label">
								<label for="OrderStartDate">#(..Get("开始日期"))#</label>
							</td> 
							<td class="i-label">
								<input id="OrderStartDate" type="text"  class="hisui-datetimebox" name="OrderStartDate" data-options="onChange:SetOrderFirstDayTimes" style="width:135px"/>
							</td> 
						</tr>
						<tr> 
							<td  class="r-label">
								<label for="OrderEndDate">#(..Get("结束日期"))#</label>
							</td> 
							<td  class="i-label">
								<input id="OrderEndDate" type="text"  class="hisui-datetimebox maxtext" name="OrderEndDate" style="width:150px"/>
							</td> 
							<!--csp:if condition='PAAdmType="I"'-->
								<td  class="r-label">
									<label for="OrderFirstDayTimes">#(..Get("首日次数"))#</label>
								</td>
								<td class="i-label">
									<input id="OrderFirstDayTimes" type="text" class="hisui-numberbox" style="width:130px" name="OrderFirstDayTimes"/>
								</td>
							<!--/csp:if-->
							<td class="r-label">
								<label for="OrderPrice">#(..Get("费用"))#</label>
							</td> 
							<td class="i-label">
								<input id="OrderPrice" type="text"  class="hisui-validatebox textbox maxtext" name="OrderPrice" disabled/>
							</td> 
						</tr>
						<tr> 
							<td  class="r-label">
								<label for="CureItemPlan">#(..Get("方案模板"))#</label>
							</td>
							<td class="i-label">
								<input id="CureItemPlan" type="text" class="hisui-combobox maxtext" name="CureItemPlan" style="width:150px"/>
							</td>
							<csp:if condition='DCARowId=""'>
								<td class="r-label" colspan="2" style="text-align:left;">
									<label for="FindLocByLog">#(..Get("按登录取接收科室"))#</label>
									<input id="FindLocByLog" class='hisui-checkbox' type="checkbox" name="FindLocByLog"/>
									<label for="CureItemCoverMainIns">#(..Get("医保"))#</label>
									<input id="CureItemCoverMainIns" class='hisui-checkbox' type="checkbox" style="" name="CureItemCoverMainIns"/>
								</td>
							<csp:else>
								<td class="r-label"><label for="ApplyRemark">#(..Get("提示信息"))#备注</label></td>
								<td style="text-align:left">
									<input id="ApplyRemark"  name="ApplyRemark" class="textbox" style="width:124px"/>
								</td>
							</csp:if>
							<csp:if condition='ParaType="OEOrdEntry"'>
								<!--医嘱录入[治疗申请]按钮弹出交到医嘱录入界面提示处理-->
							<csp:else>
								<td class="r-label">
									<label id="label_ChronicDiag" for="ChronicDiag">#(..Get("慢病病种"))#</label>
								</td>
								<td>
									<input id="ChronicDiag" placeholder="慢病病种" class="textbox">
								</td>
							</csp:if>
						</tr>
						<tr class="ifhidden">
							<td class="ifhidden"><label style="padding: 0px 10px 0px 0px;">#(..Get("申请时间"))#</label></td>
							<td class="ifhidden" style="text-align:left"><input id="ApplyDate"  name="ApplyDate" style="width:150px" disabled/></td>
							<td class="ifhidden"><label style="padding: 0px 10px 0px 0px;">#(..Get("申请状态"))#</label></td>
							<td class="ifhidden" style="text-align:left"><input id="ApplyStatus" class="textbox" name="ApplyStatus" style="width:124px" disabled/></td> 
							<td class="ifhidden"><label style="padding: 0px 10px 0px 0px;">#(..Get("申请医生"))#</label></td>
							<td class="ifhidden" style="text-align:left"><input id="ApplyUser"  name="ApplyUser" class="textbox" style="width:130px" disabled/></td>
						</tr>
						<tr>
							<td class="r-label"><label for="ApplyPlan">#(..Get("治疗方案"))#</label></td>
							<td colspan="6"><textarea rows="3" id="ApplyPlan" name="ApplyPlan"></textarea></td>
						</tr>
						<csp:if condition='DCARowId=""'>
							<tr>
								<td class="r-label"><label for="ApplyRemark">#(..Get("备注"))#</label></td>
								<td colspan="6"><textarea rows="1" id="ApplyRemark" name="ApplyRemark"></textarea></td>
							</tr> 
						</csp:if>
					</table>
				</div>
			</div>
		</div>
		<div data-options="region:'south',border:false,collapsible:false,headerCls:'panel-header-gray'" style="height:300px;padding:0px 10px;">
			<div class="hisui-layout" fit="true" border="false">
				<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray'">
					<div id="apply_append_panel" class="hisui-panel" style="border-radius:0px;border-color:#cccccc" 
					data-options="headerCls:'panel-header-card-gray',fit:true,closable:false,collapsible:false,minimizable:false,maximizable:false"> 
						<table id="CureAppendItemList"></table>
					</div>
				</div>
				<div data-options="region:'south',border:false,collapsible:false,headerCls:'panel-header-gray'" style="height:50px;padding:0px 10px;">
					<div class="hisui-panel"  title="" data-options="fit:true,border:false,closable:false,collapsible:false,minimizable:false,maximizable:false"> 
						<csp:if condition='DCARowId=""'>
							<div class="ach-div" style="padding:10px 0px;margin: 0px auto;height: 25px;width:100%;text-align:center;">   	
								<a id="btnAddItem" href="javascript:void(0)" class="hisui-linkbutton" data-options="iconCls:'icon-w-add'">增加申请</a> 
								<a id="btnUpdateItem" href="javascript:void(0)" class="hisui-linkbutton" data-options="iconCls:'icon-w-update'">修改申请</a>
								<a id="btnClose" href="javascript:void(0)" class="hisui-linkbutton" data-options="iconCls:'icon-w-close'">关闭页面</a>  
							</div>
						<csp:else>
							<div class="ach-div" style="padding:10px 0px;margin: 0px auto;height: 25px;width: 500px;">   
								<a id="btnSave" href="javascript:void(0)" class="hisui-linkbutton" data-options="iconCls:''">保存</a> 
								<a id="btnPrint" href="javascript:void(0)" class="hisui-linkbutton" data-options="iconCls:''">打印</a> 
								<a id="btnCancel" href="javascript:void(0)" class="hisui-linkbutton" data-options="iconCls:''">撤销申请</a> 
							</div>
						</csp:if>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
