  <!-- 药学分类-->
  <script id="scrDHCPHCCat" type="text/html">
    <div id="lyDHCPHCCat" class="hisui-layout" fit="true">
        <div data-options="region:'north',border:false" style="overflow: hidden;">
            <div class="datagrid-toolbar" style="border-bottom:0">
                <a class="hisui-linkbutton" plain='true' iconCls="icon-clear-screen" id="btnAddDHCPHCCat">#(..Get("清屏"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-save" id="btnSaveDHCPHCCat">#(..Get("保存"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-save" id="btnSaveDHCPHCCatS">#(..Get("保存为同级"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-save" id="btnSaveDHCPHCCatI">#(..Get("保存为下级"))#</a>
                <a class="hisui-linkbutton" plain='true' iconCls="icon-cancel" id="btnDelDHCPHCCat">#(..Get("删除"))#</a>
            </div>

            <div class="pha-line"></div>
        </div>
        <div data-options="region:'center',border:false" id="dataDHCPHCCat">
            <div class="pha-row">
                <div class="pha-col">
                    <div class="pha-drug-phccat-label">
                        <label for="phcCatDescAll"><span style="color:red;visibility:hidden;">*</span>#(..Get("全称"))#</label>
                    </div>
                </div>
                <div class="pha-col">
                    <div class="pha-drug-phccat-text">
                        <input id="phcCatDescAll" class="hisui-validatebox" readonly=true
                            data-pha='class:"hisui-validatebox",requied:true'>
                    </div>
                </div>
            </div>
            <div class="pha-line"></div>
            <div class="pha-row">
                <div class="pha-col">
                    <div class="pha-drug-phccat-label">
                        <label for="phcCatCode"><span style="color:red">*</span>#(..Get("代码"))#</label>
                    </div>
                </div>
                <div class="pha-col">
                    <div class="pha-drug-phccat-text">
                        <input id="phcCatCode" class="hisui-validatebox"
                            data-pha='class:"hisui-validatebox",requied:true,clear:true,save:0'>
                    </div>
                </div>
            </div>
            <div class="pha-row">
                <div class="pha-col">
                    <div class="pha-drug-phccat-label">
                        <label for="phcCatDesc"><span style="color:red">*</span>#(..Get("名称"))#</label>
                    </div>
                </div>
                <div class="pha-col">
                    <div class="pha-drug-phccat-text">
                        <input id="phcCatDesc" class="hisui-validatebox"
                            data-pha='class:"hisui-validatebox",requied:true,clear:true,save:1'>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>