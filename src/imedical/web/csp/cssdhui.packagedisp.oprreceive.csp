<!--手术包接收-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:4px">
		<div class="hisui-panel" data-options="title:'手术包接收',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="UomTB">
				<table class="Condition" id="MainCondition">
					<tr>
						<td>
							发放科室
							<input id="fromLocDr" name="fromLocDr" class="textbox hisui-combobox">
						</td>
						<td>
							接收科室
							<input id="toLocDr" name="toLocDr" class="textbox hisui-combobox">
						</td>
						<td>
							单据号
							<input id="FNo" name="FNo" class="textbox" >
						</td>
						<td><td>
					</tr>
					<tr>
						<td>
							开始日期
							<input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq">
						</td>
						<td>
							结束日期
							<input id="FEndDate" name="FEndDate" class="textbox hisui-dateboxq" >
						</td>
						<td>
							<label>接收人</label><input class="textbox" id="toUser" name="toUser">
							<input class="textbox" hidden id="toUserDr" name="toUserDr">
						</td>
						<td>
							<input id="isOPRFlag" name="isOPRFlag" value="Y" hidden >
						</td>
						<td>
							<input id="isRecFlag" name="isRecFlag" value="Y" hidden >
						</td>
					</tr>
					
				</table>
				<table>
					<tr>
						<td style="padding-right:30px;padding-left:25px">
							<input class='hisui-radio' type="radio" label='仅未接收' data-options="checked:true" name='FStatu' value='N'>
							<input class='hisui-radio' type="radio" label='包含已接收' name='FStatu' value=''>
        				</td>
        				<td>
        					<a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" >查询</a>
        				</td>
        				<td>
        					<a href="#" id="ReceAll" class="hisui-linkbutton" data-options="iconCls:'icon-upload',plain:true">批量接收</a>
        				</td>
					</tr>
				</table>
			</div>
			<table id="MainList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:500px;padding-left:0px">
		<div class="hisui-panel" data-options="title:'接收详情',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<!--div id="InputTB"> 
				<table  class="Condition ">
					<tr>
						<td><label>条码</label><input class="hisui-numberbox" id="BarCode" name="BarCode"></td>
					</tr>
				</table>
			</div-->
			<table id="ItemList"></table>
		</div>
	</div>	

<script type="text/javascript" src="../scripts/cssd/Disp/DispCommon.js"></script>
<script type="text/javascript" src="../scripts/cssd/Disp/FindWinByOprRece.js"></script>	
<script type="text/javascript" src="../scripts/cssd/Disp/OprReceInfo.js"></script>

</body>
</html>