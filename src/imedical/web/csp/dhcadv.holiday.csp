<html>
<head>
<DHCADV:EASYUI/>
    <script type="text/javascript" src="../scripts/dhcadvEvt/holiday.js"></script>
</head>

<body>
<div id="mainpanel" class="easyui-layout"  fit="true">
		<div data-options="region:'center'">
			<table id="holiday" toolbar="#tb"></table>
		</div>	
</div>
	<div id="tb" style="padding:5px;height:auto">
		<div style="margin-bottom:5px">
			<tr>
			<td class="textLabel">开始日期<input id="stdate" type="text" class="easyui-datebox" required="required" style="width:120"></input> </td>	
			<td class="textLabel">结束日期<input id="enddate" type="text" class="easyui-datebox" required="required" style="width:120"> </td>        
			<td ><a class="easyui-linkbutton" style="width:90px" data-options="iconCls:'icon-search'" id="find">查询</a> </td>					
			</tr>								
		    <div class="tool-bar-line">	</div>
		</div>
	
		<table cellspacing="0" cellpadding="0" >
			<tr>
		      <td><a href="#" class="easyui-linkbutton" plain="true" iconCls="icon-add" id="insert">添加</a></td>
		      <td><div class="datagrid-btn-separator"> </div> </td>
		      <td><a href="#" class="easyui-linkbutton" plain="true" iconCls="icon-remove" id="delete">删除</a></td>
		      <td><div class="datagrid-btn-separator"> </div> </td>
		      <td><a href="#" class="easyui-linkbutton" plain="true" iconCls="icon-save" id="save">保存</a></td>
		      <td><div class="datagrid-btn-separator"> </div> </td>	
		    </tr>
		</table>	
	</div>
   
     
</body>
</html>
