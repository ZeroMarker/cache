			}
			//file.Move("D:/ExportPDF/IReport/"+RegNo+" "+PatName+".pdf");
			file.Copy("D:/ExportPDF/IReport/"+RegNo+" "+PatName+".pdf");
			window.close();

		}else{
			i=i+1;
			ReNamePDF();
		}
}
