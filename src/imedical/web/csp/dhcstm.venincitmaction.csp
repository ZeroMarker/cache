<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))
	s StrParam=$Get(%request.Data("StrParam",1))

i Action = "GetAllVendor" d
	.d ##class(web.DHCSTM.VenIncitm).GetAllVendor(Start,Limit,Sort,Dir,StrParam)

//获取供应商已授权的物资
i Action = "GetVenInci" d
	.d ##class(web.DHCSTM.VenIncitm).GetVenInci(Start,Limit,StrParam)

//获取某物资已授权的供应商
i Action = "GetInciApc" d
	.d ##class(web.DHCSTM.VenIncitm).GetInciApc(Start,Limit,Sort,Dir,StrParam)

//获取某物资未授权的供应商
i Action = "GetUnInciApc" d
	.d ##class(web.DHCSTM.VenIncitm).GetUnInciApc(Start,Limit,Sort,Dir,StrParam)
//根据供应商授权库存项
i Action = "Save" d
	.s InciStr=$Get(%request.Data("InciStr",1))
	.s Venid=$Get(%request.Data("Venid",1))
	.s ret=##class(web.DHCSTM.VenIncitm).Save(Venid,InciStr)
	.i ret="" d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"
//根据库存项授权供应商
i Action = "AddInciVendor" d
	.s VendorIdStr=$Get(%request.Data("VendorIdStr",1))
	.s Incid=$Get(%request.Data("Incid",1))
	.s result=##class(web.DHCSTM.VenIncitm).AddInciVendor(Incid,VendorIdStr)
	.i result="" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
//取消授权
i Action = "StopInciVendor" d
	.s RowIdStr=$Get(%request.Data("RowIdStr",1))
	.s result = ##class(web.DHCSTM.VenIncitm).StopInciVendor(RowIdStr)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

</script>