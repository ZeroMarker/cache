<div id="mainPanel" class="hisui-layout" data-options="fit:true,border:false">
	<csp:if condition='PageShowFromWay="ApplyEntry"'>
		<div data-options="region:'center',title:'',border:false,split:false,headerCls:'panel-header-gray'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div id="BPanel" data-options="region:'center',title:'患者近3个月内的治疗记录',headerCls:'panel-header-gray'" style="height:350px"></div>
			</div>
		</div>
	<csp:elseif condition='PageShowFromWay="AssEntry"'>
		<div id="BPanel" data-options="region:'center',title:'治疗评估信息',headerCls:'panel-header-gray'"></div>
	</csp:if>
</div>

<div id="add-dialog" class="hisui-dialog"  title="治疗明细" data-options="iconCls:'icon-save',shadow:false,modal:true,closed:true,resizable:true">
	<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
		<table id="tabCureDetail" toolbar="#diag-table-toolbar"></table>
	</div>
</div>