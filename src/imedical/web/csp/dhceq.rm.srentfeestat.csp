<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI/>
<DHCEQ:HISUI></DHCEQ:HISUI>
</HEAD>
<SCRIPT language="Cache" RUNAT="SERVER">
    s CurUser=##Class(web.DHCEQCommon).getMapIDBySource("user",%session.Get("LOGON.USERID"))
    s Job=$J
</Script>
<body class="hisui-layout">
	<div id='Loading' class="eq-loading"><h1><img src="../skin/default/images/loading.gif"><font>加载中・・・</font></h1></div>
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="eqtitle:'maintitle',fit:true,title:'调配费用统计'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #ccc;padding:5px 0 10px 0;">
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cLoc" for="Loc">科室</label></div>
					<div class="eq-table-td"><input class="hisui-validatebox" id="Loc" data-options="'component':'PLAT.L.Loc'"></div>
					<div class="eq-table-td eq-table-label"><label id="cStartDate" for="StartDate">开始日期</label></div>
					<div class="eq-table-td ">
						<input class="hisui-datebox textbox" id="StartDate">
					</div>
					<div class="eq-table-td eq-table-label"><label id="cEndDate" for="EndDate">结束日期</label></div>
					<div class="eq-table-td ">
						<input class="hisui-datebox textbox" id="EndDate">
					</div>
					<div class="eq-table-td eq-table-label"><a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" data-options="stopAllEventOnDisabled:true">查询</a></div>
				</div>
			</div>
			<div data-options="region:'center',border:false" style="position:relative">
				<table id="tDHCEQSRentFeeStat" class="hisui-datagrid"></table>
				<span id="sumTotal" class="eq-total-sum" style="position:absolute;right:10px;top:5px;">合计</span>	
			</div>
    	</div>
	</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
		<INPUT type="hidden" id="CurUser" value=#(CurUser)#>
		<INPUT type="hidden" id="Job" value=#(Job)# >
		<INPUT type="hidden" id="LocDR" >
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/rm/srentfeestat.js'></script>
</body>
</HTML>

