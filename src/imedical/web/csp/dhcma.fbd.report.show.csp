<!-- 主页面-->
<div id="FBDReport" class="hisui-layout" data-options="fit:true" style="width:100%;height:100%;">
	<div data-options="region:'center',fit:true,border:false,collapsible:false">
		<div class="page-body" id="divbody" style="width:1285px;">
			<div class="report-header">
				<span>食源性疾病报告</span>
			</div>
			<div class="report-content">
				<div class="hisui-panel" id="BaseInfo" data-options="title:'患者基本信息',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="PatInfoTab"  class="report-tab">
						<tr class="report-tr">
							<td class="report-td">报告科室</td>				
							<td><input class="textbox" id="txtRepLoc" style="width:218px;" disabled /></td>
							<td class="report-td">报告人</td>				
							<td><input class="textbox" id="txtRepUser" style="width:218px;" disabled /></td>
							<td class="report-td">报告位置</td>					
							<td><input class="textbox" id="txtRepPlace" style="width:218px;" disabled /></td>
							<td class="report-td">报告状态</td>					
							<td><input class="textbox" id="txtRepStatus" style="width:200px;" disabled /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>姓名/性别</td>				
							<td>
								<input class="textbox" id="txtPatName" style="width:140px;"/>
								<input class="textbox" id="txtSex" style="width:68px;"/>
							</td>
							<td class="report-td"><font color='red'>*</font>年龄/生日</td>					
							<td>
								<input class="textbox" id="txtAge" style="width:68px;"/>
								<input class="hisui-datebox textbox" id="dtBirthday" style="width:147px;" />
							</td>
							<td class="report-td">患者级别</td>					
							<td><input class="textbox" id="txtPatLevel" style="width:218px;" disabled /></td>		
							<td class="report-td">患者密级</td>					
							<td><input class="textbox" id="txtEncryptLevel" style="width:200px;" disabled /></td>
												    </tr>
						<tr class="report-tr">
							<td class="report-td">门诊号</td>				
							<td><input class="textbox" id="txtOPNo" style="width:218px;"/></td>
							<td class="report-td"><font color='red'>*</font>是否住院</td>				
							<td>
								<input class='hisui-radio' type="radio" label='是' name='IsInHosp' id='IsInHosp-1' value='1'>
								<input class='hisui-radio' type="radio" label='否' name='IsInHosp' id='IsInHosp-0' value='0'>
							</td>
							<td class="report-td">住院号</td>					
							<td><input class="textbox" id="txtIPNo" style="width:218px;" /></td>
							<td class="report-td">病例编号</td>					
							<td><input class="textbox" id="txtCardNo" style="width:200px;" /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td">身份证号</td>					
							<td><input class="textbox" id="txtPersonalID" style="width:218px;" /></td>	
							<td class="report-td"><font color='red'>*</font>患者职业</td>					
							<td><input class="textbox" id="cboOccupation" style="width:225px;" /></td>
							<td class="report-td">单位</td>					
							<td colspan="3"><input class="textbox" id="txtCompany" style="width:525px;"/></td>	
						</tr>						
						<tr class="report-tr">
							<td class="report-td">监护人姓名</td>					
							<td><input class="textbox" id="txtContactor" style="width:218px;" /></td>	
							<td class="report-td"><font color='red'>*</font>联系方式</td>					
							<td><input class="textbox" id="txtTelephone" style="width:218px;" /></td>								
							<td class="report-td"><font color='red'>*</font>病人属于</td>					
							<td><input class="textbox" id="cboPatArea" style="width:225px;"/></td>	
						</tr>						
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>省</td>					
							<td><input class="textbox" id="cboCurrProvince" style="width:225px;"/></td>	
							<td class="report-td"><font color='red'>*</font>市</td>				
							<td><input class="textbox" id="cboCurrCity" style="width:225px;"/></td>
							<td class="report-td"><font color='red'>*</font>县</td>					
							<td><input class="textbox" id="cboCurrCounty" style="width:225px;" /></td>
							<td class="report-td"><font color='red'>*</font>乡</td>					
							<td><input class="textbox" id="cboCurrVillage" style="width:207px;" /></td>	
						</tr>				
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>街道</td>		
							<td><input class="textbox" id="txtCurrRoad" style="width:218px;"/></td>	
							<td class="report-td">现住地址</td>				
							<td colspan="7"><input class="textbox" id="txtCurrAddress" style="width:848px;"/></td>					
						</tr>						
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>疾病分类</td>					
							<td><input class="textbox" id="cboDisCate" style="width:225px;"/></td>	
							<td class="report-td"><font color='red'>*</font>疾病名称</td>				
							<td><input class="textbox" id="cboDisDesc" style="width:225px;"/></td>
							<td class="report-td">疾病备注</td>					
							<td colspan="3"><input class="textbox" id="txtDisText" style="width:525px;" /></td>
						</tr>						
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>发病时间</td>					
							<td>
								<input class="hisui-datebox" id="dtSickDate" style="width:120px;"/>
								<input class="hisui-timespinner" id="tmSickTime" data-options="showSeconds:true," style="width:100px;"/>
							</td>
							<td class="report-td"><font color='red'>*</font>就诊时间</td>				
							<td>
								<input class="hisui-datebox" id="dtAdmitDate" style="width:120px;"/>
								<input class="hisui-timespinner" id="tmAdmitTime" data-options="showSeconds:true," style="width:100px;"/>
							</td>
							<td class="report-td">死亡时间</td>					
							<td>
								<input class="hisui-datebox" id="dtDeathDate" style="width:120px;"/>
								<input class="hisui-timespinner" id="tmDeathTime" data-options="showSeconds:true," style="width:100px;"/>
							</td>
						</tr>										
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>初步诊断</td>				
							<td colspan="3">
								<div id="chkPreDiagnosDrs">
								</div>
								<input class="textbox" id="txtPreDiagnos" style="width:540px;"  placeholder="其他初步诊断" disabled />
							</td>
							<td class="report-td"><font color='red'>*</font>既往病史</td>				
							<td colspan="3">
								<div id="chkAnamnesisDrs">
								</div>	
								<input class="textbox" id="txtAnamnesis" style="width:520px;"   placeholder="其他既往病史"  disabled />
							</td>					
						</tr>
						<tr class="report-tr">
							<td class="report-td" colspan="2" style="text-align: center;">
								<font color='red'>*</font>就诊前是否使用抗生素
								<input class='hisui-radio' type="radio" label='是' name='IsUseAnti' id='IsUseAnti-1' value='1' />
								<input class='hisui-radio' type="radio" label='否' name='IsUseAnti' id='IsUseAnti-0' value='0' />
							</td>
							<td class="report-td">抗生素名称</td>				
							<td colspan="7"><input class="textbox" id="txtUseAntiDesc" style="width:845px;" disabled /></td>
						</tr>
					</table>
				</div>			
				<div class="hisui-panel" data-options="title:'主要症状与体征（在相应症状或体征的□中打√，至少填写一项）',headerCls:'panel-header-gray',iconCls:'icon-paper'">	
					<table id="SignInfoTab"  class="report-tab">
					</table>
				</div>
				
				<table id="gridFoodInfo" data-options="toolbar:'#FoodInfoBar'"  style="height:500px;"></table>
				<div id="FoodInfoBar" style="height:242px;" >
					<table class="report-tab">
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>食品名称</td>					
							<td><input class="textbox" id="txtFoodName" style="width:307px;" /></td>	
							<td class="report-td"><font color='red'>*</font>食品分类</td>				
							<td><input class="textbox" id="cboFoodType" style="width:314px;" /></td>
							<td class="report-td"><font color='red'>*</font>加工或包装方式</td>				
							<td><input class="textbox" id="cboPacking" style="width:220px;" /></td>
						</tr>	
						<tr class="report-tr">
							<td class="report-td">食品品牌</td>				
							<td><input class="textbox" id="txtFoodBrand" style="width:307px;" /></td>
							<td class="report-td">生产厂家</td>					
							<td colspan="3"><input class="textbox" id="txtManufacturer" style="width:710px;" /></td>
						</tr>	
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>进食场所类型</td>				
							<td>
								<input class="textbox" id="cboEatingSiteCate" />
								<input class="textbox" id="cboEatingSite" />
							</td>
							<td class="report-td"><font color='red'>*</font>购买场所类型</td>				
							<td>
								<input class="textbox" id="cboBuySiteCate" />
								<input class="textbox" id="cboBuySite" />
							</td>			
						</tr>	
						<tr class="report-tr">	
							<td class="report-td"><font color='red'>*</font>进食地点</td>					
							<td colspan="5">
								<div id="radEatTypeList" style="float:left;width:125px;margin-top:3px;">
								</div>
								<span style="padding-left:10px;padding-right:10px;">省</span><input class="hisui-combobox textbox" id="cboEatProvince"  disabled />
								<span style="padding-left:10px;padding-right:10px;">市</span><input class="hisui-combobox textbox" id="cboEatCity" style="width:172px;" disabled />
								<span style="padding-left:10px;padding-right:10px;">县</span><input class="hisui-combobox textbox" id="cboEatCounty" style="width:175px;" disabled />
								<span style="padding-left:10px;padding-right:10px;">详细地址</span><input class="textbox" id="txtEatingPlaces"  style="width:315px;"/>
							</td>
						</tr>	
						<tr class="report-tr">	
							<td class="report-td"><font color='red'>*</font>购买地点</td>					
							<td colspan="5">
								<div id="radBuyTypeList" style="float:left;width:125px;margin-top: 3px;">
								</div>
								<span style="padding-left:10px;padding-right:10px;">省</span><input class="hisui-combobox textbox" id="cboBuyProvince"  disabled />
								<span style="padding-left:10px;padding-right:10px;">市</span><input class="hisui-combobox textbox" id="cboBuyCity" style="width:172px;" disabled />
								<span style="padding-left:10px;padding-right:10px;">县</span><input class="hisui-combobox textbox" id="cboBuyCounty" style="width:175px;" disabled />
								<span style="padding-left:10px;padding-right:10px;">详细地址</span><input class="textbox" id="txtWhereToBuy"  style="width:315px;"/>
							</td>	
						</tr>	
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>进食时间</td>					
							<td>
								<input class="hisui-datebox textbox" id="dtEatingDate"/>
								<input class="hisui-timespinner textbox" id="tmEatingTime" data-options="showSeconds:true," style="width:155px;" />
							</td>
							<td class="report-td"><font color='red'>*</font>进食人数</td>					
							<td colspan="2">
								<input class="hisui-numberbox textbox" id="NumEatingNum" style="width:60px;" />
								<span style="margin-left:30px;"><font color='red'>*</font>其他人是否发病</span>						
								<input class='hisui-radio' type="radio" label='是' name='IsIncidence' id='IsIncidence-1' value='1'>
								<input class='hisui-radio' type="radio" label='否' name='IsIncidence' id='IsIncidence-0'value='0'>
								<span style="margin-left:30px;"><font color='red'>*</font>是否采样</span>					
								<input class='hisui-radio' type="radio" label='是' name='IsSampling' id='IsSampling-1' value='1'>
								<input class='hisui-radio' type="radio" label='否' name='IsSampling' id='IsSampling-0' value='0'>
							</td>
							<td style="text-align:center;">
								<a id="btnSaveFood" class="hisui-linkbutton">保存</a>
								<a id="btnDeleteFood" class="hisui-linkbutton" style="margin-left:30px;">删除</a>
							</td>
						</tr>
					</table>
				</div>
				
				<table id="gridSampleInfo" data-options="toolbar:'#SampleInfoBar'"  style="height:260px;"></table>
				<div id="SampleInfoBar" style="height:90px;" >
					<table class="report-tab">
							<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>样本编号</td>					
							<td><input class="textbox" id="txtSampleNo" style="width:240px;"/></td>	
							<td class="report-td"><font color='red'>*</font>样本类型</td>				
							<td><input class="textbox" id="cboSampleType" style="width:247px;"/></td>
							<td class="report-td"><font color='red'>*</font>采样日期</td>					
							<td><input class="hisui-datebox textbox" id="dtSampleDate" style="width:247px;"/></td>
							<td>
								<a id="btnSaveSample" class="hisui-linkbutton">保存</a>
							</td>
						</tr>	
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>样本数量</td>					
							<td><input class="hisui-numberbox textbox" id="NumSampleNumber" style="width:247px;" /></td>	
							<td class="report-td"><font color='red'>*</font>单位</td>				
							<td><input class="hisui-combobox textbox" id="cboSampleUnit" style="width:247px;" /></td>
							<td class="report-td">备注</td>					
							<td><input class="textbox" id="txtSampleResume" style="width:240px;" /></td>
							<td>
								<a id="btnDeleteSample" class="hisui-linkbutton">删除</a>
							</td>
						</tr>	
					</table>
				</div>
			</div>
        </div>			
		<div class="page-footer">
			<a id="btnSaveTmp" class="hisui-linkbutton" style="display:none">草稿</a>	
			<a id="btnSaveRep" class="hisui-linkbutton" style="display:none">报卡</a>
			<a id="btnExecheck" class="hisui-linkbutton" style="display:none">审核</a>
			<a id="btnCancheck" class="hisui-linkbutton" style="display:none">取消审核</a>		
			<a id="btnReturn" class="hisui-linkbutton" style="display:none">退回</a>	
			<a id="btnDelete" class="hisui-linkbutton" style="display:none">作废</a>
			<a id="btnPrint" class="hisui-linkbutton" style="display:none">打印</a>
			<a id="btnClose" class="hisui-linkbutton" style="display:none">关闭</a>	
			<a id="btnReported" class="hisui-linkbutton" style="display:none">外院已报</a>
		</div>
	</div>
</div>
