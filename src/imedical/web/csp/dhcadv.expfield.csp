			<table cellspacing="0" cellpadding="0" style="height:auto;">
				<tr>
					<td class="textLabel"><span style="margin-left:10px;"></span>报告类型<span style="margin-left:10px;"></span><input type="text" id="reportType" class="hisui-combobox" name="reportType" ></input> </td>
				<tr>
			</table>
	 	</div>
	 	
	 	<div data-options="region:'center',title:''" style="border:0px;padding:10px 0px 0px 0px;">
	 		
	 		<div class="floatLeft" id="allItmTableDiv">
	 			<table id="allItmTable" toolbar="#alltb" iconCls="icon-paper" headerCls="panel-header-gray"></table>
	 		</div>
	 		<div class="floatLeft" id="cuidDiv">
	 			<table id="cuidTable" headerCls="panel-header-gray">
	 				<tr></tr>
	 				<tr></tr>
