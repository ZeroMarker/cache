<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML>
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />

<TITLE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>

</HEAD>
<SCRIPT language="Cache" RUNAT="SERVER">
	Set RowID=$Get(%request.Data("RowID",1))
</Script>
<body class="hisui-layout" style="height:100%;">
		<div data-options="region:'center',border:false" style="padding:10px">
			<div class="hisui-panel" title="设备属性" data-options="eqtitle:'maintitle',fit:true">
			<!--modify by lmm 2020-04-09-->
				<div class="eq-table" style="border-bottom:solid 1px #ccc;padding-bottom:5px">
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label"><lable id="cEACode" for="EACode">代码</lable></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="EACode"></div>
						<div class="eq-table-td eq-table-label"><lable id="cEAName" for="EAName">名称</lable></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="EAName"></div>
						<div class="eq-table-td "><a id="BAdd" href="#" class="hisui-linkbutton" iconCls="icon-w-add" data-options="stopAllEventOnDisabled:true">新增</a></div>	<!--MZY0025	1318607	2020-05-13-->
						<div class="eq-table-td "><a id="BFind" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">查询</a></div>		<!--MZY0025	1318607	2020-05-13-->
					</div>
				
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label"><lable id="cEAName" for="EAName">分组</lable></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="EAGroup"></div>
						<div class="eq-table-td eq-table-label"><lable id="cEARemark" for="EARemark">备注</lable></div>
						<div class="eq-table-td eq-table-input"><textarea style="width:150px" colspan="2" id="EARemark"></textarea></div>
						<div class="eq-table-td "><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">保存</a></div>		<!--MZY0025	1318607	2020-05-13-->
						<div class="eq-table-td "><a id="BDelete" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">删除</a></div>	<!--MZY0025	1318607	2020-05-13-->
					</div>

				</div>
				<!--modify by lmm 2020-04-09-->
				<div data-options="border:false,fit:true">
						<table id="maindatagrid" data-options="border:false">
        				</table>
        			</div>
			</div>	
		</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">

			<INPUT type="hidden" id="EARowID" value=#(RowID)#>
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/plat/ctequipattribute.js'></script>
</body>
</HTML>
