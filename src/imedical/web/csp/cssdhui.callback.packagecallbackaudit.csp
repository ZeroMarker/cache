<!--回收确认界面-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<SERVER>
	d ##class(web.DHCXMLPConfig).LODOPInit()  //Lodop打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:0px;padding-bottom:15px">
		<div class="hisui-panel" data-options="title:'请领单据',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="CondTB">
				<table class="Condition" id="MainCondition">
					<tr>
						<td>申请科室</td>
						<td><input id="FReqLoc" name="FReqLoc" class="textbox hisui-combobox" style="width:140px;"></td>
						<td>供应科室</td>
						<td><input id="FSupLoc" name="FSupLoc" class="textbox hisui-combobox" style="width:140px;"></td>
						<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:90px;">查询</a></td>
						<td><a href="#" id="Print" class="textbox hisui-linkbutton" data-options="iconCls:'icon-w-print'" style="width:90px">打印</a></td>
					</tr>
					<tr>
						<td>单据号</td>
						<td><input id="FReqNo" name="FReqNo" class="hisui-numberbox textbox"  style="width:140px"></td>
						<td>状态查询</td>
						<td>
							<select id="StatuQuery" name="Statu" class="hisui-combobox" style="width:140px">
								<option value="1" >提交状态</option>
								<option value="5" >确认状态</option>
								<option value="8" >拒绝状态</option>
							</select>
						</td>
						<td><a href="#" id="AuditBT" class="textbox hisui-linkbutton" data-options="iconCls:'icon-w-ok'" style="width:90px">确认</a></td>
						<td><a href="#" id="RefuseBT" class="textbox hisui-linkbutton" data-options="iconCls:'icon-w-close'" style="width:90px">拒绝</a></td>
					</tr>
					<tr>
						<td>开始日期</td>
						<td><input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq"  style="width:140px;"></td>
						<td>结束日期</td>
						<td><input id="FEndDate" name="FEndDate" class="textbox hisui-dateboxq"  style="width:140px;"></td>
						
					</tr>		
				</table>
			</div>
				<table id="MainList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:550px;padding-left:10px;padding-bottom:15px">
		
		<div class="hisui-panel" data-options="title:'单据明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<table id="ItemList"></table>
		</div>
	</div>
	
<script type="text/javascript" src="../scripts/cssd/CallBack/CallBackCommon.js"></script>
<script type="text/javascript" src="../scripts/cssd/CallBack/CallBackAuditPrint.js"></script>
<script type="text/javascript" src="../scripts/cssd/Common/commonPrint.js"></script>		
<script type="text/javascript" src="../scripts/cssd/CallBack/PackageCallBackAudit.js"></script>
</body>
</html>]]></CSP>
</Export>
