<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>

<body class="hisui-layout">
		<div data-options="region:'west',border:false" style="width:860px;padding:10px">
		<div class="hisui-panel"  title="资源项目" data-options="fit:true,eqtitle:'maintitle'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #ccc;height:135px;">
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cSIShareType" for="SIShareType">资源类型</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-combobox textbox" id="SIShareType" ></div>
					<div class="eq-table-td eq-table-label"><label id="cSICode" for="SICode">代码</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="SICode" ></div>
					
					<div class="eq-table-td eq-table-label"><input class="hisui-checkbox" type="checkbox" style="width: 50px;" label="需要清洗" id="SIWashFlag"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"><a id="BAdd" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-add',stopAllEventOnDisabled:true">新增</a></div>
					<div class="eq-table-td eq-table-label"><a id="BSave" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save',stopAllEventOnDisabled:true">保存</a></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cSIShareItemCat" for="SIShareItemCat">资源项目分类</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'RM.L.ShareItemCat'" id="SIShareItemCat" ></div><!--Modify by zc0076 2020-06-09 将引用组件RM.L.GetSCShareItemCat改为RM.L.ShareItemCat-->
					<div class="eq-table-td eq-table-label"><label id="cSIDesc" for="SIDesc">描述</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="SIDesc" ></div>
					
					<div class="eq-table-td eq-table-label"><input class="hisui-checkbox" type="checkbox" style="width: 50px;" label="需要检测" id="SIInspectFlag"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"><a id="BDelete" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-close',stopAllEventOnDisabled:true">删除</a></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cSIHospital" for="SIHospital">院区</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Hospital'" id="SIHospital" ></div>
					<div class="eq-table-td eq-table-label"><label for="SIRemark">备注</label></div>
					<div class="eq-table-td"><textarea id="SIRemark" style="width:151px"></textarea></div>
				</div>
				</div>
				<div data-options="region:'center',border:false">
					<table id="tDHCEQSCShareItem" class="hisui-datagrid" data-options="border:false,fit:true"> </table>
				</div>
    	</div>
	</div>
	</div>
	<div data-options="region:'center',border:false" style="padding:10px">
		<div class="hisui-panel"  title="资源项目明细" data-options="fit:true,eqtitle:'maintitle'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #ccc;height:135px;">
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cSILItem" for="SILItem">设备项</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'EM.L.GetMasterItem'" id="SILItem" ></div>
					<div class="eq-table-td eq-table-label"><label id="cSILName" for="SILName">名称</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="SILName" ></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"><a id="BAddList" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-add',stopAllEventOnDisabled:true">新增</a></div>
					<div class="eq-table-td eq-table-label"><a id="BSaveList" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save',stopAllEventOnDisabled:true">保存</a></div>
					
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cSILModel" for="SILModel">规格型号</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Model'" id="SILModel" ></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"></div>
					<div class="eq-table-td eq-table-label"><a id="BDeleteList" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-close',stopAllEventOnDisabled:true">删除</a></div>
				</div>
				</div>
				<div data-options="region:'center',border:false">
					<table id="tDHCEQSCShareItemList" class="hisui-datagrid" data-options="border:false,fit:true"> </table>
				</div>
    	</div>
	</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="SIRowID">
			<INPUT type="hidden" id="SIShareItemCatDR">
			<INPUT type="hidden" id="SIHospitalDR">
			<INPUT type="hidden" id="SILRowID">
			<INPUT type="hidden" id="SILItemDR">
			<INPUT type="hidden" id="SILModelDR">
			<INPUT type="hidden" id="EQItemDR">
			
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/rm/dhceqscshare.js'></script>
</body>
</HTML>
