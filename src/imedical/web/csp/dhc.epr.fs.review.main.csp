<HTML>
<HEAD>
    <TITLE>病案复核</TITLE> 
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">  
    <script language="cache" runat="server">
        s userID = %session.Get("LOGON.USERID")
        if (userID = "")
        {
            s userID = "159"    
        }
        s ssGroupID = %session.Get("LOGON.GROUPID")
        if (ssGroupID = "")
        {
            s ssGroupID = "54"
        }
        s userLocID = %session.Get("LOGON.CTLOCID")
        if (userLocID = "")
        {
            s userLocID = "9"
        }
        s dataServiceURL = ##class(DHCEPRFS.BL.BLSysOption).GetValueByName("DataServiceURL")
        s hospitalFlag = ##class(DHCEPRFS.BL.BLSysOption).GetValueByName("HospitalFlag")
        s showTimeLine = ##class(DHCEPRFS.BL.BLSysOption).GetValueByName("ShowTimeLine")
        s superSSGroup = ##class(DHCEPRFS.BL.BLSysOption).GetValueByName("SuperSSGroup")
        s isSuper="0"
        f i=1:1:$l(superSSGroup,"^")
        {
            s superGroupID=$p(superSSGroup,"^",i)
            s:(superGroupID = ssGroupID) isSuper="1"
        }
        
        s enableAction = "N"
    </script>
    <script type="text/javascript">
        var userID = '#(userID)#';
        var userLocID = '#(userLocID)#';
        var isSuper = '#(isSuper)#';
        var dataServiceURL = '#(dataServiceURL)#';
        var hospitalFlag = '#(hospitalFlag)#';
        var showTimeLine = '#(showTimeLine)#';
        var enableAction = '#(enableAction)#';
    </script>
    <script src="../scripts/epr/JQuery/jquery.min.js"></script>
    <script src="../scripts/epr/JQuery/jquery.easyui.min.js"></script>
    <script src="../scripts/epr/JQuery/easyui-lang-zh_CN.js"></script>
    <link rel="stylesheet" type="text/css" href="../scripts/epr/JQuery/themes/metro/easyui.css">
    <link rel="stylesheet" type="text/css" href="../scripts/epr/JQuery/themes/icon.css">

    <link rel="stylesheet" type="text/css" href="../scripts/epr/css/fs.barcode.code39.css">
    <script src="../scripts/epr/js/FS.Barcode.Code39.js"></script>
    <script src="../scripts/epr/js/FS.Review.Common.js"></script>
    <script src="../scripts/epr/js/FS.Review.Main.js"></script>
    
    <style type="text/css">
        label {
            font-size:13px;
            height:25px;   
            line-height:25px;   
        }
    </style>
</HEAD>
<body id="reviewMainLayout" class="easyui-layout" data-options="fit:true">
    <div data-options="region:'center'" style="border:0">
        <div id="episodeListTableTBar" class="easyui-panel" style="height:75px;border:0">    
            <table cellpadding="2" cellspacing="0" border="0" style="width:1320px;">
                <tr>
                    <td>
                        <label for="inputType">&nbsp;&nbsp;显示类型:&nbsp;&nbsp;</label>
                        <select id="inputType" name="inputType" style="width:100px;height:25px;"></select>
                    </td>
                    <td>
                        <label for="inputDateStart">&nbsp;&nbsp;起始日期:&nbsp;&nbsp;</label>
                        <input id="inputDateStart" class="easyui-datebox" style="width:100px;height:25px;" />
                    </td>
                    <td>
                        <label for="inputDateEnd">&nbsp;&nbsp;截止日期:&nbsp;&nbsp;</label>
                        <input id="inputDateEnd" class="easyui-datebox" style="width:100px;height:25px;" />
                    </td>
                    <td>
                        <label for="inputDocCommit">&nbsp;&nbsp;医生提交:&nbsp;&nbsp;</label>
                        <input id="inputDocCommit" class="easyui-combobox" style="width:80px;height:25px;" />
                         
                    </td>
                    <td>
                        <label for="inputNurCommit">&nbsp;&nbsp;护士提交:&nbsp;&nbsp;</label>
                        <input id="inputNurCommit" class="easyui-combobox" style="width:80px;height:25px;" />
                        
                    </td>
                    <td>
                        <label for="inputPDFCreated">&nbsp;&nbsp;PDF是否生成:&nbsp;&nbsp;</label>
                        <input id="inputPDFCreated" class="easyui-combobox" style="width:80px;height:25px;" />           
                    </td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <td>
                        <label for="inputMedRecord">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;病案号:&nbsp;&nbsp;</label>
                        <input id="inputMedRecord" class="easyui-textbox" style="width:100px;height:25px;" />
                    </td>
                    <td>
                        <label for="inputRegNo">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;登记号:&nbsp;&nbsp;</label>
                        <input id="inputRegNo" class="easyui-textbox" style="width:100px;height:25px;" />
                    </td>
                    <td id="IPLoctd">
                        <label for="inputPatientLoc">&nbsp;&nbsp;住院科室:&nbsp;&nbsp;</label>
                        <div class="datagrid-toolbar" id="inputPatientLocTbar" name="inputPatientLocTbar" style="width:100%;height:30px;">  
                            <table>  
                                <tr>  
                                    <th>检索:</th>  
                                    <td>  
                                        <input type="inputPatientLocText" id="inputPatientLocText" name="inputPatientLocText"/>  
                                    </td>  
                                    <td>  
                                        <div class="dialog-tool-separator"></div>  
                                    </td>  
                                    <td>  
                                        <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-no',plain:true" id="inputPatientLocClear">清空</a>  
                                    </td>  
                                </tr>  
                            </table>  
                        </div>
                        <select id="inputPatientLoc" name="inputPatientLoc" class="easyui-combogrid" style="width:100px;height:25px;"></select>
                    </td>
                    <td>
                        <a href="#" id="resetBtn" class="easyui-linkbutton" iconCls="icon-reload">重置&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</a>
                    </td>
                    <td>
                        <a href="#" id="searchBtn" class="easyui-linkbutton" iconCls="icon-search">查询&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</a>
                    </td>
                    <td>
                        <a href="#" id="passBtn" class="easyui-linkbutton" iconCls="icon-ok">审核通过</a>
                    </td>
                    <td>
                        <a href="#" id="back2DocBtn" class="easyui-linkbutton" iconCls="icon-no">退回医生</a>
                    </td>
                    <td>
                        <a href="#" id="back2NurBtn" class="easyui-linkbutton" iconCls="icon-no">退回护士</a>
                    </td>
                    <td>
                        <a href="#" id="back2AllBtn" class="easyui-linkbutton" iconCls="icon-no">全部退回</a>
                    </td>
                </tr>
            </table>    
        </div>  
        <table id="episodeListTable" data-options="fit:true"></table>
    </div>
    <div data-options="region:'east'" style="border:0;width:320">
        <div id="iframeDIV">
            <iframe id="iframeTrak" src='./dhc.epr.fs.review.timeline.csp' frameBorder=0 scrolling=yes style='height:100%;width:100%'></iframe>
        </div>
    </div>
</body>
</HTML>