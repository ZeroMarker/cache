<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s ParamStr=$Get(%request.Data("ParamStr",1))
	s MainStr=$Get(%request.Data("MainStr",1))
	
	//≤È—Ø
	i Action = "QueryTasks" d
	.d ##class(web.DHCSTM.Desk).QueryTasks(ParamStr)
	i Action = "QueryReq" d
	.d ##class(web.DHCSTM.Desk).QueryReq(Start,Limit,MainStr,ParamStr)
	i Action = "QueryPur" d
	.d ##class(web.DHCSTM.Desk).QueryPur(Start,Limit,MainStr,ParamStr)
	i Action = "QueryPo" d
	.d ##class(web.DHCSTM.Desk).QueryPo(Start,Limit,MainStr,ParamStr)
	i Action = "QueryRec" d
	.d ##class(web.DHCSTM.Desk).QueryRec(Start,Limit,MainStr,ParamStr)
	i Action = "QueryRet" d
	.d ##class(web.DHCSTM.Desk).QueryRet(Start,Limit,MainStr,ParamStr)
	i Action = "QueryTrf" d
	.d ##class(web.DHCSTM.Desk).QueryTrf(Start,Limit,MainStr,ParamStr)
	i Action = "QueryAdj" d
	.d ##class(web.DHCSTM.Desk).QueryAdj(Start,Limit,MainStr,ParamStr)
	i Action = "QueryScrap" d
	.d ##class(web.DHCSTM.Desk).QueryScrap(Start,Limit,MainStr,ParamStr)
	
	
	
</script>