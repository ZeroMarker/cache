<div class="hisui-layout" fit="true">
	<div data-options="region:'center',border:false,headerCls:'panel-header-gray'"> <!-- style="padding:10px;"-->
		<!--div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-radius:4px;"-->
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="PatInfo">#(..Get("患者基本信息"))#</label></td>
					<td>
						<input class="textbox" id="PatInfo" style="width:400px;"/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="CancelReason">#(..Get("撤销/退出原因"))#</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="CancelReason" style="width:405px;"/>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="ExitDate">#(..Get("患者退出日期"))#</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="ExitDate" data-options="panelHeight:'180',formatter:myformatter,parser:myparser" style="width:405px;"/>
					</td>
				</tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr></tr>
				<tr>
					<td colspan="3" align=center>
						<a href="#" id="Complete" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'" style="width:116px;">完成</a>
						<div style="display:inline-block;margin:0 15px;"></div>
						<a href="#" id="Cancel" class="hisui-linkbutton" data-options="iconCls:'icon-w-cancel'" style="width:116px;">撤销</a>
						<div style="display:inline-block;margin:0 15px;"></div>
						<a href="#" id="Exit" class="hisui-linkbutton" data-options="iconCls:'icon-w-back'" style="width:116px;">中途退出</a>
					</td>
				</tr>
			</table>
		<!--/div-->
	</div>
</div>