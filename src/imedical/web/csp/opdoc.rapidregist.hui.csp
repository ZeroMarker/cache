<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<!--opdoc.rapidregist.hui.csp HUI便民挂号-->
<HTML lang="zh-CN">
<HEAD>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI></HISUI>
<ADDINS require="RWCard"/>
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
}
.r-label{
	padding-left: 10px;
}
</style>
<Server>
s winfrom=%request.Get("winfrom")
s AdmType=%request.Get("AdmType")
s RegConDisForRapidReg=##Class(web.DHCDocConfig).GetConfigNode("RegConDisForRapidReg")
d ##class(web.UDHCCardCommLinkRegister).GetCardHardJSFunction()
w "<input id='ReadAccExpEncrypt' name='ReadAccExpEncrypt' type='hidden' value='"_##Class(%CSP.Page).Encrypt($lb("web.UDHCAccManageCLSIF.getaccinfofromcardno"))_"'>",$C(13,10)
w "<input id='CardTypeRowID' name='CardTypeRowID' type='hidden' value=''>",$C(13,10)
w "<input id='Securityno' name='Securityno' type='hidden' value=''>",$C(13,10)
w "<input id='GetCardTypeEncrypt' name='GetCardTypeEncrypt' type='hidden' value='"_##Class(%CSP.Page).Encrypt($lb("DHCDoc.OPDoc.PatientList.GessCardType"))_"'>",$C(13,10)
w "<input id='ReadCardTypeEncrypt' name='ReadCardTypeEncrypt' type='hidden' value='"_##Class(%CSP.Page).Encrypt($lb("web.UDHCOPOtherLB.ReadCardTypeDefineListBroker"))_"'>",$C(13,10)
</Server>
</head>
<body>
	<input id='PatientID' name='PatientID' type='hidden' value=''>
	<csp:Include Page="opdoc.rapidregist.show.hui.csp">
	<SCRIPT language = 'javascript' >
		//全局请求后台服务对象
		var ServerObj={
			AdmType:"#(AdmType)#",
			winfrom:"#(winfrom)#"
		};
	</SCRIPT>
    <script type="text/javascript" src="../scripts/DHCWeb.OPCommonManageCard.js"></script>
	<script type="text/javascript" src="../scripts/OPDoc.RapidRegist.hui.js"></script>
</body>
</html>