if(dataNew.ExecType == "DataLoaded" && !WindowCompleted) return;
if(dataNew.IsPrompt == "True")
{
$.messager.confirm($g("提示"), dataNew.PromptMsg, function (r) {
if (r) {
ChangeData(dataNew,currentvalue,'HiddenTextElement_122');
}
});
}
else{
ChangeData(dataNew,currentvalue,'HiddenTextElement_122');
}
});

}
