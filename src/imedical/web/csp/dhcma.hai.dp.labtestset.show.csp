<div class="hisui-layout" data-options="fit:true" style="border:none;">
		<div data-options="region:'west'" style="width:420px;border:none;">
			<div  class="hisui-panel "data-options="fit:true"  style="border:none;border-right:0px;padding:10px 5px 10px 10px;">
				<table id="gridLabSetCat" data-options="toolbar:'#custtb'"></table>
				<div id="custtb" style="padding:3px" >
					<a class="hisui-linkbutton" id="btnAdd_one"  data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true"> 新增 </a>
					<a class="hisui-linkbutton" id="btnEdit_one" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"> 修改 </a>
					<a class="hisui-linkbutton" id="btnDelete_one"  data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true" > 删除 </a>
					<div style="float: right;">
						<input id="search_one" data-options="prompt:'搜索'" class="hisui-searchbox"/>
					</div>
				</div>
			</div>
		</div>		
		
		<div data-options="region:'center'" style="padding:0;border:none;">
			<div class="hisui-panel" data-options="fit:true" style="border:none;padding:10px 10px 10px 5px;">
				<div class="hisui-panel"  data-options="fit:true" >
					<table id="gridLabTestSet" data-options="toolbar:'#custtb2',border:false"></table>
					<div id="custtb2" style="padding:3px;overflow:hidden" >
						<a class="hisui-linkbutton" id="btnAdd_two"  data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true"> 新增 </a>
						<a class="hisui-linkbutton" id="btnEdit_two" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"> 修改 </a>
						<a class="hisui-linkbutton" id="btnDelete_two"  data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true" > 删除 </a>
						<div style="float: right;">
							<input id="btnsearch_two" data-options="prompt:'搜索'" class="hisui-searchbox"/>
						</div>
					</div>
				</div>	
			</div>
			
		</div>
</div>

<div id="winEdit" style="border:none;overflow:hidden;padding-right:10px;display:none">
	<table class="search-table">
		<tr>
			<td class="r-label" style="width:70px">
				<label for="txtBTCode"><font color="red">*</font>分类代码</label>
			</td>
			<td>
				<input class="textbox" id="txtBTCode" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label" style="width:70px">
				<label for="txtBTDesc" ><font color="red">*</font>分类名称</label>
			</td>
			<td>
				<input class="textbox" id="txtBTDesc" style="width:260px"/>
			</td>
		</tr>
	</table>
</div>
			
<div id="winEdit_two" style="border:none;overflow:hidden;padding-right:10px;display:none">
	<table class="search-table">
		<tr>
			<td class="r-label" style="width:100px">
				<label for="txtTSCode"><font color="red">*</font>检验医嘱代码</label>
			</td>
			<td>
				<input class="textbox" id="txtTSCode" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label" style="width:100px">
				<label for="txtTestSet" ><font color="red">*</font>检验医嘱名称</label>
			</td>
			<td>
				<input class="textbox" id="txtTestSet" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="txtNote" >备注</label>
			</td>
			<td>
				<input class="textbox" id="txtNote" style="width:260px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label">
				<label for="chkActive" >是否送检项目</label>
			</td>
			<td>
