<!-- 
http://127.0.0.1/imedical/web/csp/dhcpedjdtemplate.header.csp?PAADM=652 


-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	q 1
</csp:method>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=5">
<TITLE>

</TITLE>
<style type="text/css">

	.base{
	    text-align:"center";
	    height:40
	}
</style>

</head>
<BODY>
<SCRIPT language="Cache" RUNAT="SERVER">
	s HospID=$g(%request.Data("HospID",1))
	S UserID=$g(%request.Data("UserID",1))
	s PatientID=$g(%request.Data("PAADM",1))
	s NoPrintAmount=$P(PatientID,"^",2)
	s PatientID=$P(PatientID,"^",1)
	//s:PatientID="" PatientID="1094"
	i PatientID=""{
		s Loc=$O(^DHCPESetting("DHCPE","DefaultPAADM",""))
		s:Loc'="" PatientID=$G(^DHCPESetting("DHCPE","DefaultPAADM",Loc))
	}
	i PatientID'=""{ 
	s IADM=$o(^DHCPEIADM(0,"PAADM",PatientID,0))
	s PreIADM=$p($g(^DHCPEIADM(IADM)),"^",4)
	s BaseInfo=##class(web.DHCPE.PrintNewDirect).GetPatBaseInfo(PreIADM,"",NoPrintAmount,HospID,UserID)
	//s ItemInfo=##class(web.DHCPE.PrintNewDirect).GetItemInfo(PreIADM)
	/*
	s BaseInfo=PatName_"^"_Sex_"^"_PatAge_"^"_MobilePhone_"^"_RegNo_"^"_BarRegNo
	s BaseInfo=BaseInfo_"^"_RegDate_"^"_OrderSetsDesc_"^"_Company_"^"_VIPType
	;s DietDesc="早餐"
	;s PhotoDesc="片子"
	s BaseInfo=BaseInfo_"^"_Position_"^"_TotalFee_"^"_DietDesc_"^"_PhotoDesc
	s BaseInfo=BaseInfo_"^"_SortForNorCus
	
	*/
	s PatName=$P(BaseInfo,"^",1)
	s LocID=$P(^PAADM(+PatientID),"^",4)
	
	s ReportCorderInfo=$G(^DHCPESetting("DHCPE","DJDInfo",LocID))
	}else{
		s BaseInfo=""
		s ReportCorderInfo="导检单抬头^注意事项^备注内容"
	}
	s PatName=$P(BaseInfo,"^",1)
	s PatSex=$P(BaseInfo,"^",2)
	s PatAge=$P(BaseInfo,"^",3)
	s MobilePhone=$P(BaseInfo,"^",4)
	s HPNo=$P(BaseInfo,"^",5)
	s BarRegNo=$P(BaseInfo,"^",6)
	s CheckDate=$P(BaseInfo,"^",7)
	s OrdSetsDesc=$P(BaseInfo,"^",8)
	s GroupName=$P(BaseInfo,"^",9)
	s DepartName=$P(BaseInfo,"^",11)
	s TotalAmt=$P(BaseInfo,"^",12)
	s TotalAmt=$P(TotalAmt,"额",2)
	s CapacityNum=$P(BaseInfo,"^",16)
	s CapacityNum=$P(CapacityNum,":",2)
	s DJDTitel=$P(ReportCorderInfo,"^",1)
	s DJDNotice=$P(ReportCorderInfo,"^",2)
	s DJDRemark=$P(ReportCorderInfo,"^",3)
	
	
	s width1=72
	s width2=86
	s width3="15%"
	s height=30
</SCRIPT>
	
	<TABLE width=655 border=0 cellspacing=0>
	
		<TR >
			<TD colspan=5 width="100%">
				<TABLE width=650 border=0 cellspacing=0>
					<!--TR><TD colspan=6  align="center">< span style="font-size:36px;">体检导检单</span></TD><TD colspan=2 align="center">< span style="font-size:36px;font-family:C39HrP24DhTt">#(BarRegNo)#</span></TD></TR-->
					<TR>
						<TD colspan=6  align="center">< span style="font-size:36px;">#(DJDTitel)#</span></TD><TD colspan=2 align="center">&nbsp;</TD>
						<TD width=15% class="base" colspan=2>< span style="font-size:36px;font-family:C39HrP24DhTt">#(BarRegNo)#</span></TD>

					</TR>
					
					<TR align="left">
						<TD width="#(width1)#">姓&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;名:</TD><TD colspan=2 style="border-bottom-style: solid;border-bottom-width: 1px">#(PatName)#</TD>
						<TD width="#(width1)#">性&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;别:</TD><TD width="#(width2)#"style="border-bottom-style: solid;border-bottom-width: 1px">#(PatSex)#</TD>
						<TD width="#(width1)#">年&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;龄:</TD><TD width="#(width2)#"style="border-bottom-style: solid;border-bottom-width: 1px">#(PatAge)#</TD>
						
					</TR>
					<TR align="left">
						<TD>体检编号:</TD><TD colspan=2 style="border-bottom-style: solid;border-bottom-width: 1px">#(HPNo)#</TD>
						<TD>日&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;期:</TD><TD colspan=2 style="border-bottom-style: solid;border-bottom-width: 1px">#(CheckDate)#</TD>
						<TD>电&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;话:</TD><TD style="border-bottom-style: solid;border-bottom-width: 1px" colspan=2>#(MobilePhone)#</TD>
						
						<!--TD >病&nbsp;&nbsp;案&nbsp;&nbsp;号:</TD><TD colspan=2 style="border-bottom-style: solid;border-bottom-width: 1px">#(CapacityNum)#</TD-->
					</TR>
					<TR align="left">
						<TD >单&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;位:</TD><TD colspan=4 style="border-bottom-style: solid;border-bottom-width: 1px">#(GroupName)#</TD>
						<TD >部&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;门:</TD><TD colspan=3 style="border-bottom-style: solid;border-bottom-width: 1px">#(DepartName)#</TD>	
					</TR>
					<TR>
					<TD>套&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;餐:</TD><TD colspan=5 style="border-bottom-style: solid;border-bottom-width: 1px">#(OrdSetsDesc)#</TD>
					<TD>金&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;额:</TD><TD colspan=2 style="border-bottom-style: solid;border-bottom-width: 1px">#(TotalAmt)#</TD>
					</TR>
					<TR><TD colspan=9 height=3px>#(DJDNotice)#</TD></TR>
				</TABLE>
			</TD>
		</TR>
		</TABLE>
		

</BODY>
</HTML>
