<!--发票管理-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<SERVER>
 d ##class(web.DHCSTMHUI.Util.RQServerConfig).GetApplet()  //润乾打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',border:false,collapsible:false">
		<div class="hisui-panel" title="入库退货单" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'north',border:false,collapsible:false" style="border-bottom:1px dashed #ccc;">
					<table id="MainConditions" class="Condition">
						<tr>
							<td><label>入库科室</label><input id="IngrLoc" name="IngrLoc" class="hisui-combobox" style="width:140px;"></td>
							<td><label>起始日期</label><input id="StartDate" name="StartDate" class="hisui-datebox textbox" style="width:115px;"></td>
							<td><label>物资名称</label><input id="InciDesc" name="InciDesc" class="textbox" style="width:126px;"></td>
							<td><label>接收科室</label><input id="ReqLoc" name="ReqLoc" class="hisui-combobox" style="width:140px;"></td>
							<td><label style="margin-right:14px;">发票状态</label><select id="InvFlag" name="InvFlag" class='hisui-combobox' style="width:80px;">
								<option value="" selected>全部</option>
								<option value="Y">有发票</option>
								<option value="N">无发票</option>
							</select></td>
							<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
							</tr>
						<tr>
							<td><label>供应商</label><select id="Vendor" name="Vendor" class="hisui-combobox" style="width:140px;"></select></td>
							<td><label>截止日期</label><input id="EndDate" name="EndDate" class="hisui-datebox textbox" style="width:115px;"></td>
							<td><label>发票号</label><input id="InvNo" name="InvNo" class="textbox" style="width:120px;"></td>
							<td><label>随行单号</label><input id="SxNo" name="SxNo" class="textbox" style="width:133px;"></td>
							<td align="left"><label style="margin-right:0;">确认标志</label><input id="Confirm" name="Confirm" class='hisui-checkbox' type="checkbox" value="Y"></td>
							<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
						</tr>
					</table>
				</div>
				<div data-options="region:'center',border:false,collapsible:false">
					<table id="GRMainGrid"></table>
				</div>
			</div>
			</div>
		</div>
	</div>
	<div class="GridPadding" data-options="region:'south',border:false,collapsible:false" style="padding-top:0;height:250px;">
		<div class="hisui-panel" title="入库退货单明细" data-options="headerCls:'panel-header-gray',iconCls:'icon-write-order',fit:true">
			<table id="GRDetailGrid"></table>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InvManage/InvCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InvManage/InvManage.js"></script>
</body>
</html>	