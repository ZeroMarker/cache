<!--字典安全组授权--> 
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="hisui-panel " data-options="region:'center',border:false">
		<div class="hisui-layout" data-options="fit:true">
			<div id="DicGroupTB" >
				<table class="Condition" >
					<tr>
						<td><label>安全组</label><input id="Group" name="Group" class="combobox hisui-combobox" ></td>
						<td><a href="#" id="InciDataChoseBT" class="hisui-linkbutton" >基础信息全选</a></td>
						<td> <a href="#" id="PhanfChoseBT" class="hisui-linkbutton" >厂商供应商全选</a></td>
						<td><a href="#" id="VendorChoseBT" class="hisui-linkbutton" >供应商信息全选</a></td>
						<td><a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a></td>
						<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
					</tr>
				</table>
			</div>
			<div data-options="border:false" class="GridPadding" style="padding-bottom: 0px;padding-top: 0px">
				<div id="InciData" class="hisui-panel" data-options="iconCls:'icon-paper',title:'基础信息',headerCls:'panel-header-gray',fit:true" style="height:115px" >
					<table class="Condition">
						<tr align="left">
							<td><input id="DGCode" name="DGCode" value="Y" class='hisui-checkbox' type="checkbox" label="物资代码"></td>
							<td><input id="DGDesc" name="DGDesc" value="Y" class='hisui-checkbox' type="checkbox" label="物资名称"></td>
							<td><input id="DGChargeType" name="DGChargeType" value="Y" class='hisui-checkbox' type="checkbox" label="收费类型"></td>
							<td><input id="DGSp" name="DGSp" value="Y" class='hisui-checkbox' type="checkbox" label="当前售价"></td>
							<td><input id="DGPbRp" name="DGPbRp" value="Y" class='hisui-checkbox' type="checkbox" label="招标进价"></td>
							<td><input id="DGSpec" name="DGSpec" value="Y" class='hisui-checkbox' type="checkbox" label="规格"></td>
							<td><input id="DGScg" name="DGScg" value="Y" class='hisui-checkbox' type="checkbox" label="类组"></td>
							<td><input id="DGStkcat" name="DGStkcat" value="Y" class='hisui-checkbox' type="checkbox" label="库存分类"></td>
							<td><input id="DGBrand" name="DGBrand" value="Y" class='hisui-checkbox' type="checkbox" label="品牌"></td>
						</tr>
						<tr align="left">
							<td><input id="DGUom" name="DGUom" value="Y" class='hisui-checkbox' type="checkbox" label="单位"></td>
							<td><input id="DGPbManf" name="DGPbManf" value="Y" class='hisui-checkbox' type="checkbox" label="生产厂商"></td>
							<td><input id="DGPbVendor" name="DGPbVendor" value="Y" class='hisui-checkbox' type="checkbox" label="供应商"></td>
							<td><input id="DGHVFlag" name="DGHVFlag" value="Y" class='hisui-checkbox' type="checkbox" label="高值"></td>
							<td><input id="DGCertNo" name="DGCertNo" value="Y" class='hisui-checkbox' type="checkbox" label="注册证号码"></td>
							<td><input id="DGProvLoc" name="DGProvLoc" value="Y" class='hisui-checkbox' type="checkbox" label="供应科室"></td>
							<td><input id="DGPruPic" name="DGPruPic" value="Y" class='hisui-checkbox' type="checkbox" label="物资主图"></td>
							<td><input id="DGPruCommonPic" name="DGPruCommonPic" value="Y" class='hisui-checkbox' type="checkbox" label="物资普通图片"></td>
							<td><input id="DGPruDocumentPic" name="DGPruDocumentPic" value="Y" class='hisui-checkbox' type="checkbox" label="物资说明书图片"></td>
						</tr>
						<tr align="left">
							<td><input id="DGItmSA" name="DGItmSA" value="Y" class='hisui-checkbox' type="checkbox" label="供应链"></td>
							<td><input id="DGHospAllowed" name="DGHospAllowed" value="Y" class='hisui-checkbox' type="checkbox" label="入院审批材料"></td>
						</tr>
					</table>
				</div>
			</div>
			<div data-options="border:false" class="GridPadding" style="padding-bottom: 0px">
				<div id="PhamnfData" class="hisui-panel" data-options="iconCls:'icon-paper',title:'厂商',headerCls:'panel-header-gray',fit:true" style="height:45px">
					<table class="Condition">
						<tr align="left">
							<td><input id="DGMCert" name="DGMCert" value="Y" class='hisui-checkbox' type="checkbox" label="厂商注册证"></td>
							<td><input id="DGMMatProduct" name="DGMMatProduct" value="Y" class='hisui-checkbox' type="checkbox" label="器械生产许可"></td>
							<td><input id="DGMComLic" name="DGMComLic" value="Y" class='hisui-checkbox' type="checkbox" label="工商执照"></td>
							<td><input id="DGMBusinessReg" name="DGMBusinessReg" value="Y" class='hisui-checkbox' type="checkbox" label="工商注册号"></td>
							<td><input id="DGMOrgCode" name="DGMOrgCode" value="Y" class='hisui-checkbox' type="checkbox" label="组织机构代码"></td>
							<td><input id="DGMMatManLic" name="DGMMatManLic" value="Y" class='hisui-checkbox' type="checkbox" label="器械经营许可证"></td>
						</tr>
					</table>
				</div>
			</div>
			<div data-options="border:false" class="GridPadding">
				<div id="VendorData" class="hisui-panel" data-options="iconCls:'icon-paper',title:'供应商',headerCls:'panel-header-gray',fit:true" style="height:80px">
					<table class="Condition">
						<tr align="left">
							<td><input id="DGVLic" name="DGVLic" value="Y" class='hisui-checkbox' type="checkbox" label="授权书"></td>
							<td><input id="DGVComlic" name="DGVComlic" value="Y" class='hisui-checkbox' type="checkbox" label="工商执照"></td>
							<td><input id="DGVRevreg" name="DGVRevreg" value="Y" class='hisui-checkbox' type="checkbox" label="税务登记"></td>
							<td><input id="DGVMatmanlic" name="DGVMatmanlic" value="Y" class='hisui-checkbox' type="checkbox" label="医疗器械经营许可"></td>
							<td><input id="DGVMatenrol" name="DGVMatenrol" value="Y" class='hisui-checkbox' type="checkbox" label="医疗器械注册证"></td>
							<td><input id="DGVSanitation" name="DGVSanitation" value="Y" class='hisui-checkbox' type="checkbox" label="卫生许可证"></td>
							<td><input id="DGVOrgcode" name="DGVOrgcode" value="Y" class='hisui-checkbox' type="checkbox" label="组织机构代码"></td>
							<td><input id="DGVGsp" name="DGVGsp" value="Y" class='hisui-checkbox' type="checkbox" label="GSP认证"></td>
						</tr>
						<tr align="left">
							<td><input id="DGVMatpro" name="DGVMatpro" value="Y" class='hisui-checkbox' type="checkbox" label="器械生产许可"></td>
							<td><input id="DGVPropermit" name="DGVPropermit" value="Y" class='hisui-checkbox' type="checkbox" label="生产制造认可表"></td>
							<td><input id="DGVQuality" name="DGVQuality" value="Y" class='hisui-checkbox' type="checkbox" label="质量承诺书"></td>
							<td><input id="DGVSales" name="DGVSales" value="Y" class='hisui-checkbox' type="checkbox" label="业务员授权书"></td>
						</tr>
					</table>
				</div>
			</div>
		</div>
	</div>
<script type="text/javascript" src="../scripts/dhcstmhisui/DicGroup/DicGroup.js"></script>
</body>
</html>