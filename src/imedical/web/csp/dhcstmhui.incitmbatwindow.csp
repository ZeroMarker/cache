<!--批次弹窗-->
<div id="IncItmBatWindow" class="hisui-dialog FindWin" title="库存批次" data-options="closed:'true',modal:true">
	<div class="hisui-layout" data-options="fit:true">
		<div class="GridPadding" data-options="region:'north',border:false,collapsible:false" style="height:290px;">
			<div class="hisui-panel" title="物资信息" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
				<table id="IncItmBatMasterGrid" style="padding:10px 0px 10px 0px"></table>
				<div id="IncItmBatTB">
					<table class="Condition">
						<td>规格</td>
						<td><input id="IncItmBatSpec" name="Spec" class="textbox"></td>
						<td>品牌</td>
						<td><input id="IncItmBatBrand" name="Brand" class="textbox"></td>
						<td>进价</td>
						<td><input id="IncItmBatRp" name="Rp" class="textbox"></td>
						<td><a href="#" id="IncItmFilterBtn" class="hisui-linkbutton" data-options="iconCls:'icon-search',plain:true">过滤</a></td>
					</table>
				</div>
			</div>
		</div>
		<div class="GridPadding" data-options="region:'center',border:false,collapsible:false" style="padding-top:0;">
			<div class="hisui-panel" title="物资批次信息 (ALT返回物资信息)" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
				<table id="IncItmBatDetailGrid"></table>
			</div>
		</div>
		<div align="center" class="GridPadding" data-options="region:'south',border:false" style="padding-top: 0px;">
			<td><a href="#" id="IncItmBatSelBT" class="hisui-linkbutton">选取</a></td>
		</div>
	</div>
</div>