<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired()
 i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<HTML XMLNS=TRAK>
<HEAD>
<TITLE><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></TITLE>
<TRAK:HEAD></TRAK:HEAD>
<meta http-equiv="Content-Type" content="text/html;">	 
<EASYUI:CLINICAL/>   
	<style type="text/css">
	.datagrid-row-selected {
  		background: #ADFAFC;
	}
   </style>	
	<script type="text/javascript" src="../scripts/dhcpha/clinical/medadvpatlist.js"></script>
</head> 
<body>
	<div id="mainpanel" class="easyui-layout" fit="true">
		<!--用药信息-->
		<div data-options="region:'west',title:'待处理建议列表【临床药师】'" style="width:600px">
		    <div id="patList" toolbar="#tb"></div>
		</div>
		<!--建议填写-->
		<div data-options="region:'center'">
			<div id="mainpanel" class="easyui-layout" fit="true">
				<div data-options="region:'north',title:'问题明细',split:false" style="height:440px">
					<div id="medAdvCon"></div>
				</div>
				<div data-options="region:'center'" style="background:E8F1FF;border: 2px solid #95B8E7;">
					<div style="background:E8F1FF;height:35px;padding:5px;">
						<div id="bt" style="margin-left:5px;background:E8F1FF;">
							<a href="#" id="Main" style="width:90" class="easyui-linkbutton" iconCls="icon-ok">模板维护</a>
							<a href="#" id="Quo" style="width:90" class="easyui-linkbutton" iconCls="icon-ok">引用模板</a>
							<a href="#" id="Agr" style="width:90" class="easyui-linkbutton" iconCls="icon-ok">同意</a>
							<a href="#" id="App" style="width:90" class="easyui-linkbutton" iconCls="icon-ok">提交意见</a>
							<a href="#" id="Del" style="width:90" class="easyui-linkbutton" iconCls="icon-cancel">删除</a>
						</div>
					</div>
					<textarea id="textarea" style="overflow-y:hidden;width:100%;height:70%;font-size:16px;margin:5px;border: 2px solid #95B8E7;">请输入...</textarea>
					<span id="medAdvDetID" style="display:none"></span>
					<span id="medAdvID" style="display:none"></span>
				</div>
			</div>
		</div>
	</div>
	<div id="tb" style="padding:5px">
		<div style="margin:0px;margin-top:10px">
			<span style="margin:0px 25px 0px 5px"><span style='margin-right:5px'>开始日期</span><input id="StartDate" type="text" class="easyui-datebox" style="width:130"></input></span>
			<span style="margin:0px 25px 0px 5px"><span style='margin-right:5px'>结束日期</span><input id="EndDate" type="text" class="easyui-datebox" style="width:130" ></input></span>
			<a href="#" id="Find" style="width:90" class="easyui-linkbutton" iconCls="icon-search">查询</a>
		</div>
		<div style="margin:0px;margin-top:10px;margin-bottom:10px">
			<span style="margin:0px 25px 0px 17px"><span style='margin-right:5px'>登记号</span><input id="patno" style="width:130" class="easyui-validatebox"/></span>
			<span style="margin:0px 25px 0px 35px"><span style='margin-right:5px'>状态</span><input id="status" style="width:130" class="easyui-combobox" data-options="valueField:'val',textField:'text'"/></span>
			<a href="#" id="DelMed" style="width:90" class="easyui-linkbutton" iconCls="icon-remove">删除</a>
		</div>
	</div>
	<div id="medAdvWin">
		<div id="medAdvdg"></div>
	</div>
	
	<!--用药建议模板维护-->
	<div id="medAdvTempWin"></div>
</body>
</html>
