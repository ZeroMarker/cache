function DHCC_StrToArray(str) {
  var x = new Array();
  var Arr = str.split('^');
  for (var i = 0; i < Arr.length; i++) {
    var Arr1 = Arr[i].split(String.fromCharCode(1));
    var label = Arr1[1];
    var val = Arr1[0];
    if ((typeof(val) == "undefined") || (val === null)) val = label;
    x[i] = [val, label];
  }
  return x;
}

