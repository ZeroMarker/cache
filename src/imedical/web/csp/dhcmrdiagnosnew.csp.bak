//去掉左右空格
function ltrim(str) {
str = str + "";
var i;
var l = str.length;
for (i = 0; i < l; i++) {
if (str.indexOf(" ") == 0) {
str = str.substring(1, str.length);
} else {
break;
}
}
return str;
}
function rtrim(str) {
str = str + "";
var i;
var l = str.length;
for (i = 0; i < l; i++) {
if (str.lastIndexOf(" ") == str.length - 1) {
str = str.substring(0, str.length - 1);
} else {
break;
}
}
return str;
}
function trim(str) {
str = str + "";
str = rtrim(str);
str = ltrim(str);
return str;
}

