						dataType: 'text'
					},function(res){
						if(res==0){
							// 允许对执行记录操作
								var reason=""
								if(selectVal){
									reason=selectVal;
								}else{
									reason=inputVal;
								}
								if(orderTypeLong){
									// 长期医嘱逻辑	
									$cm({
										ClassName:'web.DHCDocInPatPortalCommon',
										MethodName:'MulOrdExecDealWithCom',
										OrderExecStr:str,
										date:'',
										time:'',
										type:'D',
										ReasonId:reason,
										ExpStr:session['LOGON.USERID']+'^'+session['LOGON.CTLOCID']+'^'+session['LOGON.GROUPID']+'^^',
										dataType:'text'
									},function(res){
											if(res.indexOf('0')==0){
