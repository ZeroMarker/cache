					recordArr.forEach(function(item){
						var tmpCode=String.fromCharCode(1);
						var tmp=item.TOEORI+tmpCode+item.SttDate;
						resArr.push(item.TOrdExcRowID);
						resArrDate.push(tmp);
					})
					var str=resArr.join('^'); // 执行记录医嘱串
