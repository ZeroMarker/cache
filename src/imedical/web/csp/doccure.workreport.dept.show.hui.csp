<div title="治疗站科室工作量报表" class="hisui-panel" data-options="border:true,iconCls:'icon-paper-table',headerCls:'panel-header-gray',fit:true,closable:false">   
	<div class="hisui-layout" data-options="fit:true,border:false">
		<div data-options="region:'north',split:false,border:false" style="width:100%;height:50px">
			<div class="hisui-panel" style="padding:10px" data-options="fit:true,border:false,closable:false">  
				<label class="first-qlab" for="StartDate">#(..Get("开始日期"))#</label>
				<input id="StartDate" type="text" class="hisui-datebox cure-w-box"/>
				<label class="qlab" for="EndDate">#(..Get("结束日期"))#</label>
				<input id="EndDate" type="text" class="hisui-datebox cure-w-box"/>
				<label class="qlab" for="ComboArcim">#(..Get("医嘱项"))#</label>
				<input id="ComboArcim" class="textbox" name="ComboArcim"/>
				<a id="btnFind" class="hisui-linkbutton" style="margin:0px 10px" data-options="iconCls:'icon-w-find'">查询</a>
				<a id="btnExport" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">导出并打印</a>
			</div>
		</div>
		<div data-options="region:'center',border:true" class="panel-header-gray cure-p-bd-tb">
			<table id="tabRecordReportList"></table> 
		</div>
		<div data-options="region:'south',border:false" style="width:100%;height:280px;">
			<div class="hisui-panel panel-header-gray" data-options="fit:true,border:false">  
				<div id="container" style="height: 100%"></div>
			</div>
		</div>
	</div>
</div>