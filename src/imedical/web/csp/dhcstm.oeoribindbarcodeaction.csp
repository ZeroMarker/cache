<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))
	s StrParam=$Get(%request.Data("StrParam",1))
	
///检索需要绑定的高值医嘱信息
i Action = "QueryOeori" d
	.d ##class(web.DHCSTM.OeoriBindBarcode).QueryOeori(Start,Limit,Sort,Dir,StrParam)

///检索需要绑定的高值医嘱信息
i Action = "QueryDetail" d
	.d ##class(web.DHCSTM.OeoriBindBarcode).QueryDetail(Start,Limit,Sort,Dir,StrParam)

///将批次和条码进行绑定
i Action = "Save" d
	.s MainInfo=$Get(%request.Data("MainInfo",1))
	.s ListDetail=$Get(%request.Data("ListDetail",1))
	.s result = ##class(web.DHCSTM.OeoriBindBarcode).Save(MainInfo,ListDetail)
	.i +result=0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

i Action = "Audit" d
	.s Oeori=$Get(%request.Data("Oeori",1))
	.s UserId=$Get(%request.Data("UserId",1))
	.s result = ##class(web.DHCSTM.OeoriBindBarcode).Audit(Oeori,UserId)
	.i result="" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

i Action = "Delete" d
	.s RowId=$Get(%request.Data("RowId",1))
	.s result = ##class(web.DHCSTM.OeoriBindBarcode).Delete(RowId)
	.i result="" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
//取消审核一条记录
i Action = "CancelAudit" d
	.s RowId=$Get(%request.Data("DHCOBBRowId",1))
	.s UserId=$Get(%request.Data("UserId",1))
	.s result = ##class(web.DHCSTM.OeoriBindBarcode).Cancel(RowId,UserId)
	.i result="" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
</script>