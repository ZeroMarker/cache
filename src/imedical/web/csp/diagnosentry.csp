<!--//add by zf 20150330 引用医政组接口-->
<SCRIPT SRC='../scripts/dhcmed/ss/interface/MRDiagnos.js'></SCRIPT>
<Server>
    s EpisodeID=%request.Get("EpisodeID")
    s PAAdmType="",mradm="",PatientID=""
    if EpisodeID'="" {
        s EpisodeType=$p(^PAADM(EpisodeID),"^",2)
        s PAAdmType=EpisodeType
        s mradm=$p(^PAADM(EpisodeID),"^",61)
        s PatientID=$p(^PAADM(EpisodeID),"^",1)
        s FirstAdm=0,ReAdmis=0,OutReAdm=0,TransAdm=0,ILIFlag=0,AdmWay=""
        s FirstOrReadmis=$p(^PAADM(EpisodeID),"^",72)
        s AdmReadm=$p(^PAADM(EpisodeID),"^",56)
        s TransFlag=$p($g(^PAADM(EpisodeID,"DHC")),"^",5)
        s ILIFlag=$p($g(^PAADM(EpisodeID,"DHC")),"^",13)
        s QueStateDesc=""
        s QueRowId=$o(^User.DHCQueueI("QuePaadmDrIndex",EpisodeID,""))
        if (QueRowId'=""){
            s QueStateDr=$list(^User.DHCQueueD(QueRowId),14)
            if (QueStateDr'=""){
                s QueStateDesc=$list(^User.DHCPerStateD(QueStateDr),4)
            }
        }
        ;默认初诊
        s FirstAdm=1
        s AdmWayID=$p($g(^PAADM(EpisodeID,2)),"^",47)
        I AdmWayID'="" S AdmWay=$P(^PAC("RTR",AdmWayID),"^",1)
        if (FirstOrReadmis="R")&&(AdmWay="CYFZ") s OutReAdm=1,FirstAdm=0
        if (FirstOrReadmis="F") s FirstAdm=1
        if ((AdmReadm="R")&&(AdmWay="MZFZ"))||(QueStateDesc="复诊") s ReAdmis=1,FirstAdm=0
        if (TransFlag="Y")&&(AdmWay="ZZ") s TransAdm=1,FirstAdm=0
        ;流感样
        if ILIFlag="Y" s ILIFlag=1
        //血压
        s BPSystolic=$p($g(^MR(mradm,"PRO",1)),"^",4)
        s BPDiastolic=$p($g(^MR(mradm,"PRO",1)),"^",5)
        s DiagnosStatus=$o(^MRC("DSTAT",0,"Desc","确诊",""))
        //诊断状态
        s DiaW=$O(^MRC("DSTAT",0,"Desc","待诊",0))
        s DiaH=$O(^MRC("DSTAT",0,"Desc","疑诊",0))
        s DiaD=$O(^MRC("DSTAT",0,"Desc","确诊",0))
        //体重
        s Weight=$p($g(^MR(mradm,"PRO",1)),"^",27)
        //
        //特殊人群
       s SpecialistStr=$p($G(^MR(mradm,"DHC")),"^",8) 
       s SpecialStrCreat=""
       if (SpecialistStr'=""){
         s LengSpec=$L(SpecialistStr,"!")
         for Sub=1:1:LengSpec
         {
          s SubStr=$P(SpecialistStr,"!",Sub)
          s Va=$P(SubStr,$C(1),1)
          continue:Va=""
          i SpecialStrCreat="" s SpecialStrCreat=Va
         e  s SpecialStrCreat=SpecialStrCreat_"^"_Va
         }
       }
        s DocID=$P(^SSU("SSUSR",%session.Get("LOGON.USERID")),"^",14)
        s DiagnosStatus=DiaD
        //住院患者的入院诊断
        s INDiag=##class(web.DHCDocDiagnosNew).GetMRDiagnosByEpisodeID(EpisodeID)
        //必须录入血压(科室扩展第12位)
        s NeedStolicMast=+(##class(web.DHCDocConfig).GetDHCCTLOCFieldValue(%session.Get("LOGON.CTLOCID"),12))
        //传染病上报诊断提示
        s GetSeriousDiseaseByICDMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.GetSeriousDiseaseByICD"))
        
        s FindMRDIAICDCodeDR =##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.GetOneICD"))
        s GetMRDiagnoseList=##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.GetMRDiagnoseList"))
        s UpdateMRDiagnosNote=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.UpdateMRDiagnosNoteBroker"))
        s InsertMRDiagnosMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.InsertMRDiagnos"))
        s SetArrivedStatus=##Class(%CSP.Page).Encrypt($lb("web.DHCDocOrderEntry.SetArrivedStatus"))
        //更新发病日期
        s UpdateOnsetDateMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.UpdateOnsetDate"))
        s FindClinicPathWayByICDMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCMRClinicalPathWays.FindClinicPathWayByICD"))
        s UpdatePAADM=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.UpdatePAADM"))
        s DeleteMRDiagnosMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.DeleteMRDiagnos"))
        s InsuPatTypeCheck=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.InsuPatTypeCheck"))
        s FlagMarchDiagnose=##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.FlagMarchDiagnose"))
        s UpdateMRDiagnosMethod =##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.UpdateMRDiagnos"))
        s AddPrivateTemplateMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.AddPrivate"))
        s PrivateSaveMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.PrivateSaveNew"))
        s CheckAdd=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.CheckAdd"))
        ;获取审核的诊断是否是中医诊断
        s GetZYMRDiagnosFlag =##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.GetZYMRDiagnosFlag"))
        s SetPAADMSingleDisMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCDocOrderCommonExpand.SetPAADMSingleDis"))
         ;判断是否可以接诊
        s CheckIsAdmissionsMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCDocOutPatientList.CheckIsAdmissions"))
        s CopyFromHistMRDiagMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCDocDiagnosNew.CopyFromHistMRDiag"))
        s GetGroupICDInfoMethod=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.GetGroupICDInfo"))
    }
    ;是否中医科室
    s LogonLocID=%session.Get("LOGON.CTLOCID")
    s ChinaMedFlag=+$p($g(^CTLOC(LogonLocID,"DHC")),"^",16)
    //启用标准证型诊断
    s UserICDSyndrome=##class(web.DHCDocConfig).GetConfigNode("UserICDSyndrome")
    s WeekGestationDia=##class(web.DHCDocConfig).GetConfigNode("WeekGestationDia")
    s GetWeekDiagnoseMethod=##Class(%CSP.Page).Encrypt($lb("DHCDoc.DHCDocConfig.ItmDiagnose.GetWeekDiagnose"))
    //判断科室是否启用诊断知识库
    s UseDKBFlag=+(##class(web.DHCDocConfig).GetDHCCTLOCFieldValue(%session.Get("LOGON.CTLOCID"),19)) //##class(web.DHCDocConfig).GetConfigNode("UseDDiagnosKB")
	s UserOpenUseDKBFlag=##class("web.DHCDocConfig").GetConfigNode1("UserOpenDKB",%session.Get("LOGON.USERID"))
	;根据诊断取诊断模板
    s GetDiagTempData=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTremExtend.GetDoctorInfo"))
    s GetChildTreeJson=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTremExtendDetail.GetDocTreeJson"))
    s GetDiagListProperty=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTremExtendDetail.GetMyList"))
    s GetDiagTXProperty=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTremExtendDetail.GetTextInfo"))
    s GetSoursePropertyInfo=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTremExtendDetail.GetSourseInfo"))
    s GetDocSourseList=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTrem.GetDocSourseList"))
    s GetSourseTreeJson=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTrem.GetSourseTreeJson"))
    s GetDocCatList=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBTremExtend.GetDocCatList"))
    s GetDiagDisPlayNameJson=##Class(%CSP.Page).Encrypt($lb("web.DHCMRDiagnos.GetDiagDisPlayName"))
    s TKBTremBaseId=##class(web.DHCBL.MKB.TKBTrem).GetBaseID("Diagnose") //诊断 术语注册表id TKB_TremBase
    s GetDiagPLinkICDJson=##Class(%CSP.Page).Encrypt($lb("web.DHCBL.MKB.TKBICDRelation.GetRecommend"))
</Server>
