<!DOCTYPE html>
<!--药房药库-公共字典维护-不放开用户
	csp:pha.sys.v1.comdict.csp
	js: pha/sys/v1/comdict.js
-->
<html>
<!-- 验证session过期 -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
</csp:method>

<head>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>
    </TRAK:HEAD>
    <HISUI />
    <PHALIBV1 />
</head>

<body>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',border:false" class="pha-body">
            <div class="hisui-layout" fit="true">
                <div data-options="region:'center',border:false">
                    <div class="hisui-panel"
                        data-options="headerCls:'panel-header-gray',fit:true,bodyCls:'panel-body-gray',iconCls:'icon-paper-info'">
                        <table id="gridComDict"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- 表格工具栏 -->
    <div id="gridComDictBar">
        <div class="pha-row">
            <div class="pha-col">
                <input id="conQText" data-pha='class:"hisui-searchbox"'>
            </div>
        </div>
        <div class="pha-line"></div>
        <div>
            <a class="hisui-linkbutton" plain='true' iconCls="icon-add" id="btnAdd">增加</a>
            <div class="pha-col"> <a class="hisui-linkbutton" plain='true' iconCls="icon-save" id="btnSave">保存</a></div>
            <div class="pha-col"> <a class="hisui-linkbutton" plain='true' iconCls="icon-remove" id="btnDelete">删除</a>
            </div>
        </div>
    </div>
    <!-- 本页面js -->
    <script type="text/javascript" src="../scripts/pha/sys/v1/comdict.js"></script>
</body>

</html>