
<!-- 
 * FileName: dhcpecontract.hisui.csp
 * Anchor: xy
 * Date: 2019-05-27
 * Description: HISUI 团体合同
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 20px;
}
/*弹出框里标签右对齐 并且有10px内边距*/
.tdlabel
{
  padding-left: 10px;
  padding-right: 10px;
  text-align:right;
}
.panel-header{
	border-bottom: 0;
}

</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:96px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'团体合同',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-bottom: 1px dashed #C0C0C0;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="Name">合同名称</label></td>
					<td>
						<input class="textbox" id="Name"/>
					</td>
					
					<td class="r-label r-label-1"><label for="No">合同编号</label></td>
					<td>
						<input class="textbox" id="No"/>
					</td>

					<td class="r-label r-label-1"><label for="StartDate">开始日期</label></td>
					<td>
						<input id="StartDate" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1"><label for="EndDate">结束日期</label></td>
					<td>
						<input id="EndDate" class="hisui-datebox textbox"/>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BClear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
					
					<td class="r-label r-label-1">
						<a id="BHomeSet" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'">主场设置</a>
					</td>
				</tr>
				
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="ContractGrid" toolbar="#allbar"></table>
			</div>
        </div>
	      
</div>

   <!--增改弹窗-->
     <div id="myWin" style="width:320px;height:300px;padding:10px;display:none;">   
		<form id="form-save" method="post" data-options="fit:true,border:false,plain:true">

			<table id="addtable">
				 <tr>
				 	<td class="r-label"><label for="SignDate"><font color=red>*</font>签订日期</label></td>
					<td>
						<input id="SignDate" class="hisui-datebox textbox"/>
					</td>
							
				</tr>
				<tr>
					<td class="r-label"><label for="CName"><font color=red>*</font>合同名称</label></td>
					<td>
						<input class="hisui-validatebox" type="text" id="CName"/>
					</td>
					
				</tr>
				<tr>
					<td class="r-label"><label for="CNo"><font color=red>*</font>合同编号</label></td>
					<td>
						<input class="hisui-validatebox" type="text" id="CNo"/>
					</td>
				</tr>
	
				<tr>
					<td class="r-label"><label for="Remark">备注</label></td>
					<td>
						<textarea id="Remark" class="textbox" style="width:150px; height:80px;"> </textarea>
					</td>
							
					<td><input type="hidden" id="ID"/></td>
				</tr>
	             								
			</table >
		</form>
	</div>
    <!--数据面板工具栏-->
    <div id="allbar" style='padding-top:4px'>
  
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true" id="add_btn">新增</a>
    	<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true" id="update_btn">修改</a> 
    </div> 
<script type="text/javascript" src="../scripts/DHCPEContract.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/DHCPECommon.hisui.js"></script>
</body>		
</html>
			
		
 



