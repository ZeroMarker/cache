<!--灭菌进锅-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:0px">
		<div class="hisui-panel" data-options="title:'灭菌单信息',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="UomTB">
				<div> 
					<table class="Condition" id="MainCondition">
						<input id="SelReqInciId" name="InciId" value="" type="hidden">
						<tr>
							<td><label>起始日期</label><input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq" style="width:118px"></td>
							<td><label>截止日期</label><input id="FEndDate" name="FEndDate" class="textbox hisui-dateboxq" style="width:118px"></td>
							<td><label>灭菌锅</label><input id="FMachineNo" name="FMachineNo" class="textbox hisui-combobox" style="width:100px"></td>
							<td><a href="#" id="FSuerBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:90px;">查询</a></td>
						</tr>
						<tr>
							<td>
								<label>灭菌锅号</label>
								<input class="textbox" id="MachineNo" name="MachineNo" style="width:110px">
								<input class="textbox" id="MachineNoValue" name="MachineNoValue" hidden>
								<input class="textbox" id="MachineNoSterType" name="MachineNoSterType" hidden></td>
							<td><label>灭菌车</label><input class="textbox" id="SterCar" name="SterCar" style="width:110px"><input class="textbox" hidden id="SterCarValue" name="SterCarValue"></td>
							<td><a href="#" id="BDTest" class="hisui-linkbutton" data-options="iconCls:''" style="width:100px">BD测试</a></td>
							<td><a href="#" id="BioTest" class="hisui-linkbutton" data-options="iconCls:''" style="width:90px">生物监测</a></td>
							<td><a href="#" id="SterRepeat" class="hisui-linkbutton" data-options="iconCls:''" style="width:90px">重新进锅</a></td>
						</tr>
						<tr>
							<td><label>灭菌程序</label><input class="textbox" id="SterPro" name="SterPro" style="width:110px"> <input class="textbox" id="SterProValue" name="SterProValue" hidden></td>
							<td>
								<label>灭菌人</label>
								<input class="textbox" id="SterUser" name="SterUser" style="width:110px">
								<input class="textbox" hidden id="SterUserValue" name="SterUserValue">
								<input class="textbox" id="BoiValue" name="BoiValue" hidden>
							</td>
							<td><a href="#" id="OKBT" class="hisui-linkbutton" data-options="iconCls:''" style="width:100px">确认进锅</a></td>
							<td><a href="#" id="SterOver" class="hisui-linkbutton" data-options="iconCls:''" style="width:90px">灭菌完成</a></td>
						</tr>
					</table>
				</div>
			</div>
			<table id="MainList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:450px;padding-left:10px">
		<div class="hisui-panel" data-options="title:'灭菌详情',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="IpuntTB" > 
				<div class="BorderLine">
					<table class="Condition">
						<tr>
							<td><label>条码</label><input class="hisui-numberbox" id="BarCode" name="BarCode"  style="width:200px"></td>
						</tr>
					</table>
				</div>
				<a href="#" id="AddItemBT" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">待灭菌包</a>
			</div>
			<table id="ItemList"></table>
		</div>
	</div>
<CSP:INCLUDE PAGE="cssdhui.packagesterilize.selbarcode.csp?type=car">
<script type="text/javascript" src="../scripts/cssd/Sterilize/SterCommon.js"></script>
<script type="text/javascript" src="../scripts/cssd/Sterilize/SterIn.js"></script>
</body>
</html>