<div class="hisui-layout" data-options="fit:true,border:false">
	<div id="dregion" data-options="region:'center',border:false,split:false" style="padding:10px 10px 10px 10px;">
		<div id="dpan" title="治疗站个人工作量报表" class="hisui-panel" style="padding:0px"
		data-options="border:true,iconCls:'icon-edit',headerCls:'panel-header-gray',fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false">   
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',split:false,border:false" style="width:100%;height:50px">
					<div id="p1" class="hisui-panel" style="padding:10px" 
					data-options="fit:true,border:false,closable:false, collapsible:false,minimizable:false,maximizable:false">  
						<label class="first-qlab" for="StartDate">开始日期</label><input id="StartDate" type="text" class="hisui-datebox" style="width:130px">
						<label class="qlab" for="EndDate">结束日期</label><input id="EndDate" type="text" class="hisui-datebox" style="width:130px">
