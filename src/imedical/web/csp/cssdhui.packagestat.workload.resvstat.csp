<!--消毒包接收工作量-->
<!DOCTYPE html>
<html lang="zh-CN">
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<SERVER>
    d ##class(web.CSSDHUI.Util.RQServerConfig).GetApplet()
</SERVER>
<head>
    <title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>
    <CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
    <div class="GridPadding" style="width:300px;" data-options="region:'west',border:false" >
        <div class="hisui-panel" data-options="title:'查询条件',headerCls:'panel-header-gray',fit:true,iconCls:'icon-paper'">
            <div class="hisui-layout" data-options="fit:true">
                <div data-options="region:'center',border:false">
                    <table id="Conditions" class="ReportCondition"  align="center">
                        <tr>
                            <td><label class="required">*</label><label>起始日期</label></td><td><input id="StartDate" name="StartDate" class="textbox hisui-dateboxq" style="width:155px;"></td>
                        </tr>
                        <tr>
                            <td><label>起始时间</label></td><td><input id="StartTime" name="StartTime" class=" textbox hisui-timespinner" style="width:155px;"></td>
                        </tr>
                        <tr>
                            <td><label class="required">*</label><label>截止日期</label></td><td><input id="EndDate" name="EndDate" class="textbox hisui-dateboxq" style="width:155px;"></td>
                        </tr>
                        <tr>
                            <td><label>截止时间</label></td><td><input id="EndTime" name="EndTime" class=" textbox hisui-timespinner" style="width:155px;"></td>
                        </tr>
                        <tr>
                            <td><label>供应科室</label></td><td><input id="Loc" name="Loc" class="textbox hisui-combobox" style="width:155px;"></td>
                        </tr>
                        <tr>
                            <td><label>接收科室</label></td><td><input id="SedLoc" name="SedLoc" class="textbox hisui-combobox" style="width:155px;"></td>
                        </tr>
						<tr>
							<td><label>消毒包分类</label></td><td align="left"><input id="PackageClass" name="PackageClass" class="textbox hisui-combobox" style="width:155px;"></td>
						</tr>
                        <tr>
                            <td><label>汇总类型</label></td><td align="left"><input class='hisui-radio' type="radio" label='按消毒包汇总' name='ReportType' value='FlagPackage' data-options='checked:true'></td>
                        </tr>
                        <tr>
                            <td></td><td align="left"><input class='hisui-radio' type="radio" label='按科室汇总' name='ReportType' value='FlagLoc'></td>
                        </tr>
                    </table>
                    <table class="Condition" align="center">
                         <tr>
                            <td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
                            <td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="hisui-tabs tabs-gray GridPadding" data-options="region:'center'" id="tabs" style="border:0px;padding-left: 0px">
        <div title="报表" data-options="iconCls:'icon-add-note'" class="ReportImage"></div>
    </div>
    
    <script type="text/javascript" src="../scripts/cssd/Common/encode.js"></script>
    <script type="text/javascript" src="../scripts/cssd/Stat/RevStat.js"></script>
</body>
</html>