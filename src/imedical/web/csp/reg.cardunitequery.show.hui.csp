<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false" style="height:95px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'卡合并查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="StartDate">开始日期</label></td>
					<td>
						<input id="StartDate" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label r-label-1"><label for="EndDate">结束日期</label></td>
					<td>
						<input id="EndDate" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label r-label-1"><label for="Name">姓名</label></td>
					<td >
						<input id="Name"  class="textbox">
					</td>
					<td class="r-label r-label-1">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					<td style="padding-left:20px;">
						<a id="BCancelUnit" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-back'">撤销合并</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="CardUniteQueryTab"></table>
		</div>
	</div>
</div>
