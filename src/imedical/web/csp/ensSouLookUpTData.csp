			<thead>
				<tr style="">
					<th data-options="field:'rowId'">序号</th>
					<th data-options="field:'sourceCode'">源数据代码</th>
					<th data-options="field:'sourceDesc'">源数据名称</th>
				</tr>
			</thead>
		</table>
	</div>
	<div id="sourceLookUpMm" class="easyui-menu">
		<div id="deleteSourceLookUpData" onclick="deleteSourceLookUpData()" data-options="iconCls:'icon-add'">删除</div>
	</div>
