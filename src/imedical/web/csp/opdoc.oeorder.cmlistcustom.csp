w "<input id='ARCOSRowId' type='hidden' value=''>"
w "<input id='PrescEmergency' type='hidden' value=''>"
w "<input id='StartDate' type='hidden' value=''><input id='EndDate' type='hidden' value=''>"

 ///加载合理用药,初始化;
