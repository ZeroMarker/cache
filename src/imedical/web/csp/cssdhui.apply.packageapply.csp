<!--请领-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<SERVER>
	d ##class(web.DHCXMLPConfig).LODOPInit()  //Lodop打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:0px;padding-bottom:15px">
		<div class="hisui-panel" data-options="title:'请领单据',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="CondTB">
				<table class="Condition" id="MainCondition">
					<tr>
						<td>申请科室</td>
						<td><input id="ReqLoc" name="FReqLoc" class="textbox hisui-combobox" style="width:130px"></td>
						<td>紧急程度</td>
						<td>
			            	<select name="ReqUrgent" class="hisui-combobox" style="width:130px">
			            		<option value="" >全部</option>
								<option value="0" >一般</option>
								<option value="1" >紧急</option>
							</select>
						</td>
						<td><label>单据号</label><input id="FReqNo" name="FReqNo" class="hisui-numberbox textbox" style="width:130px"></td>
					</tr>
					<tr>
						<td>供应科室</td>
						<td><input id="SupLoc" name="FSupLoc" class="textbox hisui-combobox" style="width:130px"></td>
						<td>单据类型</td>
						<td>
							<select name="ReqType" class="hisui-combobox" style="width:130px">
								<option value="" >全部</option>
								<option value="0" >请领单</option>
								<option value="1" >借包单</option>
								<option value="2" >敷料申请单</option>
								<option value="3" >手术申请单</option>
							</select>					
						</td>
						<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:130px">查询</a></td>
						<!-- <td><a href="#" id="SaveBT" class="textbox hisui-linkbutton" data-options="iconCls:'icon-w-add'" style="width:90px;">新增</a></td> -->
					</tr>
					<tr>
						<td>开始日期</td>
						<td><input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq" style="width:123px"></td>
						<td>结束日期</td>
						<td><input id="FEndDate" name="FEndDate" class="textbox hisui-dateboxq" style="width:123px"></td>
						<td><a href="#" id="Print" class="textbox hisui-linkbutton" data-options="iconCls:'icon-w-print'" style="width:130px">打印请领单</a></td>
					</tr>	
				</table>
				<table>
					<tr>
						<td><a href="#" id="AddBT" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">新增</a></td>
						<td><a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">保存</a></td>
						<td><a href="#" id="DeleteBT" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true">删除</a></td>
						<td><a href="#" id="ReturnBT" class="hisui-linkbutton" data-options="iconCls:'icon-return',plain:true">归还</a></td>
						<td hidden><a href="#" id="SaveMoudleBT" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">设置模板</a></td>
						<td hidden><a href="#" id="ApplyMoudleBT" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">选取请领模板</a></td>
					</tr>
					<div class="BorderLine"></div>	
				</table>
			</div>
				<table id="MainList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:560px;padding-left:10px;padding-bottom:15px">	
		<div class="hisui-panel" data-options="title:'单据明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<table id="ItemList"></table>
		</div>
	</div>
<!--选取请求单模板Dialog-->
	<div id="SelReqWin" class="hisui-dialog FindWin" data-options="closed:'true',modal:true,iconCls:'icon-w-paper'" title="选取请领单">
		<div class="hisui-layout" data-options="fit:true">
			<div data-options="region:'north',border:false">
				<table id="SelReqConditions" class="Condition">
					<tr>
						<td><a href="#" id="SelApplyMoudleBT" class="hisui-linkbutton" style="width:116px;">选取请领模板</a></td>
					</tr>
				</table>
			</div>
			<div class="GridPadding" style="width:560px;padding-top:0;" data-options="region:'west',border:false" >
				<div class="hisui-panel" data-options="title:'请求单',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
					<table id="SelReqMasterGrid"></table>
				</div>
			</div>
			<div class="GridPadding" style="padding-top:0;padding-left:0;" data-options="region:'center',border:false" >
				<div class="hisui-panel" data-options="title:'单据明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
					<table id="SelReqDetailGrid"></table>
				</div>
			</div>
		</div>
	</div>
<script type="text/javascript" src="../scripts/cssd/Apply/ApplyCommon.js"></script>	
<script type="text/javascript" src="../scripts/cssd/Apply/PackageApply.js"></script>
<script type="text/javascript" src="../scripts/cssd/Common/commonPrint.js"></script>
<script type="text/javascript" src="../scripts/cssd/Apply/ApplyPrint.js"></script>
<script type="text/javascript" src="../scripts/cssd/Apply/ApplyMoudle.js"></script>
</body>
</html>