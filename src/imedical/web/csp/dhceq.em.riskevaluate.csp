<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>
<script type="text/javascript" src="../scripts/dhceq/em/riskevaluate.js"></script>
<SCRIPT LANGUAGE=CACHE RUNAT=SERVER>
s SourceType=$g(%request.Data("SourceType",1))
s SourceID=$g(%request.Data("SourceID",1))
s RowID=$g(%request.Data("RowID",1))
s Name=$g(%request.Data("Name",1))
s ReadOnly=$g(%request.Data("ReadOnly",1))
</SCRIPT>
</head>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px">
		<div class="hisui-panel"  title="医疗设备风险等级评估" data-options="fit:true,eqtitle:'maintitle'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:dashed 1px #ccc;height:80px;">
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cName" for="Name">名称</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox"  disabled="true" id="Name" value=#(Name)#></div>
					<div class="eq-table-td eq-table-label"><label id="cResult" for="Result">风险等级</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" disabled="true" id="Result" ></div>
					<div class="eq-table-td eq-table-label"><a id="BEvaluate" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">评估</a></div>
					<div class="eq-table-td eq-table-label"><a id="BPreEvaluate" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-list'">历史评估</a></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label for="Score">总评分</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" disabled="true" id="Score" ></div>
					<div class="eq-table-td eq-table-label"><label for="Remark">备注</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Remark" ></div>
					<!--modify by lmm 2020-06-05-->
					<div class="eq-table-td"></div>
					<div class="eq-table-td eq-table-label"><a id="BReEvaluate" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">再评估</a></div>
				</div>
				</div>
				<div data-options="region:'center',border:false" style="padding:0px;position:relative">
					<table id="tDHCEQRiskEvaluate" data-options="border:false,fit:true"></table>
				
				</div>
			</div>
		</div>
	</div>
	<div id="hiddenEncrypt" style="width:0;height:0;">
		<form name="DHCEQ">
			
			<INPUT TYPE="hidden" id="RowID" value=#(RowID)#>
			<input type="hidden" id="SourceType" name="SourceType" value=#(SourceType)#>
	        <input type="hidden" id="SourceID" name="SourceID"  value=#(SourceID)#>
	        <input type="hidden" id="ReadOnly" name="ReadOnly" value=#(ReadOnly)#>
	        <input type="hidden" id="RiskGradeDR" name="RiskGradeDR">
		</form>
	</div>
</body>

</html>
