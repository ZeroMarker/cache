						<textarea id="Desc" rows = "1" style="overflow:hidden"></textarea>
					</td>
				</tr>
				<tr>
					<td style="float:right">编码：</td>
					<td>
						<!--<input id="Code" class="easyui-textbox" type="text" data-options="required:true"></input>-->
