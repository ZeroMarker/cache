<!DOCTYPE html>
<!--
    药房药库-移动-菜单字典
	csp:pha.mob.v2.menu.csp
	js: pha/mob/v2/menu.js
-->
<html>
<!-- 验证session过期 -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    d ##Class(websys.SessionEvents).SessionExpired() q 1
</csp:method>

<head>
    <!-- iMedical版本标题 -->
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>
    </TRAK:HEAD>
    <HISUI />
    <PHALIBV1 />
</head>

<body>       
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',border:false" class="pha-body">
            <div class="hisui-layout" fit="true">
                <div data-options="region:'center',border:false">
                    <div class="hisui-panel" title="" data-options="headerCls:'panel-header-gray',fit:true,bodyCls:'panel-body-gray'">
                        <table id="gridMenu"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- 表格工具栏 -->
    <div id="gridMenuBar">
        <a class="hisui-linkbutton" plain='true' iconCls="icon-add" id="btnAdd">新增</a>
        <a class="hisui-linkbutton" plain='true' iconCls="icon-save" id="btnSave">保存</a>
        <a class="hisui-linkbutton" plain='true' iconCls="icon-remove" id="btnDel">删除</a>
    </div>
    <!-- window dialog -->
    <!-- 本页面js -->
    <script type="text/javascript" src="../scripts/pha/mob/v2/menu.js"></script>
</body>

</html>