<!-- dhcckb.libsoupriority.csp -->
<html >
<head>
<title>知识来源目录优先级配置</title>
<HISUI/>
<DHCCKB/>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/component.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/libsoupriority.js"></script>
	<style type="text/css">
		.datagrid-row-selected {
	  		background: #ADFAFC;
		}
		.panel-header, .panel-body {
		    border-color: #ccc;
		}
   </style>
</head>

<body>	
	<div class="hisui-layout" fit="true">
		<!--查询条件区-->
		<div data-options="region:'west',title:''" style="width:450px;padding:10px 10px 10px 10px;" border=0>					
			<div class="hisui-panel" data-options="title:' ',fit:true,headerCls:'panel-header-gray'" style="border:1px solid #ccc;border-top:0;border-radius:0px;" >	
				<div id="griddiv" style="height:100%;">
					<div id="diclist" toolbar="#dictb"></div>
				</div>
	    	</div>						
		</div>
		
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray'" style="padding:10px 10px 10px 10px;" border=0>
			<div class="hisui-panel" data-options="title:'  ',fit:true,headerCls:'panel-header-gray'" style="border:1px solid #ccc;border-top:0;border-radius:0px;">	
				  <div title="知识来源配置" style="padding:0px;">   
					<iframe scrolling="yes" frameborder="0" src="" style="width:100%;height:100%;" id="saveTreeDic"></iframe> 
				 </div> 
			</div>	
	    </div>		
	</div> 		
<div id="dictb">	
	<div id="queryForm" style="margin:10 10 10 10;">
		<table cellspacing="0" cellpadding="0" >
			
			<tr>
				<td style="padding-left:0px">名称<input id="queryCode" href="#" class="hisui-searchbox" style="width:150px;" prompt='请输入代码/名称/拼音码'/> </td>
				<td class="tdlabel-b"><a style="margin-left:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" id="find" onclick='QueryDicList()'>查询</a> </td>
				<td class="tdlabel-b"><a style="margin-left:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="reset" onclick='InitPageInfo()'>重置</a> </td>					
			</tr>
			
		</table>
	</div>
	<div style="margin-top:10px;border-bottom-color:#cccccc;border-bottom-width:1px; border-bottom-style:dashed;">	</div>
</div>
</body>
</html>