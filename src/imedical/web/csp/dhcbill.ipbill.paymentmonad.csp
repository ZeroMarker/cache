 * Description: 住院押金催款单
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<style>
		.td-first-col {
			width: 30px;
		}
	</style>
	<script type="text/javascript" src="../scripts/DHCCPMRQCommon.js"></script>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true,border:false">
		<div data-options="region:'center',border:false">
			<div class="hisui-panel" data-options="title:'押金催款单',fit:true,headerCls:'panel-header-gray',iconCls:'icon-apply-check'">
				<table class="hisui-datagrid" id="paymentmonadList" data-options="toolbar:'#toolbar'"></table>
			</div>
			<div id="toolbar">
				<table class="search-table" style="width:85%">
					<tr>
						<td class="r-label td-first-col"><label>病区</label></td>
						<td><input id="menu-ward" class="textbox"/></td>
						<td class="r-label"><label>费别</label></td>
						<td><input id="menu-feetype" class="textbox"/></td>
						<td class="r-label"><label>余额</label></td>
						<td><input id="menu-balance" class="hisui-numberbox textbox" data-options="precision:2,validType:['checkMinAmt','checkMaxAmt']"/></td>
						<td><a href="javascript:;" class="hisui-linkbutton" id="btnSearch" data-options="iconCls:'icon-w-find'">查询</a></td>
						<td><a href="javascript:;" class="hisui-linkbutton" id="btnExport" data-options="iconCls:'icon-w-export'">导出</a></td>
						<td><a href="javascript:;" class="hisui-linkbutton" id="btnPrint" data-options="iconCls:'icon-w-print'">打印1</a></td>
						<td><a href="javascript:;" class="hisui-linkbutton" id="btnPrint2" data-options="iconCls:'icon-w-print'">打印2</a></td>
					</tr>
				</table>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.paymentmonad.js"></script>
</body>
</html>