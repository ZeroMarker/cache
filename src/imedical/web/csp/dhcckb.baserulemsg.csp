<html>
<!--dhcckb.checkrule.csp -->
<head>
	<HISUI/>
	<DHCCKB/>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/component.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/baserulemsg.js"></script>	
</head>
	<style>
	.datagrid {
	    border-radius: 4px 4px 4px 4px;
	    border: 1px solid #ccc;
	}
	.tdlabel		{
		  padding-left: 10px;
		  padding-right: 10px;
		  text-align:right;
		}
		/*弹出框里标签左对齐 并且有10px内边距*/
		.tdleftlabel		{
		  padding-left: 10px;
		  padding-right: 10px;
		  text-align:left;
		}
		.tdlabel-b		{
		  padding-left: 10px;
		  text-align:right;
		}
	</style>
<body>
	
<div class="hisui-layout" fit="true">
	<div data-options="region:'center',title:'',border:false,collapsible:false">
		<div id="errmsg" toolbar="#toolbar" ></div> <!-- toolbar="#toolbar" -->
	</div>
</div>
<div id="toolbar" class="datagrid-toolbar">
	<div id="queryForm" style="margin:10 0 10 10;">
		<table cellspacing="0" cellpadding="0" >
			<tr>
			 	<td style="padding-left:10px;padding-right:10px">药品名称</td>
				<td>
					<input id="drugName" class="hisui-validatebox textbox"/>
				</td>
				<td style="padding-left:10px;padding-right:10px">拒绝原因</td>
				<td>
					<input type="text" id="result" class="hisui-combobox"></input> 
				</td>				
				<td><a style="margin-left:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" id="find">查询</a> </td>
				<td><a style="margin-left:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" id="reset">重置</a> </td>					
			
			</tr>								
		</table>
	</div>
</div>
</body>
</html>
