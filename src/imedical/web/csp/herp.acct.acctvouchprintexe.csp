	//´òÓ¡
	i action = "vouchPrint" d
	.s result= ##class(herp.acct.udata.uAcctVouchPrint).vouchPrint(VouchID,userid)
	.i result '= "" d
	..w "{success:'true',refresh:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'´òÓ¡Ê§°Ü£¡'}"

	//Ì×´ò
	i action = "print" d
	.;s ^aa=userCode
