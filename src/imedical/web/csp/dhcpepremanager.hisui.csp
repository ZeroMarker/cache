<!--dhcpepremanager.hisui.csp-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	/*padding: 0 10px;*/
}
.r-label{
	padding-left: 10px;
}
.r-label-r{
	padding-right: 5px;
}
.newclsInvalid{
	border-color: red;
	color: red;
}
.panel-header{
	border-bottom: 0;
}


</style>
<SCRIPT language="Cache" RUNAT="SERVER"> 
 s DateStr=$g(%request.Data("DateStr",1))  
 
</SCRIPT> 
<script type="text/javascript">
	var DateStr="#(DateStr)#"
	
	
</script>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:62px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:'',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:4px 4px 0 0;">
			<table class="search-table">
				<tr>
				  <td class="r-label"><label for="DateStr">预约管理</label></td>
					<td class="r-label-r">
						<input class="textbox" id="DateStr" disabled>
					</td>
					<td class="r-label"><label for="NewDate">替换日期</label></td>
					<td class="r-label-r">
						<input class="hisui-datebox" type="text" id="NewDate">
					</td>
					
					<td class="r-label">
						<a id="BCreate" href="#" class="hisui-linkbutton">复制</a>
					</td>
					<td class="r-label">
						<a id="BReplace" href="#" class="hisui-linkbutton">替换</a>
					</td>
					<td class="r-label">
						<a id="BUpdate" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-update'">更新</a>
					</td>
					
					
				</tr>
				
					
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="dhcpepremanagerQueryTab"></table>
				</div>
        	</div>
</div>

<script type="text/javascript" src="../scripts/DHCPEPreManager.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
  
  
</body>		
</html>
			
		
