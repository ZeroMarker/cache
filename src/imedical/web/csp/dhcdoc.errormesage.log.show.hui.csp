<div data-options="region:'north',split:false,border:false" style="height:91px;border-bottom:1px solid #ccc;">
	<table class='search-table'>	
		<tr>
			<td class="r-label"><label class="clsRequired">开始日期</label></td>
			<td ><input id="StartDate" type="text" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser"></td>
			
			<td class="r-label"><label class="clsRequired">结束日期</label></td>
			<td ><input id="EndDate" type="text" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser"></td>
			
			<td class='r-label'><label>调用类</label></td>
			<td><input id="CallClass" class="textbox" name="CallClass"/></td>
			
			<td class='r-label'><label>调用方法</label></td>
			<td><input id="CallMeth" class="textbox" name="CallMeth"/></td>	
			<td class="r-label"><a href="#" id="Find" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查找</a></td>		
		</tr>
		<tr>
			<td class='r-label'><label>日志描述</label></td>
			<td><input id="LogDesc" class="textbox" name="LogDesc"/></td>
			
			<td class='r-label'><label>主键</label></td>
			<td><input id="LogKey" class="textbox" name="LogKey"/></td>
			
			<td class='r-label'><label>日志记录内容</label></td>
			<td><input id="LogValue" class="textbox" name="LogValue"/></td>
			<td></td>
		</tr>
	</table>
</div>   
<div data-options="region:'center',split:false,border:false" > 
	<table id='errormesagetab'></table>
</div>