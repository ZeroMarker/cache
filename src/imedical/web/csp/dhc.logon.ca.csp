<script type="text/javascript">
/// CALogonUserName-ca登录后的用户名，currUserName为会话中的用户或登录界面用户框中的用户名
var CALogonUserName="",CALogonUserId="",currUserName="",faceSureTime=5,loadCALogonTypeEndTimer="";
