			<CSP:IF condition=OriginLogonType="SOUND">
			<server>
			if ##class(websys.Conversions).IsValidClassName("websys.Sound"){
				d ##class(websys.Sound).Init("HISLogon",1,"HIS","1.0")
			}
			</server>
			<TR class="soundlogon">
				<td class="bgleft">
					<img/>
