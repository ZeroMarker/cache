<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI/>
<DHCEQ:HISUI></DHCEQ:HISUI>
</HEAD>
<script language=Cache runat=server>
	s LocID=##Class(web.DHCEQCommon).getMapIDBySource("dept",%session.Get("LOGON.CTLOCID"))
	s LocType=##class("web.DHCEQ.RM.BUSShareResource").CheckLocType(LocID)
	s StatusDR=$Get(%request.Data("StatusDR",1))  //Add by QW20200512
</Script>
<body class="hisui-layout">
	<div id='Loading' class="eq-loading"><h1><img src="../skin/default/images/loading.gif"><font>加载中・・・</font></h1></div>
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="eqtitle:'maintitle',fit:true,title:'上架设置'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #ccc;padding:5px 0 10px 0;"> <!--Modify by QW20200506 资源项begin-->
					<div class="eq-table">
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cPOSShareType" for="POSShareType">资源类型</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSShareType" data-required="true" data-options="prompt:'必填项'"></div> <!--Modify by QW20200506 Bug:QW0058 需求号:1294296-->
							<div class="eq-table-td eq-table-label"><label id="cPOSEquipDR_EQName" for="POSEquipDR_EQName">资产名称</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSEquipDR_EQName" data-options="'component':'EM.L.Equip'"></div>
							 <!--Modify by QW20200506 资源项begin-->
							<div class="eq-table-td eq-table-label"><label id="cPOSShareItemDR_SIDesc" for="POSShareItemDR_SIDesc">资源项</label></div>
							<div class="eq-table-td "><input class="hisui-validatebox" id="POSShareItemDR_SIDesc" data-options="'component':'RM.L.ShareItem'"></div>
							<!--
							<div class="eq-table-td eq-table-label"><label id="cPOSSub" for="POSSub">辅助信息</label></div>
							<div class="eq-table-td "><input class="hisui-validatebox" id="POSSub" data-options="'component':'PLAT.L.BuildingUnit'"></div>-->
							 <!--Modify by QW20200506 资源项end-->
							<div class="eq-table-td eq-table-label"><label id="cPOSFromDate" for="POSFromDate">设置开始日期</label></div>
							<div class="eq-table-td "><input class="hisui-datebox textbox" id="POSFromDate"></div>
							<!--Modify by QW20200506 修正按钮 begin-->
							<div class="eq-table-td eq-table-label"><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">保存</a></div>
							<div class="eq-table-td eq-table-label"><a id="BAudit" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">审核</a></div>
							<!--Modify by QW20200506 end-->
						</div>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cPOSItemDR_MIDesc" for="POSItemDR_MIDesc">设备项</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSItemDR_MIDesc" data-options="'component':'EM.L.GetMasterItem'"></div>
							<div class="eq-table-td eq-table-label"><label id="cPOSModelDR_MDesc" for="POSModelDR_MDesc">规格型号</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSModelDR_MDesc" data-options="'component':'PLAT.L.Model'"></div>
							<div class="eq-table-td eq-table-label"><label id="cPOSLocDR_DeptDesc" for="POSLocDR_DeptDesc">科室</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSLocDR_DeptDesc" data-options="'component':'PLAT.L.Loc'"></div>
							<div class="eq-table-td eq-table-label"><label id="cPOSToDate" for="POSToDate">设置结束日期</label></div>
							<div class="eq-table-td "><input class="hisui-datebox textbox" id="POSToDate"></div>
							<!--Modify by QW20200506 修正按钮 begin-->
							<div class="eq-table-td eq-table-label"><a id="BFind" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">查询</a></div>
							<div class="eq-table-td eq-table-label"><a id="BCancel" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">无效</a></div>
							<!--Modify by QW20200506 end-->
						</div>
						<div class="eq-table-tr">
						 	<!--Modify by QW20200506 去掉院区增加状态end-->
							<!--div class="eq-table-td eq-table-label"><label id="cPOSHospitalDR_HOSPDesc" for="POSHospitalDR_HOSPDesc">院区</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSHospitalDR_HOSPDesc" data-options="'component':'PLAT.L.Hospital'"></div-->
							<div class="eq-table-td eq-table-label"><label id="cPOSStatus" for="POSStatus">状态</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSStatus"></div>
							 <!--Modify by QW20200506 去掉院区增加状态end-->
							<div class="eq-table-td eq-table-label"><label id="cPOSManageLocDR_DeptDesc" for="POSManageLocDR_DeptDesc">管理科室</label></div>
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSManageLocDR_DeptDesc" data-options="'component':'PLAT.L.Loc'"></div>
							<div class="eq-table-td eq-table-label"><label id="cPOSOuterType" for="POSOuterType">共享方式</label></div> <!--Modify by QW20200506 修改描述begin-->
							<div class="eq-table-td"><input class="hisui-validatebox" id="POSOuterType"></div>
							<div class="eq-table-td eq-table-label"><label id="cPOSAutoPutOnFlag" for="POSAutoPutOnFlag">是否自动上架</label></div>
							<div class="eq-table-td"><input type="checkbox" class='hisui-checkbox' id="POSAutoPutOnFlag"></div>
						</div>
					</div>
				</div>
				<div data-options="region:'center',border:false" style="position:relative;">
					 <table id="tDHCEQPutOnSet" class="hisui-datagrid" ></table>
					 <span id="sumTotal" class="eq-total-sum" style="position:absolute;right:10px;top:5px;">合计</span> <!--Add by QW20200506 增加合计-->
					 <span id="ShareItemWarn" class="eq-total-sum" style="position:absolute;left:10px;top:5px;"></span> <!--Modify by zx 20200609 BUG ZX0100-->
				</div>
			</div>
		</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="LocType" value=#(LocType)#>
			<INPUT type="hidden" id="Type">
			<INPUT type="hidden" id="DRUseLocDR">
			<INPUT type="hidden" id="POSRowID">
			<INPUT type="hidden" id="POSEquipDR">
			<INPUT type="hidden" id="POSSubID">
			<INPUT type="hidden" id="POSItemDR">
			<INPUT type="hidden" id="EQItemDR"> <!--列定义参数所需-->
			<INPUT type="hidden" id="POSModelDR">
			<INPUT type="hidden" id="POSLocDR">
			<INPUT type="hidden" id="POSHospitalDR">
			<INPUT type="hidden" id="POSManageLocDR">
			<INPUT type="hidden" id="DREquipTypeDR"> <!--列定义参数所需-->
			<INPUT type="hidden" id="POSShareItemDR"> <!--Add by QW20200506 资源项-->
			<INPUT type="hidden" id="StatusDR" value=#(StatusDR)#> <!--Add by QW20200512 状态-->
		</form>
	</div>
	<script type="text/javascript" src="../scripts/dhceq/rm/putonset.js"></script>
</body>
</html>

