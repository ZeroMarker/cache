<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<HTML lang="zh-CN">
<HEAD>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE><EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI></HISUI>
<ADDINS require="RWCard"/>
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
}
.r-label{
	padding-left: 10px;
}
.timerangediv{
	padding:0px;
	width: 98px;
	height: 78px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	font-size: 14px;
	-moz-border-radius: 0 0 4px 4px;
	-webkit-border-radius: 0 0 4px 4px;
	border-color: #40a2de;
	background-color: #f0f9ff;
	border-top-width: 1px;
	overflow: auto;
	border-width: 1px;
	border-style: solid;
	color: #017bce;
	text-align: left;
} 
.timerangediv-disabled{
	padding:0px;
	width: 98px;
	height: 78px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	font-size: 14px;
	-moz-border-radius: 0 0 4px 4px;
	-webkit-border-radius: 0 0 4px 4px;
	border-color: #cccccc;
	background-color: #efefef;
	border-top-width: 1px;
	overflow: auto;
	border-width: 1px;
	border-style: solid;
	color: #666666;
	text-align: left;
} 
.timerangediv-select{
	padding:0px;
	width: 98px;
	height: 78px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	font-size: 14px;
	-moz-border-radius: 0 0 4px 4px;
	-webkit-border-radius: 0 0 4px 4px;
	border-style:none;
	border-color: #40a2de;
	background-color: #40a2de;
	border-top-width: 1px;
	overflow: auto;
	border-width: 1px;
	border-style: solid;
	color: #ffffff;
	text-align: left;
} 

/*分时段预约样式*/
.diytable{
	border-collapse:collapse;
	border:1px solid #ccc; 
}
.diytable td{
	text-align:center;
	word-break:keep-all;
	white-space:nowrap;
	padding:5px;
}
.td-time{
	color:black;
}
.td-seqno{
	font-weight:bold;
	color:#40A2DE;
	line-height:24px;
}
.td-seqno-invalid{
	background:#FFCFCF;
}
.td-timerange{
	color:black;
	font-weight:bold;
}
.diytable td[id*="_table_"]:hover{
	cursor:pointer;
	background:#FFCFCF;
}
.td-selecct{
	background-color: #40a2de;
}
.td-selecct span{
	color:#fff;
}
</style>	

<SCRIPT language="Cache" RUNAT="SERVER">
	d ##class(web.DHCXMLPConfig).LODOPInit()
	s RBASRowID=$g(%request.Data("RBASRowID",1))
	s PatientID=$g(%request.Data("PatientID",1))
	s AppMethCodeStr=$g(%request.Data("AppMethCodeStr",1))
	s CanNoCardApp=$g(%request.Data("CanNoCardApp",1))
	s sysDateFormat=##class(websys.Conversions).DateFormat()
	s PatNumLength=+$p(^CF("PATCF",1,3),"^",5)
	s:CanNoCardApp="" CanNoCardApp="N" ;是否可以进行无卡预约 Y/N
	s selectcolor="#FF5151" ;分时段预约 选中颜色
	d ##Class(web.DHCBillPrint).InvBillPrintCLSID()
	s PatNo=""
	s:PatientID'="" PatNo=$p($G(^PAPER(PatientID,"PAT",1)),"^",2)
	
	w "<input id='XMLObject' name='XMLObject' type='hidden' value='"_##Class(%CSP.Page).Encrypt($lb("web.DHCXMLIO.ReadXML"))_"'>",$C(13,10)
	
	;读卡相关引用
	d ##class(web.UDHCCardCommLinkRegister).GetCardHardJSFunction()
	w "<input id='ReadAccExpEncrypt' name='ReadAccExpEncrypt' type='hidden' value='"_##Class(%CSP.Page).Encrypt($lb("web.UDHCAccManageCLSIF.getaccinfofromcardno"))_"'>",$C(13,10)
	w "<input id='CardTypeRowID' name='CardTypeRowID' type='hidden' value=''>",$C(13,10)
	w "<input id='GetCardTypeEncrypt' name='GetCardTypeEncrypt' type='hidden' value='"_##Class(%CSP.Page).Encrypt($lb("DHCDoc.OPDoc.PatientList.GessCardType"))_"'>",$C(13,10)
	w "<input id='ReadCardTypeEncrypt' name='ReadCardTypeEncrypt' type='hidden' value='"_##Class(%CSP.Page).Encrypt($lb("web.UDHCOPOtherLB.ReadCardTypeDefineListBroker"))_"'>",$C(13,10)

	
</Script> 

<SCRIPT language = 'javascript' >
		//全局请求后台服务对象
		var ServerObj={
			RBASRowID:"#(RBASRowID)#",
			PatientID:"#(PatientID)#",
			AppMethCodeStr:"#(AppMethCodeStr)#",
			PatNumLength:"#(PatNumLength)#",
			CanNoCardApp:"#(CanNoCardApp)#",
			selectcolor:"#(selectcolor)#",
			PatNo:"#(PatNo)#"
		};
	</SCRIPT>
 </head> 
