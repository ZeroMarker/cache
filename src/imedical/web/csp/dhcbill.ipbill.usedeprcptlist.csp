<!-- 
 * FileName: dhcbill.ipbill.usedeprcptlist.csp
 * Anchor: ZhYW
 * Date: 2019-12-06
 * Description: 收费员已使用押金收据查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<link rel='stylesheet' type='text/css' href='../scripts/dhcbill/themes/default/dhcbill.ipbill.usedeprcptlist.css'/>
	<script type="text/javascript" src="../scripts/DHCCPMRQCommon.js"></script>
	<style>
		.textbox{
			width:110px !important;	
		}
	</style>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',collapsible:false,title:'收费员已使用押金收据查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:87px;">
			<table class="search-table" style="width:95%">
				<tr>
					<td class="r-label td-first-col-58"><label>开始日期</label></td>
					<td><input id="stDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>结束日期</label></td>
					<td><input id="endDate" class="hisui-datebox textbox"/></td>
					<td class="r-label"><label>开始号段</label></td>
					<td><input id="stNo" class="textbox"/></td>
					<td class="r-label"><label>结束号段</label></td>
					<td><input id="endNo" class="textbox"/></td>
					<td class="r-label"><label>字母</label></td>
					<td><input id="invTitle" class="textbox"/></td>
					<td class="r-label">
						<a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a>
						<a href="javascript:;" class="hisui-linkbutton" id="btn-export" data-options="iconCls:'icon-w-export'" style="margin-left:15px;">导出</a>
					</td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'" style="border-radius:0 0 4px 4px;">
			<table class="hisui-datagrid" id="depRcptList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbill/dhcipbill/dhcbill.ipbill.usedeprcptlist.js"></script>
</body>
</html>
