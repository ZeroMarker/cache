						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.EquipType'" id="ISEquipTypeDR_ETDesc"></div>
						<div class="eq-table-td eq-table-label"><label id="cISInDate" for="ISInDate">制单日期</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="ISInDate"></div>
						<div class="eq-table-td eq-table-label"><label id="cISOriginDR_ODesc" for="ISOriginDR_ODesc">来源</label></div>
						<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Origin'" id="ISOriginDR_ODesc"></div>
						<!--modify by lmm 2019-05-29 892873-->
						<!--modify by lmm 2019-11-22 LMM0050-->
						<div class="eq-table-td eq-table-label15"><a id="BCancelSubmit" href="#" class="hisui-linkbutton" iconCls="icon-w-stamp" data-options="stopAllEventOnDisabled:true">退回</a></div>
						<div class="eq-table-td eq-table-label15"><a id="BApprove1" href="#" class="hisui-linkbutton" iconCls="icon-w-stamp" data-options="stopAllEventOnDisabled:true">审核1</a></div>
					</div>
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label" ><label for="ISRemark" >备注</label></div>
