<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>
<script language=Cache runat=server>
</script>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="eqtitle:'maintitle',fit:true,title:'部门科室对照查询'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:dashed 1px #e2e2e2;padding-bottom:10px;">
					<div class="eq-table">
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label-first"><label id="cCode" for="Code">科室代码</label></div>
							<div class="eq-table-td"><input class="textbox" id="Code" ></div>
							<div class="eq-table-td eq-table-label-new"><label id="cDesc" for="Desc">科室描述</label></div>
							<div class="eq-table-td"><input class="textbox" id="Desc"></div>
							<div class="eq-table-td eq-table-label-new"><label  for="DiffFlag">只显示差异</label></div>
							<div class="eq-table-td eq-table-input"><input  class='hisui-checkbox' type="checkbox" id="DiffFlag" ></div>
							<div class="eq-table-td eq-table-button"><a id="BFind" href="#" class="hisui-linkbutton" Style="margin-left:20px" iconCls="icon-w-find" data-options="stopAllEventOnDisabled:true">查询</a></div>
						</div>
					</div>
				</div>
				<div data-options="region:'center',border:false" style="position:relative;">
					<table class="hisui-datagrid" id="tLocDeptMapList" data-options="border:false,fit:true"></table>
				</div>
			</div>
		</div>
	</div>
	
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/plat/locdeptmaplist.js'></script>
</body>
</html>