<script type="text/javascript" src="../scripts/dhcadvEvt/fishbone.js" charset="gbk"></script>
<style media="print">   
	.pageNext{PAGE-BREAK-AFTER:always;}
	 .no-print { display: none; } 
	@page {
	  size: auto;  
	}
</style>
</head>
<body >
<div id="sample">
	<div id="myDiagramDiv" class='no-print' style="height:530px;width:100%;border:1px solid black;"></div>
	<div id="centerdiv"></div>
  <br />
 <!--点击打印按钮直接打印，不弹出打印设置参数窗体-->
 	<div style="display:none;">
 		<OBJECT ID='WebBrowser' NAME="WebBrowser" WIDTH=0 HEIGHT=0  CLASSID='CLSID:8856F961-340A-11D0-A96B-00C04FD705A2'></OBJECT>
    </div>
    <!--底部按钮  2021-04-11 cy 增加鱼骨图打印导出按钮-->
	<div class='no-print' id="footer" style="text-align:center;float:right;opacity:0.9;height:0px;position:fixed;left:0px;bottom:0px;z-index:2;margin-bottom:30px;">
		<button id="PrintBut" class="dhcc-btn-icon" style="margin-left:500px;">#(..Get("打印"))#</button> 
		<button id="ExpBut" class="dhcc-btn-icon" style="margin-left:50px;">#(..Get("导出"))#</button> 

    </div>
</div>
