<html>
<head>
	<title>电子病历综合查询</title>
	<HISUI/>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
	<link rel="stylesheet" type="text/css" href="../scripts/epr/css/query.css"/>
	 <script type="text/javascript" src="../scripts/epr/js/hisui.dateformat.js"></script>
	 <script type="text/javascript" language="javascript">
	 var saveType = '#($Get(%request.Data("saveType",1),""))#';
	 var dialogId = '#($Get(%request.Data("DialogId",1),"dialogDiv"))#';
	</script>
</head>
<body class="hisui-layout">
  	<div data-options="region:'north',border:false" style="height:55px;padding-top:5px;">
  		<div><label style="padding-left:6px;margin-right:3px;">#(..Get("方案名称"))#</label><input id="caseName" class="textbox" style="width:370px;"/></div>
  		<div><label style="padding-left:70px;color:red;">#(..Get("注意:方案名称中不能包含 逗号 及 @,#,$,%,^,&,* 等符号！"))#</label></div>
  	</div>
  	<div data-options="region:'center',border:false" style="padding:3px">
  		<div class="hisui-layout" data-options="fit:true">
  			<div data-options="region:'north',border:false" style="height:25px;">
		  		<div><input class="hisui-checkbox" id="oneselfCR" label="个人可见" type="checkbox" data-options="checked:true,disabled:true"/></div>
		  	</div>
		  	<div data-options="region:'center',border:false" style="width:100%;">
		  		<div class="hisui-layout" data-options="fit:true">
		  			<!--科室-->
		  			<div data-options="region:'west',border:true" style="width:300px;">
		  				<div class="hisui-layout" data-options="fit:true">
		  					<div data-options="region:'west',border:false" style="width:120px;">
				  				<table id="CTLocGrid"></table>
					  		</div>
					  		<div data-options="region:'center',border:false" style="width:56px;">
				  				<a href="#" id="btCTLocAdd" class="hisui-linkbutton" style="margin-top:100px;">增加</a>
								<a href="#" id="btCTLocDelete" class="hisui-linkbutton" style="margin-top:100px;">撤回</a>
					  		</div>
					  		<div data-options="region:'east',border:false" style="width:120px;">
				  				<table id="CTLocCRGrid"></table>
					  		</div>
		  				</div>
			  		</div>
			  		<!--安全组-->
			  		<div data-options="region:'center',border:true">
			  			<div class="hisui-layout" data-options="fit:true">
		  					<div data-options="region:'west',border:false" style="width:122px;">
				  				<table id="GroupGrid"></table>
					  		</div>
					  		<div data-options="region:'center',border:false" style="width:56px;">
				  				<a href="#" id="btGroupAdd" class="hisui-linkbutton" style="margin-top:100px;">增加</a>
								<a href="#" id="btGroupDelete" class="hisui-linkbutton" style="margin-top:100px;">撤回</a>
					  		</div>
					  		<div data-options="region:'east',border:false" style="width:121px;">
				  				<table id="GroupCRGrid"></table>
					  		</div>
		  				</div>
			  		</div>
		  		</div>
		  	</div>
  		</div>
  	</div>
  	<div data-options="region:'south',border:false" style="height:33px;padding:0  3px 0 3px;">
  		<a href="#" id="btCancel" class="hisui-linkbutton fright">取消</a>
		<a href="#" id="btSave" class="hisui-linkbutton fright">保存</a>
		
  	</div>
  	
    <script type="text/javascript" src="../scripts/epr/js/query.hisui.popwincase.js"></script>
</body>
</html>