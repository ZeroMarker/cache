<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',iconCls:'icon-drug-audit',collapsible:false,headerCls:'panel-header-gray'" title="化疗方案审核" style="height:90px;border-bottom:1px dashed #ccc;padding:10px;">
		<span class="c-span">#(..Get("开始日期"))#</span>
		<input class="textbox hisui-datebox" id="startDate"/>
		<span class="c-span2"></span>
		<span class="c-span">#(..Get("结束日期"))#</span>
		<input class="textbox hisui-datebox" id="endDate"/>
		<span class="c-span2"></span>
		<span class="c-span">#(..Get("申请科室"))#</span>
		<input class="hisui-combobox textbox" id="RLocdesc"/>
		<span class="c-span2"></span>
		<input class="hisui-checkbox" type="checkbox" label="#(..Get("已审"))#" id="HasPass">
		<span class="c-span2"></span>
		<a id="Find" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
		<span class="c-span2"></span>
		<a id="Clear" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
	</div> 
	<div data-options="region:'center'">
		<div id="i-list"></div>
	</div> 
</div>