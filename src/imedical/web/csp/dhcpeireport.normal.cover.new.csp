		<!-- 报告中的检查项目 strat -->
		<tr>
			<td>
				<table border='1' cellspacing=0 style='width:100%;font-size:12pt; font-family:宋体;'>
					<tr>
						<th colspan=4 algin='center' valign='middle'>检查科室及项目</th>
					</tr>
					<tr>
						<th algin='center'>检查科室</th>
						<th algin='center'>项目名称</th>
						<th algin='center'>检查状态</th>
						<th algin='center'>检查日期</th>
					</tr>
					<SCRIPT language="Cache" RUNAT="SERVER">
						
						s STRowId=0,num=0
						f  s STRowId=$o(^TMPReport(UserID,"Result","StationItem",STRowId)) q:STRowId=""  d
						.s STData=$g(^TMPReport(UserID,"Result","StationItem",STRowId))
						.s rowspan=$p(STData,"^",$l(STData,"^"))
						.f i=1:1:$l(STData,$c(1)) d
						..
						..s ItemData=$p(STData,$c(1),i)
						..q:ItemData=""
						..s ItemStatus="已检"
						..i $p(ItemData,"^",3)=1 s ItemStatus="弃检"
						..e  i (($p(ItemData,"^",4)="")||($p(ItemData,"^",4)="/")) s ItemStatus="未检"
						..
						..w "<tr>"
						..w:i=1 "<td rowspan='"_rowspan_"' style='text-align:center;'>"_$p(ItemData,"^",1)_"</td>"
						..w "<td>&nbsp;"_$p(ItemData,"^",2)_"</td>"
						..w "<td style='text-align:center;'>"_ItemStatus_"</td>"
						..w "<td style='text-align:center;'>"_$p(ItemData,"^",4)_"</td>"
						..w "</tr>"

					</SCRIPT>
				</table>
			</td>
