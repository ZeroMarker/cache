<!--设置 nur.hisui.nurexcutecolorset.csp 医嘱背景颜色设置--> 
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	;d ##Class(Nur.CSPDebug).setSession() q 1 ;
	i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html XMLNS=TRAK>
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta http-equiv="Content-Type" content="text/html;"/>
<title><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></title>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI></HISUI>
<style>
	.search-table{
		border-collapse:separate;
		border-spacing:0 10px;
	}
	.r-label{
		padding-left: 10px;
	}
</style>
</head>

<body>
	<div id="Loading" style="position:absolute;z-index:1000;top:0px;left:0px;width:100%;height:100%;background:#ffffff;text-align:center;padding-top:25%;filter:alpha(opacity=80);opacity:1;">
		<img src="../scripts/bdp/Framework/icons/mkb/page_loading.gif"/>
	</div>
	<div class="hisui-layout" data-options="fit:true,border:false">
		<div data-options="region:'north',split:false,border:false" style="height:51px;border-bottom:1px solid #ccc;">
			<table class="search-table">
	  	 	 	<tr>
		            <td class="r-label">
		              <label id="_HospListLabel" for="_HospList" style="color:red;">医院</label>
		            </td>
		            <td>
		              <input id="_HospList"/>
		            </td>
		         </tr>
	  	 	 </table>
		</div>
		<div class="opertionSetPanel" data-options="region:'center',split:false,border:false" style="">
			<table id="colorTab"></table>
		</div>	
	</div>
	<script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
	<script type="text/javascript" src="../scripts/nurse/hisui-0.1.0/dist/js/jquery.color.js"></script>
	<script type="text/javascript" src="../scripts/nurse/hisui/nurExcuteColorSet.js"></script>
</body>
</html>
