<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'">
	<div style="margin:10px;">
		<div class="hisui-panel" title="退卡" style="height:210px;width:775px;padding:10px 0 0 0" data-options="headerCls:'panel-header-card'">
