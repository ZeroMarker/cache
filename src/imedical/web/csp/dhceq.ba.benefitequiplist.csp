<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>
<script language="cache" runat="server">

	s BELRowID = $Get(%request.Data("RowID",1))
	s EquipDR=""
	i BELRowID'=""
	{
		s BELEquipDR=$p($g(^DHCEQBenefitEquipList(BELRowID)),"^",1)
		s StoreLocDR=$p($g(^DHCEQEquip(BELEquipDR)),"^",67)
	}
	s QXType = $Get(%request.Data("QXType",1))
	s SplitNumCode=##class(web.DHCEQCommon).GetSysInfo("990026")
	s SplitRowCode=##class(web.DHCEQCommon).GetSysInfo("990025")
</script>
<body class="hisui-layout" >
	<div data-options="region:'north',border:false" style="height:48px;overflow:hidden;">
		<iframe id="Banner" frameborder="0" src="" style="width:100%;"></iframe>
	</div>
	<div data-options="region:'center',border:false" style="height:180px;overflow:hidden;">
		<div class="eq-table-tr">
			<div class="eq-table-td eq-table-label"><label id="cBELInitYear" for="BELInitYear">分析初始年度</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELInitYear"></div>
			<div class="eq-table-td eq-table-label"><label id="cBELPreWorkLoad" for="BELPreWorkLoad">预测工作量</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELPreWorkLoad"></div>
			<div class="eq-table-td eq-table-label"><label id="cBELNewFunction" for="BELNewFunction">填补诊治空白</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELNewFunction"></div>
			<div class="eq-table-td eq-table-label"><label id="cBELSpecialService" for="BELSpecialService">服务特殊人群</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELSpecialService"></div>
			<div class="eq-table-td eq-table-label15"><a id="BSave" href="#" class="hisui-linkbutton" iconCls="icon-w-save"  data-options="stopAllEventOnDisabled:true">保存</a></div>
			<div class="eq-table-td eq-table-label15"><a id="BDelete" href="#" class="hisui-linkbutton" iconCls="icon-w-delete"  data-options="stopAllEventOnDisabled:true">删除</a></div>
		</div>
		<div class="eq-table-tr">
			<div class="eq-table-td eq-table-label"><label id="cBELOtherSocial" for="BELOtherSocial">其他社会效益</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELOtherSocial"></div>
			<div class="eq-table-td eq-table-label"><label id="cBELGraduateNum" for="BELGraduateNum">培养研究生数量</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELGraduateNum"></div>
			<div class="eq-table-td eq-table-label"><label id="cBELStaffNum" for="BELStaffNum">培养进修人员数量</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELStaffNum"></div>
			<div class="eq-table-td eq-table-label"><label id="cBELOtherTasks" for="BELOtherTasks">其他科研教学任务</label></div>
			<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="BELOtherTasks"></div>
		</div>
	</div>
	<div data-options="region:'south',border:false" >
		<div id="tDHCEQBene" class="hisui-tabs" style="height:500px" >
			<div title="数据来源">
				<table id="tDHCEQDeviceMap" class="hisui-datagrid"></table>
			</div>
			<div title="年度评价">
				<table id="tDHCEQUseContextYear" class="hisui-datagrid"></table>
			</div>
			<div title="月度经济信息">
				<table id="tDHCEQUseContextMonth" class="hisui-datagrid"></table>
			</div>
			<div title="月度成本信息">
				<table id="tDHCEQUsedResource" class="hisui-datagrid"></table>
			</div>
			<div title="服务项">
				<table id="tDHCEQServiceMap" class="hisui-datagrid"></table>
			</div>
			<div title="其他内容">
				<table id="tDHCEQResearch" class="hisui-datagrid"></table>
			</div>
		</div>
	</div>
		
	<div id="hiddenEncrypt" style="width:0;height:0;">
		<form name="DHCEQ">
			<INPUT type="hidden" id="BELRowID" value=#(BELRowID)#>
			<INPUT type="hidden" id="QXType" value=#(QXType)#>
			<INPUT type="hidden" id="BELEquipDR" value=#(BELEquipDR)#>
			<INPUT type="hidden" id="DeviceSourceDR">
			<INPUT type="hidden" id="YearFlag">
			<INPUT type="hidden" id="StoreLocDR" value=#(StoreLocDR)#>
			<INPUT type="hidden" id="SplitNumCode" value=#(SplitNumCode)#>
			<INPUT type="hidden" id="SplitRowCode" value=#(SplitRowCode)#>
			
		</form>
	</div>
	<script type="text/javascript" src="../scripts/dhceq/ba/benefitequiplist.js"></script>
	<!--
	<script type="text/javascript" src="../scripts/dhceq/ba/billitem.js"></script>
	<script type="text/javascript" src="../scripts/dhceq/ba/usecontextyear.js"></script>
	<script type="text/javascript" src="../scripts/dhceq/ba/usecontextmonth.js"></script>
	<script type="text/javascript" src="../scripts/dhceq/ba/research.js"></script>
	<script type="text/javascript" src="../scripts/dhceq/ba/usedresource.js"></script>
	-->
</body>

</html>