<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false" style="height:215px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" fit="true" data-options="title:'门诊日志',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="PatNo">日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="OpDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td><label style="padding:0 20px;">至</label></td>
					<td>
						<input class="hisui-datebox textbox" id="OpDate2" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label-1">
						<label for="CardTypeNew">卡类型</label>
					</td>
					<td>
						<input class="textbox" disabled id="CardTypeNew"/>
					</td>
					<td class="r-label" style="padding-left:20px;">
						<label for="CardNo">卡号</label>
					</td>
					<td>
						<input class="textbox" id="CardNo"/>
					</td>
					<td class="r-label-1">
						<a id="BReadCard" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-card'">读卡</a>
					</td>
					<td class="r-label">
						<a id="BAmount" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">门诊日志汇总</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="PatName">时间</label>
					</td>
					<td>
						<input class="hisui-timespinner textbox" id="Time" data-options="showSeconds:'true'" style="width:155px;"/>
					</td>
					<td><label style="padding:0 20px;">至</label></td>
					<td>
						<input class="hisui-timespinner textbox" id="Time2" data-options="showSeconds:'true'" style="width:155px;" />
					</td>
					
					<td class="r-label-1">
						<label for="LocQuery">科室</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="LocQuery"/>
					</td>
					<td class="r-label">
						<label for="FindDoc">医生</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="FindDoc"/>
					</td>
					<td class="r-label">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="PatName">年龄</label>
					</td>
					<td>
						<input class="textbox" id="SPatientAge"/>
					</td>
					<td><label style="padding:0 20px;">至</label></td>
					<td>
						<input class="textbox" id="EPatientAge"/>
					</td>
					
					<td class="r-label-1">
						<label for="SerCon">状态条件</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="SerCon"/>
					</td>
					<td class="r-label">
						<label for="MRDiagnos">诊断</label>
					</td>
					<td>
						<input class="textbox" id="MRDiagnos"/>
					</td>
					<td class="r-label">
						<a id="BPrint" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">打印</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="SearchConditions">查询条件</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="SearchConditions"/>
					</td>
					<td><label style="padding:0 20px;">为</label></td>
					<td>
						<input class="textbox" id="Gsearchmessage"/>
					</td>
					<td class="r-label-1">
						<label for="SearhLoc">查询本科</label>
					</td>
					<td>
						<input id="SearhLoc" class='hisui-checkbox' type="checkbox" checked label="">
					</td>
					<td></td><td></td>
					<td class="r-label">
						<a id="BExport" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">导出</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false" style="padding:0 10px 10px 10px;;border:0;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="OPDocLogTab"></table>
		</div>
	</div>
</div>