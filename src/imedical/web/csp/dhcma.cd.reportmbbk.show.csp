<!-- 主页面-->
<div id="MBBKReport" class="hisui-layout" data-options="fit:true" style="width:100%;height:100%;">
	<div data-options="region:'center',border:false,collapsible:false">
		<div class="page-body" id="divbody" style="width:1285px;">
			<div class="report-header">
				<span>慢性病报病卡</span>
			</div>
			<div class="report-content">
				<div class="hisui-panel" id="BaseInfo" data-options="title:'病人基本信息',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="BaseInfoTab"  class="report-tab">
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>报卡类型</td>
							<td colspan="3">
								<div id="radBGKLXList">
								</div>
							</td>
							<td class="report-td">报告日期</td>				
							<td><input class="hisui-datebox textbox" id="txtCRReportDate" disabled /></td>
							<td class="report-td">报告人</td>				
							<td><input class="textbox" id="txtCRReportUser" disabled /></td>
							<td class="report-td">卡片编号</td>
							<td><input class="textbox" id="txtCRKPBH" disabled /></td>
						</tr>
						<tr class="report-tr">
							<td class="report-td">门诊号</td>				
							<td><input class="textbox" id="txtMZH" /></td>
							<td class="report-td">住院号</td>					
							<td><input class="textbox" id="txtZYH" /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>患者姓名</td>				
							<td><input class="textbox" id="txtPatName" disabled /></td>
							<td class="report-td"><font color='red'>*</font>性别</td>					
							<td><input class="textbox" id="txtPatSex" disabled /></td>
							<td class="report-td"><font color='red'>*</font>年龄</td>					
							<td>
								<input class="textbox" id="txtPatAge" style="width:60px;" disabled />
								<input class="textbox" id="cboPatAgeDW" style="width:90px;" disabled />
							</td>
							<td class="report-td"><font color='red'>*</font>出生日期</td>				
							<td><input class="hisui-datebox textbox" id="txtBirthDay" /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>民族</td>				
							<td><input class="textbox" id="cboCRMZ"/></td>
							<td class="report-td"><font color='red'>*</font>文化程度</td>				
							<td><input class="textbox" id="cboEducation"/></td>
							<td class="report-td"><font color='red'>*</font>职业</td>					
							<td colspan="3"><input class="textbox" id="cboCRZY" style="width:415px;" /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>身份证号</td>				
							<td colspan="3"><input class="textbox" id="txtPatCardNo" style="width:405px;" /></td>
					    </tr>
					   <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>户口地址 省</td>				
							<td><input class="textbox" id="cboProvince1"/></td>
							<td class="report-td"><font color='red'>*</font>市</td>				
							<td><input class="textbox" id="cboCity1"/></td>
							<td class="report-td"><font color='red'>*</font>县</td>					
							<td><input class="textbox" id="cboCounty1" /></td>
							<td class="report-td"><font color='red'>*</font>乡/镇</td>					
							<td><input class="textbox" id="cboVillage1" /></td>
							<td class="report-td"><font color='red'>*</font>村</td>					
							<td><input class="textbox" id="txtCUN1" /></td>
					    </tr>
						 <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>或现地址 省</td>				
							<td><input class="textbox" id="cboProvince2"/></td>
							<td class="report-td"><font color='red'>*</font>市</td>				
							<td><input class="textbox" id="cboCity2"/></td>
							<td class="report-td"><font color='red'>*</font>县</td>					
							<td><input class="textbox" id="cboCounty2" /></td>
							<td class="report-td"><font color='red'>*</font>乡/镇</td>					
							<td><input class="textbox" id="cboVillage2" /></td>
							<td class="report-td"><font color='red'>*</font>村</td>					
							<td><input class="textbox" id="txtCUN2" /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>工作单位</td>				
							<td colspan="3"><input class="textbox" id="txtGZDW" style="width:405px;"/></td>
							<td class="report-td"><font color='red'>*</font>电话</td>				
							<td><input class="textbox" id="txtLXDH"/></td>
					    </tr>
					</table>
				</div>
				
				<div class="hisui-panel" id="DetailInfo" data-options="title:'明细信息',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="DetailInfoTab"  class="report-tab">
					   <tr class="report-tr">
							<td class="report-td">冠心病</td>				
							<td><input class="textbox" id="cboCRGXBZDList"  style="width:247px;"/></td>
							<td class="report-td">脑卒中</td>					
							<td><input class="textbox" id="cboCRNCZZDList"  style="width:207px;" /></td>
							<td class="report-td">糖尿病</td>				
							<td><input class="textbox" id="cboCRTNBZDList" style="width:207px;" /></td>
					    </tr>
						
						<tr class="report-tr">	
							<td class="report-td">肿瘤病名</td>					
							<td>
								<input class="textbox" id="cboCRZDMC"  style="width:247px;" />
								<span class="report-span">ICD编码</span>			
								<input class="textbox" id="txtCRZDBM" style="width:80px;"  disabled />
							</td>
							<td class="report-td">原发部位</td>				
							<td><input class="textbox" id="txtCRYFBW" style="width:200px;"/></td>
							<td rowspan="2"><span class="report-span"><font color='red'>*</font>肿瘤病例需填写此5项!</span></td>
					    </tr>
						<tr class="report-tr">			
							<td class="report-td">病理类型</td>				
							<td>
								<input class="textbox" id="txtCRBLXLX" style="width:240px;"  />
							</td>
							<td class="report-td">继发(转移)部位</td>				
							<td><input class="textbox" id="txtCRJFBW" style="width:200px;"/></td>
					    </tr>
						
						<tr class="report-tr">
							<td class="report-td">更正病名</td>				
							<td>
								<input class="textbox" id="cboCRGZZD" style="width:247px;"/>
								<span class="report-span">ICD编码</span>			
								<input class="textbox" id="txtCRGZZDICD" style="width:80px;"  disabled />
							</td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>发病日期</td>
							<td><input class="hisui-datebox textbox" id="txtFBRQ"  style="width:247px;" /></td>	
							<td colspan="2">
								<div id="radSFFBRQGJList" style="width:120px;" >
								</div>	
							    <span class="report-span"><font color='red'>*</font>发病日期为估计</span>
							</td>									
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>确诊日期</td>				
							<td><input class="hisui-datebox textbox" id="txtQZRQ"  style="width:247px;"/></td>
							<td class="report-td">死亡日期</td>					
							<td><input class="hisui-datebox textbox" id="txtCRSWRQ"  style="width:207px;" /></td>
							<td class="report-td">死亡原因</td>				
							<td><input class="textbox" id="txtCRJTSWYY" style="width:200px;" /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>诊断依据</td>					
							<td colspan="5">
								<div id="chkCRZDYJList">
								</div>
							</td>
					    </tr>
						
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>诊断单位</td>				
							<td><input class="textbox" id="cboCRZDDW" style="width:247px;" /></td>
							<td class="report-td">报告单位</td>					
							<td><input class="textbox" id="txtCRReportOrgan"  style="width:200px;"/></td>
							<td class="report-td"><font color='red'>*</font>科室</td>				
							<td>
								<input class="textbox" id="cboCRReportLoc"  style="width:207px;"/>
							</td>
							<td>
								<div id="radCRSBWZList">
								</div>
							</td>
						</tr>
					</table>
				</div>
			</div>
		</div>
		<div class="page-footer">	
			<a id="btnSave" class="hisui-linkbutton">保存</a>
			<a id="btnCheck" class="hisui-linkbutton">审核</a>
			<a id="btnCanCheck" class="hisui-linkbutton">取消审核</a>
			<a id="btnDelete" class="hisui-linkbutton">作废</a>
			<a id="btnExport" class="hisui-linkbutton">导出</a>
			<a id="btnPrint" class="hisui-linkbutton">打印</a>	
			<a id="btnCancle" class="hisui-linkbutton">关闭</a>		
		</div>
	</div>
</div>