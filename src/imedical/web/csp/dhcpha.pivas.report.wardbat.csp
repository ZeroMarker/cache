<!DOCTYPE html>
<!--配液中心病区交接单
	csp:dhcpha.pivas.report.wardbat.csp
	js: scripts/pharmacy/pivas/report.wardbat.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD>
    </TRAK:HEAD>
    <HISUI/>
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/plugins/CustomScrollbar/jquery.mCustomScrollbar.css" />
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css"></link>
    <STYLE>
        #tabsWardBat div {
            overflow: hidden;
        }
    </STYLE>
</HEAD>

<body style="overflow:hidden;">
	<div class="dhcpha-win-mask">
		<img src="../scripts/pharmacy/common/image/page-loading.gif"></img>
	</div>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',split:true,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">
            <div class="hisui-layout" fit="true">
                <div data-options="region:'west',split:true,headerCls:'panel-header-gray',title:'病区批次汇总统计',iconCls:'icon-sum'" style="width:350px;">

                    <div class="hisui-layout" fit="true">
                        <div data-options="region:'center'" class="newScroll" border="false" style="overflow:hidden">
                            <table class="dhcpha-north-tbl">
                                <tr>
                                    <td>
                                        <label for="cmbPivaLoc">配液中心</label>
                                        <input id="cmbPivaLoc" type="text" class="hisui-combobox" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="dateOrdStart">用药开始时间</label>
                                        <input id="dateOrdStart" class="hisui-datebox dhcpha-date" type="text" data-options="width:120" />
                                        <input id="timeOrdStart" class="hisui-timespinner" showSeconds="true" data-options="width:90">

                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="dateOrdEnd">用药结束时间</label>
                                        <input id="dateOrdEnd" class="hisui-datebox dhcpha-date" type="text" data-options="width:120" />
                                        <input id="timeOrdEnd" class="hisui-timespinner" showSeconds="true" data-options="width:90">

                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="datePrtStart">打签开始时间</label>
                                        <input id="datePrtStart" class="hisui-datebox dhcpha-date" type="text" data-options="width:120" />
                                        <input id="timePrtStart" class="hisui-timespinner" showSeconds="true" data-options="width:90">

                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="datePrtEnd">打签结束时间</label>
                                        <input id="datePrtEnd" class="hisui-datebox dhcpha-date" type="text" data-options="width:120" />
                                        <input id="timePrtEnd" class="hisui-timespinner" showSeconds="true" data-options="width:90">

                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbWard">病区</label>
                                        <input id="cmbWard" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbLocGrp">科室组</label>
                                        <input id="cmbLocGrp" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbWorkType">工作组</label>
                                        <input id="cmbWorkType" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbPriority">医嘱优先级</label>
                                        <input id="cmbPriority" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbPassResult">配伍审核</label>
                                        <input id="cmbPassResult" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbPivaStat">配液状态</label>
                                        <input id="cmbPivaStat" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmgIncItm">药品</label>
                                        <input id="cmgIncItm" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbBatNo">批次</label>
                                        <input id="cmbBatNo" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="txtPogsNo">流程单号</label>
                                        <input id="txtPogsNo" type="text" class="hisui-searchbox" style="width:214px" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="txtPatNo">登记号</label>
                                        <input id="txtPatNo" type="text" class="hisui-validatebox" style="width:207px" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbInstruc">用法</label>
                                        <input id="cmbInstruc" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbOeoreStat">执行记录状态</label>
                                        <input id="cmbOeoreStat" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbPack">打包</label>
                                        <input id="cmbPack" type="text" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <label for="cmbPivaRefuse">配液拒绝</label>
                                        <input id="cmbPivaRefuse" type="text" />
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <div data-options="region:'south'" border="false" style="text-align:center;padding:10px 0px">
                            <a class="hisui-linkbutton" iconCls='icon-w-find' plain="false" style="overflow:hidden;" id="btnFind">查询</a>
                        </div>
                    </div>
                </div>
                <div data-options="region:'center'" border="false" style="border-radius: 4px;">
                    <div id="tabsWardBat" class="hisui-tabs tabs-gray" fit="true">
                        <div id="tabWardBatGrp" data-options="title:'病区交接单'">
                            <iframe></iframe>
                        </div>
                        <div id="tabWardBatOrdDetail" data-options="title:'病区交接明细单'">
                            <iframe></iframe>
                        </div>
                        <div id="tabWardBatTbl" data-options="title:'病区批次汇总'">
                            <iframe></iframe>
                        </div>
                        <div id="tabWardBatIncTbl" data-options="title:'病区批次药品汇总'">
                            <iframe></iframe>
                        </div>
                        <!--div id ="tabWardWTTbl" data-options="title:'工作组汇总'">
							<iframe></iframe>
						</div-->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="../scripts/pharmacy/plugins/CustomScrollbar/jquery.mousewheel.min.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/plugins/CustomScrollbar/jquery.mCustomScrollbar.min.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/print.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/report.wardbat.js"></script>
</body>

</HTML>