<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 //i ##Class(websys.SessionEvents).SessionExpired()
 i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<HTML XMLNS=TRAK>
<HEAD>
<TITLE><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></TITLE>
<!-- <TRAK:HEAD></TRAK:HEAD> -->
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>

</HEAD>

<server>
	;s %request.Data("PatientBanner",1)=%request.Get("PatientBanner")
	n wkfl,wkfli,CompoDetails
	s trafficID=""
	s ID=%request.Get("TRFID")
	s wkfl=%request.Get("TWKFL")
	s wkfli=%request.Get("TWKFLI")
	s patientID=%request.Get("PatientID")
	s episodeID=%request.Get("EpisodeID")
	s payCategory=%request.Get("payCategory")
	s NewApptIDFlag=%request.Get("NewApptIDFlag")
	s insur=%request.Get("InsurPayor")
	s hid=%request.Get("hiddenLinks")
	s RSTDesc=%request.Get("RSTDesc")
	s CONTEXT=%request.Get("CONTEXT")
	S obj=##Class(User.PAAdm).%OpenId(episodeID,0)
	i obj d
	.i obj.PAADMTrafficAccidentDR d
	..s trafficID=obj.PAADMTrafficAccidentDR.%Id()
	.d obj.%Close()
	;s data=a=""
	;f  s a=%request.Next(a)  q:a=""  s data=data_"&"_a_"="_%request.Get(a)
	; s OtherAddresses=$p(##class(web.PAPerson).FindIfLinksDataPAPersonEdit(%request.Get("PARREF")),"^",2)
		s CompoDetails="1"
 	w "<SCRIPT>",$c(13,10)
 	w "if (window.opener) {",$c(13,10)
 	w "	try {",$c(13,10)
 	w "		if (window.opener) ;",$c(13,10)
 	w "	}",$c(13,10)
 	w "	catch(e) {}",$c(13,10)
 	w "}",$c(13,10)
 	w "</SCRIPT>",$c(13,10)
	
</server>
<frameset rows="35%,*">
  <frame src="websys.default.csp?WEBSYS.TCOMPONENT=PATrafficAccident.List&TWKFL=#(wkfl)#&TWKFLI=#(wkfli)#&PatientID=#(patientID)#&EpisodeID=#(episodeID)#&payCategory=#(payCategory)#&NewApptIDFlag=#(NewApptIDFlag)#&ID=#(ID)#&trafficID=#(trafficID)#&InsurPayor=#(insur)#&RSTDesc=#(RSTDesc)#&CONTEXT=#(CONTEXT)#&PatientBanner=1" name="upper">
  <frame src="websys.default.csp?WEBSYS.TCOMPONENT=PATrafficAccident.Edit&TWKFL=#(wkfl)#&TWKFLI=#(wkfli)#&PatientID=#(patientID)#&EpisodeID=#(episodeID)#&payCategory=#(payCategory)#&ID=#(trafficID)#&payCode=#(insur)#&hiddenLinks=#(hid)#&NewApptIDFlag=#(NewApptIDFlag)#&RSTDesc=#(RSTDesc)#&CONTEXT=#(CONTEXT)#" name="lower">
  </frameset>

<BODY>
</BODY>

</HTML>



