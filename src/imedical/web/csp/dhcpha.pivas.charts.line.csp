<!DOCTYPE html>
<!--配液时间段统计
	csp:dhcpha.pivas.statetime.csp
	js:scripts/pharmacy/pivas/statetime.js
-->
<HTML XMLNS=TRAK>

<HEAD>
    <title>
        <TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE>
    </title>
    <TRAK:HEAD></TRAK:HEAD>
    <HISUI />
    <link rel="stylesheet" type="text/css"
        href="../scripts/pharmacy/plugins/CustomScrollbar/jquery.mCustomScrollbar.css" />
    <link rel="stylesheet" type="text/css" href="../scripts/pharmacy/common/css/dhcpha.hisui.css">
    </link>
    <STYLE type="text/css">
        .dhcpha-tmp-border {
            border: 1px solid #cccccc;
            border-radius: 4px;
        }

        .dhcpha-no-data {
        height:100%;
        width:100%;
            background: url('../scripts/pharmacy/common/image/bg-nodata.png') no-repeat center;
        }
    </STYLE>
</HEAD>

<body style="overflow:hidden;">
	<div class="dhcpha-win-mask">
		<img src="../scripts/pharmacy/common/image/page-loading.gif"></img>
	</div>
    <div class="hisui-layout" fit="true">
        <div data-options="region:'center',split:true,headerCls:'panel-header-gray'" class="dhcpha-hisui-container">

            <div class="hisui-layout" fit="true" border="false">
                <div data-options="region:'north',title:'配液数据折线图',split:true,headerCls:'panel-header-gray',iconCls:'icon-green-chart',collapsible:false"
                    class="dhcpha-north-r1">
                    <table class="dhcpha-north-tbl">
                        <tr>
                            <td>
                                <label for="dateStart">开始日期</label>
                                <input id="dateStart" class="hisui-datebox" />
                            </td>
                            <td>
                                <label for="dateEnd">结束日期</label>
                                <input id="dateEnd" class="hisui-datebox" />
                            </td>
                            <td>
                                <a class="hisui-linkbutton" iconCls="icon-w-find" id="btnFind">查询</a>
                            </td>
                        </tr>
                    </table>
                </div>
                <div data-options="region:'center'" border="false" style="border-radius: 4px;">
                    <div id="tabsOne" class="hisui-tabs tabs-gray" fit="true">
                        <div data-options="title:'配置量'">
                            <div class="hisui-layout" fit="true" border="false">
                                <div data-options="region:'center'" border="false" id="eChartPS60" class="dhcpha-no-data">
                           
                                </div>
                            </div>
                        </div>
                        <div data-options="title:'退药量'">
                            <div class="hisui-layout" fit="true" border="false">
                                <div data-options="region:'center'" border="false" id="eChartReturn"  class="dhcpha-no-data">

                                </div>
                            </div>
                        </div>
                        <div data-options="title:'排批修改量'">
                                <div class="hisui-layout" fit="true" border="false">
                                    <div data-options="region:'center'" border="false" id="eChartPS3"  class="dhcpha-no-data">
    
                                    </div>
                                </div>
                            </div>
                        <!-- <div data-options="title:'配伍审核'">
                            <div class="hisui-layout" fit="true" border="false">
                                <div data-options="region:'center'" border="false" id="eChartAudit"  class="dhcpha-no-data">

                                </div>
                            </div>
                        </div> -->
                    </div>
                </div>

            </div>
        </div>
    </div>

    <script type="text/javascript" src="../scripts/pharmacy/plugins/echarts/echarts.min.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.extend.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/common/js/dhcpha.hisui.com.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/common.js"></script>
    <script type="text/javascript" src="../scripts/pharmacy/pivas/charts.line.js"></script>
</body>


</HTML>