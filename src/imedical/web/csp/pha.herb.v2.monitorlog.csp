<!-- 草药处方审核日志查询弹窗 -->
<div id="diagAuditLog" class="hisui-dialog" data-options="closed:true,width:1350,height:700,title:'审核日志列表',modal:true,iconCls:'icon-search'" style="padding:10px">
	<div class="hisui-layout" fit="true">
		<div data-options="region:'north',border:false,split:true" style="width:350px;height:300px;">
			<div class="hisui-panel" title="日志记录"
				data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
				<table id="gridAuditLog"></table>
			 </div>	             
		</div>
		<div data-options="region:'center',border:false" >
			<div class="hisui-panel" title="问题列表"
				data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true,bodyCls:''">
				<table id="gridAuditLogDetail"></table>
			</div>
		</div>
		<div data-options="region:'south',border:false,height:40" style="text-align:center;overflow:hidden;" class="pha-body">
			<a class="hisui-linkbutton" id="btnCancel">取消审核</a> 
        </div>
	</div>
</div>
