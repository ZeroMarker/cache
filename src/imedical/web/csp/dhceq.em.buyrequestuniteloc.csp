<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>
<body class="hisui-layout">
	<script language=Cache runat=server>
		s BRRowID=$Get(%request.Data("BRRowID",1))
		s ApproveType=$Get(%request.Data("ApproveType",1))
		s ReadOnly=$Get(%request.Data("ReadOnly",1))
	</script>
	
	<div id='Loading' class="eq-loading"><h1><img src="../skin/default/images/loading.gif"><font>加载中・・・</font></h1></div>
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" data-options="eqtitle:'maintitle',fit:true,title:'采购科室维护'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="padding:5px 0 10px 0;">
					<div class="eq-table">
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cMASourceID_CTLOCDesc" for="MASourceID_CTLOCDesc">申请科室</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="MASourceID_CTLOCDesc" data-options="'component':'PLAT.L.Loc'"></div>
							<div class="eq-table-td"><a id="BSave" href="#" class="hisui-linkbutton" iconCls="icon-w-save" data-options="stopAllEventOnDisabled:true">保存</a></div>
							<div class="eq-table-td"><a id="BDelete" href="#" class="hisui-linkbutton" iconCls="icon-w-close" data-options="stopAllEventOnDisabled:true">删除</a></div>
							<!--modify by lmm 2020-06-04-->
							<!--Add by QW20191205 begin 需求号:1123791-->
							<!--div class="eq-table-td"><a id="BClosed" href="#" class="hisui-linkbutton" iconCls="icon-w-close" data-options="stopAllEventOnDisabled:true">关闭</a></div-->
							<!--Add by QW20191205 end 需求号:1123791-->						</div>
					</div>
				</div>
				<div data-options="region:'center',border:false" style="position:relative;">
					<table id="DHCEQMultipleApproveInfo" class="hisui-datagrid" data-options="border:false">
					</table>
				</div>
			</div>
		</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="MASourceID" value="">
			<INPUT type="hidden" id="MABusID" value="#(BRRowID)#">
			<INPUT type="hidden" id="MARowID">
			<INPUT type="hidden" id="MAApproveTypeDR" value="#(ApproveType)#">
			<INPUT type="hidden" id="ReadOnly" value="#(ReadOnly)#">

		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/em/buyrequestuniteloc.js'></script>
</body>
</html> 