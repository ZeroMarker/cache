<!-- 
 * FileName: dhcbill.opbill.deppaymcollect.csp
 * Author: ZhYW
 * Date: 2019-12-07
 * Description: 预交金支付方式汇总查询
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD/>
	<HISUI/>
	<DHCBILL/>
	<link rel="stylesheet" type="text/css" href="#(##class(BILL.Util.Library).#CSSPath)#dhcbill.opbill.deppaymcollect.css"/>
	<script type="text/javascript" src="../scripts/DHCCPMRQCommon.js"></script>
	<server>
		set DefDate=##class(websys.Conversions).DateLogicalToHtml(+$h)
	</server>
	<script type="text/javascript">
		//常量
		var CV = {
			DefDate: "#(DefDate)#"
		};
		
		//全局变量
		var GV = {};
	</script>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',collapsible:false,title:'预交金支付方式汇总查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="height:87px;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label>#(..Get("开始日期"))#</label></td>
					<td><input id="stDate" class="hisui-datebox textbox"/></td>
					<td class="r-label r-label-30"><label>#(..Get("结束日期"))#</label></td>
					<td><input id="endDate" class="hisui-datebox textbox"/></td>
					<td class="r-label r-label-30"><label>#(..Get("押金类型"))#</label></td>
					<td><input id="depositType" class="hisui-combobox textbox"/></td>
					<td class="r-label r-label-30">
						<a href="javascript:;" class="hisui-linkbutton" id="btn-find" data-options="iconCls:'icon-w-find'">查询</a>
						<a href="javascript:;" class="hisui-linkbutton" id="btn-export" data-options="iconCls:'icon-w-export'" style="margin-left:17px;">导出</a>
					</td>
				</tr>
			</table>
		</div>
		<div data-options="region:'center',bodyCls:'panel-header-gray'">
			<table id="paymList"></table>
	  	</div>
	</div>
	<script type="text/javascript" src="#(##class(BILL.Util.Library).#JSPath)#dhcopbill/dhcbill.opbill.deppaymcollect.js"></script>
</body>
</html>