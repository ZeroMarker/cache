<!-- 按标记统计日志数据 -->
<!--dhcckb.monlogquery.csp --> 
<html>
<head>
<title>按审核标记统计日志数据</title>
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI/>
<DHCCKB/>
<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/component.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcckb/monlogquery.js"></script>
	<style>
		.datagrid-wrap
		{
			border:none;
		}
	</style>
</head>
<body>
<div id="mainpanel" class="hisui-layout" fit="true">
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray'"  style='padding:2px 2px 0px 2px;'>
		<div class="hisui-panel" data-options="title:'按审核标记统计日志数据',fit:true,headerCls:'panel-header-gray',iconCls:'icon-paper'" style="border:1px solid #ccc;border-top:0px;border-radius:0px;">		
			<div id="monloglist" toolbar="#montb"></div>
		</div>
	</div>
</div>
<div id="montb" class="toolbar">
	<div id="queryForm" style="margin:5 0 5 5;">
		<table cellspacing="0" cellpadding="0" >
			<tr>
		      <td><span style="margin-left:10px;margin-right:5px;">开始日期</span><input class="hisui-datebox" id="fromdate"></td>
		      <td><span style="margin-left:10px;margin-right:5px;">结束日期</span><input class="hisui-datebox" id="todate"></td>
		      <td><span style="margin-left:10px;margin-right:5px;">审核标记</span><input class="hisui-combobox" id="sign"></td>
		      <td><span style="margin-left:10px;margin-right:5px;">院区</span><input class="hisui-combobox" id="hosp"></td>
		      <td><a style="margin-left:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" id="find">查询</a></td>
		    </tr>
		</table>
	</div>
</div>
</body>
</html>


