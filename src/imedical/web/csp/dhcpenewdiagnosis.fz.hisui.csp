<!--dhcpenewdiagnosis.fz.hisui.csp-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
<script language="JavaScript"> 

</script>
<SCRIPT language="Cache" RUNAT="SERVER"> 
s MainDoctor=%request.Get("MainDoctor")
s OnlyRead=%request.Get("OnlyRead")
</SCRIPT> 
<script type="text/javascript">
var MainDoctor="#(MainDoctor)#"
var OnlyRead="#(OnlyRead)#"
</script>
 <style type="text/css">
/* 病人卡片#036bc2 */
.celllabel{ font-family: "微软雅黑";height:50px; color:#666; padding: 0; margin: 0; line-height: 25px;} 
.celllabel h3{color:#000; font-size: 14px; font-weight: normal;  padding: 0; margin: 0;}
.celllabel h3 span{font-size:14px; padding-right:5px;color:#666}
.celllabel h4{font-size:14px;font-weight: normal;color:#999;padding: 0,margin: 0.3em;-webkit-margin-before: 0.3em;-webkit-margin-after: 0.3em;margin-bottom: 2px;margin-top: 2px;}
.celllabel h4 span{font-size:14px;padding-right:5px;margin: 0;margin-bottom: 2px;margin-top: 2px;}


.sline, .infoLabel {
	color: #787878;
}

.patientInfo {
	padding-bottom: 10px;
	padding-left: 60px;
	display: inline-block;
	position: absolute;
	top: 10px;
}

.patientInfo span {
	padding-left: 5px;
}

.patName {
	font-size: 16px;
	margin-right: 15px;
}
 
.man {
 	background:url(../images/man.png) no-repeat;
 	background-position: center center;
 	width: 30px;
 	height: 30px;
 	position: absolute;
    top: 4px;
    left: 20px;
}

.woman {
 	background: url(../images/woman.png) no-repeat;
 	background-position: center center;
 	width: 30px;
 	height: 30px;
 	position: absolute;
	top: 4px;
	left: 20px;
}

.icon-w-update{
background:url("") no-repeat;
}

</style>
<server>
		
    d ##Class(web.DHCBillPrint).InvBillPrintCLSID()
   w "<input id='InvPrintEncrypt' name='InvPrintEncrypt' type='hidden' value='"_##class(%CSP.Page).Encrypt($lb("web.DHCXMLIO.ReadXML"))_"'>",$c(13,10)
</server>

</head>
<body>
<div class="hisui-layout" data-options="fit:true" border="false" style="border-radius:0;">
<input type="hidden" id="PAADM"/>




<div data-options="region:'west'" border="false" style="width:280px;border-radius:0;padding:4px 4px 4px 4px;">


 <div class="hisui-panel" data-options="fit:true" style="border-radius:0;">


	<div class="hisui-layout" data-options="headerCls:'panel-header-gray',border:false,fit:true" style="border-radius:0;">
	
	
    <div data-options="region:'north',split:false,collapsible:false,title:'患者列表',border:false" style="height:230px;border-radius:0;padding:4px;">
        

      <table style="width:100%;border-collapse:separate; border-spacing:10px 4px;">
      <tr>
      <td align="right" style="width:100px;">
         <label for="RegNo">登记号</label></td>
      <td align="left">
        <div><input class="textbox" type="text" Id="RegNo" data-options="prompt:'登记号'" style="width:151px;" /></div> </td>
       <td align="right" style="width:10px;"></td>
      </tr>
      <tr>
       <td align="right" style="width:100px;">
         <label for="VIP">VIP等级</label></td>
      <td align="left">
     	<div><input class="hisui-combobox" type="text" Id="VIP" data-options="prompt:'VIP等级'" style="width:158px;" /></div> </td>
      </tr>
      
       <tr>
      <td align="right" style="width:100px;">
         <label for="BDate">开始日期</label></td>
      <td align="left">
        <div><input class="hisui-datebox" type="text" Id="BDate" style="width:158px;" data-options="prompt:'开始日期'" /></div></div> </td>
      
      </tr>
      <tr>
      <td align="right" style="width:100px;">
         <label for="EDate">结束日期</label></td>
      <td align="left">
        <div><input class="hisui-datebox" type="text" Id="EDate" style="width:158px;" data-options="prompt:'结束日期'" /></div></div> </td>
      
      </tr>
       <tr>
       </tr>
        <tr>
        </tr>
        <tr>
       <td colspan="2" align="center">
       <div><a id="Query" href="#" class="hisui-linkbutton" style="width:80px;">查询</a></div></td>
       </tr>
  	  </table>
  	 </div>	
  	 
  	 
  	  <div data-options="region:'center',border:false">
      <div class="hisui-panel panel-header-gray" data-options="fit:true,headerCls:'panel-header-gray',border:false" style="border-radius:0;">
		<table class="hisui-datagrid" id="CanDiagnosisList" data-options="bodyCls:'panel-header-gray',fit:true,singleSelect:true,border:false"></table>	
		</div>
     
      </div>
  	  
  	</div>
  	</div>
   </div>
 <div class="hisui-layout" data-options="region:'center',border:false" style="padding:4px 0px 0px 4px;border-radius:0;">
  
    <div class="hisui-panel" data-options="region:'north',headerCls:'panel-header-gray',collapsible:false,border:false" style="height:40px;border-radius:0;">
	<div class="hisui-panel" data-options="border:false" style="height:20px;border-radius:0;">	
		
		<div id="sex" style="position:absolute;left:10px;"></div>
		<div class="patientInfo">
		<span class="patName" id="patName"></span>
		<span class="infoLabel">性别:</span><span id="sexName"></span>
		<span class="sline">/</span>
		<span class="infoLabel">年龄:</span><span id="Age"></span>
		<span class="sline">/</span>
		<span class="infoLabel" id="PatNoName">登记号:</span><span id="PatNo"></span>
		<span class="sline">/</span>
		<span class="infoLabel">体检日期:</span><span id="PEDate"></span>
		<span class="sline">/</span>
		<span class="infoLabel">VIP等级:</span><span id="VIPLevel"></span>
		</div>
    </div>
  </div>  
  
 <div data-options="region:'center',headerCls:'panel-header-gray',iconCls:'icon-stamp',title:'发证管理'" style="padding:0px 0px 0px 0px;border-radius:0 4px 4px 4px;">
  <div class="hisui-panel" data-options="headerCls:'panel-header-gray',iconCls:'icon-stamp',fit:true,border:false" style="border-radius:0;">
 
  <div class="hisui-layout" data-options="headerCls:'panel-header-gray',border:false,fit:true">
	
	<div data-options="region:'north',headerCls:'panel-header-gray'" style="height:50px;padding:10px 0px 0px 0px;border:0;">
  	<table style="border-collapse:separate; border-spacing:10px 0px;">
       <tr>
        <td><label for="Conclusion">结论</label></td>
		<td>
  		 <div>
          <select class="hisui-combobox" type="text" Id="Conclusion" name="Conclusion" style="width:220px;"></select>
          </div>
        <td>
        <td><label for="Suggestions">处理意见</label></td>
        <td>
  		  <div><input class="textbox" type="text" Id="Suggestions" style="width:220px;" /></div> 
  		</td>
      	<td style="width:20px;"></td>
        <td>
		 <div><a id="BSave" href="#" class="hisui-linkbutton" style="width:100px;" data-options="stopAllEventOnDisabled:'true'">保存</a></div>
		</td>
		<td>
		 <div><a id="BCancel" href="#" class="hisui-linkbutton" style="width:100px;" data-options="stopAllEventOnDisabled:'true'">撤销保存</a></div>
		</td>
		
		<td>
		<a id="BPrint" href="#" class="hisui-linkbutton" style="width:100px;" data-options="stopAllEventOnDisabled:'true'">打印</a>
		</td>
		</tr>
		
		
	</table>
	
	</div>
  
	<div data-options="region:'center',headerCls:'panel-header-gray',border:false" style="padding:1px 0px 0px 0px;">
	
 	<div class="hisui-panel panel-header-gray" data-options="fit:true,headerCls:'panel-header-gray'" style="border:1px solid #ccc;border-left:0;border-bottom:0;border-right:0;border-radius:0;">
		<table class="hisui-datagrid" id="CardManager" data-options="bodyCls:'panel-header-gray',fit:true,singleSelect:true,border:false"></table>	
		</div>
  	</div>
  	</div>
   </div>
  </div>
  </div>

 </div>
	
  <script type="text/javascript" src="../scripts/dhcpediagnosispatient.fz.hisui.js"></script>
  <script type="text/javascript" src="../scripts/DHCPECommon.js"></script>
  <script type="text/javascript" src="../scripts/DHCPrtComm.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPCommon.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPOEData.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.OPCommonManageCard.js"></script>
  <script type="text/javascript" src="../scripts/DHCWeb.COMMHardManager.js"></script>
  <script type="text/javascript" src="../scripts/DHCCPM_DOMComm.js"></script>
  <script type="text/javascript" src="../scripts/websys.jquery.js"></script>
  <script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
  <script type="text/javascript" src="../scripts_lib/lodop/LodopFuncs.js" charset="UIF-8"></script>
</body>
</html>
