<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
<style type="text/css">
.patientInfo {
	padding: 10px;
	display: inline-block;
	position: absolute;
}
.sline, .infoLabel {
	color: #787878;
}


</style>
</head>

<body  id="DiagnosisPanel" class="hisui-layout">
	<div class="hisui-panel" data-options="title:'健康分析指导',headerCls:'panel-header-gray',iconCls:'icon-big-book-eye'"></div>
	
	
	<div id="HMNew" class="hisui-layout" style="height:750px" data-options="clickExpand:true">
		
		<!--客户信息-start-->
		<div class="hisui-panel" data-options="region:'north',title:'',headerCls:'panel-header-gray'" style="height:45px;padding:5px 0px 5px 0px;">
		
			<div style="width:270px;float:left">
					<table style="width:100%;border-collapse:separate; border-spacing:10px 1px;"> 
						<tr>
						  <td align="right" style="width:100px;">
							 <label for="RegNo">登记号</label></td>
						  <td align="left">
							<div><input class="hisui-validatebox" type="text" Id="RegNo" data-options="prompt:'登记号'" style="width:158px;" /></div> </td>
						 </tr>				
					 </table>
			</div>
			<div  style="float:left;width:300px;justify-content: center;">
				<div class="patientInfo">
				<span class="infoLabel">姓名:</span>
				<span class="patName" id="patName"></span>
				<span class="sline">/</span>
				<span class="infoLabel">性别:</span><span id="sexName"></span>
				<span class="sline">/</span>
				<span class="infoLabel">生日:</span><span id="Age"></span>
				<span class="sline">/</span>
				<span class="infoLabel" id="PatNoName">登记号:</span><span id="PatNo"></span>
				<span class="sline"></span>
				</div>		
			</div>
			
			<!--模板制作-->
			<div style="float:right;width:270px;">
				<div class="patientInfo">
				<input class='hisui-checkbox' type="checkbox" id="HGTemplate" label="模板制作">
				</div>
			</div>  
		</div>
		<!--客户信息-end-->
		
		
		<!--就诊记录（左侧、按时间查）-start-->
		<div data-options="region:'west',title:'就诊记录',split:true,collapsible:false,headerCls:'panel-header-gray'" style="width:270px;">
			<div data-options="region:'north',title:''" style="height:120px;border: 1px solid #ccc;">
				<form class="form">
				  <table style="width:100%;border-collapse:separate; border-spacing:10px 4px;"> 
					  <tr>
						  <td align="right" style="width:100px;">
							 <label for="BDate">开始日期</label></td>
						  <td align="left">
							<div><input class="hisui-datebox" type="text" Id="BDate" style="width:158px;" data-options="prompt:'开始日期'" /></div></td>
					  </tr>
					  <tr>
						  <td align="right" style="width:100px;">
							 <label for="EDate">结束日期</label></td>
						  <td align="left">
						  <div><input class="hisui-datebox" type="text" Id="EDate" style="width:158px;" data-options="prompt:'结束日期'" /></div> </td>
					  </tr>
					   <tr>
						   <td colspan="2" align="center">
						   <div><a id="Query" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></div></td>
					   </tr>
				  </table>
			  </form>
			</div>
			<div class="hisui-panel panel-header-gray" data-options="fit:true,headerCls:'panel-header-gray'" style="border-top:0;border-radius:0 0 4px 4px;">
				<table class="hisui-datagrid" id="CanDiagnosisList"></table>
			</div>

		</div>
		<!--就诊记录（左侧、按时间查）-end-->


		<!--健康分析指导主录入窗-start-->
		<div id="HGMain" class="hisui-layout" data-options="region:'center', title:'',headerCls:'panel-header-gray',border:false">
			<div id="DiagnosisTab" class="hisui-tabs" data-options="region:'center',border:false">
	  		</div>	
		</div>
		<!--健康分析指导主录入窗-start-->


		
		<!--就诊记录（右侧、全部显示）-start-->
		<div data-options="region:'east',title:'历史记录',split:true,collapsible:false,headerCls:'panel-header-gray'" style="width:270px;">
			<div data-options="region:'north',title:''" style="height:120px;border: 1px solid #ccc;">
				<form class="form">
				  <table style="width:100%;border-collapse:separate; border-spacing:10px 4px;"> 
					  <tr>
						  <td align="right" style="width:100px;">
							 <label for="ViewBDate">开始日期</label></td>
						  <td align="left">
							<div><input class="hisui-datebox" type="text" Id="ViewBDate" style="width:158px;" data-options="prompt:'开始日期'" /></div></td>
					  </tr>
					  <tr>
						  <td align="right" style="width:100px;">
							 <label for="ViewEDate">结束日期</label></td>
						  <td align="left">
						  <div><input class="hisui-datebox" type="text" Id="ViewEDate" style="width:158px;" data-options="prompt:'结束日期'" /></div> </td>
					  </tr>
					   <tr>
						   <td colspan="2" align="center">
						   <div><a id="ViewQuery" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></div></td>
					   </tr>
				  </table>
			  </form>
			</div>
			<div class="hisui-panel panel-header-gray" data-options="fit:true,headerCls:'panel-header-gray'" style="border-top:0;border-radius:0 0 4px 4px;">
				<table class="hisui-datagrid" id="ViewDiagnosisList"></table>
			</div>
		</div>
		<!--就诊记录（右侧、全部显示）-end-->
		<!--就诊记录（右侧、全部显示）-start-->
		<!--div id="HGHistory"  class="hisui-layout" data-options="region:'center', title:'',headerCls:'panel-header-gray',border:false" style="display:none,top:'45px',left:'420';" >
		</div-->
	  		<!--div id="ViewDiagnosisTab" class="hisui-tabs" data-options="region:'center',border:false" style="position:relative">
	  		</div-->	
		
		<!--就诊记录（右侧、全部显示）-end-->
		<div>	
		 	<input type="hidden" id="PatientID"/>
		 	<input type="hidden" id="CurPAADM"/>
		</div>
	
	</div>

  		<script type="text/javascript" src="../scripts/dhcpe.healthguide.hisui.js"></script>
  	  <script type="text/javascript" src="../scripts/DHCPECommon.js"></script>
	  <script type="text/javascript" src="../scripts/DHCWeb.OPCommon.js"></script>
	  <script type="text/javascript" src="../scripts/DHCWeb.OPOEData.js"></script>
	  <script type="text/javascript" src="../scripts/DHCWeb.OPCommonManageCard.js"></script>
	  <script type="text/javascript" src="../scripts/DHCWeb.COMMHardManager.js"></script>
	  <script type="text/javascript" src="../scripts/DHCCPM_DOMComm.js"></script>
	  <script type="text/javascript" src="../scripts/websys.jquery.js"></script>
	  <script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
</body>
</html>
