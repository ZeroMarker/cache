<!-- 新检查申请 dhcapp.appreppartwin.csp -->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<script type="text/javascript" src="../scripts/dhcdoc/common/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcdoc/common/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcdoc/dhcapp/layout.js"></script>
	<script type="text/javascript" src="../scripts/dhcdoc/dhcapp/appreppartwin.js"></script>
	<style type='text/css'>
	.datagrid-header-row, .datagrid-row {
		height: 33px;
	}
	.combo-arrow{height: 29px !important;}
	.combo{height: 29px !important;}
	.combo-text{height: 29px !important;line-height: 29px !important;}
	.datagrid-editable-input{height: 30px !important;line-height: 30px !important;}
	/*页面表格样式*/
	.form-table{
		table-layout:fixed ; 
		border-collapse:collapse;
		border:1px solid #95B8E7;
		width:100%;
		border:none;
	}
	.form-table td{margin:0;padding:0;}
	.form-table td{
		border:1px dotted #ccc;
		padding:5px 3px;
		font-size: 14px;
	}
	.form-table tr:first-child td{
		padding:0px;
	}
	.btn-lightred{
		background-color:#F16E57 !important;
		border:1px solid #F16E57;}
	.panel-header, .panel-body {
    	border-color: #ccc;}
    .pt-butttom{border:1px solid #fff;height:45px;}
	.pt-butttom a{
		margin:8px 0px 0px 60px;
	}
	.btn-lightred{background-color:#F16E57 !important;border:1px solid #F16E57;}
	.btn-lightgreen{background-color:#40C27B !important;border:1px solid #40C27B;}
	.l-btn{border-radius:3px;}
	.pt-butttom .l-btn-text{
		min-width:60px
	}
	</style>
</head>
<body>
<div id="PopPanel" class="hisui-layout" fit="true">
	<div data-options="region:'center',headerCls:'panel-header-gray'" border=false>
		<div class="hisui-layout" fit="true">
			<div data-options="region:'north',title:'检查部位',headerCls:'panel-header-gray',collapsible:false" style='height:260px;'>
				<table id="ItmExaPart" border="1" cellspacing="0" cellpadding="1" class="form-table">
				</table>
			</div>
			<div data-options="region:'center',title:'已选部位',headerCls:'panel-header-gray'">
				<div id="dmPartList"></div>
			</div>
		</div>
	</div>
	<div data-options="region:'east',title:'后处理',split:true,headerCls:'panel-header-gray'" style='width:180px;'>
	    <table id="ItmExaDisp" border="1" cellspacing="0" cellpadding="1" class="form-table">
		</table>
	</div>
	<div data-options="region:'south'" style='height:50px;'>
		<div class="pt-butttom">
			<div style="float:right;padding-right:460px;">
				<a href="#" class="hisui-linkbutton btn-lightred" data-options="iconCls:''" onclick="cancel()">关闭</a>
				<a href="#" class="hisui-linkbutton btn-lightgreen" data-options="iconCls:''" onclick="sure()">确认</a>
			</div>
		</div>
	</div>
</div>
</body>
</html>
