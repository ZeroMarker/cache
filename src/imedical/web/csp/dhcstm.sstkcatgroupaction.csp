<csp:content charset="UTF-8">
<SERVER>

	s Action=$Get(%request.Data("actiontype",1))
	s Start=$Get(%request.Data("start",1))
	s Limit=$Get(%request.Data("limit",1))
	s Sort=$Get(%request.Data("sort",1))
	s Dir=$Get(%request.Data("dir",1))

	s listData=$Get(%request.Data("data",1))
	s RowId=$Get(%request.Data("rowid",1))
	s StkGrpId=$Get(%request.Data("StkCatGroupId",1))
	s Type=$Get(%request.Data("Type",1))

	//查询库存类组
	i Action = "selectAll" d
	.s result = ##class(web.DHCSTM.INCSSTKCAT).SelectAll(Type)
	.i result ="" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result

	//查询库存类组
	i Action = "Select" d
	.s Desc=$Get(%request.Data("Desc",1))
	.w ##class(web.DHCSTM.INCSSTKCAT).Select(Start,Limit,Desc,Type)
	.

	//保存库存类组
	i Action = "save" d
	.s result = ##class(web.DHCSTM.INCSSTKCAT).Save(listData)
	.i result = "" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	//删除库存类组
	i Action = "delete" d
	.s result = ##class(web.DHCSTM.INCSSTKCAT).Delete(RowId)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
</SERVER>