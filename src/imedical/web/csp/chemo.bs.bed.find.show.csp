<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',iconCls:'icon-format-line',collapsible:false,headerCls:'panel-header-gray'" title="化疗预约数量查询" style="height:95px;border-bottom:1px solid #ccc;">
		<table class="search-table">
			<tr>
				<td class="r-label">
					<label for="stage">#(..Get("院区"))#</label>
				</td>
				<td>
					<input class="textbox" id="Hosp" style="width:250px;"/>
				</td>
				<td class="r-label">
					<label for="stage">#(..Get("开始日期"))#</label>
				</td>
				<td>
					<input class="textbox hisui-datebox" id="StartDate"/>
				</td>
				<td class="r-label">
					<label for="stage">#(..Get("结束日期"))#</label>
				</td>
				<td>
					<input class="textbox hisui-datebox" id="EndDate"/>
				</td>
				
				<td class="r-label">
					<a id="Find" class="hisui-linkbutton">查询</a>
				</td>
				<td class="r-label">
					<a id="Clear" class="hisui-linkbutton red">清屏</a>
				</td>
			</tr>
		</table>
	</div> 
	<div data-options="region:'west',split:true" style="width:500px;" title="日期预约数量">
		<div id="i-list"></div>
	</div> 
	<div data-options="region:'center',split:true" title="科室分布">
		<div id="Loclist"></div>
	</div> 
</div>