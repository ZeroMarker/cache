<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray'" style="height:52px;">
		<div class="hisui-panel" style="border:0;border-bottom:1px dashed #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label id="_HospListLabel" style='color:red;'>医院</label>
					</td>
				
					<td>
						<input id="_HospList"/>
					</td>
					<td class="r-label">
						<label for="Dept">科室</label>
					</td>
					<td>
						<input class="textbox" id="Dept"/>
					</td>
					<td class="r-label">
						<label for="Mark">号别</label>
					</td>
					<td>
						<input class="textbox" id="Mark"/>
					</td>
					<td class="r-label">
						<a id="Bfind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',border:false">
		<table id="MarkListTab"></table>
	</div>
</div>
