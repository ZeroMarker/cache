			<div data-options="region:'center',split:false,border:false"> 
				<table id="tabPrescriptSet"></table> 
			</div>
		</div>
 	</div>   		
   		
   	<!--div id="dialog-ItemCatSelect" title="子类定义" style="display:none"-->
