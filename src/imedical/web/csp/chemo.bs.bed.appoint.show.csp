<div class="hisui-layout" data-options="fit:true">
	<div data-options="region:'north',iconCls:'icon-w-list',collapsible:false" title="门诊患者预约一览表" style="height:95px;border-bottom:1px dashed #ccc;padding:10px;">
		<span class="c-span">#(..Get("院区"))#</span>
  		<input class="textbox" id="Hosp" style="width:250px;"/>
  		<span class="c-span"></span>
		<span class="c-span">#(..Get("日期"))#</span>
  		<input id="ChemoDate" class="hisui-datebox textbox"></input>
		<span class="c-span"></span>
		<a id="i-find" class="hisui-linkbutton" >查询</a>
		<span class="c-span"></span>
		<a id="i-printAM" class="hisui-linkbutton green">打印上午</a>
		<span class="c-span"></span>
		<a id="i-printPM" class="hisui-linkbutton yellow" >打印下午</a>
	</div> 
	<div data-options="region:'center'">
		<div id="i-list"></div>
	</div> 
</div>