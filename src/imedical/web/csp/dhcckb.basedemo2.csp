						<td colspan="7"><input id="PatFamHis" value=""></input></td>
					</tr>
					<tr>
						<td align="right" class="tb_td_bk">过<span style="margin-left:3px;">敏</span><span style="margin-left:3px;">史</span></td>
						<td colspan="7"><input id="PatAnsHis" value=""></input></td>
					</tr>
					<tr>
						<td align="right" class="tb_td_bk">药<span style="margin-left:18px;">物</span></td>
						<td colspan="7"><input id="itemMedi" value=""></input></td>
					</tr>
					<tr style="display:none;">
