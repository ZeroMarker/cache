				<div style="border-bottom: 1px solid #40a2de;"></div>
				<table border="1" cellspacing="0" cellpadding="1" class="report_table">
					<tr>
						<td align="right" class="tb_td_bk">登<span style="margin-left:6px;">记</span><span style="margin-left:6px;">号</span></td>
						<td width="150px"><input id="PatNo" value="0000000001"></input></td>
						<td align="right" class="tb_td_bk tb_td_required">姓<span style="margin-left:24px;">名</span></td>
						<td width="150px"><input id="PatName" value="刘舸"></input></td>
						<td align="right" class="tb_td_bk">体<span style="margin-left:24px;">重</span></td>
						<td width="150px"><input id="PatWeight" value=""></input></td>
					</tr>
					<tr>
						<td align="right" class="tb_td_bk tb_td_required">年<span style="margin-left:24px;">龄</span></td>
						<td width="150px">
							<input id="PatAge" value="23" style="width:105px"></input>
							<select id="PatAgeUnit" style="width:50px;">
							  <option value ="岁">岁</option>
							  <option value ="月">月</option>
							  <option value="周">周</option>
							  <option value="天">天</option>
							  <option value="小时">小时</option>
							</select>
						</td>
						<td align="right" class="tb_td_bk tb_td_required">性<span style="margin-left:24px;">别</span></td>
						<td width="150px">
							<div class="container-radio">
								<label><input name="PatSex" type="radio" value="男"/>男</label> 
								<label><input name="PatSex" type="radio" value="女" checked/>女</label>
							</div>
						</td>
						<td align="right" class="tb_td_bk ">怀<span style="margin-left:24px;">孕</span></td>
						<td width="150px">
							<div class="container-radio">
								<label><input name="PreFlag" type="radio" value="1"/>是</label> 
								<label><input name="PreFlag" type="radio" value="0"/>否</label>
							</div>
						</td>
					</tr>
					<tr>
