
<!-- 
 * FileName: dhcpeireport.hisui.csp
 * Anchor: xy
 * Date: 2019-04-15
 * Description: HISUI 个人报告
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<ADDINS require="CmdShell"/>
<SCRIPT language="Cache" RUNAT="SERVER"> 
set IsSecret=$g(%request.Data("IsSecret",1))
</SCRIPT> 
<script type="text/javascript">
 var IsSecret="#(IsSecret)#"
 if(IsSecret=="N") var title="个人报告"
 if(IsSecret=="Y") var title="保密报告"
 </SCRIPT>

	
<style>
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
	/*padding: 0 10px;*/
}
.r-label{
	padding-left: 10px;
}
.r-label-1{
	padding-left: 20px;
}
.newclsInvalid{
	border-color: red;
	color: red;
}
.panel-header{
	border-bottom: 0;
}


</style>
</head>
<body>
<div class="hisui-layout" fit="true">
	<div data-options="region:'north',split:false,border:false,title:'',collapsible:false" style="height:215px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" data-options="title:title,headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
				  <td class="r-label"><label for="RegNo">登记号</label></td>
					<td>
						<input class="textbox" id="RegNo">
					</td>
					<td class="r-label r-label-1"><label for="DateFrom">到达开始日期</label></td>
					<td>
						<input id="DateFrom" class="hisui-datebox textbox">
					</td>
					<td class="r-label r-label-1"><label for="GroupName">团体</label></td>
          			<td> 
              			<input class="hisui-combogrid" type="text" Id="GroupName">
         			 </td>
					<td class="l-label r-label-1">
						<input class='hisui-checkbox' id="STatusIsNoAudit" type="checkbox" label="未审核">
					</td>
					<td class="l-label r-label-1">
						<input class='hisui-checkbox' id="STatusIsSend" type="checkbox" label="已取报告">
					</td>
					<td class="r-label">
						<a id="BFind" href="#" class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					<td class="r-label r-label-1">
						<a id="BPrintView" href="#" onclick="BPrintView(this)" class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-print'">打印预览</a>
					</td>
					<td class="r-label">
						<a id="BExportPDF" onclick="BPrintView(this)" href="#" class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-export'">导出pdf</a>
					</td>
					
					
					
				</tr>
				<tr>
				<td class="r-label"><label for="PatName">姓名</label></td>
					<td>
						<input class="textbox" id="PatName">
					</td>
					<td class="r-label r-label-1" ><label for="DateTo">到达结束时间</label></td>
					<td>
						<input id="DateTo" class="hisui-datebox textbox">
					</td>
					<td class="r-label r-label-1"><label for="TeamName">分组</label></td>
          			<td> 
              			<input class="hisui-combogrid" type="text" Id="TeamName">
         			 </td>
					<td class="l-label r-label-1">
						<input class='hisui-checkbox' id="STatusIsCheched" type="checkbox" label="已审核">
					</td>
					<!--td class="l-label r-label-1">
						<input class='hisui-checkbox' id="IsSendMessage" type="checkbox" label="是否发送短信">
					</td-->
					<td class="l-label r-label-1">
						<input class='hisui-checkbox' id="HadCompete" type="checkbox" label="已完成">
					</td>
					<td class="r-label">
						<a id="BClear" href="#" class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
					<td class="r-label">
						<a id="BPrint" href="#" onclick="BPrintView(this)" class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-print'">打印报告</a>
					</td>
					<td class="r-label r-label-1">
						<a id="BExport" onclick="BPrintView(this)" href="#" class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-export'">导出word</a>
					</td>
					
					
				</tr>
				<tr>
				<td class="r-label"><label for="DepartName">部门</label></td>
					<td>
						<input class="textbox" id="DepartName">
					</td>
					<td class="r-label r-label-1" ><label for="AuditDateFrom">审核开始日期</label></td>
					<td>
						<input id="AuditDateFrom" class="hisui-datebox textbox">
					</td>
					<td class="r-label r-label-1"><label for="VIPLevel">VIP等级</label></td>
          			<td> 
              			<input class="hisui-combobox" type="text" Id="VIPLevel">
         			 </td>
         			
					<td class="l-label r-label-1">
						<input class='hisui-checkbox' id="STatusIsPrint" type="checkbox" label="已打印 ">
					</td>
				   <td class="l-label r-label-1"></td>
				   
					<td class="r-label r-label-1">
						<a id="BExportED" href="#"  class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-export'">导出结论</a>
					</td>
					
					<td class="r-label">
						<a id="BPrintYGReport" onclick="BPrintView(this)" href="#" class="hisui-linkbutton" style="width:125px;" data-options="iconCls:'icon-w-print'">乙肝报告</a>
					</td>
				</tr>
				<tr>   
				     
				     <td class="r-label r-label-1"><label for="DocName">医生</label></td>
          			<td>
            			<input style="width:155px" class="hisui-combogrid" type="text" Id="DocName">
         			 </td>
         			 <td class="r-label r-label-1"><label for="AuditDateEnd">审核结束日期</label></td>
					<td>
						<input id="AuditDateEnd" class="hisui-datebox textbox" >
					</td>
			
				</tr>
					
			</table>
		</div>
	</div>
		<div data-options="region:'center',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
			<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
					<table data-options="bodyCls:'panel-header-gray'" id="IReportQueryTab"></table>
				</div>
        	</div>
</div>


<script type="text/javascript" src="../scripts/DHCPEIReport.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
<script type="text/javascript" src="../scripts/DHCPEPrintReportCommon.js"></script>
  
  
</body>		
</html>
			
		
