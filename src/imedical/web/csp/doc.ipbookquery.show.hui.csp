<div class="hisui-layout" data-options="fit:true,border:false">
	<div data-options="region:'north',border:false,collapsible:false" style="height:176px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" fit="true" data-options="title:'住院证查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label"><label for="StDate">开始日期</label></td>
					<td>
						<input id="StDate" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label"><label for="EdDate">结束日期</label></td>
					<td>
						<input id="EdDate" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label"><label for="CardNo">卡号</label></td>
					<td colSpan='3'>
						<input id="CardTypeNew" disabled class="textbox" style="width:128px;">
						<input id="CardNo" class="textbox">
						<a id="BReadCard" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-card'">读卡</a>
					</td>
					<td class="r-label">
						<a id="BFind" style="margin-right:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					<!--
					<td class="r-label"><label for="OrderOrCreateDate">预约(建证)日期</label></td>
					<td>
						<input id="OrderOrCreateDate" class='hisui-checkbox' type="checkbox">
					</td>
					-->
				</tr>
				<tr>
					<td class="r-label"><label for="RegNo">登记号</label></td>
					<td>
						<input class="textbox" id="RegNo"/>
					</td>
					<td class="r-label"><label for="CtLoc">科室</label></td>
					<td>
						<input class="hisui-combobox textbox" id="CtLoc"/>
					</td>
					<td class="r-label"><label for="PacWardId">病区</label></td>
					<td>
						<input class="hisui-combobox textbox" id="PacWardId"/>
					</td>
					
					<td class="r-label"><label for="State">当前状态</label></td>
					<td>
						<input class="hisui-combobox textbox" id="State"/>
					</td>
					<td style="padding-left:10px;">
						<a id="BExport" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">导出</a>
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="OrdLoc">开单科室</label></td>
					<td>
						<input class="textbox" id="OrdLoc"/>
					</td>
					<td class="r-label"><label for="DocCreateBookId">录入人</label></td>
					<td>
						<input class="hisui-combobox textbox" id="DocCreateBookId"/>
					</td>
					<td class="r-label"><label style="display:none;">站位</label></td>
					
					<td>
						<input id="OrderOrCreateDate" class='hisui-checkbox' type="checkbox" label="预约(建证)日期">
					</td>
					<td colSpan='3' >
						<div id ="SumallStatus"></div>
					</td>
					<!--<td colSpan='3' >
						<a id="BFind" style="margin-right:10px;" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
						<a id="BExport" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'">导出</a>
					</td>
					-->
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="IPBookQueryTab"></table>
		</div>
	</div>
</div>