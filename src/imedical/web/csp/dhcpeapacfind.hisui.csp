<!-- 
 * FileName: dhcpeapacfind.hisui.csp
 * Anchor: xy
 * Date: 2019-04-10
 * Description: HISUI 体检卡明细查询
-->
<!---->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<ADDINS/>
<style>
body{
	background: backgroundcolor;
}
.search-table{
	border-collapse:separate;
	border-spacing:0 10px;
}
.r-label{
	padding-left: 10px;
}

.newclsInvalid{
	border-color: red;
	color: red;
}

/*弹出框里标签右对齐 并且有10px内边距*/

.tdlabel
{
  padding-left: 10px;
  padding-right: 10px;
  text-align:right;
}
.addtable{
	border-collapse:separate;
	border-spacing:0 6px;
	/*padding: 0 10px;*/
}
</style>

<script type="text/javascript">
if (("undefined"==typeof HISUIStyleCode)||(HISUIStyleCode==null)){
	var backgroundcolor="#fff"
}else{
	if(HISUIStyleCode=="lite") {
		var backgroundcolor="#F5F5F5"
	}else{
		var backgroundcolor="#fff"
	}
}
</script>

<server>
        d ##Class(web.DHCBillPrint).InvBillPrintCLSID()
		w "<input id='InvPrintEncrypt' name='InvPrintEncrypt' type='hidden' value='"_##class(websys.Page).Encrypt($lb("web.DHCXMLIO.ReadXML"))_"'>",$c(13,10)
	</server>
</head>
<body style="padding:10px;box-sizing: border-box;">
<div class="hisui-panel" fit="true" data-options="title:'体检卡明细查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'">
	<div class="hisui-layout" fit="true">
		<div data-options="region:'north',border:false,collapsible:false" style="height:92px;border-bottom:1px solid #e2e2e2;border-radius:0">
			<table class="search-table">
				<tr>
				   <td class="r-label"><label id="RegNoL">#(..Get("登记号"))#</label></td>
					<td>
						<input class="textbox" id="RegNo">
					</td>
					
					<td class="r-label"><label for="BeginDate">#(..Get("开始日期"))#</label></td>
					<td>
						<input id="BeginDate" class="hisui-datebox textbox">
					</td>
					<td class="r-label"><label for="Type">#(..Get("类型"))#</label></td>
          			<td> 
              			<input class="hisui-combobox" type="text" Id="Type">
         			 </td>
					
					<td class="r-label">
						<a id="BRePRintInv" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">原号重打</a>
					</td>
					<td>
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
					
					
				</tr>
				<tr>
					<td class="r-label"><label for="Name">#(..Get("姓名"))#</label></td>
					<td>
						<input class="textbox" id="Name">
					</td>
					<td class="r-label"><label for="EndDate">#(..Get("结束日期"))#</label></td>
					<td>
						<input id="EndDate" class="hisui-datebox textbox">
					</td>
					<td class="r-label"><label for="CardType">#(..Get("体检卡类型"))#</label></td>
          			<td> 
              			<input class="hisui-combobox" type="text" Id="CardType">
         			 </td>
         			 <td class="r-label">
						<a id="BRePrintBalance" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">重打凭条</a>
					</td>
					<td>
						 <input type="hidden" id="RowId"/>
					</td>
				</tr>
				
						
			</table>
		</div>
		    <div data-options="region:'center',title:'',border:false" >
				<table id="APACFindQueryTab"></table>
			</div>	
    </div>
</div>
<!--退费弹窗-->
     <div id="myWin" style="width:550px;height:385px;padding:3px 10px 10px 10px;display:none;">   
		<form id="form-save" method="post" data-options="fit:true,border:false,plain:true">

			<table class="addtable">
				 <tr>
				 	<td class="tdlabel"><label for="InvNo">#(..Get("发票号"))#</label></td>
					<td>
						<input class="textbox"  type="text" id="InvNo" disabled/>
					</td>
					<td class="tdlabel"><label for="CurInvNo">#(..Get("当前发票号"))#</label></td>
					<td>
						<input class="textbox" type="text" id="CurInvNo" disabled/>
					</td>		
				</tr>
	
				<tr>
				 	<td class="tdlabel"><label for="RRegNo">#(..Get("登记号"))#</label></td>
					<td>
						<input class="textbox"  type="text" id="RRegNo" disabled/>
					</td>
					<td class="tdlabel"><label for="RName">#(..Get("姓名"))#</label></td>
					<td>
						<input class="textbox"  type="text" id="RName" disabled/>
					</td>		
				</tr>
				<tr>
				 	<td class="tdlabel"><label for="Amount">#(..Get("金额"))#</label></td>
					<td>
						<input class="textbox"  id="Amount" disabled/>
					</td>
					<td class="tdlabel"><label for="PayMode">#(..Get("支付方式"))#</label></td>
					<td>
						<input lass="hisui-combobox" type="text" id="PayMode" style="width:155px;"/>
					</td>		
				</tr>
				
				<tr>
				 	<td class="tdlabel"><label for="RFee">#(..Get("退款金额"))#</label></td>
					<td>
						<input class="textbox"  type="text" id="RFee"/>
					</td>
					<td class="tdlabel"><label for="RemainAmount">#(..Get("账户余额"))#</label></td>
					<td>
						<input class="textbox" type="text" id="RemainAmount" disabled/>
					</td>		
				</tr>
				
				<tr>
				 	<td class="tdlabel"><label for="RRemark">#(..Get("退费备注"))#</label></td>
					<td>
						<input class="textbox" type="text" id="RRemark"/>
					</td>
					<td class="tdlabel"><label for="No">#(..Get("号码"))#</label></td>
					<td>
						<input class="textbox" type="text" id="No"/>
					</td>

							
				</tr>
				<tr>
					<td class="tdlabel"><label for="Remark">#(..Get("备注"))#</label></td>
					<td colspan='4'>
						<textarea id="Remark" type="text" style="width:407px;height:60px;border:1px solid #40a2de;" disabled> </textarea>
					</td>
				</tr>
	             								
			</table >
		</form>
	</div>

<script type="text/javascript" src="../scripts/DHCPEAPAC.Find.hisui.js"></script>
<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
<script type="text/javascript" src="../scripts/DHCPrtComm.js"></script>
<script type="text/javascript" src="../scripts/DHCBillMisPosPay.js"></script>
<script type="text/javascript" src="../scripts/DHCBillPayService.js"></script> 
<script type="text/javascript" src="../scripts/DHCBillPayScanCodeService.js"></script> 
<script type="text/javascript" src="../scripts_lib/lodop/LodopFuncs.js" charset="UIF-8"></script> 
</body>		
</html>
			
		
