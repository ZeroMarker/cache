<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!--dhcem.waittohospat.csp 急诊待入院管理-->
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<ADDINS require="CmdShell"/>
	<HISUI/>
	<link href="../scripts/dhcnewpro/dhcem/css/waittohospat.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/js/common.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/waittohospat.js"></script>
	
</head>
<body>

<div class="hisui-layout" fit="true">
	<div data-options="region:'center',headerCls:'panel-header-gray',title:''" style="padding:10px">
		<div class="hisui-layout" fit="true" style="border:1px solid #ccc">
			<div data-options="region:'north',headerCls:'panel-header-gray',title:'急诊待入院管理',collapsible:false,border:false" style="height:120px;padding:2px;overflow:hidden;">
				<div style="margin:8px;">
					<label>开始日期</label>
					<input id="StartDate" class="hisui-datebox textbox" style="width:150px;">
					<label style="margin-left:10px;">登记科室</label>
					<input id="RLoc" class="textbox" style="width:150px;height:30px;line-height:30px;"/>
					<label style="margin-left:10px;">病人姓名</label>
					<input id="aPatName" class="hisui-validatebox" style="width:143px;vertical-align:bottom;"/>
					<label style="margin-left:23px;">登记号</label>
					<input id="aPatNo" class="hisui-validatebox" style="width:143px;vertical-align:bottom;"/>
					<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" onclick="QryWaitHosPat()" style="margin-left:10px;">查询</a>
					<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-export'" onclick="ExpWaitToHos()" style="margin-left:10px;">导出</a>
				</div>
				<div style="margin:8px;">
		        	<label>结束日期</label>
		        	<input id="EndDate" class="hisui-datebox textbox" style="width:150px;">
					<label style="margin-left:10px;">当前分级</label>
					<input id="aEmPcLv" class="hisui-validatebox" style="width:150px;vertical-align:bottom;"/>
					<label style="margin-left:10px;">病人状态</label>
					<input id="WalkStatus" class="textbox" style="width:150px;height:30px;line-height:30px;"/>
					<label style="margin-left:10px;">删除理由</label>
					<input id="DelReason" class="textbox" style="width:150px;height:30px;line-height:30px;"/>
				</div>
			</div>
			<div data-options="region:'center',headerCls:'panel-header-gray',title:'',border:false">
				<table id="bmDetList"></table>
			</div>
		</div>
	</div>
</div>
<!-- 新建窗口 -->
<div id="newConWin" style="padding:10px">
	<div id="" class="hisui-layout" style="width:600px;height:400px;" data-options="clickExpand:true,fit:true,collapsible:false">  
		<div data-options="region:'north',title:'病人基本信息',split:true,headerCls:'panel-header-gray'" style="height:215px;">
			<div style="display:none">
				<span>ID</span>
				<span width="180px"><input class="textbox" id="WtID"/></span>
				<span>EpisodeID</span>
				<span width="180px"><input class="textbox" id="EpisodeID"/></span>
			</div>
			
			<div class="line">
				<span class="line-itm1">登记号</span><input id="PatNo" class="textbox" style="width:170px;"/>
				<span class="line-itm1">姓名</span><input id="PatName" class="textbox" style="width:170px;" disabled/>
				<span class="line-itm1">年龄</span><input id="PatAge" class="textbox" style="width:170px;" disabled/>
			</div>
			<div class="line">
				<span class="line-itm1">性别</span><input id="PatSex" class="textbox" style="width:170px;" disabled/>
				<span class="line-itm1">出生日期</span><input id="PatBDay" class="textbox" style="width:170px;" disabled/>
				<span class="line-itm1">来诊日期</span><input id="LvDate" class="textbox" style="width:170px;" disabled/>
			</div>
			<div class="line">
				<span class="line-itm1">来诊时间</span><input id="LvTime" class="textbox" style="width:170px;" disabled/>
				<span class="line-itm1">就诊天数</span><input id="AdmDays" class="textbox" style="width:170px;" disabled/>
				<span class="line-itm1">主管医生</span><input id="PatDoc" class="textbox" style="width:170px;" disabled/>
			</div>
			<div class="line">
				<span class="line-itm1">急诊诊断</span><input id="PatDiags" class="textbox" style="width:170px;" disabled/>
				<span class="line-itm1">呼吸类型</span><input id="ResType" class="textbox" style="width:177px;"/>
			</div>
		</div>
		<div data-options="region:'center',title:'拟入院信息',split:true,headerCls:'panel-header-gray'" style="">
			<div class="line">
				<span class="line-itm2">拟入院科室1</span><input id="InLoc_1" class="hisui-validatebox" style="width:150px;">
				<span class="line-itm2">拟入院病区1</span><input id="InWard_1" class="hisui-validatebox" style="width:150px;" disable>
				<span class="line-itm2">会诊情况1</span><input id="CsRes_1" class="hisui-validatebox" style="width:150px;">
			</div>
			<div class="line">
				<span class="line-itm2">拟入院科室2</span><input id="InLoc_2" class="hisui-validatebox" style="width:150px;">
				<span class="line-itm2">拟入院病区2</span><input id="InWard_2" class="hisui-validatebox" style="width:150px;" disable>
				<span class="line-itm2">会诊情况2</span><input id="CsRes_2" class="hisui-validatebox" style="width:150px;">
			</div>
			<div class="line">
				<span class="line-itm2">拟入院科室3</span><input id="InLoc_3" class="hisui-validatebox" style="width:150px;">
				<span class="line-itm2">拟入院病区3</span><input id="InWard_3" class="hisui-validatebox" style="width:150px;" disable>
				<span class="line-itm2">会诊情况3</span><input id="CsRes_3" class="hisui-validatebox" style="width:150px;">
			</div>
		</div>  
	</div>
</div>

<div id="toolbar" class="toolbar">
	<table cellspacing="0" cellpadding="0" >
		<tr>
			<td><a class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true" onclick="javascript:newWaitHosPat()" >新增</a></td>
	     	<td><a class="hisui-linkbutton" data-options="iconCls:'icon-back',plain:true" onclick="javascript:updWaitToHos()" >修改</a></td>
	     	<td><a class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true" onclick="javascript:delWaitToHos()">删除</a> </td>
	    	<td><a class="hisui-linkbutton" data-options="iconCls:'icon-edit',plain:true" onclick="javascript:ModPatChkLev()">修改分级</a></td>	
	    </tr>
	</table>
</div>
</body>
</html>
