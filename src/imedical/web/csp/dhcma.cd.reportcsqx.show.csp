<!-- 主页面-->
<div id="CSQXReport" class="hisui-layout" data-options="fit:true" style="width:100%;height:100%;">
	<div data-options="region:'center',border:false,collapsible:false">
		<div class="page-body" id="divbody" style="width:1285px;">
			<div class="report-header">
				<span>出生缺陷儿报告卡</span>
			</div>
			<div class="report-content">
				<div class="hisui-panel" id="BaseInfo" data-options="title:'产母基本信息',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="BaseInfoTab"  class="report-tab">
						<tr class="report-tr">
							<td class="report-td">卡片编号</td>
							<td><input class="textbox" id="txtKPBH" disabled /></td>
							<td class="report-td">登记号</td>
							<td><input class="textbox" id="txtRegNo" disabled /></td>
							<td class="report-td"><font color='red'>*</font>姓名</td>				
							<td><input class="textbox" id="txtPatName" disabled /></td>
							<td class="report-td"><font color='red'>*</font>性别</td>					
							<td><input class="textbox" id="txtSex" disabled /></td>
							<td class="report-td"><font color='red'>*</font>年龄</td>
							<td>
								<input class="textbox" id="txtAge" style="width:60px;" disabled />
								<input class="textbox" id="cboPatAgeDW" style="width:84px;" disabled />
							</td>
						</tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>出生日期</td>				
							<td><input class="hisui-datebox textbox" id="txtBirthDay"/></td>
							<td class="report-td"><font color='red'>*</font>户籍类型</td>					
							<td ><input class="textbox" id="cboRegType"/></td>
							<td class="report-td"><font color='red'>*</font>民族</td>				
							<td><input class="textbox" id="cboNation" /></td>
							<td class="report-td"><font color='red'>*</font>文化</td>					
							<td><input class="textbox" id="cboEducation"/></td>
							<td class="report-td"><font color='red'>*</font>职业</td>					
							<td ><input class="textbox" id="cboOccupation"/></td>
							
					   </tr>
					   <tr class="report-tr">
						
							<td class="report-td"><font color='red'>*</font>孕次</td>					
							<td><input class="hisui-numberbox textbox" style="width:125px;" id="txtGestational"/>		次</td>
							<td class="report-td"><font color='red'>*</font>产次</td>					
							<td><input class="hisui-numberbox textbox" style="width:125px;" id="txtParity"/>		次</td>
							<td class="report-td"><font color='red'>*</font>家庭年<br>人均收入</td>					
							<td><input class="textbox" id="cboPreIncome"/></td>
							<td class="report-td">是否婚检</td>
							<td><input class='hisui-checkbox' type="checkbox" id="IsPreExam" /></td>
							<td class="report-td">是否辅助生殖</td>	
							<td><input class='hisui-checkbox' type="checkbox" id="IsReproduc" /></td>
						</tr>
					   <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>省</td>				
							<td><input class="textbox" id="cboCurrProvince"/></td>
							<td class="report-td"><font color='red'>*</font>市</td>				
							<td><input class="textbox" id="cboCurrCity"/></td>
							<td class="report-td"><font color='red'>*</font>县</td>					
							<td><input class="textbox" id="cboCurrCounty" /></td>
							<td class="report-td"><font color='red'>*</font>乡/镇</td>					
							<td><input class="textbox" id="cboCurrVillage" /></td>
							<td class="report-td"><font color='red'>*</font>村(小区)</td>					
							<td><input class="textbox" id="txtCurrRoad" /></td>
					    </tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>居住地址</td>				
							<td colspan="5"><input class="textbox" id="txtCurrAddress" style="width:654px;"></td>
							<td class="report-td"><font color='red'>*</font>居住地所属区</td>					
							<td><input class="textbox" id="txtArea" /></td>
							<td class="report-td"><font color='red'>*</font>居住时间</td>					
							<td><input  class="hisui-numberbox textbox" style="width:125px;" id="txtAreaTime"/>	年</td>
					   </tr>
					   
					</table>
				</div>
				
				<div class="hisui-panel" id="DetailInfo" data-options="title:'出生缺陷儿情况',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="DetailInfoTab"  class="report-tab">
						<tr class="report-tr">
							<td class="report-td">报卡科室</td>				
							<td><input class="textbox" id="cboCRReportLoc" disabled /></td>
							<td class="report-td"><font color='red'>*</font>报卡医生</td>					
							<td><input class="textbox" id="txtDoctor" disabled /></td>
							<td class="report-td"><font color='red'>*</font>报卡日期</td>				
							<td><input class="hisui-datebox textbox" id="dtRepDate" disabled /></td>
							<td class="report-td"><font color='red'>*</font>出生日期</td>				
							<td><input class="hisui-datebox textbox" id="dtBirthday"/></td>
							<td class="report-td"><font color='red'>*</font>出生医院</td>					
							<td><input class="textbox" id="txtBirHospital" /></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>性别</td>				
							<td><input class="textbox" id="cboChildSex"/></td>
							<td class="report-td"><font color='red'>*</font>胎龄</td>					
							<td><input class="hisui-numberbox textbox" id="txtGestAge" style="width:125px;" placeholder="单位/周" />	周</td>
							<td class="report-td"><font color='red'>*</font>体重</td>				
							<td><input class="hisui-numberbox textbox" data-options="precision:2" id="txtWeight" style="width:125px;" placeholder="单位/克"/>	克</td>
							<td class="report-td"><font color='red'>*</font>身长</td>				
							<td><input class="hisui-numberbox textbox" id="txtLength" style="width:125px;" placeholder="单位/厘米"/>		cm</td>
							<td class="report-td"><font color='red'>*</font>畸形数</td>				
							<td><input class="hisui-numberbox textbox" style="width:125px;" id="txtAbnormal"/>		处</td>	
					    </tr>
						
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>胎数</td>
							<td><input class="textbox" id="cboFoetNum" /></td>
							<td class="report-td"><font color='red'>*</font>若双胎或多胎请选择</td>					
							<td><input class="textbox" id="cboMultiple" disabled/></td>
							<td class="report-td">为出生缺陷后治疗性引产</td>					
							<td><input class='hisui-checkbox' type="checkbox" id="chkIsInduced" /></td>
							<td class="report-td"><font color='red'>*</font>转归</td>					
							<td colspan="3"><input class="textbox" style="width:415px;" id="cboOutCome" /></td>
					    </tr>
					    <tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>畸形确诊<br>时间</td>
							<td><input class="textbox" id="cboMalfTime" /></td>
							
							<td class="report-td"><font color='red'>*</font>产前诊断孕周</td>					
							<td><input class="textbox" id="txtPreWeek" placeholder="若诊断为产前，请填写" disabled/></td>	
							<td class="report-td">产前诊断医院</td>					
							<td><input class="textbox" id="txtDiagHos" placeholder="若诊断为孕前，请填写" disabled/></td>
							<td class="report-td"><font color='red'>*</font>诊断依据</td>				
							<td><input class="textbox" id="cboDiagBasis" /></td>
							<td class="report-td"><font color='red'>*</font>诊断备注</td>					
							<td><input class="textbox" id="txtDiagBasis" placeholder="其他诊断依据"/></td>	
					    </tr>
						<tr class="report-tr">
							<td class="report-td"><font color='red'>*</font>缺陷诊断</td>				
							<td><input class="textbox" id="cboDefect"/></td>
							<td class="report-td">位置</td>					
							<td><input class="textbox" id="cboPosition"/></td>
							<td class="report-td"><font color='red'>*</font>先心病类型</td>				
							<td><input class="textbox" id="cboChdType" disabled/></td>
							<td class="report-td">尿道下裂</td>				
							<td  colspan="3"><input class="textbox" id="txtBelowUre" style="width:407px;" placeholder="写明尿道下裂类型" disabled/></td>
					    </tr>
						<tr class="report-tr">
							<td class="report-td">胎儿水肿<br>综合征</td>				
								<td  colspan="3"><input class="textbox" id="txtFeDSyndrome" style="width:407px;" placeholder="是否确诊为*型（中重度）地中海贫血" disabled/></td>
								<td class="report-td">其他缺陷名</td>				
							<td  colspan="5"><input class="textbox" id="txtOtherDef" style="width:652px;" placeholder="写明病名或详细描述" disabled/></td>
					    </tr>
						
					</table>
				</div>			
			<div class="hisui-panel" id="DetailInfo" data-options="title:'孕早期情况 (如有类似情况，请在下面的文本框中填写)',headerCls:'panel-header-gray',iconCls:'icon-paper'">
					<table id="DetailInfoTab"  class="report-tab">
					    <tr class="report-tr">
							<td class="report-td">发热（>38度）</td>					
							<td><input class="textbox" id="txtYZQHBFR" /></td>	
							<td class="report-td">病毒感染类型</td>					
							<td><input class="textbox" id="txtYZQHBBDGR" /></td>
							<td class="report-td">糖尿病</td>					
							<td><input class="textbox" id="txtYZQHBTNB" /></td>
							<td class="report-td">其他患病类型</td>					
							<td><input class="textbox" id="txtYZQHB" /></td>
						</tr>
						<tr class="report-tr">
							<td class="report-td">磺胺类药物名称</td>					
