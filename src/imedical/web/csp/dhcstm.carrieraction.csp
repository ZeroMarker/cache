<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

    s Action=$Get(%request.Data("actiontype",1))
    s Start=$Get(%request.Data("start",1))
    s Limit=$Get(%request.Data("limit",1))
    s Sort=$Get(%request.Data("sort",1))
    s Dir=$Get(%request.Data("dir",1))

    s conditionCode=$Get(%request.Data("conditionCode",1))
    s conditionName=$Get(%request.Data("conditionDesc",1))
     s conditionType=$Get(%request.Data("conditionType",1))
     s MPNumber=$Get(%request.Data("MPNumber",1))
    s listData=$Get(%request.Data("data",1))
    s RowId=$Get(%request.Data("rowid",1))
    //"0","35","RowId","desc","","",""
    //查询配送商信息
    i Action = "query" d
    .s result = ##class(web.DHCSTM.DHCCarrier).Query(Start,Limit,Sort,Dir,conditionCode,conditionName,conditionType,MPNumber)
    .i result = "" d
    ..w "{results:0,rows:[]}"
    .e  d
    ..w result

    //保存配送商
    i Action = "save" d
    .s result = ##class(web.DHCSTM.DHCCarrier).Save(listData)
    .s result=+result
	.i result =0 d
    ..w "{success:'true',info:''}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"

    //删除配送商
    i Action = "delete" d
    .s result = ##class(web.DHCSTM.DHCCarrier).Delete(RowId)
    .i result = 0 d
    ..w "{success:'true',info:''}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"
    
    //查询物资信息
    //删除配送商
    i Action = "select" d
    .s result = ##class(web.DHCSTM.DHCCarrier).Select(RowId)
    .i result = "" d
    ..w "{results:0,rows:[]}"
    .e  d
    ..w result

    

</script>