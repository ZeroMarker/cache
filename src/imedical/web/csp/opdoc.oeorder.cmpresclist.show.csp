<div class="hisui-layout" data-options="fit:true,border:false">
	<div id="Search" data-options="region:'north',border:false" style="height:130px;">
		<table class="search-table">
			<tr>
				<td class="r-label">
					<label>登记号</label>
				</td>
				<td>
					<input id="PatNo" class="textbox">
				</td>
				<td class="r-label">
					<label>科室</label>
				</td>
				<td>
					<input id="LocList" class="textbox">
				</td>
				<td class="r-label">
					<label>卡类型</label>
				</td>
				<td>
					<input id="CardTypeNew" disabled="disabled" class="textbox" placeholder="卡类型...">
				</td>
				<td class="r-label">
					<label>卡号</label>
				</td>
				<td>
					<input id="CardNo" class="textbox">
				</td>
				<td class="r-label">
					<a id="ReadCard" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-card'">读卡</a>
				</td>
			</tr>
			<tr>
				<td class="r-label">
					<label>姓名</label>
				</td>
				<td>
					<input id="PatName" disabled="disabled" class="textbox">
				</td>
				<td class="r-label">
					<label>性别</label>
				</td>
				<td>
					<input id="PatSex" disabled="disabled" class="textbox">
				</td>
				<td class="r-label">
					<label>开始日期</label>
				</td>
				<td>
					<input id="StartDate" class="textbox hisui-datebox" data-options="formatter:myformatter,parser:myparser">
				</td>
				<td class="r-label">
					<label>结束日期</label>
				</td>
				<td>
					<input id="EndDate" class="textbox hisui-datebox" data-options="formatter:myformatter,parser:myparser">
				</td>
				<td class="r-label">
					<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
				</td>
				
			</tr>
			<tr>
				<td class="r-label">
					<label>患者级别</label>
				</td>
				<td>
					<input id="PoliticalLevel" disabled="disabled" class="textbox">
				</td>
				<td class="r-label">
					<label>患者密级</label>
				</td>
				<td>
					<input id="SecretLevel" disabled="disabled" class="textbox">
				</td>
				<td class="r-label">
					<label>病案号</label>
				</td>
				<td>
					<input id="Patmed" class="textbox">
				</td>
				<td colspan="2" class="r-label" style="text-align:left;">
					<input class='hisui-checkbox' type="checkbox" label="全选/取消全选" id="selAllPresno">
				</td>
				<td class="r-label">
					<a id="CopyOrd" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-copy'">复制</a>
				</td>
			</tr>
		</table>
	</div>
	<div  data-options="region:'center',border:false" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" data-options="" style="border:1px solid #ccc;border-radius:4px;">
			<table id="CMPrescListTable"></table>
		</div>
	</div>
</div>
