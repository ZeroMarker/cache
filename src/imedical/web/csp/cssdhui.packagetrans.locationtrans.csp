<!--手术器械位置跟踪-->
<!DOCTYPE html>
<html lang="zh-CN">
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<SERVER>
    d ##class(web.CSSDHUI.Util.RQServerConfig).GetApplet()
</SERVER>
<head>
    <title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
    <EXTHEALTH:HEAD></EXTHEALTH:HEAD>
    <HISUI/>
    <CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
    <div class="GridPadding" style="width:300px;" data-options="region:'west',border:false" >
        <div class="hisui-panel" data-options="title:'查询条件',headerCls:'panel-header-gray',fit:true,iconCls:'icon-paper'">
            <div class="hisui-layout" data-options="fit:true">
                <div data-options="region:'center',border:false">
                    <table id="Conditions" class="ReportCondition"  align="center">
                        <tr id="ptype">
                          <td><label>区域</label></td><td><select id="PsnType" name="PsnType" class="textbox hisui-combobox" style="width:155px;">
                             <option value="污染区">污染区</option>
                             <option value="清洗机">清洗机</option>
                             <option value="打包区">打包区</option>
                             <option value="灭菌机">灭菌机</option>
                             <option value="无菌区">无菌区</option>
                          </select></td>
                        <tr id="pkgdr">
                            <td><label>消毒包名称</label></td><td><input id="OprPkg" name="OprPkg" class="textbox hisui-combobox" style="width:155px;"></td>
                        </tr>
                        <tr>
                            <td><label>汇总类型</label></td><td align="left"><input class='hisui-radio' type="radio" label='位置跟踪' name='ReportType' value='FlagPackageProcessTrans' data-options='checked:true' id="aa1"></td>
                        </tr>
                    </table>
                    <table class="Condition" align="center" class="Condition">
                        <tr>
                            <td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
                            <td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
                        </tr>
                    </table>
                </div>
                <!--<div class="PartLine" data-options="region:'south',border:false" style="height:340px;">
                </div>
                -->
            </div>
        </div>
    </div>
    <div class="hisui-tabs tabs-gray GridPadding" data-options="region:'center'" id="tabs" style="border:0px;padding-left: 0px">
        <div title="报表" data-options="iconCls:'icon-add-note'" class="ReportImage"></div>
    </div>
    
    <script type="text/javascript" src="../scripts/cssd/Common/encode.js"></script>
    <script type="text/javascript" src="../scripts/cssd/Trans/LocationTrans.js"></script>
</body>
</html>