<div data-options="region:'west',border:false,split:false" style="padding:10px 5px 10px 10px;width:432px;">
	<div class="hisui-panel" data-options="title:'#(..Get("治疗项目列表"))#',fit:true,headerCls:'panel-header-gray',iconCls:'icon-add-note'" style="border-top:0px;padding:10px 0px 0px 0px;border-radius:0px 0px 4px 4px;">			
		<div class="hisui-panel" id="tree_panel" style="padding:0px 0px 10px 10px" data-options="border:false,fit:false">
			<input id="QryArcDesc" class="hisui-searchbox" style="width:395px" data-options="searcher:ReloadCureItemTree,prompt:'#(..Get("请输入名称/拼音码"))#'"></input> 
		</div>
		<div style="padding:0px 10px">
			<ul id="CureItemTree" class="hisui-tree" data-options="lines:true"></ul> 
		</div>
	</div> 
</div>   
<div data-options="region:'center',border:false,split:false" style="padding:10px 10px 10px 0px;"> 
	<div class="hisui-panel" title="#(..Get("治疗项目可预约资源排程列表"))#"
	data-options="iconCls:'icon-add-note',border:true,headerCls:'panel-header-gray',fit:true">   
		 <div class="hisui-layout" data-options="fit:true,border:false,collapsible:true">
			<div data-options="region:'north',split:false,border:false" style="padding:10px;">
				<label for="Apply_StartDate" class="fr-label">#(..Get("开始日期"))#</label>
				<input id="Apply_StartDate" class="hisui-datebox" type="text" name="cureAppStartDate" style="width:130px;"/>
				<a href="javascript:void(0)" id="btnSearchApp" data-options="iconCls:'icon-w-find'" class="hisui-linkbutton" style="margin:0px 10px;">#(..Get("查询"))#</a>
			</div>
			<div data-options="region:'center',split:false,border:false">
		    	<!--<table id="tabCureItemSchedule"></table>-->
		    	<div id="ScheduleTab" class="hisui-tabs tabs-gray c-p-bd-t" data-options="tabPosition:'top',fit:true,border:false">
		    	</div>     
		    </div>			
		</div>  
	</div>
</div>
