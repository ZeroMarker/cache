<csp:content charset="UTF-8">
<SERVER>
s Action=$Get(%request.Data("actiontype",1))

i Action = "SaveDataInci" d
	.S ListInc=$Get(%request.Data("ListInc",1))
	.S drugRowid=$Get(%request.Data("drugRowid",1))
	.s NewItmRowId=$Get(%request.Data("NewItmRowId",1))
	.S result = ##class(web.DHCSTM.INCITM).SaveDataInci(drugRowid,ListInc,NewItmRowId)
	.i result>0 d
	..w "{success:'true',info:'"_result_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

</SERVER>