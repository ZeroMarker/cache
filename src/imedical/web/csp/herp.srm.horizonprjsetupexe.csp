<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	 i ##Class(websys.SessionEvents).SessionExpired() q 1
	 q 1
</csp:method>
<csp:content charset="utf-8">
<script language="cache" runat="server">

	s action = $Get(%request.Data("action",1))
	s sortField = $Get(%request.Data("sort",1))
	s sortDir = $Get(%request.Data("dir",1))
	s start = $Get(%request.Data("start",1))
	s limit = $Get(%request.Data("limit",1))
	s searchField = $Get(%request.Data("searchField",1))
	s searchValue = $Get(%request.Data("searchValue",1))
	s str=$Get(%request.Data("query",1))
	//查询条件
	s ApplyStart=$Get(%request.Data("ApplyStart",1))
	s Applyend=$Get(%request.Data("Applyend",1))
	s deptdr=$Get(%request.Data("deptdr",1))
	s SubSource=$Get(%request.Data("SubSource",1))
	s projStatus=$Get(%request.Data("projStatus",1))
	s HeadDr=$Get(%request.Data("HeadDr",1))
	s PName=$Get(%request.Data("PName",1))
	s ResAudit=$Get(%request.Data("ResAudit",1))
	s userdr=$Get(%request.Data("userdr",1))
	

	//添加项
	s StartDate= $Get(%request.Data("StartDate",1))
	s Name = $Get(%request.Data("Name",1))
	s SubNo = $Get(%request.Data("SubNo",1))
	s DeptDr = $Get(%request.Data("DeptDr",1))
	s AppFunds = $Get(%request.Data("AppFunds",1))
	s EndDate = $Get(%request.Data("EndDate",1))
	s PatentNum = $Get(%request.Data("PatentNum",1))
	s Inventors = $Get(%request.Data("Inventors",1))
	s ConDate = $Get(%request.Data("ConDate",1))
	s Participants =  $Get(%request.Data("Participants",1))
	s ParticipantsIDs = $Get(%request.Data("ParticipantsIDs",1))
	s RelyUnitsIDs = $Get(%request.Data("RelyUnitsIDs",1))
	s HeadDR=$Get(%request.Data("HeadDR",1))	
	s RelyUnit = $Get(%request.Data("RelyUnit",1))
	s SubUser = $Get(%request.Data("SubUser",1))
	s Remark = $Get(%request.Data("Remark",1))
	s usercheckdr =  $Get(%request.Data("usercheckdr",1))
	s checker =  $Get(%request.Data("checker",1))
	s view =  $Get(%request.Data("view",1))
    s ProjectsName = $Get(%request.Data("ProjectsName",1))
			
	s FundGov =  $Get(%request.Data("FundGov",1))
	s FundOwn =  $Get(%request.Data("FundOwn",1))
	s FundMatched =  $Get(%request.Data("FundMatched",1))
	s IsGovBuy =  $Get(%request.Data("IsGovBuy",1))
	s AlertPercent =  $Get(%request.Data("AlertPercent",1))
	
	s mainrowid =  $Get(%request.Data("mainrowid",1))
	s headdr = $Get(%request.Data("headdr",1))
	s participantsdrs = $Get(%request.Data("participantsdrs",1))
	s subsource = $Get(%request.Data("subsource",1))

	s MonographNum = $Get(%request.Data("MonographNum",1))
	s PaperNum = $Get(%request.Data("PaperNum",1))
	s PatentNum = $Get(%request.Data("PatentNum",1))
	s InvInCustomStanNum = $Get(%request.Data("InvInCustomStanNum",1))
	s TrainNum = $Get(%request.Data("TrainNum",1))
	s HoldTrainNum = $Get(%request.Data("HoldTrainNum",1))
	s InTrainingNum = $Get(%request.Data("InTrainingNum",1))
	s IsEthicalApproval = $Get(%request.Data("IsEthicalApproval",1))
	s CompleteUnit = $Get(%request.Data("CompleteUnit",1))
	//立项
	s rowid=$Get(%request.Data("rowid",1))
	s subno=$Get(%request.Data("subno",1))
	s issuedDate=$Get(%request.Data("issuedDate",1))
	s grafunds=$Get(%request.Data("grafunds",1))
	s FundOwn=$Get(%request.Data("FundOwn",1))
	s FundMatched=$Get(%request.Data("FundMatched",1))
	s Type=$Get(%request.Data("Type",1))
	s Year=$Get(%request.Data("Year",1))
	s Department=$Get(%request.Data("Department",1))
	s PrjLife=$Get(%request.Data("PrjLife",1))
	s ProjectDR=$Get(%request.Data("ProjectDR",1))
	
	i action = "list" d
	.w ##class(herp.srm.udata.uHorizonPrjSetUp).list(sortField, sortDir, start, limit, ApplyStart, Applyend, deptdr, SubSource, HeadDr, PName,ResAudit,userdr,Type)
	
	i action = "setup" d
	.s result = ##class(herp.srm.udata.uHorizonPrjSetUp).projectSetupApply(rowid,subno,grafunds,issuedDate,FundOwn,FundMatched)
	.i result = 0 d
	..w "{success:'true',refresh:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	i action = "deptList" d
	.w ##class(herp.srm.udata.uSRMCommon).DeptList(start, limit, str)
	
	i action = "sourceList" d
	.w ##class(herp.srm.udata.uSRMCommon).CalSubSource(start, limit, str)
	
	i action = "GetRelyUnit" d 
	.w ##class(herp.srm.udata.uSRMCommon).GetUintInfo(start, limit, str)
	
	i action = "applyerList" d
	.w ##class(herp.srm.udata.uSRMCommon).UserList(start, limit, str)
	
	/*
	i action = "RelyUnitsID" d
	.w ##class(herp.srm.udata.uSRMProjectsInfos).RelyUnitsList(start, limit, RelyUnitsIDs)
	*/
	
	i action = "add" d
	.s result = ##class(herp.srm.udata.uHorizonPrjSetUp).InsertRec(ProjectsName, DeptDr, HeadDr, Participants, SubSource, SubNo,AppFunds, StartDate, EndDate, RelyUnit, SubUser, Remark,FundGov, FundOwn, FundMatched, IsGovBuy, AlertPercent,MonographNum,PaperNum,PatentNum,InvInCustomStanNum,TrainNum,HoldTrainNum,InTrainingNum,IsEthicalApproval,CompleteUnit,Type,Year,Department,PrjLife)
	.i result = 0 d
	..w "{success:'true',refresh:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
	i action = "edit" d
	.s result = ##class(herp.srm.udata.uHorizonPrjSetUp).UpdateProjectsInfo(rowid, ProjectsName, Participants, HeadDr, EndDate, SubSource, AppFunds, StartDate, SubUser, DeptDr, RelyUnit, Remark,FundGov, FundOwn, FundMatched, IsGovBuy, AlertPercent,MonographNum,PaperNum,PatentNum,InvInCustomStanNum,TrainNum,HoldTrainNum,InTrainingNum,IsEthicalApproval,CompleteUnit,Type,Year,Department,PrjLife)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
	i action = "del" d
	.s result = ##class(herp.srm.udata.uHorizonPrjSetUp).DelProjectsInfo(rowid)
	.i result = 0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

	i action = "ChangeDataStatus" d
	.s result = ##class(herp.srm.udata.uHorizonPrjSetUp).ChangeDataStatus(rowid)
	.i result = 0 d
	..w "{success:'true',refresh:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
	i action= "GetYear" d
   .w ##class(herp.srm.udata.uSRMCommon).GetYear(start, limit, str)
	
	//页签形式的科研课题人员信息
	i action = "listparticipantsInfo" d
	.w ##class(herp.srm.udata.uHorizonPrjSetUp).ListParticipantsInfo(sortField, sortDir, start, limit, participantsdrs)
    
    //弹出框形式的人员位次
    i action = "listparticipants" d
	.w ##Class(herp.srm.udata.uSRMCommon).ListPrjParticipantsInfo(start, limit, ParticipantsIDs)
	//弹出框项目负责人信息
	i action = "listresponsepeople" d
	.w ##Class(herp.srm.udata.uHorizonPrjSetUp).ListresponsepeopleInfo(start, limit, HeadDR)
	//弹出框项目经费信息
	i action = "listprjreimbursemen" d
	.w ##Class(herp.srm.udata.uHorizonPrjSetUp).ListprjreimbursemenInfo(start, limit, ProjectDR)
    i action = "gettitle" d
	.s result = ##Class(herp.srm.udata.uSRMCommon).GetTitle(rowid)
	.i result = "" d
	..w "{success:'false',info:'"_result_"'}"	
	.e  d
	..w "{success:'true',refresh:'true',info:'"_result_"'}"
	
	i action = "RelyUnitsID" d
	.w ##class(herp.srm.udata.uSRMCommon).RelyUnitsList(start, limit, RelyUnitsIDs)
	i action = "listrelyunits" d
	.w ##class(herp.srm.udata.uSRMCommon).PrjRelyUnitsList(start, limit, RelyUnitsIDs)
</script>