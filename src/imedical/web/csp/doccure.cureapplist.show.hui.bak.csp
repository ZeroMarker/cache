<div class="hisui-layout" data-options="fit:true,border:false">
  	<div id="region_north" data-options="region:'north',split:true,border:false" style="height:300px;"> 
  	   <div class="hisui-layout" data-options="fit:true,border:false"> 
  	     <div data-options="region:'north',split:true,border:false" style="height:100px"> 
     			<div id="p2" class="hisui-panel" title="治疗申请单列表"  data-options="iconCls:'icon-edit',fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false"> 
     				<!--治疗记录申请单列表-->
		        	<table border="0"> 
			      	<tbody>
			      	<tr>
			       		<td><p align="right">卡类型</p></td>
						<td><input id="cardType" type="text" name="cardType" style="width:110px"/></td> 
			      		<td><p align="right">卡号</p></td>
						<td><input id="cardNo" type="text" name="cardNo" style="width:100px"/></td>  
			        	<td><p align="right">登记号</p></td>
						<td><input id="patNo" type="text" name="patNo" style="width:100px"/><input id="PatientID" type="hidden" name="PatientID" style="width:100px"/></td>
			        	<td><p align="right">姓名</p></td>
						<td><input id="patName" type="text" name="patName" style="width:80px"/></td>
			        	<td><p align="right">开始日期</p></td>
						<td><input id="StartDate" type="text" class="hisui-datebox" style="width:115px"></input></td>
			        	<td><p align="right">结束日期</p></td>
						<td><input id="EndDate" type="text" class="hisui-datebox" style="width:115px"></input></td>
						<csp:if condition='myTriage="N"'>
							<td><input id="CancelDis" type="checkbox" name="CancelDis" class="hisui-checkbox" style="height:15px;padding: 2px 5px;" label="已撤销"/></td>
							<td><input id="FinishDis" type="checkbox" name="FinishDis" class="hisui-checkbox" style="height:15px;padding: 2px 5px;" label="已完成"/></td>
						<csp:else>
							<td><input id="Distributed" type="checkbox" name="Distributed" class="hisui-checkbox" style="height:15px;padding: 2px 5px;" label="已分配"/></td>
							<td><input id="ALLDis" type="checkbox" name="ALLDis" class="hisui-checkbox" style="height:15px;padding: 2px 5px;" label="全部"/></td>
						</csp:if>
						<td align="right"><a id="btnFind" class="hisui-linkbutton big" data-options="iconCls:'icon-search',plain:true">查找</a></td>	 
			       		<td align="right"><a id="btnReadCard" class="hisui-linkbutton big" data-options="iconCls:'icon-read-card',plain:true">读卡</a></td>
			       		<td align="right"><a id="btnClear" class="hisui-linkbutton big" data-options="iconCls:'icon-clear',plain:true">清屏</a></td>	 
			       		<csp:if condition='myTriage="N"'>
			       			<td align="right"><a id="btnExecOrd" class="hisui-linkbutton big" data-options="iconCls:'icon-exe-order',plain:true">执行</a></td>
			       		</csp:if>
			       		<!--<csp:if condition='CureNeedTriage="1"'>
			       			<td align="right"><a id="btnAccept" class="hisui-linkbutton">接收分配</a></td>
			       		</csp:if>
					    <td align="right"><a id="btnCancelExecOrd" class="hisui-linkbutton">撤销执行</a></td>
			       		<td align="right"><a id="btnReadInsuCard" class="hisui-linkbutton">读医保卡</a></td>-->	 
			       </tr>
			       </tbody>
			     </table>
     			</div> 
    	  </div>
  	     <div data-options="region:'center',split:true,border:false" > 
     			<div id="p1" class="hisui-panel" data-options="iconCls:'icon-edit',fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false"> 
      				 <!--治疗记录申请单列表table-->
	                 <table id="tabCureApplyList"></table>
     			</div> 
    	  </div> 
    	  
  	   </div>
  	</div>
    <div data-options="region:'center',split:true,closable:false,collapsible:true">
        <div id="tabs" class="hisui-tabs" style="padding:1px;height:355px" data-options="border:false">
       		<csp:if condition='myTriage="N"'>
	       		<div id="Apply_Reslist" title="预约"  style="padding:5px;">
	        		<iframe name="applyreslist" scrolling="no" frameborder="0"  src="doccure.applyreslist.hui.csp?OperateType=ZLYY" style="width:100%;height:100%;"></iframe>
	        	</div>
	        	<div id="Apply_Applist" title="预约列表" style="padding:5px">			
	        		<iframe name="applyapplist" scrolling="no" frameborder="0"  src="doccure.applyapplist.hui.csp?OperateType=ZLYY" style="width:100%;height:100%;"></iframe>
	        	</div>
        	<csp:else>
        		<div id="Apply_TriageReslist" title="可分配资源列表" style="padding:5px">			
	        		<iframe name="triagereslist" scrolling="no" frameborder="0"  src="doccure.triagereslist.hui.csp?OperateType=ZLYY" style="width:100%;height:100%;"></iframe>
	        	</div>
	        	<div id="Apply_Triagelist" title="分配列表" style="padding:5px">			
	        		<iframe name="triagelist" scrolling="no" frameborder="0"  src="doccure.curetriagelist.hui.csp?OperateType=ZLYY" style="width:100%;height:100%;"></iframe>
	        	</div>
        	</csp:if>
     </div>
   </div>          
</div>