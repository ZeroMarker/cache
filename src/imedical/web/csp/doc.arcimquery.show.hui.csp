<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false" style="height:135px;padding:10px 10px 0 10px;">
		<div id="title-panel" class="hisui-panel" data-options="title:'医嘱字典查询',headerCls:'panel-header-gray',iconCls:'icon-apply-check'" style="border:1px solid #ccc;border-top:0;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="Category">#(..Get("大类"))#</label>
					</td>
					<td>
						<input class="textbox" id="Category"/>
					</td>
					<td class="r-label r-label-1">
						<label for="SubCategory">#(..Get("子类"))#</label>
					</td>
					<td>
						<input class="textbox" id="SubCategory"/>
					</td>
					<td class="r-label r-label-1">
						<label for="OrderStatus">#(..Get("状态"))#</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="OrderStatus"/>
					</td>
					<td class="r-label-1">
						<input id="ARCIM" class='hisui-checkbox' type="checkbox" data-options="checked:true" label="显示医嘱项">
					</td>
					<td class="r-label-1">
						<a href="#" id="BFind" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:116px;">查询</a>            
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="Alias">#(..Get("别名"))#</label>
					</td>
					<td>
						<input class="textbox" id="Alias"/>
					</td>
					<td class="r-label r-label-1">
						<label for="Desc">#(..Get("名称(模糊查询)"))#</label>
					</td>
					<td>
						<input class="textbox" id="Desc"/>
					</td>
					<td>
					</td>
					<td>
					</td>
					<td class="r-label-1">
						<input id="ARCOS" class='hisui-checkbox' type="checkbox" data-options="checked:true" label="#(..Get("显示医嘱套"))#">
					</td>
					<td class="r-label-1">
						<a href="#" id="BAddAlias" class="hisui-linkbutton" data-options="iconCls:'icon-w-add'">添加别名</a>   
					</td>
					<!--td class="r-label">
						<a href="#" id="BPrint" class="hisui-linkbutton" data-options="iconCls:'icon-w-print'">打印</a>            
					</td-->
				</tr>
				
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="ArcimTab"></table>
		</div>
	</div>
</div>
