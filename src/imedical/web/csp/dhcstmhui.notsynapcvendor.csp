<!--托管配送-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
		<style>
		.imgbox{
			position: relative;
			width: 400px;
			height: 300px;
			margin-top: 20px;
			background: #000000;
		}
		.imgbox img{
			position: absolute;
			top: 0;
			bottom: 0;
			left: 0;
			right: 0;
			margin: auto;
			max-width: 100%;
			max-height: 100%;
			outline: 1px solid #000;
		}
		.selectedpic { border:4px solid #378ec4; padding:0;position: relative;z-index:0}
	</style>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div data-options="region:'west',border:false,collapsible:false" style="width:650px;">
		<div class="hisui-layout" data-options="fit:true">
			<div class="GridPadding" data-options="region:'center',border:false,collapsible:false">
				<div class="hisui-panel" title="供应商" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
					<div class="hisui-layout" data-options="fit:true">
						<div data-options="region:'north',border:false,collapsible:false">
							<table id="MainConditions" class="Condition">
								<tr>
									<td><label>供应商代码</label><input id="VendorCode" name="VendorCode" class="textbox"></td>
									<td><label>供应商名称</label><input id="VendorDesc" name="VendorDesc" class="textbox"></td>
									<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
								</tr>
							</table>
						</div>
						<div data-options="region:'center',border:false,collapsible:false">
							<table id="VendorMainGrid"></table>
						</div>
					</div>
				</div>
			</div>
			<div class="GridPadding" data-options="region:'south',border:false,collapsible:false" style="padding-top:0;height:300px;">
				<div class="hisui-panel" title="资质明细" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
					<table id="VendorDetailGrid"></table>
				</div>
			</div>
		</div>
	</div>
	<div data-options="region:'center',border:false,collapsible:false">
		<div class="hisui-layout" data-options="fit:true">
			<div class="GridPadding" data-options="region:'center',border:false,collapsible:false" style="padding-left:0px">
				<div class="hisui-panel" title="资质图片" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
					<ul id="PicsList" class="" style="padding:5px 30px 5px 30px;">
					</ul>
				</div>
			</div>
		</div>
	</div>
	
	<script type="text/javascript" src="../scripts/hisui/websys.comm.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonHosp.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/jquery.tmpl.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/Viewer/viewer.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/webuploader/webuploader.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/commonuploader.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/ApcVendor/NotSynVendor.js"></script>

</body>
</html>