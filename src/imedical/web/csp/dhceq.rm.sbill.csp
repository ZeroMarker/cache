<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI/>
<DHCEQ:HISUI></DHCEQ:HISUI>
<!--added by LMH 20230206 UI 合计行字体样式-->
<style>
	.messager-popover
	{
		font-weight: normal;
	}
	/* added by LMH 20230302 操作栏高度调整迎合合计行上下一致调整*/
	.datagrid-toolbar{
		height: 31px;
	}
</style>
</HEAD>
<SCRIPT language="Cache" RUNAT="SERVER">
    s CurMonth=##Class(web.DHCEQReport).GetReportMonthByDate(+$H)
    s PreMonth=##Class(web.DHCEQReport).GetPreMonth(CurMonth)
	s StartDate=##Class(web.DHCEQ.RM.BUSSBill).GetMonthDate(PreMonth,1)
	s EndDate=##Class(web.DHCEQ.RM.BUSSBill).GetMonthDate(PreMonth,2)
</Script>
<body class="hisui-layout">
	<div id='Loading' class="eq-loading"><h1><img src="../skin/default/images/loading.gif"><font>加载中・・・</font></h1></div>
	<div data-options="region:'center',border:false" style="padding:10px;">
		<!--modified by LMH 20230206 面板、面板边框、间距、表单元素、合计行样式调整  -->
		<div class="hisui-panel panel-body-gray" data-options="eqtitle:'maintitle',headerCls:'panel-header-gray',fit:true,title:'调配收入账单'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:dashed 1px #ccc;padding-bottom:10px">
					<div class="eq-table-tr">
						<div class="eq-table-td eq-table-label-first"><label id="cStartDate" for="StartDate">开始日期</label></div>
						<div class="eq-table-td "><input class="hisui-datebox textbox" id="StartDate" value=#(StartDate)#></div>
						<div class="eq-table-td eq-table-label-new"><label id="cEndDate" for="EndDate">结束日期</label></div>
						<div class="eq-table-td "><input class="hisui-datebox textbox" id="EndDate"  value=#(EndDate)#></div>
						<div class="eq-table-td"><a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="margin-left:20px">查询</a></div>
						<div class="eq-table-td"><a id="BAdd" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-add'">新增</a></div>
					</div>
			    </div>
				<div data-options="region:'center',border:false" style="position:relative">
					<table id="tDHCEQSBill" class="hisui-datagrid"></table>
					<!--modified by LMH 20230302 UI 合计行上下一致调整 -->
					<div class="messager-popover info" style="position:absolute;right:0px;top:1px;margin:0px 5px 0px 0;padding:5px 10px;">
						<span id="sumTotalicon" class="messager-popover-icon info"></span>
						<span id="sumTotal"></span>
					</div>
				</div>
    	    </div>
	    </div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/rm/sbill.js'></script>
</body>
</HTML>

