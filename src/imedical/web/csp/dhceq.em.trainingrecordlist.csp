<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>
<script type="text/javascript" src="../scripts/dhceq/em/trainingrecordlist.js"></script>
<style>td{text-align:right;}</style>
<script language="cache" runat="server">
	s TRRRowID = $Get(%request.Data("RowID",1))
</script>
</head>
<body class="hisui-layout">
<div data-options="region:'center',border:false" style="width:100%">
	<div id="cc2" class="hisui-layout" style="width:100%;height:100%;">   
		<div data-options="region:'east',headerCls:'panel-header-gray',iconCls:'icon-paper',border:false" style="padding:10px 10px 10px 0;width:600px"><!--modified by csj 2020-05-25 修改宽度-->
			<div class="hisui-panel"  title="医疗设备操作使用培训人员记录" data-options="fit:true,eqtitle:'maintitle'" >
				<div class="hisui-layout" data-options="fit:true,border:false">
					<div data-options="region:'north',border:false" style="border-bottom:dashed 1px #ccc;padding:5px 0 10px 0;">
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cTUTrainUser" for="TUTrainUser">姓名</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TUTrainUser" data-options="'component':'PLAT.L.EQUser',paras:[{name:'Name',type:4,value:'TUTrainUser'},{name:'Loc',type:4,value:'TRRLoc'}]"></div><!--modified by csj 2020-05-26 添加参数-->
							<div class="eq-table-td eq-table-label"><label for="TURemark">电话</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TURemark"></div>
						</div>
					</div>
					<div data-options="region:'center',border:false" style="padding:0px;position:relative">
						<table id="tDHCEQTrainingUser" data-options="border:false"></table>	
					</div>
    			</div>
			</div>
		</div>   
		<div data-options="region:'center',bodyCls:'panel-header-gray',border:false" style="padding:10px;width:100%"> <!--modified by csj 2020-05-25 修改宽度-->
			<div class="hisui-panel"  title="医疗设备操作使用培训记录" data-options="fit:true,eqtitle:'maintitle'">
				<div class="hisui-layout" data-options="fit:true,border:false">
					<div data-options="region:'center',border:false" style="padding:10px;">
						<div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cNewCourceFlag" for="NewCourceFlag">选择已有课程</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-checkbox" id="NewCourceFlag" type="checkbox"></div>
								<div class="eq-table-td eq-table-label"><label id="cSelectCourse" for="SelectCourse">选择课程</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="SelectCourse" data-options="'component':'EM.L.Course',paras:[{name:'COName',type:1,value:'SelectCourse'}]"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cCOName" for="COName">课程名称</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="COName"></div>
								<div class="eq-table-td eq-table-label"><label for="TRRLoc">科室</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Loc'" id="TRRLoc"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cCOInstitution" for="COInstitution">培训机构及单位</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="COInstitution" ></div>
								<div class="eq-table-td eq-table-label"><label for="TRRTrainingUser">培训老师</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TRRTrainingUser"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cCOSourceTypeDesc" for="COSourceTypeDesc">来源类型</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="COSourceTypeDesc" data-options="component:'PLAT.L.TypeDefine',paras:[{name:'Desc',type:1,value:'COSourceTypeDesc'},{name:'Lookupname',type:2,value:'COSourceType'}],jsfunction:GetSourceType"></div>
								<div class="eq-table-td eq-table-label"><label id="cCOSource" for="COSource">来源</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="COSource"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cCOItem" for="COItem">设备项</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="COItem" disabled="true"></div>
								<div class="eq-table-td eq-table-label"><label for="COModel">机型</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="COModel" disabled="true"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cTRRTrainPlace" for="TRRTrainPlace">培训地点</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TRRTrainPlace" ></div>
								<div class="eq-table-td eq-table-label"><label for="TRRTrainType">培训方式</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TRRTrainType" ></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cTRRTrainStartDate" for="TRRTrainStartDate">培训开始日期</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="TRRTrainStartDate" value=#(CurDate)# ></div>
								<div class="eq-table-td eq-table-label"><label for="TRRTrainEndDate">培训结束日期</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="TRRTrainEndDate" value=#(CurDate)# ></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cTRRTrainStartTime" for="TRRTrainStartTime">培训开始时间</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-timespinner" id="TRRTrainStartTime"></div>
								<div class="eq-table-td eq-table-label"><label for="TRRTrainEndTime">培训结束时间</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-timespinner" id="TRRTrainEndTime"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cCONo" for="CONo">课程编号</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="CONo"></div>
								<div class="eq-table-td eq-table-label"><label id="cTRRNo" for="TRRNo">培训编号</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TRRNo"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cTRRStage" for="TRRStage">培训期次</label></div>
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TRRStage"></div>
								<div class="eq-table-td eq-table-label"><label id="cTRRClassHours" for="TRRClassHours">培训课时</label></div><!--add by csj 2020-06-08 添加培训课时-->
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="TRRClassHours"></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label id="cCOManager" for="COManager">负责人</label></div><!--add by csj 2020-06-08 添加负责人-->
								<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="COManager"></div>
							</div>
						</div>
						<div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label for="COCourseContent">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp课程内容</label></div>
								<div class="eq-table-td"><textarea id="COCourseContent" class="textbox" style="height:60px;width:440px"></textarea></div>
							</div>
							<!--add by csj 2020-06-08 添加培训内容-->
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label for="TRRTrainContent">培训内容</label></div> 
								<div class="eq-table-td"><textarea id="TRRTrainContent" class="textbox" style="height:60px;width:440px"></textarea></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label for="COTrainObj">培训目标</label></div>
								<div class="eq-table-td"><textarea id="COTrainObj" class="textbox" style="height:60px;width:440px"></textarea></div>
							</div>
							<div class="eq-table-tr">
								<div class="eq-table-td eq-table-label"><label for="CORemark">备注</label></div>
								<div class="eq-table-td"><textarea id="CORemark" class="textbox" style="height:60px;width:440px"></textarea></div>
							</div>
							<div data-options="region:'south',border:false" style="margin-top:10px;height:60px;overflow:hidden;" align="center">
							<table style="margin-top:10px;">
								<td><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true">保存</a></td>
								<td><a id="BSubmit" href="#" class="hisui-linkbutton" style="margin-left:50px;" data-options="stopAllEventOnDisabled:true">提交</a></td>
								<td><a id="BDelete" href="#" class="hisui-linkbutton" style="margin-left:50px;" data-options="stopAllEventOnDisabled:true">删除</a></td>
							</table>
						</div>
						</div>
						
					</div>
    			</div>
			</div>
		</div>
	</div>
</div>

<div id="hiddenEncrypt" style="display:hidden">
	<form name="DHCEQ">
		<INPUT type="hidden" name="CORowID" id="CORowID">
		<INPUT type="hidden" name="TRRRowID" id="TRRRowID" value="#(TRRRowID)#">
		<INPUT type="hidden" name="COOpenCheckDR" id="COOpenCheckDR">
    	<INPUT type="hidden" name="TRRLocDR" id="TRRLocDR" >
    	<INPUT type="hidden" name="COSourceType" id="COSourceType">
    	<INPUT type="hidden" name="COSourceID" id="COSourceID">
    	<INPUT type="hidden" name="COItemDR" id="COItemDR">
    	<INPUT type="hidden" name="COModelDR" id="COModelDR">
    	<INPUT type="hidden" name="TURowID" id="TURowID">
    	<INPUT type="hidden" name="TUTrainUserDR" id="TUTrainUserDR">
    	<INPUT type="hidden" name="TRRStatus" id="TRRStatus">
	</form>
</div>
</body>
</html>
