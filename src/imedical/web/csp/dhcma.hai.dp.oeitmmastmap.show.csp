<div class="hisui-layout" data-options="fit:true" style="border:none;">
	<div data-options="region:'center',border:false,collapsible:false" class="ma-panel" style="padding:10px;">
		<table id="gridOEItmMastMap" data-options="toolbar:'#ToolBar'"></table>
		<div id="ToolBar" style="padding:3px;" >
			<div style="padding: 7px 0 7px 7px;"> 
				<input class="hisui-searchbox" id="searchbox" style="width:260px;" data-options="prompt:'搜索'" />			 
			</div>
			<span class="line" style="display: block;border-bottom: 1px dashed rgb(204, 204, 204);margin: 3px 0px;clear: both; padding-bottom: 1px; "></span>
			<a class="hisui-linkbutton" id="btnAll"  data-options="iconCls:'icon-all-select',plain:true,stopAllEventOnDisabled:true"> 全部 </a>
			<a class="hisui-linkbutton" id="btnPend" data-options="iconCls:'icon-compare-no',plain:true,stopAllEventOnDisabled:true"> 未对照 </a>
			<a class="hisui-linkbutton" id="btnFin"  data-options="iconCls:'icon-compare-yes',plain:true,stopAllEventOnDisabled:true" > 已对照 </a>
			<a class="hisui-linkbutton" id="btnSyn"  style="height: 24px; border-left: 1px solid LightGrey; border-right: 0px solid LightGrey;margin: 1px 1px; " data-options="iconCls:'icon-radio',plain:true,stopAllEventOnDisabled:true" > 自动匹配 </a>
			<a class="hisui-linkbutton" id="btnEdit" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true"> 修改 </a>
			<a class="hisui-linkbutton" id="btnAdd"  data-options="iconCls:'icon-compare',plain:true,stopAllEventOnDisabled:true"> 对照 </a>
			<a class="hisui-linkbutton" id="btnDelete"  data-options="iconCls:'icon-undo',plain:true,stopAllEventOnDisabled:true" > 撤销 </a>

		</div>
	</div>		
	<div data-options="region:'east',border:false,collapsible:false" class="ma-panel" style="width:500px;padding:10px 10px 10px 0px;">
		<table id="gridOEItmMast" data-options="toolbar:'#ToolBar2'"></table>
		<div id="ToolBar2" style="padding:3px;overflow:hidden;display:none" >
			<div style="padding: 7px 0 7px 7px;"> 
				<input class="hisui-searchbox" id="searchbox_two" style="width:260px;" data-options="prompt:'搜索'" />			 
			</div>
		
		</div>
	</div>	
</div>

<div id="OEItmMastMapEdit" style="border:none;overflow:hidden;padding-right:10px;display:none;">
	<table class="search-table">
		<tr>
			<td class="r-label">
				<label for="txtBTMapNote">标准备注</label>
			</td>
			<td>
				<input class="hisui-validatebox textbox" id="txtBTMapNote" style="width:270px"/>
			</td>
		</tr>
		<!--ADD(2022-12) 新增'三管感染防控督查表'自动匹配规则-->
		<tr id="IA_1">
			<td class="r-label">
				<label for="cboIADr">督查表类型</label>
			</td>
			<td>
				<input class="hisui-combobox" id="cboIADr" style="width:277px"/>
			</td>
		</tr>
		<tr id="IA_2">
			<td class="r-label">
				<label for="cboIATypeDr">插管类型</label>
			</td>
			<td>
				<input class="hisui-combobox" id="cboIATypeDr" style="width:277px"/>
			</td>
		</tr>
		<tr>
			<td class="r-label"></td>
			<td>
				<input class='hisui-checkbox' type="checkbox"  label='是否有效' id="chkIsActive">
			</td>
		</tr>
		<tr>
			<td colspan="2" Style="text-align:center;padding-left:10px;">
				<a id="btnSave" class="hisui-linkbutton" style="margin-right:10px;">保存</a>
				<a id="btnClose" class="hisui-linkbutton">关闭</a>
			</td>
		</tr>	
	</table>
</div>