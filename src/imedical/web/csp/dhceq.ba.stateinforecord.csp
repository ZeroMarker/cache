<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
<meta http-equiv="x-ua-compatible" content="edge">	<!--modify by wl 2019-11-4 WL007  begin-->
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>			<!--modify by wl 2019-11-4 WL007  end-->
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>
</head>
<script language=Cache runat=server>
	 s Type = $g(%request.Data("Type",1)) ;Type	记录类型

</script>

<body class="hisui-layout"  style="height:100%;">
	<div data-options="region:'center',border:false" style="padding:10px;"> 
		<div class="hisui-panel" title="设备情况记录登记" data-options="'eqtitle':'maintitle',fit:true"> <!--add by wl 2019-11-4 WL007-->
			<div class="hisui-layout" data-options="fit:true,border:false">	  <!--add by wl 2019-11-4 WL007-->
					<div class="eq-table">
					<!--Modified By QW20181031 BUG:QW0021-->
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-item" style="font-weight:900;"><span>查询条件</span></div>
						</div>
					<!--End By QW20181031 BUG:QW0021-->
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><lable for="FileNo">档案号</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="FileNo"></div>
							<div class="eq-table-td eq-table-label"><lable for="No">设备编号</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="No"></div>
							<div class="eq-table-td eq-table-label"><lable for="Name">设备名称</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Name"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-button"><a id="BFind" href="#" class="hisui-linkbutton">查询</a></div>
						</div>
						<!--Modified By QW20181031 BUG:QW0021-->
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-item" style="font-weight:900;"><span>编辑条件</span></div>
						</div>
						<!--End By QW20181031 BUG:QW0021-->
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><lable for="StartDate">开机时间</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="StartDate"></div>
							<div class="eq-table-td eq-table-label"><lable for="EndDate">关机时间</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="EndDate"></div>
							<div class="eq-table-td eq-table-label"><lable for="User">使用人</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.EQUser'" id="User"></div>
							<div class="eq-table-td eq-table-label"></div>
							<div class="eq-table-td eq-table-button"><a id="BClear" href="#" class="hisui-linkbutton">清屏</a></div>
						</div>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><lable for="EndStateInfo">设备状况</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="EndStateInfo"></div>
							<div class="eq-table-td eq-table-label"><lable for="UseContent">使用内容</lable></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="UseContent"></div>
						</div>
					</div>
					<div class="eq-table-td eq-table-label"></div>
					<div data-options="border:false" style="padding-top:5px;border-top:dashed 1px #ccc;position:relative">
							<table id="stateinfodatagrid" data-options="border:false">
	        				</table>
	        		</div>
	        	</div>	
	        </div>  <!--add by wl 2019-11-4 WL007-->
		</div>  <!--add by wl 2019-11-4 WL007-->
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="RowID" value="">
			<INPUT type="hidden" id="EquipDR" value="">
			<INPUT type="hidden" id="Type" value="#(Type)#">
			<INPUT type="hidden" id="UserDR" value="">
			<INPUT type="hidden" id="StartTime" value="">
			<INPUT type="hidden" id="EndTime" value="">
		</form>
	</div>
	<script type="text/javascript" src="../scripts/dhceq/ba/stateinforecord.js"></script>

</body>
</html>