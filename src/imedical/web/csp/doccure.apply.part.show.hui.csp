<div class="hisui-panel panel-header-gray" data-options="fit:true,closable:false">   
	<div class="hisui-layout" data-options="fit:true"> 
		<div data-options="region:'north',border:false,split:false" style="height:38px">
			<div class="hisui-panel panel-header-gray cure-p-bd-b-bdash" data-options="fit:true">	
				<div id="PartKey" style="padding-left:5px"></div>
			</div>
		</div>
		<div data-options="region:'center',border:false,split:false">
			<div id="CurePart" class="hisui-panel panel-header-gray" data-options="border:false,fit:true">			
				<table id="tabCurePartList" toolbar="#tabCurePartList-bar"></table>
			</div>
			<div id="CurePartGrp" class="hisui-panel panel-header-gray" data-options="border:false,fit:true">			
				<table id="tabCurePartGrpList" toolbar="#tabCurePartGrpList-bar"></table>
			</div>
		</div>
		<div id="tabCurePartList-bar">
			<div class="inline-div">
				<input class="hisui-radio" type="radio" label="部位" name="PartOrAcupoint" value="BP"/>
			</div>
			<div class="inline-div">
				<input class="hisui-radio" type="radio" label="穴位" name="PartOrAcupoint" value="A"/>
			</div>
			<div class="inline-div">
				<input id="SDesc" class="hisui-searchbox" style="width:150px" data-options="searcher:LoadListGridData,prompt:'#(..Get("请输入检索信息"))#'"/>
			</div>
			<div class="inline-div">
				<span class="tb-btn-separator"></span>
			</div>
			<div class="inline-div" style="padding-left:5px">
				<a id="SaveCurPart" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">保存</a>
	    	</div>
	    </div>
		<div id="tabCurePartGrpList-bar">
			<div class="inline-div">
				<input class="hisui-radio" type="radio" label="部位组" name="PartOrAcupointGrp" value="BP"/>
			</div>
			<div class="inline-div">
				<input class="hisui-radio" type="radio" label="穴位组" name="PartOrAcupointGrp" value="A"/>
			</div>
			<div class="inline-div">
				<input id="SGrpDesc" class="hisui-searchbox" style="width:150px" data-options="searcher:LoadListGridData,prompt:'#(..Get("请输入检索信息"))#'"/>
			</div>
			<div class="inline-div">
				<span class="tb-btn-separator"></span>
			</div>
			<div class="inline-div" style="padding-left:5px">
				<a id="SaveCurPartGrp" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">保存</a>
	    	</div>
	    </div>
	</div>
</div>