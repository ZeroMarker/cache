<!--高值条码验收(使用于条码由供应商打印的情况)-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<SERVER>
	//d ##class(web.DHCSTMHUI.Util.RQServerConfig).GetApplet()	//润乾打印
</SERVER>
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="BorderLine" style="overflow:hidden;" data-options="region:'north',border:false">
		<div class="hisui-layout" style="padding:2px 0;" data-options="fit:true,border:false">
			<div style="float:left;width:740px;" data-options="region:'west',border:false">
				<table id="Conditions" class="Condition">
					<input id="Inci" name="Inci" type="hidden">
					<input id="OriginalStatus" name="OriginalStatus" type="hidden" value="Temp">
					<tr>
						<td>条码</td>
						<td><input id="BarCodeText" name="BarCodeText" class="textbox"></td>
						<td>类组</td>
						<td><input id="ScgId" name="ScgId" class="textbox hisui-stkscgcombotree"></td>
						<td>自带条码</td>
						<td><input id="OriginalCode" name="OriginalCode" class="textbox"></td>
					</tr>
					<tr>
						<td>起始日期</td>
						<td><input id="StartDate" name="StartDate" class="hisui-datebox textbox"></td>
						<td>物资名称</td>
						<td><input id="InciDesc" name="InciDesc" class="textbox"></td>
						<td>批次码</td>
						<td><input id="BatchCode" name="BatchCode" class="textbox"></td>
					</tr>
					<tr>
						<td>截止日期</td>
						<td><input id="EndDate" name="EndDate" class="hisui-datebox textbox"></td>
						<td>供应商</td>
						<td><input id="VendorId" name="VendorId" class="textbox hisui-combobox"></td>
						<td colspan="2">
							<input class="hisui-radio" type="radio" label="未使用" name="Status" value="Temp" data-options="checked:true">
							<input class="hisui-radio" type="radio" label="已使用" name="Status" value="Used">
						</td>
					</tr>
					<tr>
						<td colspan="4">
							<a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
							<a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
							<a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
							<a href="#" id="DownExcelTemplet" class="hisui-linkbutton" data-options="iconCls:'icon-w-run'">下载模板</a>
						</td>
					</tr>
				</table>
			</div>

			<div style="border-left:1px dashed #ccc;float:left;" data-options="region:'center',border:false">
				<table id="ImpConditions" class="Condition">
					<tr>
						<td><label>入库供应商</label><input id="ApcvmDr" name="ApcvmDr" class="textbox hisui-combobox"></td>
					</tr>
					
					<td colspan="2">
						<input id="File" name="File" data-options="width:200,buttonText:'',buttonIcon:'icon-w-file'">
					</td>
					<td><a href="#" id="ReadBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-run'">读取</a></td>
				</table>
			</div>
		</div>
	</div>
	
	<div style="padding-top:0;" data-options="region:'center',border:false,collapsible:false">
		<div class="hisui-panel" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true,border:false">
			<table id="BarCodeGrid"></table>
		</div>
	</div>
	
	<script type="text/javascript" src="../scripts/dhcstmhisui/Common/XLSX/xlsx.full.min.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/ItmTrack/ItmTrackCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/BarCode/BarCodeCheck.js"></script>
</body>
</html>