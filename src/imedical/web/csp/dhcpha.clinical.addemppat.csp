<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired()
 i ##Class(ext.websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<HTML XMLNS=TRAK>
<HEAD>
<TITLE><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></TITLE>
 <TRAK:HEAD></TRAK:HEAD> 
<meta http-equiv="Content-Type" content="text/html;">
<EASYUI:CLINICAL/>
<script type="text/javascript" src="../scripts/dhcpha/clinical/createPatInfoWin.js"></script>
<script type="text/javascript" src="../scripts/dhcpha/clinical/createMedadviseWin.js"></script>
<script type="text/javascript" src="../scripts/dhcpha/clinical/createMedRecordWin.js"></script>
<script type="text/javascript" src="../scripts/dhcpha/clinical/addroomlogwin.js"></script>
<script type="text/javascript" src="../scripts/dhcpha/clinical/addemppat.js"></script>
<script type="text/javascript" src="../scripts/dhcpha/clinical/ExportCommon.js"></script>

<script type="text/javascript"  >
 
    $(function(){
              BodyLoadHandler();
             
    });
</script> 
<!--行高-->
<style type="text/css">
	.datagrid-row {
		height: 38px;
	}	
	.tbl {
		table-layout:fixed;
	}
	/*.ui-font10{
		font-size:14px;*/
		/*font-family:华文楷体;*/
	
	/*
	.easyui-layout{
		overflow:scroll;
	}*/
	.datagrid-row-selected {
  		background: #ADFAFC;
	}
	.btns a {
		margin-right: 10px;
	}
	
</style> 
</HEAD>

<BODY>

	<div class="easyui-layout" data-options="fit:true" border="false" >     
		<div data-options="region:'north',split:true" style="height:100px;background:#F8F8FF;border:1px solid #95B8E7;padding-left:20px;" title="患者列表"> 
			<table cellpadding=1 class="table-b" style="table-layout: fixed;">
				<tr style="height:35;">
					<td align="right">病区：</td>
					<td width="220"><label id="labpatward"></label></td>
					<td align="right">床号：</td>
					<td width="100"><label id="labpatbed"></label></td>
					<td align="right">性别：</td>
					<td width="70"><label id="labpatsex"></label></td>
					<td align="right">体重：</td>
					<td width="70"><label id="labpatweight"></label></td>
					<td align="right">主管医生：</td>
					<td width="130"><label id="labpatdoctor"></label></td>
					<td align="right" rowspan="2" valign="center">诊断：</td>
					<td width="300" rowspan="2" valign="center" ><label id="labpatdiag" style="overflow:hidden"></label></td>
				</tr>
				<tr>
					<td align="right">登记号：</td>
					<td width="220"><label id="labpatno"></label></td>
					<td align="right">  姓名：</td>
					<td width="100"><label id="labpatname"></label></td>
					<td align="right">年龄：</td>
					<td width="70"><label id="labpatage"></label></td>
					<td align="right">身高：</td>
					<td width="70"><label id="labpatheight"></label></td>
					<td align="right">入院日期：</td>
					<td width="100"><label id="labpatindate"></label></td>
				</tr>
			</table>
		</div>  
		<div data-options="region:'center'">   
			<table data-options="fit:true" id="patgrid" border="false"></table> 
		</div>
	</div>
  
	<div id="gridtoolbar">
		<div style="padding:5px;" class="btns">
			<a class="easyui-linkbutton" iconCls="icon-add" plain="true" id="btnAddEmp">标注[监护]</a>
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btn_monitor">药学监护<font color="red" style="font-size:10px;">[New]</font></a>
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnMedWard">医学查房</a>
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnPhaWard">药学查房</a>
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnWUseRec">用药建议</a>
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnMedEducation">用药教育</a>
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnWDRec">药历书写</a> <!-- //2017-01-06  duwensheng -->
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnAdrRep">不良反应报告</a>
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnDrgQua">药品质量报告</a>
			<!-- <a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnDrgMis">用药错误报告</a> -->
			<a class="easyui-linkbutton" iconCls="icon-edit" plain="true" id="btnExport">导出患者查房列表</a>
	    	<a class="easyui-linkbutton" iconCls="icon-search" plain="true">查询</a>
		</div>
  </div>

  <div id="addroomlogWin" style="display:none" >
   	  <div class="easyui-layout" data-options="fit:true" border="false" > 
		  <div data-options="region:'north',split:true" style="height:40px"  >
		    <table cellpadding=3>
		    <tr>
		    <td align="right"><font size="2">病区:</font></td>
	    	<td width="200"><label id="txtrward"></label></td>
	    	<td align="right"><font size="2">床号:</font></td>
	    	<td width="200"><label id="txtrbed"></label></td>
	    	<td align="right"><font size="2">姓名:</font></td>
	    	<td width="100"><label id="txtrname"></label></td>
	    	</tr>
		    </table> 
		  </div> 
		  <div data-options="region:'west',split:true" style="width:280px" title="查房记录列表" >
		    <table style="height:auto" width="100%" id="tblroomlist" border="false" ></table> 
		  </div>  
		  <div data-options="region:'center'"   >
		    <table cellpadding=3>
		    　<tr>
	              <td><a  class="easyui-linkbutton" iconCls="icon-add" plain="true" id="btnAddRlog">新增</a></td>
	    		  <td><a  class="easyui-linkbutton" iconCls="icon-add" plain="true" id="btnUpdRlog">更新</a></td>
		      </tr>
		      <tr>
	              <td align="right">查房情况:</td>
                  <td>
                    <textarea id="txtroomlog" cols="100" rows="20"></textarea>
                  </td>
		      </tr>
		    </table> 
		  </div>   
	  </div>
   </div>
   <!--重点患者标注-->
   <div id="empatwin" style="display:none;">
	   <div style="margin:10px;border:1px solid #95B8E7;">
	   	   <div><input id="empPatID" style="display:none;"></input></div>
		   <div style="display:block;margin:10px;width:250px;"><span style="padding-right:5px">开始日期</span><input id="stdate" type="text" style="width:150px;" class="easyui-datebox"></input></div>
		   <div style="display:block;margin:10px;width:250px;"><span style="padding-right:5px">开始时间</span><input id="sttime" class="easyui-timespinner"  style="width:150px;" data-options="min:'08:30'" /></div>
		   <div style="display:block;margin:10px;width:250px;"><span style="padding-right:5px">责任药师</span><input id="user" style="width:150px;" class="easyui-combobox" data-options="valueField:'value',textField:'text'"/></div>
		   <div style="display:block;margin:10px;width:250px;"><span style="padding-right:5px">结束日期</span><input id="enddate" type="text" style="width:150px;" class="easyui-datebox"></input></div>
		   <div style="display:block;margin:10px;width:250px;"><span style="padding-right:5px">结束时间</span><input id="endtime" class="easyui-timespinner"  style="width:150px;" data-options="min:'08:30'" /></div>
		   <div style="display:block;margin:10px;width:250px;"><span style="padding-right:5px">监护级别</span><input id="monlevel" style="width:150px;" editable="false" /></div>
		   <div style="display:block;margin:10px;width:250px;"><span style="padding-right:5px">转为级别</span><input id="trmlevel" style="width:150px;" editable="false" /></div>
		   <div style="display:block;margin:10px 2px 10px 33px;"><span style="padding-right:5px">备注</span><textarea style="margin-left:4px" id="remark" rows="3" cols="20"></textarea></div>
		   <div style="display:block;margin:10px 2px 10px 33px;"><span style="padding-right:5px">原因</span><textarea style="margin-left:4px" id="reason" rows="3" cols="20" ></textarea></div>
	   </div>
   </div>
   <!--查询条件--> 
   <div id="tip" style="border-radius: 3px;width:300px;border:1px solid #95B8E7; padding:20px 5px 10px 5px;position: absolute; background-color: #FFF;">
   		<div style="margin:10px;padding-left:15px;width:295px;"><span id="notes" style="color:red;font-weight:bold;">注意:出院病人请直接录入登记号查询！</span></div>
   		<div style="margin:10px;width:295px;"><span>病&emsp;&emsp;区&emsp;<input id="ward" style="width:210" class="easyui-combobox" data-options="valueField:'value',textField:'text'"/></span></div>
   		<div style="margin:10px;width:295px;"><span>登&ensp;记&ensp;号&emsp;<input id="patno" style="width:210;border:1px solid #95B8E7;" class="easyui-validatebox" data-options="valueField:'val',textField:'text'"/></span></div>
   		<div style="margin:10px;padding-left:25px;">
   			<span style="margin:10px;padding-left:0px;"><a href="#" class="easyui-linkbutton"  plain="true" iconCls="icon-cancel">关闭</a></span>
   			<span style="margin:10px;padding-left:0px;"><a href="#" class="easyui-linkbutton"  plain="true" iconCls="icon-undo">返回</a></span>
   			<span style="margin:10px;padding-left:0px;"><a href="#" class="easyui-linkbutton"  plain="true" iconCls="icon-save">提交</a></span>
   		</div>
   </div>
</BODY>
</HTML>