<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI></HISUI>
<script src="../scripts/hisui/websys.hisui.edit.js" type="text/javascript"></script>
<style>
	td.l-value{
        text-align: left;
        padding-right: 20px;
    }
    body {
	    background-color: #f5f5f5;
    }
 </style>
</head>
<body>
<div class="hisui-panel" data-options="fit:true,title:'权力项配置'">
        <div class="hisui-layout" fit="true">
            <div data-options="region:'north',border:false" style="padding: 5px 10px;border-bottom: 1px dashed #E2E2E2; ">
                <table cellspacing="0" cellpadding="0" broder="0">
                    <tr style="height: 40px;">
                        
                        <td class="r-label">权力项代码</td>
                        <td class="l-value"><input type="text" class="textbox" id="AuthCode"></td>
                        <td class="r-label">模块名称</td>
                        <td class="l-value"><input type="text" class="textbox" id="AuthModuleCode"></td>
                        <td>
                            <a href="javascript:void(0);" class="hisui-linkbutton" id="btn-find">查询</a>
                        </td>
                    </tr>
                    <tr style="height: 40px;">
                        <td class="r-label">系统名称</td>
                        <td class="l-value"><input type="text" class="textbox" id="AuthAppCode"></td>
                        <td class="r-label">
                        </td>
                        <td class="l-value">
                        </td>
                        <td>
                            <a href="javascript:void(0);" class="hisui-linkbutton" id="btn-clear">清屏</a>
                        </td>
                    </tr>
                </table>
            </div> <!--end layout-north-->
            <div data-options="region:'center',border:false">
                <table id="grid"></table>
            </div> <!--end layout-center-->
        </div><!--end layout-->
    </div>
    <script src="../scripts/hisui/BSP.SYS.AuthItem.js" type="text/javascript"></script>
</body>
</html>