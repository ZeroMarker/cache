<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>
<script language=Cache runat=server>
	s QXType=$Get(%request.Data("QXType",1))
	s ManageLocDR=$Get(%request.Data("ManageLocDR",1))
</script>

<body class="hisui-layout" >
	<div data-options="region:'center',border:false" style="padding:10px;">
		<div class="hisui-panel" title="采购申请单" data-options="eqtitle:'maintitle',fit:true">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',border:false" style="border-bottom:solid 1px #ccc;height:80px;">
					<div class="eq-table">
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cRequestNo" for="RequestNo">申请单号</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="RequestNo"></div>
							<div class="eq-table-td eq-table-label"><label id="cName" for="Name">设备名称</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Name"></div>
							<div class="eq-table-td eq-table-label"><label id="cManageLocDR_CTLOCDesc" for="ManageLocDR_CTLOCDesc">管理部门</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Loc'" id="ManageLocDR_CTLOCDesc"></div>
							<div class="eq-table-td eq-table-label"><label id="cUseLocDR_CTLOCDesc" for="UseLocDR_CTLOCDesc">使用科室</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.Loc'" id="UseLocDR_CTLOCDesc"></div>
							<div class="eq-table-td eq-table-label"><label id="cYearFlag" for="YearFlag">是否年度</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-combobox textbox" id="YearFlag"></div>
						</div>
						<div class="eq-table-tr">
							<div class="eq-table-td eq-table-label"><label id="cYear" for="Year">年度</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-validatebox textbox" id="Year"></div>
							<div class="eq-table-td eq-table-label"><label id="cStartDate" for="StartDate">申请日期</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="StartDate"></div>
							<div class="eq-table-td eq-table-label"><label for="EndDate">至</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-datebox textbox" id="EndDate"></div>
							<div class="eq-table-td eq-table-label"><label for="Status">状态</label></div>
							<div class="eq-table-td eq-table-input"><input class="hisui-combobox textbox" id="Status" ></div>
							<div class="eq-table-td"></div>
							<div class="eq-table-td"><a id="BFind" href="#" class="hisui-linkbutton" iconCls="icon-w-find" data-options="stopAllEventOnDisabled:true">查询</a></div>
						</div>
					</div>
				</div>
				<div data-options="region:'center',border:false">
					<table id="tDHCEQBuyRequestDetail"></table>
				</div>
			</div>
		</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="QXType" value="#(QXType)#">
			<INPUT type="hidden" id="ManageLocDR" value="#(ManageLocDR)#">
			<INPUT type="hidden" id="UseLocDR" value="">
			<INPUT type="hidden" id="StatusDR" value="">
			<INPUT type="hidden" id="YearFlagDR" value="">
		</form>
	</div>
	<script type="text/javascript" src="../scripts/dhceq/em/buyrequestdetail.js"></script>
	<script type='text/javascript' src='../scripts/hisui/DHCEQCommon.js'></script>
</body>
</html>