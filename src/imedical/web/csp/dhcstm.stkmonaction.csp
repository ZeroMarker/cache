<csp:content charset="UTF-8">
<SERVER>
s Action=$Get(%request.Data("actiontype",1))
s Start=$Get(%request.Data("start",1))
s Limit=$Get(%request.Data("limit",1))
s Sort=$Get(%request.Data("sort",1))
s Dir=$Get(%request.Data("dir",1))
;
i Action = "CreateReport" d
	.s params=$g(%request.Data("Params",1))
	.s ret=##class(web.DHCSTM.DHCStkMon).CreateReports(params)
	.i +ret'=0  d
	..w "{success:'false',info:'"_ret_"'}"
	.e  d
	..w "{success:'true',info:'"_ret_"'}"
i Action = "Select" d
	.S IngrRowid=$Get(%request.Data("IngrRowid",1))
	.s ret=##class(web.DHCSTM.DHCINGdRec).Select(IngrRowid)	
	.w "{success:'true',info:'"_ret_"'}"
i Action = "QueryDetail" d
	.S Parref=$Get(%request.Data("Parref",1))
	.s StkGrpId=$g(%request.Data("StkGrpId",1))
	.s StkCatId=$g(%request.Data("StkCatId",1))
	.s IncDesc=$g(%request.Data("IncDesc",1))
	.d ##class(web.DHCSTM.DHCStkMonRepQuery).jsStkMonRepItm(Parref,StkGrpId,StkCatId,IncDesc)
	.
i Action = "Delete" d
	.S SmId=$Get(%request.Data("Rowid",1))
	.s ret=##class(web.DHCSTM.DHCStkMon).Delete(SmId)
	.i ret=0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"
	.
i Action = "CheckIfExist" d
	.s LocId=$Get(%request.Data("LocId",1))
	.s CurMonth=$Get(%request.Data("CurMonth",1))
	.s ret=##class(web.DHCSTM.DHCStkMon).CheckIfExist(LocId,CurMonth)
	.i +ret>0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"
	.
i Action = "CheckMonCond" d
	.s StrParam=$Get(%request.Data("StrParam",1))
	.s ret=##class(web.DHCSTM.DHCStkMon).CheckMonCond(StrParam)
	.w "{success:'true',info:'"_ret_"'}"
	.
i Action = "GetStkMonStDateTime" d
	.S LocId=$Get(%request.Data("LocId",1))
	.S CurrentMon=$Get(%request.Data("Month",1))
	.s ret=##class(web.DHCSTM.DHCStkMon).GetStkMonStDateTime(LocId,CurrentMon)
	.w "{success:'true',info:'"_ret_"'}"
	.
i Action = "Query" d
	.s LocId=$Get(%request.Data("LocId",1))
	.s StartDate=$Get(%request.Data("StartDate",1))
	.s EndDate=$Get(%request.Data("EndDate",1))
	.;d ##class(web.DHCSTM.DHCStkMon).jsDHCStkMon(LocId,StartDate,EndDate)
	.d ##class(web.DHCSTM.Common.JsonObj).getJsonByQuery("web.DHCSTM.DHCStkMon","DHCStkMon",Start,Limit,LocId,StartDate,EndDate)

i Action = "GrpLocForStkMon" d
	.s GroupId=$Get(%request.Data("GroupId",1))
	.d ##class(web.DHCSTM.DHCStkMon).jsGrpLocForStkMon(Start,Limit,GroupId)
	.
i Action = "QueryDetailRp" d
	.S Parref=$Get(%request.Data("Parref",1))
	.s StkGrpId=$g(%request.Data("StkGrpId",1))
	.s StkCatId=$g(%request.Data("StkCatId",1))
	.s IncDesc=$g(%request.Data("IncDesc",1))
	.s StrParam=$g(%request.Data("StrParam",1))
	.d ##class(web.DHCSTM.DHCStkMonRepQuery).jsStkMonRepItmRp(Parref,StkGrpId,StkCatId,IncDesc,StrParam)
	.
i Action = "QueryDetailLB" d
	.S Parref=$Get(%request.Data("Parref",1))
	.s StkGrpId=$g(%request.Data("StkGrpId",1))
	.s StkCatId=$g(%request.Data("StkCatId",1))
	.s IncDesc=$g(%request.Data("IncDesc",1))
	.s StrParam=$g(%request.Data("StrParam",1))
	.d ##class(web.DHCSTM.DHCStkMonReportLB).jsStkMonRepLcBt(Parref,StkGrpId,StkCatId,IncDesc,StrParam)
	.
i Action = "QuerySumBySCG" d
	.S Parref=$Get(%request.Data("Parref",1))
	.s Type=$g(%request.Data("Type",1))
	.s ScgType=$Get(%request.Data("ScgType",1))
	.d ##class(web.DHCSTM.DHCStkMonRepQuery).jsStkMonSumBySCG(Parref,Type,ScgType)
	.
//根据月报主表的Id查询月报主表信息
i Action = "getMain" d
	.s RowId=$Get(%request.Data("rowid",1))
	.s result = ##class(web.DHCSTM.DHCStkMon).GetMain(RowId)
	.i result'="" d
	..w "{success:'true',info:'"_$tr(result,$c(13,10))_"'}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

//根据月报主表的id查询月报凭证记录	
i Action = "VoucherStatusDetail" d
	.S smRowid=$Get(%request.Data("smRowid",1))
	.d ##class(web.DHCSTM.DHCStkMon).VoucherStatusDetail(smRowid)
	.
//根据月报主表的id提交月报
i Action="Submit"  d
    .s smRowid=$Get(%request.Data("smRowid",1))
    .s UserId=$Get(%request.Data("UserId",1))
    .s ret=##class(web.DHCSTM.DHCStkMon).Submit(smRowid,UserId)
	.i ret=0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"
	..
//根据月报主表的id取消提交月报
i Action="CancelSubmit"  d
    .s smRowid=$Get(%request.Data("smRowid",1))
    .s UserId=$Get(%request.Data("UserId",1))
    .s ret=##class(web.DHCSTM.DHCStkMon).CancelSubmit(smRowid,UserId)
	.i ret=0  d
	..w "{success:'true',info:'"_ret_"'}"
	.e  d
	..w "{success:'false',info:'"_ret_"'}"
	..
</SERVER>
