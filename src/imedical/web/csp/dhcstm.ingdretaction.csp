<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
    i ##Class(websys.SessionEvents).SessionExpired() q 1
    q 1
</csp:method>
<csp:content charset="UTF-8">

<script language="cache" runat="server">

    s Action=$Get(%request.Data("actiontype",1))
    s Start=$Get(%request.Data("start",1))
    s Limit=$Get(%request.Data("limit",1))
    s Sort=$Get(%request.Data("sort",1))
    s Dir=$Get(%request.Data("dir",1))
	s User=$G(%session.Data("LOGON.USERID"))
    s StrPar=$Get(%request.Data("strPar",1))
    s RowId=$Get(%request.Data("rowid",1))
    s Ingrt=$Get(%request.Data("Ingrt",1))
    s Rows=$Get(%request.Data("rows",1))

    s Ret=$Get(%request.Data("ret",1))
    
    //查询供应商
    i Action = "selectVendor" d
    .d ##class(web.DHCSTM.DHCINGdRet).jsVendorForLocItm(StrPar)

    //查询批次
    i Action = "selectBatch" d
    .d ##class(web.DHCSTM.DHCINGdRet).jsGdRecItmToRet(StrPar,Start,Limit)

    //保存退货单
    i Action = "save" d
    .s MainData=$Get(%request.Data("MainData",1))
    .s ListDetail=$Get(%request.Data("Detail",1))
    .s result = ##class(web.DHCSTM.DHCINGdRet).Save(Ret,MainData,ListDetail)
    .i +result > 0 d
    ..w "{success:'true',info:'"_result_"'}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"
    .
    //根据退货主表的Id查询退货单号
    i Action = "getOrder" d
    .s result = ##class(web.DHCSTM.DHCINGdRet).Select(RowId)
    .i result'="" d
    ..w "{success:'true',info:'"_$tr(result,$c(13,10))_"'}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"

    //根据退货主表Id查询退货明细记录
    i Action = "select" d
    .d ##class(web.DHCSTM.DHCINGrtItm).jsDHCINGdRetItm(Start,Limit,Sort,Dir,Ret)

    //删除退货单
    i Action = "delete" d
    .//s Ingrt=$g(%request.Data("rowid",1))
    .s result = ##class(web.DHCSTM.DHCINGdRet).Delete(Ingrt)
    .i result = 0 d
    ..w "{success:'true',info:''}"
    .e  d
    ..w "{success:'false',info:''}"
    .
    //根据退货明细Id删除退货明细记录
    i Action = "deleteDetail" d
    .s result = ##class(web.DHCSTM.DHCINGrtItm).Delete(RowId)
    .i result = 0 d
    ..w "{success:'true',info:''}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"

    //根据条件查找退货单
    i Action = "selectOrder" d
    .d ##class(web.DHCSTM.DHCINGdRet).jsDHCINGdRet(Start,Limit,Sort,Dir,StrPar)

    //退货单完成设置
    i Action = "complet" d
    .s result = ##class(web.DHCSTM.DHCINGdRet).SetCompleted(Ret)
    .i result = 0 d
    ..w "{success:'true',info:''}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"
    .

    //退货单取消完成
    i Action = "cancelComompleted" d
    .s result = ##class(web.DHCSTM.DHCINGdRet).CancelCompleted(Ret)
    .i result = 0 d
    ..w "{success:'true',info:''}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"
    .
    
    //取退货单参数设置
    i Action = "GetParamProp" d
    .s groupid=$g(%request.Data("GroupId",1))
    .s locid=$g(%request.Data("LocId",1))
    .s userid=$g(%request.Data("UserId",1))
    .s result = ##class(web.DHCSTM.DHCINGdRet).GetParamProp(groupid,locid,userid)
    .w "{success:'true',info:'"_result_"'}"
    .
    //审核退货单
    i Action = "audit" d
    .s UserId=$Get(%request.Data("userId",1))
    .s groupid=$G(%session.Data("LOGON.GROUPID"))
    .s result = ##class(web.DHCSTM.DHCINGdRet).Audit(Ret,UserId,groupid)
    .i result = 0 d
    ..w "{success:'true',info:'"_result_"'}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"
    .
    .


    //审核退货单
    i Action = "getIngriPrice" d
    .s ingri=$g(%request.Data("ingri",1))
    .s uom=$g(%request.Data("uom",1))
    .s result=""
    .i (ingri'="")&(uom'="") d
    ..s result = ##class(web.DHCSTM.DHCINGdRet).IngriPrice(ingri,uom)
    ..
    .i result '= "" d
    ..w "{success:'true',info:'"_result_"'}"
    .e  d
    ..w "{success:'false',info:'"_result_"'}"
    .
i Action = "QueryIngr" d
	.s ListParam=$Get(%request.Data("ParamStr",1))
	.d ##class(web.DHCSTM.DHCINGdRetAuxByRec).QueryIngrForRet(Start,Limit,Sort,Dir,ListParam,User)
	.
i Action = "QueryIngrDetail" d
	.S Parref=$Get(%request.Data("Parref",1))
	.d ##class(web.DHCSTM.DHCINGdRetAuxByRec).QueryIngrDetailForRet(Parref)
	.
	
	i Action = "PrintMBPL" d
	.S IngrRowid=$Get(%request.Data("IngrRowid",1))
	.s HospId=$Get(%request.Data("HospId",1))
	.S User=$Get(%request.Data("user",1))
	.s ret=##class(web.DHCSTM.DHCINGdRet).InserMBPL(IngrRowid,HospId,User)	
	.w "{success:'true',info:'"_ret_"'}"

//发送退货单到平台
	i Action = "SendIngRet" d
	.s ingretid=$Get(%request.Data("IngRet",1))
	.s result=##class(web.DHCSTM.DHCINGdRet).SendIngRetToSCM(ingretid)
	.i +result=0 d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
</script>