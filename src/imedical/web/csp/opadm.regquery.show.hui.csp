<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false," style="height:247px;">
		<div class="hisui-panel" data-options="title:'挂号查询',iconCls:'icon-apply-check',headerCls:'panel-header-gray'" style="border:1px solid #ccc;border-bottom-style:dashed;border-radius:0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label id="_HospUserListLabel" style='color:red;' class='r-label'>医院</label>
					</td>
					<td colspan="3">
						<input id="_HospUserList"/>
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="StDate">开始日期</label></td>
					<td>
						<input id="StDate" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label"><label for="EdDate">结束日期</label></td>
					<td>
						<input id="EdDate" class="hisui-datebox textbox" data-options="formatter:myformatter,parser:myparser">
					</td>
					<td class="r-label"><label for="CardNo">卡号</label></td>
					<td colSpan='3'>
						<input id="CardTypeNew" disabled class="textbox t-boxsize" style="width:129px;height:30px;line-height:30px;">
						<input id="CardNo" class="textbox t-boxsize" style="height:30px;line-height:30px;">
						<a id="BReadCard" href="#" class="hisui-linkbutton showicon" data-options="iconCls:'icon-w-card'">读卡</a>
					</td>
					<td class="l-label">
						<input class='hisui-checkbox' id="Rflag" type="checkbox" label="查询退号">
					</td>
					<td class="r-label">
						<a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="StTime">开始时间</label></td>
					<td>
						<input class="hisui-timespinner textbox" id="StTime" data-options="showSeconds:true" style="width:155px;"/>
					</td>
					<td class="r-label"><label for="EdTime">结束时间</label></td>
					<td>
						<input class="hisui-timespinner textbox" id="EdTime" data-options="showSeconds:true" style="width:155px;"/>
					</td>
					<td class="r-label"><label for="RegNo">登记号</label></td>
					<td>
						<input class="textbox" id="RegNo"/>
					</td>
					<td class="r-label"><label for="PatName">患者姓名</label></td>
					<td>
						<input class="textbox" id="PatName"/>
					</td>
					<td class="l-label">
						<input class='hisui-checkbox' id="MRRFlag" type="checkbox" label="已就诊退号">
					</td>
					<td class="r-label">
						<a id="Bclear" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
					</td>
					
				</tr>
				<tr>
					<td class="r-label"><label for="RLocdesc">科室</label></td>
					<td>
						<input class="hisui-combobox textbox" id="RLocdesc"/>
					</td>
					<td class="r-label"><label for="RMarkdesc">号别</label></td>
					<td>
						<input class="hisui-combobox textbox" id="RMarkdesc"/>
					</td>
					<td class="r-label"><label for="RDocdesc">医生</label></td>
					<td>
						<input class="hisui-combobox textbox" id="RDocdesc"/>
					</td>
					<td class="r-label"><label for="DRCDDesc">挂号优惠</label></td>
					<td>
						<input class="hisui-combobox textbox" id="DRCDDesc"/>
					</td>
					<td class="l-label">
						<input class='hisui-checkbox' id="FeeNullCheck" type="checkbox" label="不包含零费用号">
					</td>
				</tr>
				<tr>
					<td class="r-label"><label for="Ghuse">挂号员</label></td>
					<td>
						<input class="hisui-combobox textbox" id="Ghuse"/>
					</td>
					<td class="r-label"><label for="TelHome">联系电话</label></td>
					<td>
						<input class="textbox" id="TelHome"/>
					</td>
					<td class="r-label"><label for="PayMode">支付方式</label></td>
					<td>
						<input class="hisui-combobox textbox" id="PayMode"/>
					</td>
					<td class="r-label"><label for="SessionType">挂号职称</label></td>
					<td>
						<input class="hisui-combobox textbox" id="SessionType"/>
					</td>
					<td>
						&nbsp&nbsp&nbsp
						<label for="TimeRange">挂号时段</label>
						&nbsp
						<input class="hisui-combobox textbox" id="TimeRange"/>
					</td>
					<!--td class="l-label">
						<input class='hisui-checkbox' id="SelfHosp" type="checkbox" label="本院区">
					</td-->
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="RegQueryTab"></table>
		</div>
	</div>
</div>
