<!--付款封面制单-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<style type="text/css">
		.NonPadGrid .datagrid-toolbar{
			padding:5px;
			border-width:0px;
		}
</style> 
<body class="hisui-layout" data-options="fit:true">
	<div class="hisui-panel" data-options="title:'付款封面',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'"> 
		<div class="hisui-layout" data-options="fit:true">
			<div id="North" class="BorderLine" data-options="region:'north',headerCls:'panel-header-gray'" style="border-bottom-style:dashed;height:91px">
				<table id="MainConditions" class="Condition">
					<input id="RowId" name="RowId" type="hidden">
					<tr>
						<td><label>入库科室</label><input id="IngrLoc" name="IngrLoc" class="hisui-combobox" style="width:190px;"></td>
						<td><label>起始日期</label><input id="StartDate" name="StartDate" class="hisui-datebox textbox"></td>
						<td><label>发票号</label><input id="InvNo" name="InvNo" class="textbox" style="width:150px;"></td>
						<td><label>高值状态</label><select id="HvFlag" name="HvFlag" class='hisui-combobox' style="width:110px;">
							<option value="" selected>全部</option>
							<option value="Y">高值</option>
							<option value="N">非高值</option>
						</select></td>
						<td><a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></td>
					</tr>
					<tr>
						<td><label>供应商</label><select id="Vendor" name="Vendor" class="hisui-combobox" style="width:190px;"></select></td>
						<td><label>截止日期</label><input id="EndDate" name="EndDate" class="hisui-datebox textbox"></td>
						<td><label>发票状态</label><select id="InvFlag" name="InvFlag" class='hisui-combobox' style="width:157px;">
							<option value="" selected>全部</option>
							<option value="Y">有发票</option>
							<option value="N">无发票</option>
						</select></td>
						<td></td>
						<td><a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a></td>
					</tr>
				</table>
			</div>
			<div data-options="region:'center',border:false,collapsible:false" style="border-bottom:1px solid #CCCCCC;" class="NonPadGrid">
				<table style="width:100%">
					<tr>
						<td style="width: 50%;float: left">
							<a href="#" id="AddBT" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true">新增</a>
							<a href="#" id="SaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true">保存</a>
						</td>
						<td style="width: 50%;text-align: right;">
							<label>总数量：</label>&nbsp;&nbsp;<span id="QtyAmt" name="QtyAmtQtyAmt" style="color:blue"></span>&nbsp;&nbsp;&nbsp;&nbsp;<label>进价总额：</label>&nbsp;&nbsp;<span id="RpAmt" name="RpAmt" style="color:blue"></span>
						</td>
					</tr>
				</table>
			</div>
			<div data-options="region:'south',border:false" style="height:550px;">	
				<table id="GRMainGrid"></table>
			</div>	
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcstmhisui/PayCover/PayCoverCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/PayCover/PayCover.js"></script>
</body>
</html>