			<div class="eq-table-td eq-table-label"><label></label></div>
			<div class="eq-table-td eq-table-label15"><a id="BSubmit" href="#" class="hisui-linkbutton" iconCls="icon-w-save"  data-options="stopAllEventOnDisabled:true">确认</a></div>
		</div>
	</div>
</div>
<script type="text/javascript" src="../scripts/DHCEQDisUseType.js"></script>
