<!--灭菌验收-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:0px">
		<div class="hisui-panel" data-options="title:'灭菌单信息',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="UomTB">
				<div> 
					<table class="Condition" id="MainCondition">
						<input id="FComplateFlag" name="FComplateFlag" type="hidden">
						<tr>
							<td><label>灭菌锅号</label><input class="textbox" id="MachineNo" name="MachineNo" style="width:130px"></td>
							<td><label>灭菌程序</label><input class="textbox" id="SterPro" name="SterPro" style="width:130px"></td>
							<td><a href="#" id="SearchBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:110px">查询</a></td>
						</tr>
						<tr>
							<td><label>灭菌人</label><input class="textbox" id="SterUser" name="SterUser" style="width:130px"><input class="textbox" hidden id="SterUserValue" name="SterUserValue"></td>
							<td><label>锅次</label><input class="textbox" id="MachineCount" name="MachineCount" style="width:130px"></td>
							<td><a href="#" id="OKBT" class="hisui-linkbutton" data-options="iconCls:''" style="width:110px">验收合格</a></td>
							
						</tr>
						<tr>
							<td><label>开始日期</label><input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq" style="width:137px"></td>
							<td><label>结束日期</label><input id="FEndDate" name="FEndDate" class="textbox hisui-dateboxq" style="width:137px"></td>
							<td><a href="#" id="FailBT" class="hisui-linkbutton" data-options="iconCls:''" style="width:110px">验收不合格</a></td>
						</tr>
					</table>
				</div>
			</div>
				<table id="MainList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:520px;padding-left:10px">
		<div  class="hisui-panel" data-options="title:'灭菌详情',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true" style="padding-bottom:10%">
			<table class="Condition">
				<tr>
					<td><label>不合格原因</label><input id="UnqualifiedReason" class="textbox hisui-combobox" style="width:140px"></td>
					<td style="padding-left:30px"><a href="#" id="CheckFailBT" class="hisui-linkbutton" style="width:100px;">验收不合格</a></td>
				</tr>
			</table>
			<table id="ItemList"></table>
		</div>
	</div>
<script type="text/javascript" src="../scripts/cssd/Sterilize/SterCheck.js"></script>
<script type="text/javascript" src="../scripts/cssd/Sterilize/SterCommon.js"></script>
</body>
</html>