<html>
<head>
<!-- Put your page Title here -->
<title>材料码表维护</title>
<HISUI/>

<link rel="stylesheet" type="text/css" href="../scripts/dhccl/css/bpcommon.css" />
<script type="text/javascript" src="../scripts/dhccl/bp/dhcbpcconsumable.js"></script>
</head>
<body style="padding:0px;">
	<div id="noTitleList" style="height:100%;bodyCls:'panel-body-gray';">
	<table id="consumableBox"></table>
	</div>
	<div id="consumableDlg" title="材料码表维护" style="width:340px;height:370px;display:none;modal:true;border-bottom-width:0px;">
        <form id="consumableDlgTb">
            <table class="form-table">
            <tr>
              	<td class="showwindow-tdtype2">代码</td>
           		<td>
            		<input id="bpcCCode" name="bpcCCode" class="hisui-validatebox" data-options="required:true" style="width:170px;">
           		</td>
           </tr>
           <tr>
                <td class="showwindow-tdtype2">描述</td>
                <td>
            		<input id="bpcCDesc" name="bpcCDesc" class="hisui-validatebox" data-options="required:true" style="width:170px;">
                </td>
           </tr>
           <tr>
                 <td class="showwindow-tdtype2">类型</td>
                 <td colspan=3>
                    <div>
                    	<select id="bpcCType" name="bpcCType" class="hisui-combobox" style="width:177px;margin:5px 0;"></select>
                    </div>
                 </td>
           </tr> 
           <tr>
                <td class="showwindow-tdtype2">膜面积</td>
                <td>
            		<input id="bpcCMembraneArea" name="bpcCMembraneArea" class="hisui-textbox textbox" data-options="required:true" style="width:170px;">
                </td>
           </tr>
           <tr>
           <td class="showwindow-tdtype2">是否高通量</td>
                 <td colspan=3>
                    <div>
                    	<select id="bpcCHighFluxed" name="bpcCHighFluxed" class="hisui-combobox" style="width:177px;margin:5px 0;"></select>
                    </div>
                 </td>
           </tr> 
           <tr>
           <td class="showwindow-tdtype2">医嘱</td>
                 <td colspan=3>
                    <div>
                    	<select id="bpcCArcim" name="bpcCArcim" class="hisui-combobox" style="width:177px;margin:5px 0;"></select>
                    </div>
                 </td>
           </tr>                 
        </form>
    </div>
</body>
</html>
