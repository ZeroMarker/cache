 * Anchor: ZhYW
 * Date: 2019-10-23
 * Description: 单据打印列表
-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 if ##class(websys.SessionEvents).SessionExpired() quit 1
 quit 1
</csp:method>
<html>
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCBILL/>
	<script type="text/javascript" src="../scripts/DHCCPM_DOMComm.js"></script>
	<server>
		w "<input id='InitGSPrintTaskEncrypt' name='InitGSPrintTaskEncrypt' type='hidden' value='"_##class(web.DHCBL.UDHCCommFunLibary).GetClassPropertyList("web.DHCEntity.OPGS.GSPrintTask")_"'>",$c(13,10)
		w "<input id='GSPTRowID' name='GSPTRowID' type='hidden'/>",$c(13,10)
		w "<input id='XmlTemplateName' name='XmlTemplateName' type='hidden'/>",$c(13,10)
		w "<input id='ClassName' name='ClassName' type='hidden'/>",$c(13,10)
		w "<input id='MethodName' name='MethodName' type='hidden'/>",$c(13,10)
		w "<input id='TaskType' name='TaskType' type='hidden' value='"_$g(%request.Data("TaskType",1))_"'/>",$c(13,10)
		w "<input id='PrintMode' name='PrintMode' type='hidden'/>",$c(13,10)
		w "<input id='HardEquipDR' name='HardEquipDR' type='hidden'/>",$c(13,10)
	</server>
</head>

<body style="padding:10px;">
	<div class="hisui-layout" data-options="fit:true">
		<div data-options="region:'center',border:false">
			<table id="taskList"></table>
		</div>
		<div id="tl-tb">
			<table cellspacing="0" cellpadding="0">
				<tr>
					<td><a href="javascript:;" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true" id="btn-add">新增</a></td>
					<td><a href="javascript:;" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true" id="btn-delete">删除</a></td>
					<td><a href="javascript:;" class="hisui-linkbutton" data-options="iconCls:'icon-write-order',plain:true" id="btn-update">修改</a></td>
					<td><a href="javascript:;" class="hisui-linkbutton" data-options="iconCls:'icon-save',plain:true" id="btn-save">保存</a></td>
					<td><a href="javascript:;" class="hisui-linkbutton" data-options="iconCls:'icon-redo',plain:true" id="btn-reject">取消编辑</a></td>
				</tr>
			</table>
		</div>
	</div>
	<script type="text/javascript" src="../scripts/dhcbillconfig/dhcbill.conf.group.prttasklist.js"></script>
</body>
</html>
