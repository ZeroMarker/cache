<!--ICU患者日志(科室患者信息)-->
<div class="hisui-layout" data-options="fit:true">
    <!--患者信息-->
    <div data-options="region:'center',border:false,collapsible:false" style="padding: 10px 0px 10px 10px; overflow: hidden; ">
        <table id="gridICULoc" data-options="toolbar:'#ToolBarList'"></table>
        <div id="ToolBarList" style="text-align: left">
            <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-gen'" href="#" id="btnICUGreat" style="width:80px;">生成</a>
            <a class="hisui-linkbutton" data-options="plain:true,iconCls:'icon-submit'" href="#" id="btnICUSubmit" style="width: 80px;">提交</a>
        </div>
    </div>
    <!--三管信息-->
    <div id="west" data-options="region:'east',border:false,collapsible:false" style="width:470px;padding:10px;">
        <table id="gridICUOE" data-options="toolbar:'#ToolBarOEList'"></table>
        <div id="ToolBarOEList" style="text-align: center; box-sizing: border-box;height:30.6px;padding-top:5px;">
            <a href="#" id="btnICUAll" style="display: inline-block; width: 24%; background-color: #dbedf9;">#(..Get("全部"))#</a>
            <a href="#" id="btnICUPICC" style="display: inline-block; width: 24%; ">#(..Get("中央血管导管"))#</a>
            <a href="#" id="btnICUVAP" style="display: inline-block; width: 24%; ">#(..Get("呼吸机"))#</a>
            <a href="#" id="btnICUUC" style="display: inline-block; width: 24%; ">#(..Get("导尿管"))#</a>
        </div>
    </div>
</div>