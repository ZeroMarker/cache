<!--入库单制单(依据订单)-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCSTMHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
		<div data-options="region:'north',border:false" style="height:150px;">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'west',border:false" class="GridPadding" style="width:850px;"> 
					<div class="hisui-panel" title="查询条件" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">    		
						<table id="PoConditions" class="Condition">
							<tr>
								<td>入库科室</td>
								<td><input id="PoRecLoc" name="PoRecLoc" class="hisui-combobox" style="width:160px;"></td>
								<td>起始日期</td>
								<td><input id="PoStartDate" name="PoStartDate" class="hisui-datebox" style="width:160px;"></td>
								<td>入库状态</td>
								<td>
									<select id="Status" name="Status" class="hisui-combobox" style="width:160px;">
										<option value="0">未入库</option>
										<option value="1">部分入库</option>
										<option value="2">全部入库</option>
									</select>
								</td>
							</tr>
							<tr>
								<td>供应商</td>
								<td><select id="VendorBox" name="VendorBox" class="hisui-combobox" style="width:160px;"></select></td>
								<td>截止日期</td>
								<td><input id="PoEndDate" name="PoEndDate" class="hisui-datebox" style="width:160px;"></select></td>
								<td>
								<a href="#" id="PoQueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
								</td>
								<td>
								<a href="#" id="PoClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
								<a href="#" id="PoSaveBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
								</td>
								<td><input id="AuditFlag" name="AuditFlag" type="hidden" value="1"></td>
							</tr>
						</table>
					</div>
				</div>
				<div data-options="region:'center',border:false" class="GridPadding" style="padding-left:0;">
					<div class="hisui-panel" title="保存条件" data-options="headerCls:'panel-header-gray',iconCls:'icon-paper',fit:true">
						<table id="PoSaveConditions" class="Condition">
							<tr>
								<td>资金来源</td>
								<td><input id="PoSourceOfFund" name="PoSourceOfFund" class="hisui-combobox" style="width:160px;"></td>
							</tr>
						</table>
					</div>
				</div>
			</div>
		</div>
		<div data-options="region:'center',border:false">
			<div class="hisui-layout" data-options="fit:true">
				<div data-options="region:'west',border:false" class="GridPadding" style="padding-top:0;width:500px;">   
					<div class="hisui-panel" data-options="title:'订单  (注: 资金来源 作为保存条件)',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">   
						<table id="IngrInpoMainGrid"></table>
					</div>
				</div>
				<div data-options="region:'center',border:false" class="GridPadding" style="padding-left:0;padding-top:0;">   
					<div class="hisui-panel" data-options="title:'订单明细',iconCls:'icon-paper',fit:true,headerCls:'panel-header-gray'">   
							<table id="IngrInpoDetailGrid"></table>
					</div>   
				</div>
			</div>	
		</div>
	
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecCommon.js"></script>
	<script type="text/javascript" src="../scripts/dhcstmhisui/InGdRec/DHCSTINGdRecByPo.js"></script>
</body>
</html>


