<!--错误信息登记界面-->
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
</head>
<body class="hisui-layout" data-options="fit:true">
<div class="hisui-panel GridPadding" data-options="region:'center',border:false">   
	<div class="hisui-panel" data-options="iconCls:'icon-paper',title:'错误信息登记列表',headerCls:'panel-header-gray',split:true,collapsible:false,fit:true">
		<div class="hisui-layout" data-options="fit:true">
			<div id="ErrorRegistorTB" data-options="region:'north',collapsible:false,border:false" class="BorderLine"> 
				<table class="Condition">
					<tr>
						<td>消毒包</td>
						<td><input id="package" name="package" class="textbox hisui-combobox" style="width:170px"></td>
						<td>日期</td>
							<td><input id="FDate" name="FDate" class="textbox hisui-dateboxq" style="width:130px"></td>
						<td>
							<a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a>
						</td>
						<td>
							<a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'">清屏</a>
						</td>
					</tr>
				</table>
			</div>
			<div data-options="region:'center',border:false">
				<table id="ErrorRegistorGrid"></table>
			</div>
		</div>
	</div>
</div>
<script type="text/javascript" src="../scripts/cssd/System/ErrorRegister.js"></script>
<script type="text/javascript" src="../scripts/cssd/Common/commonUI.js"></script>
</body>
</html>
