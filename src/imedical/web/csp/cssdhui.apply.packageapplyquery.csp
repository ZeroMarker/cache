<!--请领-->
<!DOCTYPE html>
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
	i ##Class(websys.SessionEvents).SessionExpired() q 1
	q 1
</csp:method>
<html lang="zh-CN">
<head>
	<title><TRAK:TRANSLATE id=title>##(%session.Get("TITLE"))##</TRAK:TRANSLATE></title>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<CSSDHISUI/>
	<link type="text/css" rel="styleSheet"  href="../scripts/cssd/ExtUX/ystep/css/ystep.css" />
	<script type="text/javascript" src="../scripts/cssd/ExtUX/ystep/ystep.js"></script>
</head>
<body class="hisui-layout" data-options="fit:true">
	<div class="GridPadding" data-options="region:'center',headerCls:'panel-header-gray',split:true,border:false" style="padding-right:0px;padding-bottom:15px">
		<div class="hisui-panel" data-options="title:'请领单据',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true">
			<div id="CondTB">
				<table class="Condition" id="MainCondition">
				<tr>
					<td>申请科室</td>
					<td><input id="FReqLoc" name="FReqLoc" class="textbox hisui-combobox"></td>
					<td>供应科室</td>
					<td><input id="FSupLoc" name="FSupLoc" class="textbox hisui-combobox"></td>
					<td colspan="2" style="">
						<a href="#" id="QueryBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" style="width:100px">查询</a>
					</td>
				</tr>
				<tr>
					<td>开始日期</td>
					<td><input id="FStartDate" name="FStartDate" class="textbox hisui-dateboxq" ></td>
					<td>结束日期</td>
					<td><input id="FEndDate" name="FEndDate" class="textbox hisui-dateboxq" ></td>
					<td colspan="2" style="">
					<a href="#" id="ClearBT" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" style="width:100px">清屏</a>
					</td>
				</tr>
				<tr>
					<td>单据号</td>
					<td><input id="FReqNo" name="FReqNo" class="hisui-numberbox textbox"  ></td>
				</tr>
				</table>
			</div>
				<table id="MainList"></table>
		</div>
	</div>
	<div class="GridPadding" data-options="title:'',region:'east',border:false,headerCls:'panel-header-gray'" style="width:560px;padding-top:10px;padding-bottom:15px">
		<div class="hisui-panel" data-options="title:'单据明细',iconCls:'icon-paper',headerCls:'panel-header-gray',fit:true" style="border-bottom:none;border-left:none;border-right:none">	
			<div class="hisui-layout" data-options="fit:true">
				<div  data-options="title:'',region:'south',border:false,headerCls:'panel-header-gray'" style="height:160px;padding-top:10px">
					<div class="hisui-panel" data-options="headerCls:'panel-header-gray',fit:true" style="border-color:#ccc">
						<div class="ystep1"></div>
					</div>	
				</div>
				<div data-options="title:'',region:'center',border:false,headerCls:'panel-header-gray'">
					<div class="hisui-panel" data-options="headerCls:'panel-header-gray',fit:true" style="border-color:#ccc;border-radius:0 0 4px 4px;border-top:none">
						<table id="ItemList"></table>
					</div>
				</div>
			</div>				
		</div>
	</div>
<script type="text/javascript" src="../scripts/cssd/Apply/ApplyCommon.js"></script>	
<script type="text/javascript" src="../scripts/cssd/Apply/PackageApplyquery.js"></script>

</body>
</html>