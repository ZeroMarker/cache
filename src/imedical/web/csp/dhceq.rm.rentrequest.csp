<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<DHCEQ:HISUI></DHCEQ:HISUI>
</head>
<script language=Cache runat=server>
	s RowID=$Get(%request.Data("RowID",1))
	s RentLocID=##Class(web.DHCEQCommon).GetFirstIDByLocType("0301","")
	s RentLocDesc=##Class(web.DHCEQCommon).GetFirstIDByLocType("0301","Y")
	//Modify by zx 2020-04-20 Bug ZX0084
	s CurDateTime=$ZDATETIME($h,3,1)
	//Modify by zx 2020-06-24 Bug ZX0093
	s RentModeFlag=##class(web.DHCEQCommon).GetSysInfo("992003")
</script>
<body class="hisui-layout">
	<div id='Loading' class="eq-loading"><h1><img src="../skin/default/images/loading.gif"><font>加载中・・・</font></h1></div>
	<div data-options="region:'center',border:false">
		<div class="hisui-panel" data-options="border:false" style="padding:10px;">
			<div class="eq-table">
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cRRequestNo" for="RRequestNo">申请单号</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="RRequestNo" disabled="true"></div>
					<!--Modify by zx 2020-04-24-->
					<div class="eq-table-td eq-table-label"><label id="cRShareItemDR_SIDesc" for="RShareItemDR_SIDesc">资源项名称</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="RShareItemDR_SIDesc" data-options="'component':'RM.L.ItemByShareResource'"></div>
					<div class="eq-table-td eq-table-label"><label id="cRFromLocDR_DeptDesc" for="RFromLocDR_DeptDesc">供给科室</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="RFromLocDR_DeptDesc" data-options="'component':'RM.L.ShareResourceLoc'"></div>
				</div>
				<!--Modify by zx 2020-04-24-->
				<!--div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cRShareResourceDR_REEquipName" for="RShareResourceDR_REEquipName">设备</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="RShareResourceDR_REEquipName" data-options="'component':'RM.L.EquipByShareResource'"></div>
					<div class="eq-table-td eq-table-label"><label>设备编号</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="REquipNo" disabled="true"></div>
				</div-->
				<div class="eq-table-tr">
				<!--modify by lmm 2020-06-24 1386796 弹窗尺寸调整-->
					<div class="eq-table-td eq-table-label"><label id="cRModelDR_MDesc" for="RModelDR_MDesc">规格型号</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'RM.L.ShareItemModel','defaultsize':'width:400,height:175'" id="RModelDR_MDesc"></div>
					<div class="eq-table-td eq-table-label"><label id="cRPlanBegin" for="RPlanBegin">计划开始时间</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-datetimebox textbox" id="RPlanBegin"></div>
					<div class="eq-table-td eq-table-label"><label id="cRPlanEnd" for="RPlanEnd">计划结束时间</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-datetimebox textbox" id="RPlanEnd"></div>
				</div>
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cRRequestLocDR_DeptDesc" for="RRequestLocDR_DeptDesc">申请科室</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="RRequestLocDR_DeptDesc" data-options="'component':'PLAT.L.Loc','defaultsize':'width:400,height:160'"></div>
					<div class="eq-table-td eq-table-label"><label id="cRRequestUserDR_UName" for="RRequestUserDR_UName">联系人</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" data-options="'component':'PLAT.L.EQUser','defaultsize':'width:400,height:160'" id="RRequestUserDR_UName"></div>
					<div class="eq-table-td eq-table-label"><label id="cRHold3" for="RHold3">联系电话</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="RHold3"></div>
				</div>
			</div>
			<div style="width:100%;height:50px;text-align:center;margin-top:30px;">
				<div style="width:33%;float:left;"><a id="BSave" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true"></a></div>
				<div style="width:33%;float:left;"><a id="BDelete" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true"></a></div>
				<div style="width:33%;float:left;"><a id="BSubmit" href="#" class="hisui-linkbutton" data-options="stopAllEventOnDisabled:true"></a></div>
			</div>
		</div>
	</div>
	<!--隐藏元素-->
	<div id="hiddenEncrypt" style="display:hidden">
		<form name="DHCEQ">
			<INPUT type="hidden" id="RRowID" value="#(RowID)#">
			<INPUT type="hidden" id="RRequestLocDR">
			<INPUT type="hidden" id="RFromLocDR">
			<INPUT type="hidden" id="RRequestUserDR">
			<INPUT type="hidden" id="RShareItemDR"> <!--Modify by zx 2020-04-24-->
			<INPUT type="hidden" id="RModelDR">
			<INPUT type="hidden" id="RShareResourceDR">
			
			<INPUT type="hidden" id="RStatus">
			<INPUT type="hidden" id="RentLocID" value="#(RentLocID)#">
			<INPUT type="hidden" id="RentLocDesc" value="#(RentLocDesc)#">
			
			<INPUT type="hidden" id="RPlanBeginDate">
			<INPUT type="hidden" id="RPlanBeginTime">
			<INPUT type="hidden" id="RPlanEndDate">
			<INPUT type="hidden" id="RPlanEndTime">
			<!--Modify by zx 2020-04-20 Bug ZX0084-->
			<INPUT type="hidden" id="CurDateTime" value="#(CurDateTime)#">
			<!--Modify by zx 2020-06-24 Bug ZX0093-->
			<INPUT type="hidden" id="RentModeFlag" value="#(RentModeFlag)#">
		</form>
	</div>
	<script type='text/javascript' src='../scripts/dhceq/rm/rent.js'></script>
</body>
</html>