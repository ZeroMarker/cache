  </head>
  <body>

<!--try 2016=09-21-->
<div class="col-sm-1">	
            	<div class="input-group date form_time" id="exeDate" data-date="" data-date-format="hh:ii" style="margin-top:5px">
            		<!--<span class="input-group-addon bord-no">Ê±¼ä</span>-->
                	<input type="text" class="form-control" style="min-width:50px;">
                	<span class="input-group-addon"><i class="fa fa-clock-o"></i></span>
            	</div>
</div>
<div class="control-group">
                <div class="controls input-append date form_datetime" data-date="1979-09-16T05:25:07Z" data-date-format="yyyy-MM-dd HH:ii" data-link-field="dtp_input1">
                    <input size="16" type="text" value="" readonly>
                    <span class="add-on"><i class="icon-remove"></i></span>
					<span class="add-on"><i class="icon-th"></i></span>
                </div>
				<input type="hidden" id="dtp_input1" value="" /><br/>
            </div>
 <div class="controls input-append date form_time" data-date="" data-date-format="hh:ii" data-link-field="dtp_input3" data-link-format="hh:ii">
                    <input style="width:60px;height:26px;" size="16" type="text" value=" " readonly>
                    <span style="height:26px;" class="add-on"><i class="fa fa-close"></i></span>
					<span style="height:26px;" class="add-on"><i class="fa fa-calendar"></i></span>
                </div>
				<input type="hidden" id="dtp_input3" value="" /><br/>
            </div>
            
            
 <input size="16" type="text" value="" readonly class="form_datetime">
 
<script type="text/javascript">
    $(".form_datetime").datetimepicker({format: 'yyyy-mm-dd hh:ii'});
</script> 
            


<script type="text/javascript">
    $('.form_datetime').datetimepicker({
	    format: 'yyyy-mm-dd hh:ii',
        language:  'zh-CN',
        weekStart: 1,
        todayBtn:  1,
		autoclose: 1,
		todayHighlight: 1,
		startView: 2,
		forceParse: 0,
        showMeridian: 1
    });
	$('.form_time').datetimepicker({
        language:  'zh-CN',
        weekStart: 1,
        todayBtn:  1,
		autoclose: 1,
		todayHighlight: 1,
		startView: 1,
		minView: 0,
		maxView: 1,
		forceParse: 0
    });
</script>
