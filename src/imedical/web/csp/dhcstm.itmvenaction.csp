<csp:content charset="UTF-8">
<SERVER>
s Action=$Get(%request.Data("actiontype",1))
s Start=$Get(%request.Data("start",1))
s Limit=$Get(%request.Data("limit",1))
s Sort=$Get(%request.Data("sort",1))
s Dir=$Get(%request.Data("dir",1))

;
i Action = "Save" d
	.S ListDetail=$Get(%request.Data("ListDetail",1))
	.s result=##class(web.DHCSTM.ItmVen).Save(ListDetail)
	.i result ="" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"

i Action = "Query" d
	.S Vendor=$Get(%request.Data("Vendor",1))
	.s Stkcat=$Get(%request.Data("Stkcat",1))
	.s result=##class(web.DHCSTM.ItmVen).Query(Start,Limit,Vendor,Stkcat)
	.i result="" d
	..w "{results:0,rows:[]}"
	.e  d
	..w result
i Action = "Delete" d
	.S Parref=$Get(%request.Data("rowid",1))
	.s ret=##class(web.DHCSTM.ItmVen).Delete(Parref)
	.i +ret'=0  d
	..w "{success:'false',info:'"_ret_"'}"
	.e  d
	..w "{success:'true',info:'"_ret_"'}"
	.
	
	//根据供应商的RowId查询厂商的明细信息
	i Action = "select" d
	.S list=$Get(%request.Data("list",1))
	.s ^ZX("009")=list
	.w "{success:'true',info:'"_##class(web.DHCSTM.ItmVen).Select(list)_"'}"
   
   i Action = "insert" d
	.S data=$Get(%request.Data("data",1))
	.s result=##class(web.DHCSTM.ItmVen).SaveItmRemark(data)
	.i result ="" d
	..w "{success:'true',info:''}"
	.e  d
	..w "{success:'false',info:'"_result_"'}"
	
	i Action = "GroupUser" d
	.s GrpDesc=$Get(%request.Data("GrpDesc",1))
	.w ##class(web.DHCSTM.ItmVen).GetGroupUser(Start,Limit,GrpDesc)	



</SERVER>
