<div class="hisui-layout" fit="true">
	<div data-options="region:'north',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="height:102px;padding:10px 10px 0 10px;">
		<div class="hisui-panel" style="border:1px solid #ccc;border-radius:4px 4px 0 0;">
			<table class="search-table">
				<tr>
					<td class="r-label">
						<label for="PPRDate">到账日期</label>
					</td>
					<td>
						<input class="hisui-datebox textbox" id="PPRDate" data-options="formatter:myformatter,parser:myparser"/>
					</td>
					<td class="r-label">
						<label for="PPRRemAmount">到账金额</label>
					</td>
					<td>
						<input class="textbox hisui-numberbox" id="PPRRemAmount"/>
					</td>
					<td class="r-label">
						<label for="PPRRemitter">汇款人/单位</label>
					</td>
					<td>
						<input class="textbox" id="PPRRemitter"/>
					</td>
					<td class="r-label">
						<a id="BSave" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'">保存</a>
						<span style="margin-right:10px;"></span>
						<a id="BVerify" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'">审核</a>
					</td>
				</tr>
				<tr>
					<td class="r-label">
						<label for="PPRReceiver">接收用户</label>
					</td>
					<td>
						<input class="hisui-combobox textbox" id="PPRReceiver"/>
					</td>
					<td class="r-label">
						<label for="PPRRemark">备注</label>
					</td>
					<td>
						<input class="textbox" id="PPRRemark"/>
					</td>
					<td class="r-label">
						<label for="PPRBalance">余额</label>
					</td>
					<td>
						<input class="textbox" disabled id="PPRBalance"/>
					</td>
				</tr>
			</table>
		</div>
	</div>
	<div data-options="region:'center',title:'',border:false,collapsible:false,headerCls:'panel-header-gray',iconCls:'icon-reset'" style="padding:0 10px 10px 10px;">
		<div class="hisui-panel" fit="true" style="height:100%;border:1px solid #ccc;border-top:0;border-radius:0 0 4px 4px;">
			<table id="PilotProRemListTab"></table>
		</div>
	</div>
</div>