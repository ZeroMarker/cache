<!--
Description:  健康提示维护  dhchm.medicaltips.edit.csp  
Created by wangguoying
-->
<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<style>
		.labelTd{
			width:40px;
			text-align:right;
			padding-right:10px;
		}
		.labelTdNew{
			width:40px;
			text-align:right;
		}
		.labelWinTd{
			width:50px;
			text-align:right;
		}
		.btnTd{
			width:120px;
			text-align:right;
		}
   </style>
</head>
<body style="overflow:hidden;background-color:white">
	<div id="LayoutDiv" class="hisui-layout"  >
		<div id='WestPanel' data-options="region:'west',split:true,iconCls:'icon-tip',headerCls:'panel-header-gray',title:'健康提示'" style= "width:650px;">				
			<div style="padding:5px;height:35px;">
				<table>
					<tr>
						<td class="labelTd">代码:</td>
						<td><input id="SCode" class="hisui-validatebox textbox" style="width:150px;" ></td>
						<td class="labelTd">描述:</td>
						<td><input id="SDesc" class="hisui-validatebox textbox" style="width:150px;" ></td>
						<td class="btnTd"><a href="javascript:void(0);" onclick="" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'" >查询</a></td>
					</tr>
				</table>
			</div>
			<div id="GridDiv" style="border-top:1px solid #ccc">
				<table id="MTList" data-options="border:false" ></table>
			</div>
		</div>
	
	
		<div id="CenterPanel" data-options="region:'center',split:true, iconCls:'icon-write-order',headerCls:'panel-header-gray',title:'编辑'">
			<div  style="padding:5px;">  <!--条件div  begin-->
			<table cellspacing="10">
				<tr>
					<td class="labelTdNew">代码:</td>	
					<td><input id="ECode" class="hisui-validatebox textbox"  data-options="validType:'checkCodeExist',required:true" style="width:150px;" ></td>
					<td class="labelTdNew">描述:</td>
					<td><input id="EDesc" class="hisui-validatebox textbox" style="width:150px;" ></td>
					<td class="labelTdNew">别名:</td>
					<td><input id="EAlias" class="hisui-validatebox textbox" style="width:150px;" ></td>
					<td class="labelTdNew">激活:</td>
					<td><input id="EActive" class='hisui-checkbox' type="checkbox"></td>
				</tr>
				<tr>
					<td class="labelTdNew" >备注:</td>	
					<td colspan="5"><textarea id="ERemark" rows="2" style="width:480px;"></textarea></td>
				</tr>
				<tr>
					<td class="labelTdNew" style="vertical-align:top">详细:</td>
					<td colspan="7"><textarea id="EDetail" class='ckeditor' /></textarea></td>
				</tr>	
			</table>
			<div style="text-align:center">
				<a id="Save" onclick="Save_onclick();"   href="#" class="hisui-linkbutton head-btn" data-options="iconCls:'icon-w-save'">保存</a>		
				<a id="Clear" onclick="Clear_onclick();"  style="margin-left:40px;" href="#" class="hisui-linkbutton head-btn" data-options="iconCls:'icon-w-clean'">清屏</a>				
			</div>		
		</div>
			
		</div>
		<input type="hidden" id="MTID" value="">
	</div>
	
	
	
	
	
	<script type="text/javascript" src="../scripts/websys.jquery.js"></script>
  	<script type="text/javascript" src="../scripts/hisui/websys.hisui.js"></script>
  	<script type="text/javascript" src="../scripts_lib/ckeditor/ckeditor.js"></script>
  	<script type="text/javascript" src="../scripts/dhchm.medicaltips.edit.js"></script>
	<Script language="javascript" type="text/javascript">
	$("#LayoutDiv").height($(document).height());
	</Script>
</body>
</html>

