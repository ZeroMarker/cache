<div class="hisui-layout" data-options="fit:true,border:false">
	<div id="dregion" data-options="region:'center',border:false,split:false" style="padding:10px 10px 10px 10px;">
		<div id="dpan" title="治疗过程状态查询" class="hisui-panel" style="padding:0px"
		data-options="border:true,iconCls:'icon-apply-check',headerCls:'panel-header-gray',fit:true,closable:false, collapsible:false,minimizable:false,maximizable:false">   
			<div class="hisui-layout" data-options="fit:true,border:false">
				<div data-options="region:'north',split:false,border:false" style="width:100%;height:50px">
					<div id="p1" class="hisui-panel" style="padding:10px" 
					data-options="fit:true,border:false,closable:false, collapsible:false,minimizable:false,maximizable:false">  
						<label class="first-qlab" for="StartDate">开始日期</label><input id="StartDate" type="text" class="hisui-datebox" style="width:120px">
						<label class="qlab" for="EndDate">结束日期</label><input id="EndDate" type="text" class="hisui-datebox" style="width:120px">
						<label class="qlab" for="ComboArcim">医嘱项</label><input id="ComboArcim" type="text" name="ComboArcim" style="width:180px"/>
						<label class="qlab" for="ResGroup">服务组</label><input id="ResGroup" type="text" name="ResGroup" style="width:120px"/>
						<label class="qlab" for="CureStatus">治疗状态</label><input id="CureStatus" type="text" name="CureStatus" style="width:120px"/>
						<a id="btnFind" class="hisui-linkbutton" style="margin:0px 10px" data-options="iconCls:'icon-w-find'">查询</a>
						<!--<a id="btnExport" class="hisui-linkbutton" data-options="iconCls:'icon-print'">导出并打印</a>-->
					</div>
				</div>
				<div data-options="region:'center',border:false">
					<div id="workpanel" class="hisui-panel"
					data-options="fit:true,border:true,headerCls:'panel-header-gray',closable:false,collapsible:false,minimizable:false,maximizable:false">  
						<table id="tabCureProcessList"></table> 
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
