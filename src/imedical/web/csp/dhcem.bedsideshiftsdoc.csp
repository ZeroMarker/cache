<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 quit 1
</csp:method>
<!-- dhcem.bedsideshiftsdoc.csp 急诊医生交班本 -->
<!DOCTYPE html>
<html lang="en">
<head>
	<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
	<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
	<HISUI/>
	<link href="../scripts/dhcnewpro/dhcem/css/base.css" rel="stylesheet">
	<link href="../scripts/dhcnewpro/dhcem/css/bedsideshiftsdoc.css" rel="stylesheet">
	<script type="text/javascript" src="../scripts/dhcnewpro/js/commonfun.js"></script>		
	<script type="text/javascript" src="../scripts/dhcnewpro/js/hiscomponent.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/bedsideshiftscom.js"></script>
	<script type="text/javascript" src="../scripts/dhcnewpro/dhcem/bedsideshiftsdoc.js"></script>
</head>
<body class="background-warp">
	<div class="container">
		<div class="page-tools">
    		<div style="margin:10px 10px 10px 0px;">
				<label style="margin-left:10px;margin-right:5px;">交班日期</label>
				<input id="WrDate" class="hisui-datebox textbox" style="width:140px;height:30px;line-height:30px;" disabled>
				<label style="margin-left:10px;margin-right:5px;">医疗组</label>
				<input id="MedGrp" class="textbox" style="width:140px;height:30px;line-height:30px;"/>
				<label style="margin-left:10px;margin-right:5px;">留观区</label>
				<input id="Ward" class="textbox" style="width:140px;height:30px;line-height:30px;"/>
				<label style="margin-left:10px;margin-right:5px;">班次</label>
				<input id="Schedule" class="textbox" style="width:140px;height:30px;line-height:30px;"/>
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-edit'" onclick="takShift()" style="margin-left:10px;" id="bt_find">生成交班</a>
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-eye'" onclick="OpenBedLisWin()" style="margin-left:10px;">交班本查询</a>
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-clean'" onclick="clearPages()" style="margin-left:10px;">清空</a>
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-cancel'" onclick="delShifts()" style="margin-left:10px;" id="bt_del">删除</a>
			</div>
		</div>
		<div class="page-north">
			<div class="list-title">
				<div class="list-icon">
					<img src="../scripts/dhcnewpro/dhcem/images/infomation.png" border=0/>
				</div>
				<label>交班记录</label>
				<div class="list-tools">
				</div>
            </div>
        	<div class="list-order-item">
        		<div class="list-item-left">
        			<!--
        			<div style="margin:10px;">
    					<label style="margin-left:0px;line-height:28px;">现有</label>
						<input id="PatNo" class="hisui-validatebox" style="width:120px;vertical-align:bottom;">
					</div>
					-->
					<div style="margin:8px;">
						<table id="itemList" border="1" cellspacing="0" cellpadding="1" class="report_table">
							<tr style="display:none">
								<td align="right" class="tb_td_bk"></td>
								<td width="180px"></td>
								<td align="right" class="tb_td_bk"></td>
								<td width="180px"></td>
								<td align="right" class="tb_td_bk"></td>
								<td width="180px"></td>
								<td align="right" class="tb_td_bk"></td>
								<td width="180px"></td>
							</tr>
						</table>
					</div>
				</div>
				<div class="list-item-right">
					<div style="margin:10px;">
						<label style="line-height:28px;">交班人</label>
						<input id="CarePrv" class="hisui-validatebox" style="width:120px;vertical-align:bottom;" disabled="disabled">
						<label style="margin-left:25px;">密码</label>
						<input id="PassWord" type="password" class="hisui-validatebox" style="width:120px;vertical-align:bottom;">
					</div>
					<div style="margin:10px;">
						<label style="line-height:28px;">接班人</label>
						<input id="RecUser" class="hisui-validatebox" style="width:120px;vertical-align:bottom;">
						<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-save'" onclick="sure()" style="margin-left:30px;" id="bt_sure">确认</a>
					</div>
				</div>
			</div>
		</div>
		<div class="page-center">
			<div class="list-title">
				<div class="list-icon">
					<img src="../scripts/dhcnewpro/dhcem/images/infomation.png" border=0/>
				</div>
				<label>交班明细</label>
				<div class="list-tools">
				</div>
            </div>
        	<div class="list-order-item">
        		<table id="bmDetList" toolbar="#item_tb"></table>
			</div>
		</div>
	</div>
	<div id="item_tb">
		<table cellspacing="0" cellpadding="0" >
			<tr>
		      <td><a href="#" class="hisui-linkbutton" plain="true" iconCls="icon-add" onclick="add()" id="bt_add">新增</a></td>
		    </tr>
		</table>
	</div>
</body>
</html>