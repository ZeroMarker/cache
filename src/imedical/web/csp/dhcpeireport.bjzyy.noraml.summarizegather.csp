	<SCRIPT language="Cache" RUNAT="SERVER">
		s summarizegather=$G(^TMPReport(UserID,"Summarize", "Contentgather"))
		s SummarizeData=""
		s i=$L(summarizegather,$C(13,10))
		f j=1:1:i d
		.
		.Q:(""=$P(summarizegather,$C(13,10),j))
		.s SummarizeData=SummarizeData_$P(summarizegather,$C(13,10),j)_"</br>"
		s summarizegather=SummarizeData
		s PatName=$G(^TMPReport(UserID,"PatInfo","PatName"))
		s PatSex=$G(^TMPReport(UserID,"PatInfo","PatSex"))
		s PatSexString=""
		i PatSex="男" s PatSexString="先生"
		i PatSex="女" s PatSexString="女士"
		s HellTitle="<br>尊敬的<b>"_PatName_"</b>"_PatSexString_": 您好!<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;感谢您参加北京中医医院的健康体检A以下是您本次体检异常结果的汇总分析A希望您认真阅读和对待医生提出的每一项建议C祝您身体健康A快乐每一天I<br>"
