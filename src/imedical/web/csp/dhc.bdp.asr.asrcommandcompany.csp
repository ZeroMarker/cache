<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
/*
    //CSP：dhc.bdp.asr.asrcommandcompany.csp
    //Creator:李可凡
    //Description:应用场景关联厂商
    //CreateDate：2020-9-25
*/
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<HISUI/>
<link rel="stylesheet" type="text/css" href="../scripts/bdp/Framework/css/styleUI.css" />
<script type="text/javascript" src="../scripts/bdp/Framework/scripts/FunLibUI.js"></script>
<script type="text/javascript" src="../scripts/bdp/AppHISUI/ASR/ASR_CommandCompany.js"></script>
<style>   
</style>

</HEAD>

<!--从主界面js传参到关联界面js-->
<server>
	s ASRCParref=%request.Get("ASRCParref")
</server>
<script type="text/javascript">
	var  ASRCParref="#(ASRCParref)#"
</script>

<BODY>
	<div class="hisui-layout" data-options="fit:true" id="layout" border="false">
		<div data-options="region:'center',split:true,collapsible:false"  border="false">
			<table data-options="fit:true,bodyCls:'panel-body-gray',iconCls:'icon-paper'" style="padding:10px;" id="grid" border="true" toolbar="#partbar" title=""></table>
		</div>
	</div>

    <!--关联标本类型工具条-->
    <div id="partbar">
		<table style="padding-top:5px;padding-bottom:5px;" >
	    	<td class="tdlabel">厂商</td>
			<td><input type="text" class="hisui-combobox" id="textCompanyDR"></td>		
			<td class="tdlabel-b">
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find',stopAllEventOnDisabled:true" id="btnSearch">查询</a>
			</td>
			<td class="tdlabel-b">
				<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-reset',stopAllEventOnDisabled:true" id="btnRefresh">重置</a>
			</td>
		</table>
		<div style="border-bottom: 1px dashed #C0C0C0;"></div>	
			<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-add',plain:true,stopAllEventOnDisabled:true" id="add_btn">新增</a>
			<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-write-order',plain:true,stopAllEventOnDisabled:true" id="update_btn">修改</a>
			<a href="#" class="hisui-linkbutton" data-options="iconCls:'icon-cancel',plain:true,stopAllEventOnDisabled:true" id="del_btn">删除</a> 
		</div>  
	</div>
	
	<!--增改弹窗-->
	<div id="myWin" style="width:375px;height:260px;padding:10px;display:none;">
		<form id="form-save" method="post" data-options="fit:true,border:false,plain:true"> 
			<table id="addtable" align="center">
				<tr style="display:none">
	    		   <td class="tdlabel">ASRCCID</td>
	    		   <td><input id="ASRCCID" name="ASRCCID"  type="text" class="textbox hisui-validatebox"  data-options=""></td>
	    		</tr>
	    		<tr style="display:none">
	    		   <td class="tdlabel">应用场景</td>
	    		   <td><input id="ASRCCScenesDR" name="ASRCCScenesDR"  type="text" class="textbox hisui-validatebox"  data-options=""></td>
	    		</tr>
	    		<tr>
	    		   <td class="tdlabel"><font color=red>*</font>厂商</td>
				   <td><input id="ASRCCCompanyDR" name="ASRCCCompanyDR" type="text" class="textbox hisui-combobox" style="width:227px" data-options="required:true"></td>
	    		</tr>
	    		<tr>
	               <td class="tdlabel"><font color=red>*</font>开始日期</td>
	    		   <td><input id="ASRCCDateFrom" name="ASRCCDateFrom" type="text" class="hisui-datebox textbox" style="width:227px" data-options="required:true"></td>
	    		 </tr>
	    		 <tr>
	               <td class="tdlabel">结束日期</td>
	    		   <td><input id="ASRCCDateTo" name="ASRCCDateTo" type="text" class="hisui-datebox textbox" style="width:227px" data-options=""></td>
	    		 </tr>
			</table>
		</form>  
	</div>
   
</BODY>
</HTML>
																						