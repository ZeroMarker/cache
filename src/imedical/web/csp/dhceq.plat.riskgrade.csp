<csp:method name=OnPreHTTP arguments="" returntype=%Boolean>
 i ##Class(websys.SessionEvents).SessionExpired() q 1
 q 1
</csp:method>
<!DOCTYPE html>
<HTML lang="en">
<HEAD>
<meta http-equiv="x-ua-compatible" content="edge">
<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
<TITLE><EXTHEALTH:TRANSLATE id=title>##(%session.Get("TITLE"))##</EXTHEALTH:TRANSLATE></TITLE>
<EXTHEALTH:HEAD></EXTHEALTH:HEAD>
<DHCEQ:HISUI></DHCEQ:HISUI>
<HISUI/>
<script type="text/javascript" src="../scripts/dhceq/plat/riskgrade.js"></script>
</head>
<body class="hisui-layout">
	<div data-options="region:'center',border:false" style="padding:10px">
		<div class="hisui-panel"  title="风险规则维护" data-options="fit:true,eqtitle:'maintitle'">
			<div class="hisui-layout" data-options="fit:true,border:false">
				<!--modify by zc0062 2020-04-03 UI评审修改  begin-->
				<div data-options="region:'north',border:false" style="border-bottom:dashed 1px #ccc;height:43px;">
				<!--modify by lmm 2019-09-09 必填项提示修改-->
				<div class="eq-table-tr">
					<div class="eq-table-td eq-table-label"><label id="cCode" for="Code">代码</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Code" ></div>
					<div class="eq-table-td eq-table-label"><label id="cDesc" for="Desc">描述</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Desc" ></div>
					<div class="eq-table-td eq-table-label"><label for="MinValue">最小值</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="MinValue" ></div>
					<div class="eq-table-td eq-table-label"><label for="MaxValue">最大值</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="MaxValue" ></div>
					<div class="eq-table-td eq-table-label"><label for="Remark">备注</label></div>
					<div class="eq-table-td eq-table-input"><input class="hisui-validatebox" id="Remark" ></div>
					<div class="eq-table-td eq-table-label"><a id="BFind" href="#" class="hisui-linkbutton" data-options="iconCls:'icon-w-find'">查询</a></div>
				</div>
				<!--modify by zc0062 2020-04-03 UI评审修改  end-->
				</div>
				<div data-options="region:'center',border:false" style="padding:0px;position:relative">
					<table id="tDHCEQCRiskGrade" data-options="border:false,fit:true"></table>
				
				</div>
			</div>
		</div>
	</div>
	<div id="hiddenEncrypt" style="width:0;height:0;">
		<form name="DHCEQ">
			
			<INPUT TYPE="hidden" id="RowID">
			<INPUT type="hidden" id="InvalidFlag">	
		</form>
	</div>
</body>

</html>
